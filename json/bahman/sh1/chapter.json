{
  "id": "b0dc6814-f4ba-4ddc-a37b-b44be8c42790",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "ca96591b-7bc9-4b2f-8f43-7efea1bbf4a9",
      "m1": "چو بهمن به تخت نیا بر نشست",
      "m2": "کمر با میان بست و بگشاد دست",
      "verse_number": 1
    },
    {
      "id": "2eafe457-af3e-4dc3-9291-417847def587",
      "m1": "سپه را درم داد و دینار داد",
      "m2": "همان کشور و مرز بسیار داد",
      "verse_number": 2
    },
    {
      "id": "29a776ab-b1a7-40f8-a61f-68eecc0ac749",
      "m1": "یکی انجمن ساخت از بخردان",
      "m2": "بزرگان و کار آزموده ردان",
      "verse_number": 3
    },
    {
      "id": "ab045eae-d98d-46df-817e-32283e226ef6",
      "m1": "چنین گفت کز کار اسفندیار",
      "m2": "ز نیک و بد گردش روزگار",
      "verse_number": 4
    },
    {
      "id": "b3123273-867b-4f11-bf86-073ebb84e4d9",
      "m1": "همه یاد دارید پیر و جوان",
      "m2": "هرانکس که هستید روشنروان",
      "verse_number": 5
    },
    {
      "id": "8ad624ef-4c9b-4e6b-bd81-2400de2c7cee",
      "m1": "که رستم گه زندگانی چه کرد",
      "m2": "همان زال افسونگر آن پیرمرد",
      "verse_number": 6
    },
    {
      "id": "132815ef-c3ac-4e5f-a3c7-881c509aefa7",
      "m1": "فرامرز جز کین ما در جهان",
      "m2": "نجوید همی آشکار و نهان",
      "verse_number": 7
    },
    {
      "id": "1d402860-2d8f-40cb-82e8-24f684e3c9d7",
      "m1": "سرم پر ز دردست و دل پر ز خون",
      "m2": "جز از کین ندارم به مغز اندرون",
      "verse_number": 8
    },
    {
      "id": "ab0e49dd-a54e-404e-88f0-c2a1e81b2b51",
      "m1": "دو جنگی چو نوشآذر و مهرنوش",
      "m2": "که از درد ایشان برآمد خروش",
      "verse_number": 9
    },
    {
      "id": "e9c12c88-8e6f-47af-a714-c96ed9d088da",
      "m1": "چو اسفندیاری که اندر جهان",
      "m2": "بدو تازه بد روزگار مهان",
      "verse_number": 10
    },
    {
      "id": "c8fc4754-512f-4000-b2c9-be12e6a27428",
      "m1": "به زابلستان زان نشان کشته شد",
      "m2": "ز دردش دد و دام سرگشته شد",
      "verse_number": 11
    },
    {
      "id": "dc9bc87f-d30a-450a-9bd6-0ae591858182",
      "m1": "همانا که بر خون اسفندیار",
      "m2": "به زاری بگرید به ایوان نگار",
      "verse_number": 12
    },
    {
      "id": "e0197303-2b92-443f-ac92-c9788d786df6",
      "m1": "هم از خون آن نامداران ما",
      "m2": "جوانان و جنگی سواران ما",
      "verse_number": 13
    },
    {
      "id": "40f65b68-41ff-4be6-bb21-5e0a97f43a7c",
      "m1": "هر آنکس که او باشد از آب پاک",
      "m2": "نیارد سر گوهر اندر مغاک",
      "verse_number": 14
    },
    {
      "id": "d00f2e81-bca1-4753-85ad-f727a5b94d31",
      "m1": "به کردار شاه آفریدون بود",
      "m2": "چو خونین بباشد همایون بود",
      "verse_number": 15
    },
    {
      "id": "7917ac86-7d81-4c0c-8223-8158bb6e0c79",
      "m1": "که ضحاک را از پی خون جم",
      "m2": "ز نامآوران جهان کرد کم",
      "verse_number": 16
    },
    {
      "id": "0692a91e-b521-4183-8d69-3870e318ca4b",
      "m1": "منوچهر با سلم و تور سترگ",
      "m2": "بیاورد ز آمل سپاهی بزرگ",
      "verse_number": 17
    },
    {
      "id": "b5a43353-6264-498f-a72e-0b1625d7738d",
      "m1": "به چین رفت و کین نیا بازخواست",
      "m2": "مرا همچنان داستانست راست",
      "verse_number": 18
    },
    {
      "id": "614c916e-1c48-41cf-8a04-16860115ed7f",
      "m1": "چو کیخسرو آمد از افراسیاب",
      "m2": "ز خون کرد گیتی چو دریای آب",
      "verse_number": 19
    },
    {
      "id": "51eba881-734c-4800-b2f8-6e1f45c3dfb3",
      "m1": "پدرم آمد و کین لهراسپ خواست",
      "m2": "ز کشته زمین کرد با کوه راست",
      "verse_number": 20
    },
    {
      "id": "3cf1b531-feb1-4cd8-b5f4-571c35f0c284",
      "m1": "فرامرز کز بهر خون پدر",
      "m2": "به خورشید تابان برآورد سر",
      "verse_number": 21
    },
    {
      "id": "9edc8f98-510d-4980-a985-c3fdb9134011",
      "m1": "به کابل شد و کین رستم بخواست",
      "m2": "همه بوم و بر کرد با خاک راست",
      "verse_number": 22
    },
    {
      "id": "859c0a7f-2262-4ec5-8cf3-aeb29062e6ea",
      "m1": "زمین را ز خون بازنشناختند",
      "m2": "همی باره بر کشتگان تاختند",
      "verse_number": 23
    },
    {
      "id": "9ce2f0e8-a0fd-4e23-847d-d4062c4a0d31",
      "m1": "به کینه سزاوارتر کس منم",
      "m2": "که بر شیر درنده اسپ افگنم",
      "verse_number": 24
    },
    {
      "id": "f3015c6a-2f32-4a8c-9f91-2325201bbf8a",
      "m1": "اگر بشمری در جهان نامدار",
      "m2": "سواری نبینی چو اسفندیار",
      "verse_number": 25
    },
    {
      "id": "505a9b8e-5c29-4696-a69f-5cffa213b0c2",
      "m1": "چه بیند و این را چه پاسخ دهید",
      "m2": "بکوشید تا رای فرخ نهید",
      "verse_number": 26
    },
    {
      "id": "f031fb18-81a3-4682-85d2-e08a8f5f8949",
      "m1": "چو بشنید گفتار بهمن سپاه",
      "m2": "هرانکس که بد شاه را نیکخواه",
      "verse_number": 27
    },
    {
      "id": "312e949a-07d6-4994-9c4b-deebadc3883f",
      "m1": "به آواز گفتند ما بندهایم",
      "m2": "همه دل به مهر تو آگندهایم",
      "verse_number": 28
    },
    {
      "id": "02ebfc0e-72cb-4970-aaa3-56619c6e2eed",
      "m1": "ز کار گذشته تو داناتری",
      "m2": "ز مردان جنگی تواناتری",
      "verse_number": 29
    },
    {
      "id": "c265aa59-117a-430b-8d80-fa1957b2e245",
      "m1": "به گیتی همان کن که کام آیدت",
      "m2": "وگر زان سخن فر و نام آیدت",
      "verse_number": 30
    },
    {
      "id": "f1fbd66e-4825-4e7c-a966-1fe9a46f4f0a",
      "m1": "نپیچد کسی سر ز فرمان تو",
      "m2": "که یارد گذشتن ز پیمان تو",
      "verse_number": 31
    },
    {
      "id": "fbcbfa4a-0786-464a-99bd-619dcd0ca67d",
      "m1": "چو پاسخ چنین یافت از لشکرش",
      "m2": "به کین اندرون تیزتر شد سرش",
      "verse_number": 32
    },
    {
      "id": "2691ea2f-b5cf-49d8-b0c0-1949f28717d6",
      "m1": "همه سیستان را بیاراستند",
      "m2": "برین بر نهادند و برخاستند",
      "verse_number": 33
    },
    {
      "id": "dcef7982-8263-4ed9-a995-a204bafabfdf",
      "m1": "به شبگیر برخاست آوای کوس",
      "m2": "شد از گرد لشکر سپهر آبنوس",
      "verse_number": 34
    },
    {
      "id": "7d0f16a6-f851-455f-b9d6-458b24394b28",
      "m1": "همی رفت زان لشکر نامدار",
      "m2": "سواران شمشیرزن صد هزار",
      "verse_number": 35
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 35
  },
  "order_in_parent": 1
}