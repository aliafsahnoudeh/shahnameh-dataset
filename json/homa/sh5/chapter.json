{
  "id": "7ff5cc2c-3e9b-480d-9f76-213d2a3fd953",
  "title": "بخش ۵",
  "verses": [
    {
      "id": "b9654da8-0274-43e8-84c1-39c6aa8e7930",
      "m1": "بگفت این و زان جایگه برگرفت",
      "m2": "ازان مرز تا روم لشکر گرفت",
      "verse_number": 1
    },
    {
      "id": "35f759f1-7a31-44d5-b1dd-993976f303f9",
      "m1": "سپهبد طلایه به داراب داد",
      "m2": "طلایه سنان را به زهر آب داد",
      "verse_number": 2
    },
    {
      "id": "ba793263-9504-47f5-880c-396d47a669c3",
      "m1": "همانگه طلایه بیامد ز روم",
      "m2": "وزین سو نگهدار این مرز و بوم",
      "verse_number": 3
    },
    {
      "id": "b2a4660f-e080-4da3-b1a4-566e464e503e",
      "m1": "زناگه دو لشکر بهم بازخورد",
      "m2": "برآمد همآنگاه گرد نبرد",
      "verse_number": 4
    },
    {
      "id": "a93d8ef1-bca2-4049-8fa2-1c0d049a7ddf",
      "m1": "همه یک به دیگر برآمیختند",
      "m2": "چو رود روان خون همی ریختند",
      "verse_number": 5
    },
    {
      "id": "c6d8440e-cc75-4686-8c97-1f546e5333f4",
      "m1": "چو داراب دید آن سپاه نبرد",
      "m2": "به پیش اندر آمد به کردار گرد",
      "verse_number": 6
    },
    {
      "id": "09a951c1-eb13-48ac-97c9-5cb06fec94f1",
      "m1": "ازان لشکر روم چندان بکشت",
      "m2": "که گفتی فلک تیغ دارد به مشت",
      "verse_number": 7
    },
    {
      "id": "4dcc6513-28d4-48a5-8ade-b574d0c4adca",
      "m1": "همی رفت زان گونه بر سان شیر",
      "m2": "نهنگی به چنگ اژدهایی به زیر",
      "verse_number": 8
    },
    {
      "id": "fb3625fe-64e3-4070-bbf1-a6b2fe362708",
      "m1": "چنین تا به لشکرگه رومیان",
      "m2": "همی تاخت بر سان شیر ژیان",
      "verse_number": 9
    },
    {
      "id": "2442f6b8-6497-4ab4-90fd-39f8f490f0da",
      "m1": "زمین شد ز رومی چو دریای خون",
      "m2": "جهانجوی را تیغ شد رهنمون",
      "verse_number": 10
    },
    {
      "id": "b2f4ed4a-e0a1-49b1-bd82-80682f7592e8",
      "m1": "به پیروزی از رومیان گشت باز",
      "m2": "به نزدیک سالار گردنفراز",
      "verse_number": 11
    },
    {
      "id": "a06cc063-3eab-4983-93e8-1435bb5bf83a",
      "m1": "بسی آفرین یافت از رشنواد",
      "m2": "که این لشکر شاه بیتو مباد",
      "verse_number": 12
    },
    {
      "id": "f50cd8d2-a35c-4eac-8451-d698f25895fe",
      "m1": "چو ما بازگردیم زین رزم روم",
      "m2": "سپاه اندر آید به آباد بوم",
      "verse_number": 13
    },
    {
      "id": "0b1eb692-f660-4d48-aa9b-a12dee1f985e",
      "m1": "تو چندان نوازش بیابی ز شاه",
      "m2": "ز اسپ و ز مهر و ز تیغ و کلاه",
      "verse_number": 14
    },
    {
      "id": "2ee63129-a6e9-4015-a33a-bf103b820520",
      "m1": "همه شب همی لشکر آراستند",
      "m2": "سلیح سواران بپیراستند",
      "verse_number": 15
    },
    {
      "id": "5c28967a-80ec-4d07-8f4a-d5155337e593",
      "m1": "چو خورشید برزد سر از تیره راغ",
      "m2": "زمین شد به کردار روشن چراغ",
      "verse_number": 16
    },
    {
      "id": "ea5a2014-6ec4-4bc7-abe3-73677ce71122",
      "m1": "بهم بازخوردند هر دو سپاه",
      "m2": "شد از گرد خورشید تابان سیاه",
      "verse_number": 17
    },
    {
      "id": "082836b5-b20c-4e55-8be2-aa5b3e122b30",
      "m1": "چو داراب پیش آمد و حمله برد",
      "m2": "عنان را به اسپ تگاور سپرد",
      "verse_number": 18
    },
    {
      "id": "1d9cf8ac-73ef-4acd-957e-2e645b235e9c",
      "m1": "به پیش صف رومیان کس نماند",
      "m2": "ز گردان شمشیرزن بس نماند",
      "verse_number": 19
    },
    {
      "id": "7f430b48-4d8b-43f9-b3b2-14e4eac27572",
      "m1": "به قلب سپاه اندر آمد چو گرگ",
      "m2": "پراگنده کرد آن سپاه بزرگ",
      "verse_number": 20
    },
    {
      "id": "7e82b584-df0d-4ff1-a4f8-8c12560c92c5",
      "m1": "وزان جایگه شد سوی میمنه",
      "m2": "بیاورد چندی سلیح و بنه",
      "verse_number": 21
    },
    {
      "id": "b64998f1-8e25-41a9-b43e-2a6fd2a30376",
      "m1": "همه لشکر روم برهم درید",
      "m2": "کسی از یلان خویشتن را ندید",
      "verse_number": 22
    },
    {
      "id": "e7342818-de47-4c92-8273-d384a7b58e96",
      "m1": "دلیران ایران به کردار شیر",
      "m2": "همی تاختند از پس اندر دلیر",
      "verse_number": 23
    },
    {
      "id": "56666392-dd26-4139-af68-502a15482c68",
      "m1": "بکشتند چندان ز رومی سپاه",
      "m2": "که گل شد ز خون خاک آوردگاه",
      "verse_number": 24
    },
    {
      "id": "d83b8e9f-3e35-471c-9708-ea010a32bfde",
      "m1": "چهل جاثلیق از دلیران بکشت",
      "m2": "بیامد صلیبی گرفته به مشت",
      "verse_number": 25
    },
    {
      "id": "a271ccb2-2c64-4b16-9d23-4171a0e6f1e8",
      "m1": "چو زو رشنواد آن شگفتی بدید",
      "m2": "ز شادی دل پهلوان بردمید",
      "verse_number": 26
    },
    {
      "id": "67d005fe-17eb-4611-9332-4f442172544d",
      "m1": "برو آفرین کرد و چندی ستود",
      "m2": "بران آفرین مهربانی فزود",
      "verse_number": 27
    },
    {
      "id": "e9911476-3ae8-4445-9e27-2edb4bae4301",
      "m1": "شب آمد جهان قیرگون شد به رنگ",
      "m2": "همی بازگشتند یکسر ز جنگ",
      "verse_number": 28
    },
    {
      "id": "59de3bf2-3109-4a40-b253-59a31db6b733",
      "m1": "سپهبد به لشکرگه رومیان",
      "m2": "برآسود و بگشاد بند میان",
      "verse_number": 29
    },
    {
      "id": "19f7a080-092e-4d30-a5f0-dcd7c7c14435",
      "m1": "ببخشید در شب بسی خواسته",
      "m2": "شد از خواسته لشکر آراسته",
      "verse_number": 30
    },
    {
      "id": "6151156a-0d04-4233-887d-15f2d9dd6e8c",
      "m1": "فرستاد نزدیک داراب کس",
      "m2": "که ای شیردل مرد فریادرس",
      "verse_number": 31
    },
    {
      "id": "d19a6407-f320-43e0-8622-08d81f8a8704",
      "m1": "نگه کن کنون تا پسند تو چیست",
      "m2": "وزی خواسته سودمند تو چیست",
      "verse_number": 32
    },
    {
      "id": "afc174a0-d377-461d-b4ec-2f9d564a3f1e",
      "m1": "نگه دار چیزی که رای آیدت",
      "m2": "ببخش آنچ دل رهنمای آیدت",
      "verse_number": 33
    },
    {
      "id": "7d90d8b7-e6b8-4715-b3cd-ea3f15af62d7",
      "m1": "هرآنچ آن پسندت نیاید ببخش",
      "m2": "تو نامیتری از خداوند رخش",
      "verse_number": 34
    },
    {
      "id": "7e6952fe-cba5-4329-8ef2-357782b4d0c1",
      "m1": "چو آن دید داراب شد شادکام",
      "m2": "یکی نیزه برداشت از بهر نام",
      "verse_number": 35
    },
    {
      "id": "5901a852-ce31-4309-ae9e-937ffc19c540",
      "m1": "فرستاد دیگر سوی رشنواد",
      "m2": "بدو گفت پیروز بادی و شاد",
      "verse_number": 36
    },
    {
      "id": "78d3cc89-5ad8-4c90-966f-076e59c3d18d",
      "m1": "چو از باختر تیره شد روی مهر",
      "m2": "بپوشید دیبای مشکین سپهر",
      "verse_number": 37
    },
    {
      "id": "7f1e55a2-bd9d-46e4-974a-d28667e0449c",
      "m1": "همان پاس از تیره شب درگذشت",
      "m2": "طلایه پراگنده بر گرد دشت",
      "verse_number": 38
    },
    {
      "id": "baab98e4-6044-423e-9ff0-da3e9c323c77",
      "m1": "غو پاسبان خاست چون زلزله",
      "m2": "همی شد چو اواز شیر یله",
      "verse_number": 39
    },
    {
      "id": "c05c6835-bfcd-4456-89df-6bbba138e105",
      "m1": "چو زرین سپر برگرفت آفتاب",
      "m2": "سر جنگجویان برآمد ز خواب",
      "verse_number": 40
    },
    {
      "id": "b67b6eb3-6267-4256-bd1e-dd615e04b5aa",
      "m1": "ببستند گردان ایران میان",
      "m2": "همی تاختند از پس رومیان",
      "verse_number": 41
    },
    {
      "id": "d8f8abab-4366-47d2-9c47-ed059ebd6d2b",
      "m1": "به شمشیر تیز آتش افروختند",
      "m2": "همه شهرها را همی سوختند",
      "verse_number": 42
    },
    {
      "id": "93638043-ed47-4949-af70-d5de609b6a39",
      "m1": "ز روم و ز رومی برانگیخت گرد",
      "m2": "کس از بوم و بر یاد دیگر نکرد",
      "verse_number": 43
    },
    {
      "id": "8e84863f-890c-4a5d-b6e0-d141aabd9566",
      "m1": "خروشی به زاری برآمد ز روم",
      "m2": "که بگذاشتند آن دلارام بوم",
      "verse_number": 44
    },
    {
      "id": "dd4b83ca-06df-4995-a74a-12c1ce279cbf",
      "m1": "به قیصر بر از کین جهان تنگ شد",
      "m2": "رخ نامدارانش بیرنگ شد",
      "verse_number": 45
    },
    {
      "id": "0adbc24a-120a-4378-b690-44aadd6e9c55",
      "m1": "فرستاده آمد بر رشنواد",
      "m2": "که گر دادگر سر نپیچد ز داد",
      "verse_number": 46
    },
    {
      "id": "02ba8689-db84-4258-9deb-c64219565b46",
      "m1": "شدند آنک جنگی بد از جنگ سیر",
      "m2": "سر بخت روم اندرآمد به زیر",
      "verse_number": 47
    },
    {
      "id": "bbf5183a-8d41-437a-acac-e6cb18229eec",
      "m1": "که گر باژ خواهید فرمان کنیم",
      "m2": "بنوی یکی باز پیمان کنیم",
      "verse_number": 48
    },
    {
      "id": "9318e08a-8b08-4423-a348-6f51a1cfd7a1",
      "m1": "فرستاد قیصر ز هر گونه چیز",
      "m2": "ابا بردهها بدره بسیار نیز",
      "verse_number": 49
    },
    {
      "id": "34789171-fb43-4de5-ba56-8e09da239591",
      "m1": "سپهبد پذیرفت زو آنچ بود",
      "m2": "ز دینار وز گوهر نابسود",
      "verse_number": 50
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 50
  },
  "order_in_parent": 5
}