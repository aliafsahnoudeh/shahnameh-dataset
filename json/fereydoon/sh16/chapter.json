{
  "id": "f9cd677f-ef45-4edd-8f2a-042d3a5e8668",
  "title": "بخش ۱۶",
  "verses": [
    {
      "id": "1d38fac4-f770-451b-86a0-e5b3c0cf0272",
      "m1": "سپیده چو از تیره شب بردمید",
      "m2": "میان شب تیره اندر خمید",
      "verse_number": 1
    },
    {
      "id": "f7e1ec00-4e5f-43fe-9348-e5279d1d2dd8",
      "m1": "منوچهر برخاست از قلبگاه",
      "m2": "ابا جوشن و تیغ و رومی کلاه",
      "verse_number": 2
    },
    {
      "id": "d38ce60b-0547-4023-b835-c87c8f132724",
      "m1": "سپه یکسره نعره برداشتند",
      "m2": "سنانها به ابر اندر افراشتند",
      "verse_number": 3
    },
    {
      "id": "a79774ca-3418-4ff6-8720-04dc1a6e8721",
      "m1": "پر از خشم سر ابروان پر ز چین",
      "m2": "همی بر نوشتند روی زمین",
      "verse_number": 4
    },
    {
      "id": "3a08e7c9-296d-459a-a459-521203e0929b",
      "m1": "چپ و راست و قلب و جناح سپاه",
      "m2": "بیاراست لشکر چو بایست شاه",
      "verse_number": 5
    },
    {
      "id": "8dcbb88a-4875-4462-84bf-1c5b87fe39d3",
      "m1": "زمین شد به کردار کشتی بر آب",
      "m2": "تو گفتی سوی غرق دارد شتاب",
      "verse_number": 6
    },
    {
      "id": "44af832f-1104-4282-9e09-d226874be8f1",
      "m1": "بزد مهره بر کوههٔ ژنده پیل",
      "m2": "زمین جنبجنبان چو دریای نیل",
      "verse_number": 7
    },
    {
      "id": "4d31e5cb-6c38-4abc-b87a-fde77211f199",
      "m1": "همان پیش پیلان تبیره زنان",
      "m2": "خروشان و جوشان و پیلان دمان",
      "verse_number": 8
    },
    {
      "id": "f8138dc5-218b-4c73-ae04-f9ceb45497dc",
      "m1": "یکی بزمگاهست گفتی بهجای",
      "m2": "ز شیپور و نالیدن کره نای",
      "verse_number": 9
    },
    {
      "id": "9b4fa4f8-861c-4d39-aedb-b7e8c9116026",
      "m1": "برفتند از جای یکسر چو کوه",
      "m2": "دهاده برآمد ز هر دو گروه",
      "verse_number": 10
    },
    {
      "id": "3ff9c871-d6b6-4998-ab63-37a8fab9099d",
      "m1": "بیابان چو دریای خون شد درست",
      "m2": "تو گفتی که روی زمین لاله رست",
      "verse_number": 11
    },
    {
      "id": "16be5a74-6a85-48f7-b556-200851dde14d",
      "m1": "پی ژنده پیلان به خون اندرون",
      "m2": "چنان چون ز بیجاده باشد ستون",
      "verse_number": 12
    },
    {
      "id": "652790b2-4d82-45e9-a332-900c4602b131",
      "m1": "همه چیرگی با منوچهر بود",
      "m2": "کزو مغز گیتی پر از مهر بود",
      "verse_number": 13
    },
    {
      "id": "392bd2f8-2252-45be-8032-43ef4806a0e6",
      "m1": "چنین تا شب تیره سر بر کشید",
      "m2": "درخشنده خورشید شد ناپدید",
      "verse_number": 14
    },
    {
      "id": "779e2227-f145-4900-a4fc-aaeef9a2f232",
      "m1": "زمانه به یک سان ندارد درنگ",
      "m2": "گهی شهد و نوش است و گاهی شرنگ",
      "verse_number": 15
    },
    {
      "id": "66d21204-4e57-49b2-bba2-42143abdb335",
      "m1": "دل تور و سلم اندر آمد بهجوش",
      "m2": "به راه شبیخون نهادند گوش",
      "verse_number": 16
    },
    {
      "id": "b69d85f7-9a6c-44ab-926c-e761e0841938",
      "m1": "چو شب روز شد کس نیامد به جنگ",
      "m2": "دو جنگی گرفتند ساز درنگ",
      "verse_number": 17
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 17
  }
}