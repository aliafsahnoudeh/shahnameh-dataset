{
  "id": "858ef13e-2174-4447-b980-280af8f729a6",
  "title": "بخش ۵",
  "verses": [
    {
      "id": "ca707baf-182b-4dc4-a997-f28d471b8a36",
      "m1": "نهفته چو بیرون کشید از نهان",
      "m2": "به سه بخش کرد آفریدون جهان",
      "verse_number": 1
    },
    {
      "id": "d2d382ae-053f-4643-878f-058ebdded356",
      "m1": "یکی روم و خاور دگر ترک و چین",
      "m2": "سیم دشت گردان و ایرانزمین",
      "verse_number": 2
    },
    {
      "id": "478568e0-0485-4a74-9a4c-3d40d9649e20",
      "m1": "نخستین به سلم اندرون بنگرید",
      "m2": "همه روم و خاور مراو را سزید",
      "verse_number": 3
    },
    {
      "id": "8aea4dba-52c9-4880-907e-3fb05f72a1df",
      "m1": "بفرمود تا لشکری برگزید",
      "m2": "گرازان سوی خاور اندرکشید",
      "verse_number": 4
    },
    {
      "id": "12a7b61b-1dcf-4980-9faa-d391de5da831",
      "m1": "به تخت کیان اندر آورد پای",
      "m2": "همی خواندندیش خاور خدای",
      "verse_number": 5
    },
    {
      "id": "987a92ba-5037-4a80-90c9-aea043ede103",
      "m1": "دگر تور را داد توران زمین",
      "m2": "ورا کرد سالار ترکان و چین",
      "verse_number": 6
    },
    {
      "id": "c4395f97-4dfb-40f6-bcd6-9c5c73137e9a",
      "m1": "یکی لشکری نامزد کرد شاه",
      "m2": "کشید آنگهی تور لشکر به راه",
      "verse_number": 7
    },
    {
      "id": "07d4765e-2a4e-4365-a4db-7cfb82c9a8a2",
      "m1": "بیامد به تخت کئی برنشست",
      "m2": "کمر بر میان بست و بگشاد دست",
      "verse_number": 8
    },
    {
      "id": "5bc5f0eb-b19a-40c7-a520-d7cc7160cd85",
      "m1": "بزرگان برو گوهر افشاندند",
      "m2": "همی پاک توران شهش خواندند",
      "verse_number": 9
    },
    {
      "id": "4b7cdfc5-ec6b-4333-8e7c-68e4555be253",
      "m1": "از ایشان چو نوبت به ایرج رسید",
      "m2": "مر او را پدر شاه ایران گزید",
      "verse_number": 10
    },
    {
      "id": "7e1fefb7-e714-4fb1-a361-44aef70975df",
      "m1": "هم ایران و هم دشت نیزهوران",
      "m2": "هم آن تخت شاهی و تاج سران",
      "verse_number": 11
    },
    {
      "id": "4121910c-c74d-4a59-afe1-111556936af2",
      "m1": "بدو داد کورا سزا بود تاج",
      "m2": "همان کرسی و مهر و آن تخت عاج",
      "verse_number": 12
    },
    {
      "id": "6e1fd5d5-3785-4475-845e-11c000aed35f",
      "m1": "نشستند هر سه به آرام و شاد",
      "m2": "چنان مرزبانان فرخ نژاد",
      "verse_number": 13
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 13
  },
  "order_in_parent": 5
}