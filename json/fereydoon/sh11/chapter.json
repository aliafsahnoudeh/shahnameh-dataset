{
  "id": "ce446d32-da28-449e-b169-a4f9685794cb",
  "title": "بخش ۱۱",
  "verses": [
    {
      "id": "26673b19-266c-4870-940f-38bc405eb8c5",
      "m1": "فریدون نهاده دو دیده به راه",
      "m2": "سپاه و کلاه آرزومند شاه",
      "verse_number": 1
    },
    {
      "id": "703139e5-db4a-43fc-9e92-96b3cfce61fa",
      "m1": "چو هنگام برگشتن شاه بود",
      "m2": "پدر زان سخن خود کی آگاه بود",
      "verse_number": 2
    },
    {
      "id": "5237ffb8-d805-4225-b60a-54c524916a8c",
      "m1": "همی شاه را تخت پیروزه ساخت",
      "m2": "همی تاج را گوهر اندر نشاخت",
      "verse_number": 3
    },
    {
      "id": "da6eb48e-d6d5-4b5e-b636-d86cefaca1ed",
      "m1": "پذیره شدن را بیاراستند",
      "m2": "می و رود و رامشگران خواستند",
      "verse_number": 4
    },
    {
      "id": "9a37a8dc-1d69-4734-a5ed-15dbda0158c3",
      "m1": "تبیره ببردند و پیل از درش",
      "m2": "ببستند آذین به هر کشورش",
      "verse_number": 5
    },
    {
      "id": "31f68c82-b5c1-4726-8eb6-2f95f79f9b36",
      "m1": "به زین اندرون بود شاه و سپاه",
      "m2": "یکی گرد تیره برآمد ز راه",
      "verse_number": 6
    },
    {
      "id": "51513e3a-7439-496b-aaec-3c26e513ba38",
      "m1": "هیونی برون آمد از تیره گرد",
      "m2": "نشسته برو سوگواری به درد",
      "verse_number": 7
    },
    {
      "id": "43b96007-83e0-45cd-a7ad-8fe0356a8d34",
      "m1": "خروشی برآورد دل سوگوار",
      "m2": "یکی زر تابوتش اندر کنار",
      "verse_number": 8
    },
    {
      "id": "a8ddc83e-ce65-49a8-9300-1e3c7dac3d90",
      "m1": "به تابوت زر اندرون پرنیان",
      "m2": "نهاده سر ایرج اندر میان",
      "verse_number": 9
    },
    {
      "id": "515209c2-9f83-441f-8ad6-4c7e309c2b5f",
      "m1": "ابا ناله و آه و با روی زرد",
      "m2": "به پیش فریدون شد آن شوخ مرد",
      "verse_number": 10
    },
    {
      "id": "48ab3b97-d369-483a-b1b5-42d86779719d",
      "m1": "ز تابوت زر تخته برداشتند",
      "m2": "که گفتار او خوار پنداشتند",
      "verse_number": 11
    },
    {
      "id": "01de47fe-7c4e-4f2c-832a-9deaa0254395",
      "m1": "ز تابوت چون پرنیان برکشید",
      "m2": "سر ایرج آمد بریده پدید",
      "verse_number": 12
    },
    {
      "id": "5909b628-6851-42a4-a7af-d99091fb5a36",
      "m1": "بیافتاد ز اسپ آفریدون به خاک",
      "m2": "سپه سر به سر جامه کردند چاک",
      "verse_number": 13
    },
    {
      "id": "99d8f1c4-3063-439f-8e83-1702c8537282",
      "m1": "سیه شد رخ و دیدگان شد سپید",
      "m2": "که دیدن دگرگونه بودش امید",
      "verse_number": 14
    },
    {
      "id": "072f585a-33ec-4a1b-a836-1a6151da2b76",
      "m1": "چو خسرو برانگونه آمد ز راه",
      "m2": "چنین بازگشت از پذیره سپاه",
      "verse_number": 15
    },
    {
      "id": "7e4de91d-a0c1-43b8-a4e3-832eea9836dc",
      "m1": "دریده درفش و نگونسار کوس",
      "m2": "رخ نامداران به رنگ آبنوس",
      "verse_number": 16
    },
    {
      "id": "8cf25bd4-3075-41e6-a044-6eefbb28a550",
      "m1": "تبیره سیه کرده و روی پیل",
      "m2": "پراکنده بر تازی اسپانش نیل",
      "verse_number": 17
    },
    {
      "id": "0f4f6849-d07d-4f2a-a6ee-19f4ebc05fc1",
      "m1": "پیاده سپهبد پیاده سپاه",
      "m2": "پر از خاک سر برگرفتند راه",
      "verse_number": 18
    },
    {
      "id": "5aecb519-2dd8-432a-b0f9-cdabe31a50c6",
      "m1": "خروشیدن پهلوانان به درد",
      "m2": "کنان گوشت تن را بران رادمرد",
      "verse_number": 19
    },
    {
      "id": "c27b23ac-9645-4bc9-b9e5-eb0147b2778b",
      "m1": "برین گونه گردد به ما بر سپهر",
      "m2": "بخواهد ربودن چو بنمود چهر",
      "verse_number": 20
    },
    {
      "id": "4eb54b72-2783-4f96-8d45-274b3e647859",
      "m1": "مبر خود به مهر زمانه گمان",
      "m2": "نه نیکو بود راستی در کمان",
      "verse_number": 21
    },
    {
      "id": "f6955305-9be3-4b36-98a7-6092f5f7a076",
      "m1": "چو دشمنش گیری نمایدت مهر",
      "m2": "و گر دوست خوانی نبینیش چهر",
      "verse_number": 22
    },
    {
      "id": "4143ca5c-7975-4a72-aa15-0ddfbf97c1ce",
      "m1": "یکی پند گویم ترا من درست",
      "m2": "دل از مهر گیتی ببایدت شست",
      "verse_number": 23
    },
    {
      "id": "f640eba2-5aba-4505-84a1-682ae6f5266d",
      "m1": "سپه داغ دل شاه با های و هوی",
      "m2": "سوی باغ ایرج نهادند روی",
      "verse_number": 24
    },
    {
      "id": "0372b9a2-c077-43cb-8537-6757346ec9ef",
      "m1": "به روزی کجا جشن شاهان بدی",
      "m2": "وزان پیشتر بزمگاهان بدی",
      "verse_number": 25
    },
    {
      "id": "1fe629ba-399b-420b-95f1-5d447a7ef676",
      "m1": "فریدون سر شاه پور جوان",
      "m2": "بیامد ببر برگرفته نوان",
      "verse_number": 26
    },
    {
      "id": "0d8d408e-c888-4822-99a2-15a3bf3b24a5",
      "m1": "بر آن تخت شاهنشهی بنگرید",
      "m2": "سر شاه را نزدر تاج دید",
      "verse_number": 27
    },
    {
      "id": "973de6cd-7337-4c97-b225-4a06657962c1",
      "m1": "همان حوض شاهان و سرو سهی",
      "m2": "درخت گلفشان و بید و بهی",
      "verse_number": 28
    },
    {
      "id": "d6a98544-10e8-4a1c-91d3-534817df2d2e",
      "m1": "تهی دید از آزادگان جشنگاه",
      "m2": "به کیوان برآورده گرد سیاه",
      "verse_number": 29
    },
    {
      "id": "021deca7-439a-4486-a173-8e4aef3aefb6",
      "m1": "همی سوخت باغ و همی خست روی",
      "m2": "همی ریخت اشک و همی کند موی",
      "verse_number": 30
    },
    {
      "id": "303ec98c-9246-4b83-a8f8-3df22f4a5a47",
      "m1": "میان را به زنار خونین ببست",
      "m2": "فکند آتش اندر سرای نشست",
      "verse_number": 31
    },
    {
      "id": "3cbef1c6-11dd-4d20-80fe-753ec015b3ae",
      "m1": "گلستانش برکند و سروان بسوخت",
      "m2": "به یکبارگی چشم شادی بدوخت",
      "verse_number": 32
    },
    {
      "id": "a67f6d51-ea40-4bd9-b4b6-65aed23c81d3",
      "m1": "نهاده سر ایرج اندر کنار",
      "m2": "سر خویشتن کرد زی کردگار",
      "verse_number": 33
    },
    {
      "id": "5e9d5c9e-9aaf-470b-81aa-2902060d77d0",
      "m1": "همی گفت کای داور دادگر",
      "m2": "بدین بیگنه کشته اندر نگر",
      "verse_number": 34
    },
    {
      "id": "a049291a-3dbd-4a36-872d-7bfd69462f60",
      "m1": "به خنجر سرش کنده در پیش من",
      "m2": "تنش خورده شیران آن انجمن",
      "verse_number": 35
    },
    {
      "id": "de271018-c2b5-4149-b177-c995d70c8a9b",
      "m1": "دل هر دو بیداد از آن سان بسوز",
      "m2": "که هرگز نبینند جز تیره روز",
      "verse_number": 36
    },
    {
      "id": "92a06401-85f4-446e-9676-f315e978b81e",
      "m1": "به داغی جگرشان کنی آژده",
      "m2": "که بخشایش آرد بریشان دده",
      "verse_number": 37
    },
    {
      "id": "aa947d6f-1916-47e8-a58a-a2e35ab2ce6c",
      "m1": "همی خواهم از روشن کردگار",
      "m2": "که چندان زمان یابم از روزگار",
      "verse_number": 38
    },
    {
      "id": "2c71058d-9b07-42d6-bede-3c1c843726df",
      "m1": "که از تخم ایرج یکی نامور",
      "m2": "بیاید برین کین ببندد کمر",
      "verse_number": 39
    },
    {
      "id": "dc02e4f6-1b06-444f-95dd-b6c4bd19e98b",
      "m1": "چو دیدم چنین زان سپس شایدم",
      "m2": "اگر خاک بالا بپیمایدم",
      "verse_number": 40
    },
    {
      "id": "f5228f47-a2f1-4149-8b18-f6b5f78fd5fb",
      "m1": "برینگونه بگریست چندان بزار",
      "m2": "همی تا گیا رستش اندر کنار",
      "verse_number": 41
    },
    {
      "id": "474135f9-a512-489a-b2f4-fbcd7fbdf69e",
      "m1": "زمین بستر و خاک بالین او",
      "m2": "شده تیره روشن جهانبین او",
      "verse_number": 42
    },
    {
      "id": "a7ecabca-62eb-4ba4-a0c0-42c6ffef2e6b",
      "m1": "در بار بسته گشاده زبان",
      "m2": "همی گفت کای داور راستان",
      "verse_number": 43
    },
    {
      "id": "76135d46-7f7a-43b1-842a-9809a8a2fda6",
      "m1": "کس از تاجداران بدینسان نمرد",
      "m2": "که مردست این نامبردار گرد",
      "verse_number": 44
    },
    {
      "id": "5f5a3540-e4e8-4e46-869c-9e8cab460025",
      "m1": "سرش را بریده به زار اهرمن",
      "m2": "تنش را شده کام شیران کفن",
      "verse_number": 45
    },
    {
      "id": "be3aa0de-ab40-497d-87b9-5c119adcdfe1",
      "m1": "خروشی به زاری و چشمی پرآب",
      "m2": "ز هر دام و دد برده آرام و خواب",
      "verse_number": 46
    },
    {
      "id": "218fc702-4ab2-4d87-9882-09a2789c72df",
      "m1": "سراسر همه کشورش مرد و زن",
      "m2": "به هر جای کرده یکی انجمن",
      "verse_number": 47
    },
    {
      "id": "f164db4c-59bb-42bd-8498-bcb5496fcd05",
      "m1": "همه دیده پرآب و دل پر ز خون",
      "m2": "نشسته به تیمار و گرم اندرون",
      "verse_number": 48
    },
    {
      "id": "d5363302-1c6c-4c2a-9c76-c995973d3f75",
      "m1": "همه جامه کرده کبود و سیاه",
      "m2": "نشسته به اندوه در سوگ شاه",
      "verse_number": 49
    },
    {
      "id": "6133be65-dfde-48e7-ae06-2bd8fb5fb827",
      "m1": "چه مایه چنین روز بگذاشتند",
      "m2": "همه زندگی مرگ پنداشتند",
      "verse_number": 50
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 50
  }
}