{
  "id": "ce1030b0-6f70-47d0-a8af-37391553cbf5",
  "title": "بخش ۸",
  "verses": [
    {
      "id": "87ab9f87-d3e8-42b6-b10e-e0119e4a10e4",
      "m1": "یکی نامه بنوشت شاه زمین",
      "m2": "به خاور خدای و به سالار چین",
      "verse_number": 1
    },
    {
      "id": "5de309be-82d8-4b71-8c67-b5177f86a9d5",
      "m1": "سر نامه کرد آفرین خدای",
      "m2": "کجا هست و باشد همیشه به جای",
      "verse_number": 2
    },
    {
      "id": "5e39bdb1-ab74-4b53-b51f-85b432973d16",
      "m1": "چنین گفت کاین نامهٔ پندمند",
      "m2": "به نزد دو خورشید گشته بلند",
      "verse_number": 3
    },
    {
      "id": "73a146c7-cd50-4120-837e-06df490c8340",
      "m1": "دو سنگی دو جنگی دو شاه زمین",
      "m2": "میان کیان چون درخشان نگین",
      "verse_number": 4
    },
    {
      "id": "4396c552-77fb-433d-bdb0-3ee1585d938b",
      "m1": "از آنکو ز هر گونه دیده جهان",
      "m2": "شده آشکارا برو بر نهان",
      "verse_number": 5
    },
    {
      "id": "aff282b0-d999-4946-8354-3c8aeabb8c8e",
      "m1": "گرایندهٔ تیغ و گرز گران",
      "m2": "فروزندهٔ نامدار افسران",
      "verse_number": 6
    },
    {
      "id": "2fb5da29-07aa-4be6-85cb-74687cc2ab30",
      "m1": "نمایندهٔ شب به روز سپید",
      "m2": "گشایندهٔ گنج پیش امید",
      "verse_number": 7
    },
    {
      "id": "f9f11f95-52d6-4adb-b423-9ff0dd99b6a6",
      "m1": "همه رنجها گشته آسان بدوی",
      "m2": "برو روشنی اندر آورده روی",
      "verse_number": 8
    },
    {
      "id": "8632cc43-9a8e-4424-878f-178b1a83f8d3",
      "m1": "نخواهم همی خویشتن را کلاه",
      "m2": "نه آگنده گنج و نه تاج و نه گاه",
      "verse_number": 9
    },
    {
      "id": "16687e02-d32a-4986-9bfb-75fecadcab4b",
      "m1": "سه فرزند را خواهم آرام و ناز",
      "m2": "از آن پس که دیدیم رنج دراز",
      "verse_number": 10
    },
    {
      "id": "3bc7439e-3cc8-41c3-bac9-496d76d8492b",
      "m1": "برادر کزو بود دلتان به درد",
      "m2": "وگر چند هرگز نزد باد سرد",
      "verse_number": 11
    },
    {
      "id": "4e3558d3-ef0e-4f40-a8a8-49d6aee0964b",
      "m1": "دوان آمد از بهر آزارتان",
      "m2": "که بود آرزومند دیدارتان",
      "verse_number": 12
    },
    {
      "id": "c8a7a90c-409b-402d-925a-496eb0f1a39a",
      "m1": "بیفگند شاهی شما را گزید",
      "m2": "چنان کز ره نامداران سزید",
      "verse_number": 13
    },
    {
      "id": "e63f9d31-f450-44f2-9272-476bf69b91d6",
      "m1": "ز تخت اندر آمد به زین برنشست",
      "m2": "برفت و میان بندگی را ببست",
      "verse_number": 14
    },
    {
      "id": "e307bd0d-e08f-4222-ae8c-7541b85dd887",
      "m1": "بدان کو به سال از شما کهترست",
      "m2": "نوازیدن کهتر اندر خورست",
      "verse_number": 15
    },
    {
      "id": "af366892-aa62-457b-a2db-8c8df6c59496",
      "m1": "گرامیش دارید و نوشه خورید",
      "m2": "چو پرورده شد تن روان پرورید",
      "verse_number": 16
    },
    {
      "id": "0972e0ad-7ef1-4dd0-ae0b-dc7f49381779",
      "m1": "چو از بودنش بگذرد روز چند",
      "m2": "فرستید با زی منش ارجمند",
      "verse_number": 17
    },
    {
      "id": "cd59a515-9a5d-46be-8825-f25adc8656f6",
      "m1": "نهادند بر نامه بر مهر شاه",
      "m2": "ز ایوان بر ایرج گزین کرد راه",
      "verse_number": 18
    },
    {
      "id": "91f2d40e-e840-48fc-9c7a-05daa9cb0f53",
      "m1": "بشد با تنی چند برنا و پیر",
      "m2": "چنان چون بود راه را ناگزیر",
      "verse_number": 19
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 19
  }
}