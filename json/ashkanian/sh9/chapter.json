{
  "id": "ae9b4384-f04d-4f5d-97ea-d5164d441186",
  "title": "بخش ۹",
  "verses": [
    {
      "id": "d7dabf59-a98d-4b59-b1b4-ae8017e5b018",
      "m1": "چو شب روز شد بامداد پگاه",
      "m2": "بفرمود تا بازگردد سپاه",
      "verse_number": 1
    },
    {
      "id": "142a1e5b-aa0a-48b9-964f-1f6ae09fe345",
      "m1": "بیامد دو رخساره همرنگ نی",
      "m2": "چو شب تیره گشت اندر آمد بری",
      "verse_number": 2
    },
    {
      "id": "f319180c-02a4-4f3c-a241-61dc70b22038",
      "m1": "یکی نامه بنوشت نزد پسر",
      "m2": "که کژی به باغ اندر آورد بر",
      "verse_number": 3
    },
    {
      "id": "10a3d0a7-7a11-4fa2-a389-48ee9ef6f24c",
      "m1": "چنان شد ز بالین ما اردشیر",
      "m2": "کزان سان نجست از کمان ایچ تیر",
      "verse_number": 4
    },
    {
      "id": "88ca3528-53cf-4bf9-bb05-313b098dbe64",
      "m1": "سوی پارس آمد بجویش نهان",
      "m2": "مگوی این سخن با کسی در جهان",
      "verse_number": 5
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 5
  },
  "order_in_parent": 9
}