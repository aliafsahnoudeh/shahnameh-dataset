{
  "id": "8b43a7c0-ec64-4bbf-bff8-b2921cee639e",
  "title": "بخش ۶",
  "verses": [
    {
      "id": "2aeaf5e0-2cad-4c40-a252-c1109daf1aa3",
      "m1": "یکی کاخ بود اردوان را بلند",
      "m2": "به کاخ اندرون بندهای ارجمند",
      "verse_number": 1
    },
    {
      "id": "962bfdd5-e073-490e-b512-9379e0fb912a",
      "m1": "که گلنار بد نام آن ماهروی",
      "m2": "نگاری پر از گوهر و رنگ و بوی",
      "verse_number": 2
    },
    {
      "id": "11118695-c296-4680-9510-bfc1863d21ec",
      "m1": "بر اردوان همچو دستور بود",
      "m2": "بران خواسته نیز گنجور بود",
      "verse_number": 3
    },
    {
      "id": "cacdcb21-f88b-4d7c-93df-2cb0010d5a69",
      "m1": "بروبر گرامیتر از جان بدی",
      "m2": "به دیدار او شاد و خندان بدی",
      "verse_number": 4
    },
    {
      "id": "e2c47086-c4e3-4b26-b77f-c915a3994107",
      "m1": "چنان بد که روزی برآمد به بام",
      "m2": "دلش گشت زان خرمی شادکام",
      "verse_number": 5
    },
    {
      "id": "8e003a59-7b8c-4ee9-975e-5648d2417696",
      "m1": "نگه کرد خندان لب اردشیر",
      "m2": "جوان در دل ماه شد جایگیر",
      "verse_number": 6
    },
    {
      "id": "0c00b453-ee39-4afb-9e6d-5002a9237719",
      "m1": "همی بود تا روز تاریک شد",
      "m2": "همانا به شب روز نزدیک شد",
      "verse_number": 7
    },
    {
      "id": "510dd2b2-4547-4af0-8688-2ce981899898",
      "m1": "کمندی بران کنگره بر ببست",
      "m2": "گره زد برو چند و ببسود دست",
      "verse_number": 8
    },
    {
      "id": "58aa0e75-4a6c-45a9-9787-3bad94008d86",
      "m1": "به گستاخی از باره آمد فرود",
      "m2": "همی داد نیکی دهش را درود",
      "verse_number": 9
    },
    {
      "id": "82d764d0-6cd4-4888-9dd2-5f3b336d3169",
      "m1": "بیامد خرامان بر اردشیر",
      "m2": "پر از گوهر و بوی مشک و عبیر",
      "verse_number": 10
    },
    {
      "id": "75643012-d061-4f52-bdc6-029715659123",
      "m1": "ز بالین دیبا سرش برگرفت",
      "m2": "چو بیدار شد تنگ در بر گرفت",
      "verse_number": 11
    },
    {
      "id": "a48bae8a-377a-4a9e-9737-44654bb1d705",
      "m1": "نگه کرد برنا بران خوبروی",
      "m2": "بدان موی و آن روی و آن رنگ و بوی",
      "verse_number": 12
    },
    {
      "id": "d4129351-4c21-4403-ae24-936c47d3f0b1",
      "m1": "بدان ماه گفت از کجا خاستی",
      "m2": "که پرغم دلم را بیاراستی",
      "verse_number": 13
    },
    {
      "id": "c73dc141-189f-4dd5-8148-c06faa522426",
      "m1": "چنین داد پاسخ که من بندهام",
      "m2": "ز گیتی به دیدار تو زندهام",
      "verse_number": 14
    },
    {
      "id": "ff3b58cf-eed1-4414-91c7-cf9afb337237",
      "m1": "دلارام گنجور شاه اردوان",
      "m2": "که از من بود شاد و روشنروان",
      "verse_number": 15
    },
    {
      "id": "f1a5725d-72fe-457a-be24-67727bd875c6",
      "m1": "کنون گر پذیری ترا بندهام",
      "m2": "دل و جان به مهر تو آگندهام",
      "verse_number": 16
    },
    {
      "id": "67bb41c7-bc98-4c9f-9fd8-77e39f7712cd",
      "m1": "بیایم چو خواهی به نزدیک تو",
      "m2": "درفشان کنم روز تاریک تو",
      "verse_number": 17
    },
    {
      "id": "2d61d1cb-7a21-481e-8524-5437c7634071",
      "m1": "چو لختی برآمد برین روزگار",
      "m2": "شکست اندر آمد به آموزگار",
      "verse_number": 18
    },
    {
      "id": "91d491f1-9db4-4074-af6a-73e05d66bdc7",
      "m1": "جهاندیده بیدار بابک بمرد",
      "m2": "سرای کهن دیگری را سپرد",
      "verse_number": 19
    },
    {
      "id": "006a7614-7182-417c-9e1b-c95d617f2921",
      "m1": "چو آگاهی آمد سوی اردوان",
      "m2": "پر از غم شد و تیره گشتش روان",
      "verse_number": 20
    },
    {
      "id": "322d410d-890f-455d-ab22-0428850acf96",
      "m1": "گرفتند هر مهتری یاد پارس",
      "m2": "سپهبد به مهتر پسر داد پارس",
      "verse_number": 21
    },
    {
      "id": "4fea9a9f-45ae-4aa3-ad03-844f615bbd85",
      "m1": "بفرمود تا کوس بیرون برند",
      "m2": "ز درگاه لشکر به هامون برند",
      "verse_number": 22
    },
    {
      "id": "9f80ad38-98f2-41fe-9681-ac41cebf3992",
      "m1": "جهان تیره شد بر دل اردشیر",
      "m2": "ازان پیر روشندل و دستگیر",
      "verse_number": 23
    },
    {
      "id": "d804817e-af04-4158-8186-bff6c01bd0a6",
      "m1": "دل از لشکر اردوان برگرفت",
      "m2": "وزان آگهی رای دیگر گرفت",
      "verse_number": 24
    },
    {
      "id": "3578a6df-2abe-4d7b-8325-e82898ba0bda",
      "m1": "که از درد او بد دلش پرستیز",
      "m2": "به هر سو همی جست راه گریز",
      "verse_number": 25
    },
    {
      "id": "262f3d2b-cba2-4692-b1c7-7fc6403b5e9a",
      "m1": "ازان پس چنان بد که شاه اردوان",
      "m2": "ز اخترشناسان روشنروان",
      "verse_number": 26
    },
    {
      "id": "c39d6bdc-93c3-4309-9cd0-54f7fcfa9a59",
      "m1": "بیاورد چندی به درگاه خویش",
      "m2": "همی بازجست اختر و راه خویش",
      "verse_number": 27
    },
    {
      "id": "dfbe92c0-6189-488f-86eb-d20cdddfc29b",
      "m1": "همان نیز تا گردش روزگار",
      "m2": "ازان پس کرا باشد آموزگار",
      "verse_number": 28
    },
    {
      "id": "b3860738-9d77-4eaf-907d-ff5fe425979d",
      "m1": "فرستادشان نزد گلنار شاه",
      "m2": "بدان تا کنند اختران را نگاه",
      "verse_number": 29
    },
    {
      "id": "9362b25c-8405-4180-8158-d021b1fb73bf",
      "m1": "سه روز اندر آن کار شد روزگار",
      "m2": "نگه کرده شد طالع شهریار",
      "verse_number": 30
    },
    {
      "id": "e3abd358-799f-44d1-979e-a3531694bf75",
      "m1": "چو گنجور بشنید آوازشان",
      "m2": "سخن گفتن از طالع و رازشان",
      "verse_number": 31
    },
    {
      "id": "5ae2b2ef-25d7-4b19-948e-ec821272a429",
      "m1": "سیم روز تا شب گذشته سه پاس",
      "m2": "کنیزک بپردخت ز اخترشناس",
      "verse_number": 32
    },
    {
      "id": "9fe7c417-d78e-478d-9a77-5a6eddd02715",
      "m1": "پر از آرزو دل لبان پر ز باد",
      "m2": "همی داشت گفتار ایشان به یاد",
      "verse_number": 33
    },
    {
      "id": "7e4a6bd8-aaed-4b72-b38f-0da950e44f85",
      "m1": "چهارم بشد مرد روشنروان",
      "m2": "که بگشاید آن راز با اردوان",
      "verse_number": 34
    },
    {
      "id": "372952da-22d4-458b-86c0-65bbc2e575b7",
      "m1": "برفتند با زیجها برکنار",
      "m2": "ز کاخ کنیزک بر شهریار",
      "verse_number": 35
    },
    {
      "id": "12d8ba0b-5134-4f0a-a279-51b5f2d18e95",
      "m1": "بگفتند راز سپهر بلند",
      "m2": "همان حکم او بر چه و چون و چند",
      "verse_number": 36
    },
    {
      "id": "f9e11767-5239-4724-adf8-396fd6ca6757",
      "m1": "کزین پس کنون تانه بس روزگار",
      "m2": "ز چیزی بپیچد دل نامدار",
      "verse_number": 37
    },
    {
      "id": "86834e48-ebd6-4f74-9185-061e6aee65d2",
      "m1": "که بگریزد از مهتری کهتری",
      "m2": "سپهبد نژادی و کنداوری",
      "verse_number": 38
    },
    {
      "id": "d246e90f-e0d4-4260-9f2d-8d236da92050",
      "m1": "وزان پس شود شهریاری بلند",
      "m2": "جهاندار و نیکاختر و سودمند",
      "verse_number": 39
    },
    {
      "id": "41042cc7-708a-4ae7-b563-fcbb15c1ea0f",
      "m1": "دل نامور مهتر نیکبخت",
      "m2": "ز گفتار ایشان غمی گشت سخت",
      "verse_number": 40
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 40
  },
  "order_in_parent": 6
}