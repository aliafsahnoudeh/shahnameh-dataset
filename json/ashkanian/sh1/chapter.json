{
  "id": "ee6469c0-e7d3-4175-af05-0f8daf05e56e",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "fb55f1ce-a95b-4e57-b91b-cc61bc3af1a0",
      "m1": "کنون پادشاه جهان را ستای",
      "m2": "به رزم و به بزم و به دانش گرای",
      "verse_number": 1
    },
    {
      "id": "201e4c30-b8e6-488e-92d0-caf6cdd99178",
      "m1": "سرافراز محمود فرخندهرای",
      "m2": "کزویست نام بزرگی به جای",
      "verse_number": 2
    },
    {
      "id": "5f6e097a-f091-49f6-ae21-3d06e13e8f1b",
      "m1": "جهاندار ابوالقاسم پر خرد",
      "m2": "که رایش همی از خرد برخورد",
      "verse_number": 3
    },
    {
      "id": "102d34a1-130c-42af-a9ea-60c88ed9d68e",
      "m1": "همی باد تا جاودان شاد دل",
      "m2": "ز رنج و ز غم گشته آزاد دل",
      "verse_number": 4
    },
    {
      "id": "454959de-cc45-4fcc-ac4c-225fff231521",
      "m1": "شهنشاه ایران و زابلستان",
      "m2": "ز قنوج تا مرز کابلستان",
      "verse_number": 5
    },
    {
      "id": "19023214-4f63-4d1e-a151-e2e3abdc2e87",
      "m1": "برو آفرین باد و بر لشکرش",
      "m2": "چه بر خویش و بر دوده و کشورش",
      "verse_number": 6
    },
    {
      "id": "a3235842-0c28-498b-b1a6-22b3795db257",
      "m1": "جهاندار سالار او میر نصر",
      "m2": "کزو شادمانست گردنده عصر",
      "verse_number": 7
    },
    {
      "id": "4a4e4eb9-5acf-4c70-8e74-901f21034d76",
      "m1": "دریغش نیاید ز بخشیدن ایچ",
      "m2": "نه آرام گیرد به روز بسیچ",
      "verse_number": 8
    },
    {
      "id": "28e1fa6c-5f80-4ae8-ae5c-a2396f99b879",
      "m1": "چو جنگ آیدش پیش جنگ آورد",
      "m2": "سر شهریاران به چنگ آورد",
      "verse_number": 9
    },
    {
      "id": "19d73698-dd53-4cef-9651-afa88746f7af",
      "m1": "برآنکس که بخشش کند گنج خویش",
      "m2": "ببخشد نهاندیشد از رنج خویش",
      "verse_number": 10
    },
    {
      "id": "f03b6beb-d172-4664-8d0f-61628ac527b4",
      "m1": "جهان تاجهاندار محمود باد",
      "m2": "وزو بخشش و داد موجود باد",
      "verse_number": 11
    },
    {
      "id": "147d5d49-dc44-4a0a-8949-37763abd60d8",
      "m1": "سپهدار چون بوالمظفر بود",
      "m2": "سرلشکر از ماه برتر بود",
      "verse_number": 12
    },
    {
      "id": "f5fe0e17-7fe0-4bac-921f-4e0f90d343c5",
      "m1": "که پیروز نامست و پیروزبخت",
      "m2": "همی بگذرد تیر او بر درخت",
      "verse_number": 13
    },
    {
      "id": "0b86643b-6e21-4c5f-91a6-a68316374246",
      "m1": "همیشه تن شاه بیرنج باد",
      "m2": "نشستش همه بر سر گنج باد",
      "verse_number": 14
    },
    {
      "id": "a0b1fa18-ad93-4ddd-99d9-b83c729b7950",
      "m1": "همیدون سپهدار او شاد باد",
      "m2": "دلش روشن و گنجش آباد باد",
      "verse_number": 15
    },
    {
      "id": "f068d064-94d8-4fed-9260-cc303d08bd37",
      "m1": "چنین تا به پایست گردان سپهر",
      "m2": "ازین تخمه هرگز مبراد مهر",
      "verse_number": 16
    },
    {
      "id": "01a659c7-7fd4-4b96-ae0f-21e736af603c",
      "m1": "پدر بر پدر بر پسر بر پسر",
      "m2": "همه تاجدارند و پیروزگر",
      "verse_number": 17
    },
    {
      "id": "e38b3afb-c2ce-4a73-b88f-331b1bac5ea9",
      "m1": "گذشته ز شوال ده با چهار",
      "m2": "یکی آفرین باد بر شهریار",
      "verse_number": 18
    },
    {
      "id": "ff944be3-6740-4eb1-a869-352f589c80e2",
      "m1": "کزین مژده دادیم رسم خراج",
      "m2": "که فرمان بد از شاه با فر و تاج",
      "verse_number": 19
    },
    {
      "id": "85613e4d-e60f-43a6-bd74-ada12c557fbe",
      "m1": "که سالی خراجی نخواهند بیش",
      "m2": "ز دیندار بیدار وز مرد کیش",
      "verse_number": 20
    },
    {
      "id": "ac739320-b410-44c6-b7ce-72ae466a2e3f",
      "m1": "بدین عهد نوشینروان تازه شد",
      "m2": "همه کار بر دیگر اندازه شد",
      "verse_number": 21
    },
    {
      "id": "d00baeb6-f0e8-42da-8632-3e3b133c2c16",
      "m1": "چو آمد بران روزگاری دراز",
      "m2": "همی بفگند چادر داد باز",
      "verse_number": 22
    },
    {
      "id": "f4d3800e-67c1-4660-afd9-90d95763a423",
      "m1": "ببینی بدین داد و نیکی گمان",
      "m2": "که او خلعتی یابد از آسمان",
      "verse_number": 23
    },
    {
      "id": "6cbaa475-877c-4fea-b45b-d274ee78eb7a",
      "m1": "که هرگز نگردد کهن بر برش",
      "m2": "بماند کلاه کیان بر سرش",
      "verse_number": 24
    },
    {
      "id": "6a91f87d-42fe-4065-ac59-a826baca0f07",
      "m1": "سرش سبز باد و تنش بیگزند",
      "m2": "منش برگذشته ز چرخ بلند",
      "verse_number": 25
    },
    {
      "id": "2cc0f436-177e-4d8f-adf4-2bcc60e15592",
      "m1": "ندارد کسی خوار فال مرا",
      "m2": "کجا بشمرد ماه و سال مرا",
      "verse_number": 26
    },
    {
      "id": "9accba74-7eac-4dbf-b944-c309f3f082bb",
      "m1": "نگه کن که این نامه تا جاودان",
      "m2": "درفشی بود بر سر بخردان",
      "verse_number": 27
    },
    {
      "id": "9c372fab-c38e-4c78-a008-df1e7f28cf79",
      "m1": "بماند بسی روزگاران چنین",
      "m2": "که خوانند هرکس برو آفرین",
      "verse_number": 28
    },
    {
      "id": "390e59cd-20ff-4b9d-9cbc-b1434cdbcdfc",
      "m1": "چنین گفت نوشین روان قباد",
      "m2": "که چون شاه را دل بپیچد ز داد",
      "verse_number": 29
    },
    {
      "id": "cd7dd8dd-a8c6-48db-850d-472d46c33dd0",
      "m1": "کند چرخ منشور او را سیاه",
      "m2": "ستاره نخواند ورا نیز شاه",
      "verse_number": 30
    },
    {
      "id": "6e23bc4e-7aa5-48be-8167-c00d6965a26f",
      "m1": "ستم نامهٔ عزل شاهان بود",
      "m2": "چو درد دل بیگناهان بود",
      "verse_number": 31
    },
    {
      "id": "50f69e7d-ff56-496f-a1a3-6c41661a9a43",
      "m1": "بماناد تا جاودان این گهر",
      "m2": "هنرمند و بادانش و دادگر",
      "verse_number": 32
    },
    {
      "id": "22bda501-cb82-4eda-a584-de64591310bd",
      "m1": "نباشد جهان بر کسی پایدار",
      "m2": "همه نام نیکو بود یادگار",
      "verse_number": 33
    },
    {
      "id": "6484ef42-fb6d-409e-916b-7c45d4b3a532",
      "m1": "کجا شد فریدون و ضحاک و جم",
      "m2": "مهان عرب خسروان عجم",
      "verse_number": 34
    },
    {
      "id": "02bad6bb-6601-44ed-ae75-d892ae23410e",
      "m1": "کجا آن بزرگان ساسانیان",
      "m2": "ز بهرامیان تا به سامانیان",
      "verse_number": 35
    },
    {
      "id": "d33f4f3b-ef1c-4d3b-90e8-5404325c3ee5",
      "m1": "نکوهیدهتر شاه ضحاک بود",
      "m2": "که بیدادگر بود و ناپاک بود",
      "verse_number": 36
    },
    {
      "id": "5b4ac2e6-41fa-4844-bf4a-2a2fc63fbf15",
      "m1": "فریدون فرخ ستایش ببرد",
      "m2": "بمرد او و جاوید نامش نمرد",
      "verse_number": 37
    },
    {
      "id": "ea948b68-3268-4dc1-a7bf-191e94557ecd",
      "m1": "سخن ماند اندر جهان یادگار",
      "m2": "سخن بهتر از گوهر شاهوار",
      "verse_number": 38
    },
    {
      "id": "5731d5f6-804f-4bcd-b0e4-6a18f1ca1ab8",
      "m1": "ستایش نبرد آنک بیداد بود",
      "m2": "به گنج و به تخت مهی شاد بود",
      "verse_number": 39
    },
    {
      "id": "a92c90f3-d52d-4afb-bce9-40b67439a05b",
      "m1": "گسسته شود در جهان کام اوی",
      "m2": "نخواند به گیتی کسی نام اوی",
      "verse_number": 40
    },
    {
      "id": "539a0f13-d3d4-4416-aa26-285833c712a9",
      "m1": "ازین نامهٔ شاه دشمنگداز",
      "m2": "که بادا همه ساله بر تخت ناز",
      "verse_number": 41
    },
    {
      "id": "b6eca012-071b-49af-84f6-0d0e6bae73e1",
      "m1": "همه مردم از خانها شد به دشت",
      "m2": "نیایش همی ز آسمان برگذشت",
      "verse_number": 42
    },
    {
      "id": "976e611a-5d7e-4702-bb1c-0d26c30ee716",
      "m1": "که جاوید بادا سر تاجدار",
      "m2": "خجسته برو گردش روزگار",
      "verse_number": 43
    },
    {
      "id": "b1ef16fb-6d9c-4725-a910-655dfc196ccf",
      "m1": "ز گیتی مبیناد جز کام خویش",
      "m2": "نوشته بر ایوانها نام خویش",
      "verse_number": 44
    },
    {
      "id": "c65d55f9-c192-4a94-9f4a-532f79a63c1a",
      "m1": "همان دوده و لشکر و کشورش",
      "m2": "همان خسروی قامت و منظرش",
      "verse_number": 45
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 45
  }
}