{
  "id": "f23b0594-de93-469e-b5bf-00ccb133c1de",
  "title": "بخش ۲",
  "verses": [
    {
      "id": "0ef2cdba-361c-440a-94f6-cf8b2b6def6e",
      "m1": "کنون ای سراینده فرتوت مرد",
      "m2": "سوی گاه اشکانیان بازگرد",
      "verse_number": 1
    },
    {
      "id": "eb5f41c5-90bb-4220-aaf4-d21a986b6665",
      "m1": "چه گفت اندر آن نامهٔ راستان",
      "m2": "که گوینده یاد آرد از باستان",
      "verse_number": 2
    },
    {
      "id": "e94b122b-2f62-4f60-bb12-3657004d92fa",
      "m1": "پس از روزگار سکندر جهان",
      "m2": "چه گوید کِرا بود تخت مهان",
      "verse_number": 3
    },
    {
      "id": "fa4f3ee0-f7c2-4a2c-8ba8-59c2424a674e",
      "m1": "چنین گفت داننده دهقان چاچ",
      "m2": "کزان پس کسی را نبد تخت عاج",
      "verse_number": 4
    },
    {
      "id": "975c08df-327f-4bd4-80ce-2950ae635e4e",
      "m1": "بزرگان که از تخم آرش بدند",
      "m2": "دلیر و سبکسار و سرکش بدند",
      "verse_number": 5
    },
    {
      "id": "c53998f4-2434-4385-a1f3-341e21957222",
      "m1": "به گیتی به هر گوشهای بر یکی",
      "m2": "گرفته ز هر کشوری اندکی",
      "verse_number": 6
    },
    {
      "id": "61f481f3-0211-4488-b59a-14c9c735d064",
      "m1": "چو بر تختشان شاد بنشاندند",
      "m2": "ملوک طوایف همی خواندند",
      "verse_number": 7
    },
    {
      "id": "c89be691-3245-46b2-bd9b-404dae3afbce",
      "m1": "برین گونه بگذشت سالی دویست",
      "m2": "تو گفتی که اندر زمین شاه نیست",
      "verse_number": 8
    },
    {
      "id": "b92ca5d5-1947-4f62-ab55-6d9c73fad2a9",
      "m1": "نکردند یاد این ازان آن ازین",
      "m2": "برآسود یک چند روی زمین",
      "verse_number": 9
    },
    {
      "id": "e517b45d-b0d0-4956-8d06-1dec61805aa1",
      "m1": "سکندر سگالید زینگونه رای",
      "m2": "که تا روم آباد ماند به جای",
      "verse_number": 10
    },
    {
      "id": "ce2db2de-95cc-4ef7-9384-0431609ec3e0",
      "m1": "نخست اشک بود از نژاد قباد",
      "m2": "دگر گرد شاپور خسرو نژاد",
      "verse_number": 11
    },
    {
      "id": "a202af7c-4c2d-4ad3-bd28-81fb70d4b8cc",
      "m1": "ز یک دست گودرز اشکانیان",
      "m2": "چو بیژن که بود از نژاد کیان",
      "verse_number": 12
    },
    {
      "id": "aa8b2ae5-6ec2-4980-b4db-1e993f9c3d25",
      "m1": "چو نرسی و چون اورمزد بزرگ",
      "m2": "چو آرش که بد نامدار سترگ",
      "verse_number": 13
    },
    {
      "id": "7b7b2b7e-46c4-458f-bc79-fafafd02af2a",
      "m1": "چو زو بگذری نامدار اردوان",
      "m2": "خردمند و با رای و روشنروان",
      "verse_number": 14
    },
    {
      "id": "b1abbce2-c885-4242-9515-83f8e49cfb1e",
      "m1": "چو بنشست بهرام ز اشکانیان",
      "m2": "ببخشید گنجی بارزانیان",
      "verse_number": 15
    },
    {
      "id": "dc48df00-f58a-4870-b1c9-cf6763f730fa",
      "m1": "ورا خواندند اردوان بزرگ",
      "m2": "که از میش بگسست چنگال گرگ",
      "verse_number": 16
    },
    {
      "id": "f979b5f2-30d1-453c-a13f-58bd8f887da4",
      "m1": "ورا بود شیراز تا اصفهان",
      "m2": "که داننده خواندش مرز مهان",
      "verse_number": 17
    },
    {
      "id": "62f29a9d-eca9-4647-8bfb-fd2f2649d305",
      "m1": "به اصطخر بد بابک از دست اوی",
      "m2": "که تنین خروشان بد از شست اوی",
      "verse_number": 18
    },
    {
      "id": "bfc2f60d-ecf8-4cf9-9a23-e50fb99b1b30",
      "m1": "چو کوتاه شد شاخ و هم بیخشان",
      "m2": "نگوید جهاندار تاریخشان",
      "verse_number": 19
    },
    {
      "id": "76be30db-fba5-4b16-8635-6cdbc899a2ab",
      "m1": "کزیشان جز از نام نشنیدهام",
      "m2": "نه در نامهٔ خسروان دیدهام",
      "verse_number": 20
    },
    {
      "id": "201205cc-10a3-4474-abd3-4f44a520518b",
      "m1": "سکندر چو نومید گشت از جهان",
      "m2": "بیفگند رایی میان مهان",
      "verse_number": 21
    },
    {
      "id": "765d5795-c465-4907-952f-823137a7260e",
      "m1": "بدان تا نگیرد کس از روم یاد",
      "m2": "بماند مران کشور آباد و شاد",
      "verse_number": 22
    },
    {
      "id": "63a8714d-84d7-4a13-8826-0d2cd68123b9",
      "m1": "چو دانا بود بر زمین شهریار",
      "m2": "چنین آورد دانش شاه بار",
      "verse_number": 23
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 23
  },
