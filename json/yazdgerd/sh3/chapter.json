{
  "id": "c93d6fbd-0a31-48fe-add2-fb777451ada2",
  "title": "بخش ۳ - پادشاهی پیروز بیست و هفت سال بود",
  "verses": [
    {
      "id": "a7c8b790-129d-4753-8fad-89f6be5ac8e7",
      "m1": "بیامد بتخت کیی برنشست",
      "m2": "چنان چون بود شاه یزدانپرست",
      "verse_number": 1
    },
    {
      "id": "dffa8de0-157c-4afb-b208-6e3f2bffbcce",
      "m1": "نخستین چنین گفت با مهتران",
      "m2": "که ای پرهنر پاکدل سروران",
      "verse_number": 2
    },
    {
      "id": "3dfe8b02-83e4-4c3e-b694-95e3d4775c54",
      "m1": "همیخواهم از داور بینیاز",
      "m2": "که باشد مرا زندگانی دراز",
      "verse_number": 3
    },
    {
      "id": "1108745d-f884-41bb-9e37-b4dd13a2eb14",
      "m1": "که که را به که دارم و مه به مه",
      "m2": "فراوان خرد باشدم روز به",
      "verse_number": 4
    },
    {
      "id": "44a453d9-1eac-4077-bd9b-208a6a53ad59",
      "m1": "سر مردمی بردباری بود",
      "m2": "سبک سر همیشه بخواری بود",
      "verse_number": 5
    },
    {
      "id": "7b12d5ea-067e-47ba-8e80-f1609f892f97",
      "m1": "ستون خرد داد و بخشایشست",
      "m2": "در بخشش او را چو آرایشست",
      "verse_number": 6
    },
    {
      "id": "43879f52-6e05-4d81-b5c8-ebb20fc17548",
      "m1": "زبان چرب و گویندگی فر اوست",
      "m2": "دلیری و مردانگی پر اوست",
      "verse_number": 7
    },
    {
      "id": "69cda0fe-b1eb-40ad-803b-2cf4cfb28823",
      "m1": "هران نامور کو ندارد خرد",
      "m2": "ز تخت بزرگی کجا برخورد",
      "verse_number": 8
    },
    {
      "id": "5068279f-e177-4458-a170-c28870e7ef0f",
      "m1": "خردمند هم نیز جاوید نیست",
      "m2": "فری برتر از فر جمشید نیست",
      "verse_number": 9
    },
    {
      "id": "e558b515-178c-48ed-9a19-b49c8c799b3c",
      "m1": "چو تاجش به ماه اندر آمد بمرد",
      "m2": "نشست کیی دیگری را سپرد",
      "verse_number": 10
    },
    {
      "id": "3cc84767-e539-4a52-8213-042a692941d4",
      "m1": "نماند برین خاک جاوید کس",
      "m2": "ز هر بد به یزدان پناهید و بس",
      "verse_number": 11
    },
    {
      "id": "559e0695-425a-4a7d-9ea7-4eb96c1c4385",
      "m1": "همیبود یک سال با داد و پند",
      "m2": "خردمند وز هر بدی بیگزند",
      "verse_number": 12
    },
    {
      "id": "93abddc0-601e-4a19-a848-264a1c766e32",
      "m1": "دگر سال روی هوا خشک شد",
      "m2": "به جو اندرون آب چون مشک شد",
      "verse_number": 13
    },
    {
      "id": "0df6cc38-6f4f-4704-b375-abd982f60ef7",
      "m1": "سه دیگر همان و چهارم همان",
      "m2": "ز خشکی نبد هیچکس شادمان",
      "verse_number": 14
    },
    {
      "id": "70b4b6e7-68b0-4e0e-a88b-d22e05e18753",
      "m1": "هوا را دهان خشک چون خاک شد",
      "m2": "ز تنگی به جو آب تریاک شد",
      "verse_number": 15
    },
    {
      "id": "6999376f-d9ca-4071-94f1-741b52d62024",
      "m1": "ز بس مردن مردم و چارپای",
      "m2": "پیی را ندیدند بر خاک جای",
      "verse_number": 16
    },
    {
      "id": "d3c86ced-da59-4e2e-ab7d-a76dab2d2241",
      "m1": "شهنشاه ایران چو دید آن شگفت",
      "m2": "خراج و گزیت از جهان برگرفت",
      "verse_number": 17
    },
    {
      "id": "787bb7a0-e9e4-4bba-bf93-653121feba04",
      "m1": "به هر سو که انبار بودش نهان",
      "m2": "ببخشید بر کهتران و مهان",
      "verse_number": 18
    },
    {
      "id": "dcb79556-0541-4ab7-a50e-349db1f34b1d",
      "m1": "خروشی برآمد ز درگاه شاه",
      "m2": "که ای نامداران با دستگاه",
      "verse_number": 19
    },
    {
      "id": "1c476e24-0893-42f5-8fc3-109734d75f89",
      "m1": "غله هرچ دارید پیدا کنید",
      "m2": "ز دینار پیروز گنج آگنید",
      "verse_number": 20
    },
    {
      "id": "eed8a91b-de20-4dac-8dbb-9bac22d5ca35",
      "m1": "هر آنکس که دارد نهانی غله",
      "m2": "وگر گاو و گر گوسفند و گله",
      "verse_number": 21
    },
    {
      "id": "bce15323-2f24-4ffa-8455-7dfbe3d026be",
      "m1": "به نرخی فروشد که او را هواست",
      "m2": "که از خوردنی جانور بینواست",
      "verse_number": 22
    },
    {
      "id": "f37b94e0-74cb-4f9d-9a22-127fb76002d9",
      "m1": "به هر کارداری و خودکامهای",
      "m2": "فرستاد تازان یکی نامهای",
      "verse_number": 23
    },
    {
      "id": "f58c7a5f-08e2-46fd-b1e2-81722865c449",
      "m1": "که انبارها برگشایند باز",
      "m2": "به گیتی برآنکس که هستش نیاز",
      "verse_number": 24
    },
    {
      "id": "dbe49723-9bd2-4d15-b149-664d758c0596",
      "m1": "کسی گر بمیرد بنایافت نان",
      "m2": "ز برنا و از پیر مرد و زنان",
      "verse_number": 25
    },
    {
      "id": "2ed007a4-0e48-4c2a-9e39-b43c9e326db9",
      "m1": "بریزم ز تن خون انباردار",
      "m2": "کجا کار یزدان گرفتست خوار",
      "verse_number": 26
    },
    {
      "id": "9e8ddf62-804a-4594-b076-677d9cbbed2e",
      "m1": "بفرمود تا خانه بگذاشتند",
      "m2": "به دشت آمد و دست برداشتند",
      "verse_number": 27
    },
    {
      "id": "4d502026-2094-46df-8f75-85ba4c027dc5",
      "m1": "همی به آسمان اندر آمد خروش",
      "m2": "ز بس مویه و درد و زاری و جوش",
      "verse_number": 28
    },
    {
      "id": "861f8000-d843-4b49-983f-7022d342b864",
      "m1": "ز کوه و بیابان وز دشت و غار",
      "m2": "ز یزدان همیخواستی زینهار",
      "verse_number": 29
    },
    {
      "id": "7a265b65-9220-4f40-a11b-5eedd7c1c095",
      "m1": "برین گونه تا هفت سال از جهان",
      "m2": "ندیدند سبزی کهان و مهان",
      "verse_number": 30
    },
    {
      "id": "182424eb-7724-4c00-a273-8d5d2eeee83a",
      "m1": "بهشتم بیامد مه فوردین",
      "m2": "برآمد یکی ابر با آفرین",
      "verse_number": 31
    },
    {
      "id": "7d18227d-3732-4a9c-b6a1-01574715c0e6",
      "m1": "همی در بارید بر خاک خشک",
      "m2": "همیآمد از بوستان بوی مشک",
      "verse_number": 32
    },
    {
      "id": "06a35237-d4d8-4c56-9170-c10137ef10e7",
      "m1": "شده ژاله برگل چو مل در قدح",
      "m2": "همیتافت از ابر قوس قزح",
      "verse_number": 33
    },
    {
      "id": "b3ec867c-9e41-4d78-aa82-2bd7de7d0dda",
      "m1": "زمانهبرست از بد بدگمان",
      "m2": "به هرجای بر زه نهاده کمان",
      "verse_number": 34
    },
    {
      "id": "c92d76b1-7c65-44d5-aaf6-3a992947ac64",
      "m1": "چو پیروز ازان روز تنگیبرست",
      "m2": "بر آرام بر تخت شاهی نشست",
      "verse_number": 35
    },
    {
      "id": "3ff65afc-fc91-4190-9b68-5bfbc609bca2",
      "m1": "یکی شارستان کرد پیروز کام",
      "m2": "بفرمود کو را نهادند نام",
      "verse_number": 36
    },
    {
      "id": "78bd5b2c-b25c-4924-98a6-1bd91b2df268",
      "m1": "جهاندار گوینده گفت این ریست",
      "m2": "که آرام شاهان فرخ پیست",
      "verse_number": 37
    },
    {
      "id": "e0cd60a8-5e34-4af1-9599-31234360bb56",
      "m1": "دگر کرد بادان پیروزنام",
      "m2": "خنیده بهرجایش آرام و کام",
      "verse_number": 38
    },
    {
      "id": "ffa831c9-108f-41d5-b701-f5fadc98e51e",
      "m1": "که اکنونش خوانی همی اردبیل",
      "m2": "که قیصر بدو دارد از داد میل",
      "verse_number": 39
    },
    {
      "id": "73c35e97-f321-4eda-861a-57a871b8a9f8",
      "m1": "چو این بومها یکسر آباد کرد",
      "m2": "دل مردم پر خرد شاد کرد",
      "verse_number": 40
    },
    {
      "id": "5db6dfd9-ac4e-4579-a97f-d7b48fadad4a",
      "m1": "درم داد با لشکر نامدار",
      "m2": "سوی جنگ جستن برآراست کار",
      "verse_number": 41
    },
    {
      "id": "02e4301d-6152-40b5-986e-8318f6148bc3",
      "m1": "بدان جنگ هرمز بدی پیشرو",
      "m2": "همیرفت با کارسازان نو",
      "verse_number": 42
    },
    {
      "id": "fa5a31b8-c199-42f2-8816-e2164100c7f5",
      "m1": "قباد از پس پشت پیروز شاه",
      "m2": "همیراند چون باد لشکر به راه",
      "verse_number": 43
    },
    {
      "id": "46d33669-f5e8-48b8-aafc-28d45d298319",
      "m1": "که پیروز را پاک فرزند بود",
      "m2": "خردمند شاخی برومند بود",
      "verse_number": 44
    },
    {
      "id": "afe7556c-d705-448a-aa28-8d102e41ecf3",
      "m1": "بلاش از بر تخت بنشست شاد",
      "m2": "که کهتر پسر بود با مهر و داد",
      "verse_number": 45
    },
    {
      "id": "09a00c27-0171-4e1e-9c90-1c5b791dae77",
      "m1": "یکی پارسی بود بس نامدار",
      "m2": "ورا سوفزا خواندی شهریار",
      "verse_number": 46
    },
    {
      "id": "2f3c2f9e-4a9a-48f8-93f7-a57e1828389d",
      "m1": "بفرمود پیروز کایدر بباش",
      "m2": "چو دستور شایسته نزد بلاش",
      "verse_number": 47
    },
    {
      "id": "6230e96b-473e-4a94-8bc7-a0574989fdf0",
      "m1": "سپه را سوی جنگ ترکان کشید",
      "m2": "همی تاج و تخت کیی را سزید",
      "verse_number": 48
    },
    {
      "id": "ed6f0c98-1430-491f-8756-8248df567b98",
      "m1": "همیراند با لشکر و گنج و ساز",
      "m2": "که پیکار جویند با خوشنواز",
      "verse_number": 49
    },
    {
      "id": "c5524763-bcf2-41b5-9486-cff53fee2619",
      "m1": "نشانی که بهرام یل کرده بود",
      "m2": "ز پستی بلندی برآورده بود",
      "verse_number": 50
    },
    {
      "id": "fa7297dc-78d6-41b4-8e41-3dd284c03ed0",
      "m1": "نبشته یکی عهد شاهنشهان",
      "m2": "که از ترک و ایرانیان در جهان",
      "verse_number": 51
    },
    {
      "id": "8831ccb6-1c92-409b-8d36-4218ea943a0a",
      "m1": "کسی زین نشان هیچ برنگذرد",
      "m2": "کزان رود برتر زمین نشمرد",
      "verse_number": 52
    },
    {
      "id": "4fd0e748-e5c7-4944-9ca1-96439abbdc76",
      "m1": "چو پیروز شیراوژن آنجا رسید",
      "m2": "نشان کردن شاه ایران بدید",
      "verse_number": 53
    },
    {
      "id": "6a1a4e3a-3f79-4c88-a82a-471f64be872c",
      "m1": "چنین گفت یکسر بگردنکشان",
      "m2": "که از پیش ترکان برین همنشان",
      "verse_number": 54
    },
    {
      "id": "0f06ef2e-d9ec-4b59-a5c4-d589d98f9b69",
      "m1": "مناره برآرم به شمشیر و گنج",
      "m2": "ز هیتال تا کس نباشد به رنج",
      "verse_number": 55
    },
    {
      "id": "07d8d92b-7545-4d86-b14a-eb35903e253f",
      "m1": "چو باشد مناره به پیش برک",
      "m2": "بزرگان به پیش من آرند چک",
      "verse_number": 56
    },
    {
      "id": "6a034614-0744-48b4-8821-52f78bbb73e5",
      "m1": "بگویم که آن کرد بهرام گور",
      "m2": "به مردی و دانایی و فر و زور",
      "verse_number": 57
    },
    {
      "id": "4cef4855-2a31-4290-8fe4-fe18b88e8986",
      "m1": "نمانم بجایی پی خوشنواز",
      "m2": "به هیتال و ترک از نشیب و فراز",
      "verse_number": 58
    },
    {
      "id": "bb9631c4-4de6-4d21-a3fd-d0b02219069f",
      "m1": "چو بشنید فرزند خاقان که شاه",
      "m2": "ز جیحون گذر کرد خود با سپاه",
      "verse_number": 59
    },
    {
      "id": "1c2d5a84-aeda-4961-86b5-bf26ad4ae844",
      "m1": "همیبشکند عهد بهرام گور",
      "m2": "بدان تازه شد کشتن و جنگ و شور",
      "verse_number": 60
    },
    {
      "id": "e5d7bb78-7308-4798-95ed-4d2d2fbbbe16",
      "m1": "دبیر جهاندیده را خوشنواز",
      "m2": "بفرمود تا شد بر او فراز",
      "verse_number": 61
    },
    {
      "id": "79c7c847-f889-491b-9d0d-c3358e668d9c",
      "m1": "یکی نامه بنوشت با آفرین",
      "m2": "ز دادار بر شهریار زمین",
      "verse_number": 62
    },
    {
      "id": "b098cfa9-0423-4ae2-876b-be1c53e7303d",
      "m1": "چنین گفت کز عهد شاهان داد",
      "m2": "به گردی نخوانمت خسرونژاد",
      "verse_number": 63
    },
    {
      "id": "fef65a83-d5c9-47d2-a190-65f29947d3fa",
      "m1": "نه این بود عهد نیاکان تو",
      "m2": "گزیده جهاندار و پاکان تو",
      "verse_number": 64
    },
    {
      "id": "d9cc5f66-8191-4ad6-999e-b01856d43d63",
      "m1": "چو پیمان آزادگان بشکنی",
      "m2": "نشان بزرگی به خاک افگنی",
      "verse_number": 65
    },
    {
      "id": "be023c42-8591-42bf-b31a-64106f42f867",
      "m1": "مرا با تو پیمان بباید شکست",
      "m2": "به ناچار بردن بشمشیر دست",
      "verse_number": 66
    },
    {
      "id": "2c4abad8-f11e-4a77-bc69-d7339a89178a",
      "m1": "به نامه ز هر کارش آگاه کرد",
      "m2": "بسی هدیه با نامه همراه کرد",
      "verse_number": 67
    },
    {
      "id": "d4c6ceba-d3ca-414c-a20b-b7d080f467e2",
      "m1": "سواری سراینده و سرفراز",
      "m2": "همیرفت با نامهٔ خوشنواز",
      "verse_number": 68
    },
    {
      "id": "e63d9f3f-f942-41c3-a5aa-92fe855adbf9",
      "m1": "چو آن نامه برخواند پیروز شاه",
      "m2": "برآشفت زان نامور پیشگاه",
      "verse_number": 69
    },
    {
      "id": "53341826-39e1-429f-a1e3-463b5281c574",
      "m1": "فرستاده را گفت برخیز و رو",
      "m2": "به نزدیک آن مرد دیوانه شو",
      "verse_number": 70
    },
    {
      "id": "a0d6b9da-13b7-42f4-bdef-4619b0ad4e46",
      "m1": "بگویش که تا پیش رود برک",
      "m2": "شما را فرستاد بهرام چک",
      "verse_number": 71
    },
    {
      "id": "2d132d35-44ec-4ede-afa6-53f337fb5697",
      "m1": "کنون تا لب رود جیحون تو راست",
      "m2": "بلندی و پستی و هامون تو راست",
      "verse_number": 72
    },
    {
      "id": "8ca2fd36-5803-4acc-ac91-88db5d2f6d4d",
      "m1": "من اینک بیارم سپاهی گران",
      "m2": "سرافراز گردان جنگ آوران",
      "verse_number": 73
    },
    {
      "id": "54a0d3d5-4765-4668-861e-a7256449bbbe",
      "m1": "نمانم مگر سایهٔ خوشنواز",
      "m2": "که باشد بروی زمین بر دراز",
      "verse_number": 74
    },
    {
      "id": "e24e873f-8a28-4fe4-8afe-db1279bfc0a2",
      "m1": "فرستاده آمد بکردار گرد",
      "m2": "شنیده سخنها همه یاد کرد",
      "verse_number": 75
    },
    {
      "id": "89e8d37b-d51c-4b2e-a258-ec5edbd99cf9",
      "m1": "همیگفت یک چند با خوشنواز",
      "m2": "ازان شاه گردنکش و دیرساز",
      "verse_number": 76
    },
    {
      "id": "f4f5e5f4-7448-495f-8618-131c0347d0d4",
      "m1": "چو گفتار بشنید و نامه بخواند",
      "m2": "سپاه پراگنده را برنشاند",
      "verse_number": 77
    },
    {
      "id": "668b887b-3813-4984-9cb2-68d0cf2b7623",
      "m1": "بیاورد لشکر به دشت نبرد",
      "m2": "همان عهد را بر سر نیزه کرد",
      "verse_number": 78
    },
    {
      "id": "dd4287ee-eec8-42f6-b0f2-b60b13cebd31",
      "m1": "که بستد نیایش ز بهرامشاه",
      "m2": "که جیحون میانجیست ما را به راه",
      "verse_number": 79
    },
    {
      "id": "8238dc40-fc52-44fb-be2c-6f54be614d3f",
      "m1": "یکی مرد بینادل و چربگوی",
      "m2": "ز لشکر گزین کرد با آبروی",
      "verse_number": 80
    },
    {
      "id": "a6bf696a-75a3-4317-a71f-34a28b878463",
      "m1": "بدو گفت نزدیک پیروز رو",
      "m2": "به چربی سخنگوی و پاسخ شنو",
      "verse_number": 81
    },
    {
      "id": "3980dd3d-d413-45ef-9caa-ebc2603c095e",
      "m1": "بگویش که عهد نیای تو را",
      "m2": "بلند اختر و رهنمای تو را",
      "verse_number": 82
    },
    {
      "id": "99c2f916-0bde-49b1-9dfe-acac18134615",
      "m1": "همی بر سر نیزه پیش سپاه",
      "m2": "بیارم چو خورشید تابان به راه",
      "verse_number": 83
    },
    {
      "id": "c109f173-6e68-47c6-8c02-2a4df1537d4a",
      "m1": "بدان تا هر آنکس که دارد خرد",
      "m2": "به منشور آن دادگر بنگرد",
      "verse_number": 84
    },
    {
      "id": "17e511c2-5726-4413-ac31-d82706358d00",
      "m1": "مرا آفرین بر تو نفرین بود",
      "m2": "همان نام تو شاه بیدین بود",
      "verse_number": 85
    },
    {
      "id": "10ce79cd-fdb7-4745-9cef-78e7d54bd14f",
      "m1": "نه یزدان پسندد نه یزدانپرست",
      "m2": "نه اندر جهان مردم زیردست",
      "verse_number": 86
    },
    {
      "id": "c17d1ae0-c292-415a-ab9e-9934115f67ae",
      "m1": "که بیداد جوید کسی در جهان",
      "m2": "بپیچد سر از عهد شاهنشهان",
      "verse_number": 87
    },
    {
      "id": "7b943e23-e74d-4aff-ad1f-e1eb6fecfdc4",
      "m1": "به داد و به مردی چو بهرام شاه",
      "m2": "کسی نیز ننهاد بر سر کلاه",
      "verse_number": 88
    },
    {
      "id": "7b4f5d53-451c-445a-9522-6f8f5c6f906d",
      "m1": "برین بر جهاندار یزدان گواست",
      "m2": "که او را گوا خواستن ناسزاست",
      "verse_number": 89
    },
    {
      "id": "2b73ae15-20ea-4aae-a8e7-1ec715d34d6e",
      "m1": "که بیداد جوید همی جنگ من",
      "m2": "چنین با سپه کردن آهنگ من",
      "verse_number": 90
    },
    {
      "id": "082592a4-4684-47c8-b8a3-540ccf8cbc8c",
      "m1": "نباشی تو زین جنگ پیروزگر",
      "m2": "نیابی مگر ز اختر نیک بر",
      "verse_number": 91
    },
    {
      "id": "99f415ef-1a76-4a46-9b84-5bde4db55052",
      "m1": "ازین پس نخواهم فرستاد کس",
      "m2": "بدین جنگ یزدان مرا یار بس",
      "verse_number": 92
    },
    {
      "id": "d5f5c0fa-8186-4c23-8504-9e13592d0652",
      "m1": "فرستاده با نامه آمد چو گرد",
      "m2": "سخنها به پیروز بر یاد کرد",
      "verse_number": 93
    },
    {
      "id": "a5a0dfc5-3273-4dce-8309-a699f590b608",
      "m1": "چو برخواند آن نامهٔ خوشنواز",
      "m2": "پر از خشم شد شاه گردن فراز",
      "verse_number": 94
    },
    {
      "id": "ef432da5-aaf7-4bc7-856c-7ab8c8d1c76e",
      "m1": "فرستاده را گفت چندین سخن",
      "m2": "نگوید جهاندیده مرد کهن",
      "verse_number": 95
    },
    {
      "id": "472c5ebe-e069-4357-a92b-d1d066c0557b",
      "m1": "که از چاچ یک پی نهد نزد رود",
      "m2": "به نوک سنانش فرستم درود",
      "verse_number": 96
    },
    {
      "id": "a12cadae-fdc7-4a7d-87d4-16d4d262f83c",
      "m1": "فرستاده آمد بر خوشنواز",
      "m2": "فراوان سخن گفت با او به راز",
      "verse_number": 97
    },
    {
      "id": "01d94359-7ada-4b61-8ed7-9fbde5173961",
      "m1": "که نزدیک پیروز ترس خدای",
      "m2": "ندیدم نبودش کسی رهنمای",
      "verse_number": 98
    },
    {
      "id": "436ca0a4-42ad-482d-be85-c377bffc52bd",
      "m1": "همه دیدمش جنگ جوید همی",
      "m2": "به فرمان یزدان نگوید همی",
      "verse_number": 99
    },
    {
      "id": "3193ba2b-2bc1-4598-bf14-1f689f913b1b",
      "m1": "چو بشندی زو این سخن خوشنواز",
      "m2": "به یزدان پناهید و بردش نماز",
      "verse_number": 100
    },
    {
      "id": "de347b7c-20a3-424b-92d5-90643a53fb95",
      "m1": "چنین گفت کای داور داد و پاک",
      "m2": "تویی آفرینندهٔ هور و خاک",
      "verse_number": 101
    },
    {
      "id": "a4ec6bce-5732-4b76-8ca0-4e727f2cf8bc",
      "m1": "تو دانی که پیروز بیدادگر",
      "m2": "ز بهرام بیشی ندارد هنر",
      "verse_number": 102
    },
    {
      "id": "0a5849c4-2550-4d06-9e88-dea2497a20b4",
      "m1": "پی او ز روی زمین برگسل",
      "m2": "مه نیرو مه آهنگ جانش مه دل",
      "verse_number": 103
    },
    {
      "id": "70688332-3e5f-4735-8895-e308c3086fb6",
      "m1": "سخنهای بیداد گوید همی",
      "m2": "بزرگی به شمشیر جوید همی",
      "verse_number": 104
    },
    {
      "id": "9032f98f-5376-4e70-9ef6-552de0f7beba",
      "m1": "به گرد سپه بر یکی کنده کرد",
      "m2": "سرش را بپوشید و آگنده کرد",
      "verse_number": 105
    },
    {
      "id": "b441468a-137f-4382-932b-0e4d3a7726db",
      "m1": "کمندی فزون بود بالای اوی",
      "m2": "همان سی ارش کرده پهنای اوی",
      "verse_number": 106
    },
    {
      "id": "dd92ea70-c271-4045-91e8-865e54fdf9bd",
      "m1": "چو این کرده شد نام یزدان بخواند",
      "m2": "ز پیش سمرقند لشکر براند",
      "verse_number": 107
    },
    {
      "id": "06ef5a70-26af-4fb8-9b8a-f9deef68d025",
      "m1": "وزان روی سرگشته پیروز شاه",
      "m2": "همیراند چون باد لشکر به راه",
      "verse_number": 108
    },
    {
      "id": "4cff7cb7-b4df-4526-bc89-911f25145f4f",
      "m1": "وزین روی پر بیم دل خوشنواز",
      "m2": "چنین تا برکنده آمد فراز",
      "verse_number": 109
    },
    {
      "id": "c4780a96-c879-4cfb-8934-7a51bebe4440",
      "m1": "برآمد ز هردو سپه بوق و کوس",
      "m2": "هوا شد ز گرد سپاه آبنوس",
      "verse_number": 110
    },
    {
      "id": "e7dc8272-1d67-4bbf-ac4e-ab08528338fc",
      "m1": "چنان تیرباران بد از هر دو روی",
      "m2": "که چون آب خون اندر آمد به جوی",
      "verse_number": 111
    },
    {
      "id": "b21371c5-0bdd-498a-975a-294f6674d3e5",
      "m1": "چو نزدیکی کنده شد خوشنواز",
      "m2": "همیگفت با داور پاک راز",
      "verse_number": 112
    },
    {
      "id": "aed2cf24-23f2-411b-969b-a4b0d5133964",
      "m1": "وزان روی چون باد پیروزشاه",
      "m2": "همیتاخت با خوارمایه سپاه",
      "verse_number": 113
    },
    {
      "id": "708c0c08-d3bf-42f7-bece-8f0894243ad6",
      "m1": "چو آمد به نزدیکی خوشنواز",
      "m2": "سپهدار ترکان ازو گشت باز",
      "verse_number": 114
    },
    {
      "id": "db1798f6-c33c-497b-a0ae-619cfaf80432",
      "m1": "عنان را بپیچید و بنمود پشت",
      "m2": "پس او سپاه اندر آمد درشت",
      "verse_number": 115
    },
    {
      "id": "168cf11e-4f11-42dc-96ed-bda8561caac4",
      "m1": "برانگیخت پس باره پیروزشاه",
      "m2": "همیراند با گرز و رومی کلاه",
      "verse_number": 116
    },
    {
      "id": "10c6ae84-fa47-4433-8cd9-9711b019930e",
      "m1": "به کنده در افتاد با چند مرد",
      "m2": "بزرگان و شیران روز نبرد",
      "verse_number": 117
    },
    {
      "id": "42ea8485-f518-475d-823f-3940cc1b1cd7",
      "m1": "چو نرسی برادرش و فرخ قباد",
      "m2": "بزرگان و شاهان فرخ نژاد",
      "verse_number": 118
    },
    {
      "id": "35872633-9e49-4116-887a-40f0fbbaac03",
      "m1": "برین سان نگون شد سر هفت شاه",
      "m2": "همه نامداران زرین کلاه",
      "verse_number": 119
    },
    {
      "id": "7f0b53e6-3496-400f-b43d-35b58d5ec2aa",
      "m1": "وزان جایگه شاددل خوشنواز",
      "m2": "به نزدیکی کنده آمد فراز",
      "verse_number": 120
    },
    {
      "id": "ca8e375f-b0cc-433f-bbb5-7e8a3063c039",
      "m1": "برآورد زان کنده هر کس که زیست",
      "m2": "همان خاک بربخت ایشان گریست",
      "verse_number": 121
    },
    {
      "id": "695ab437-0d08-48f2-9b7e-e34c65b41710",
      "m1": "بزرگان و پیکارجویان هران",
      "m2": "کسی را که در کنده آمد زمان",
      "verse_number": 122
    },
    {
      "id": "c8f58502-ad1b-40a0-b49b-a050686e6224",
      "m1": "شکسته سر و پشت پیروزشاه",
      "m2": "شه نامداران با تاج و گاه",
      "verse_number": 123
    },
    {
      "id": "91424bd2-9ff3-4037-9a90-d3a9330e4ddd",
      "m1": "ز شاهان نبد زنده جز کیقباد",
      "m2": "شد آن لشکر و پادشاهی بباد",
      "verse_number": 124
    },
    {
      "id": "bc10bd4c-b9a1-4eea-89d7-b2e3a59abe1e",
      "m1": "همیراند با کام دل خوشنواز",
      "m2": "سرافراز با لشکر رزمساز",
      "verse_number": 125
    },
    {
      "id": "ce022197-f2a8-4427-90eb-dec6186760ec",
      "m1": "به تاراج داده سپاه و بنه",
      "m2": "نه کس میسره دید و نه میمنه",
      "verse_number": 126
    },
    {
      "id": "d507cea7-63fa-4abb-8ecc-e731014c6835",
      "m1": "ز ایرانیان چند بردند اسیر",
      "m2": "چه افگنده بر خاک و خسته به تیر",
      "verse_number": 127
    },
    {
      "id": "eca56ac2-93c5-48f3-8c2c-122307becfcc",
      "m1": "نباید که باشد جهانجوی زفت",
      "m2": "دل زفت با خاک تیرهست جفت",
      "verse_number": 128
    },
    {
      "id": "5d6ec14d-946e-4bd2-aa8c-bd00814be7b0",
      "m1": "چنین آمد این چرخ ناپایدار",
      "m2": "چه با زیردست و چه با شهریار",
      "verse_number": 129
    },
    {
      "id": "119f04b6-57fe-4fff-8092-e245ca703834",
      "m1": "بپیچاند آن را که خود پرورد",
      "m2": "اگر تو شوی پاسبان خرد",
      "verse_number": 130
    },
    {
      "id": "9f6c020b-6cbb-4845-8054-5caf2a20b1cf",
      "m1": "نماند برین خاک جاوید کس",
      "m2": "تو را توشه از راستی باد و بس",
      "verse_number": 131
    },
    {
      "id": "59e9b349-9803-448a-b3c3-14ec10ff23ef",
      "m1": "چو بگذشت برکنده بر خوشنواز",
      "m2": "سپاهش شد از خواسته بینیاز",
      "verse_number": 132
    },
    {
      "id": "daa2773b-cbec-4345-a1b9-e8ba1a3e4f72",
      "m1": "به آهن ببستند پای قباد",
      "m2": "ز تخت و نژادش نکردند یاد",
      "verse_number": 133
    },
    {
      "id": "666627ed-5b59-4620-acc1-bf3ae4baf62a",
      "m1": "چو آگاهی آمد به ایران سپاه",
      "m2": "ازان کنده و رزم پیروز شاه",
      "verse_number": 134
    },
    {
      "id": "667cd26b-0fbf-4f4e-97b2-fe5ce8852de3",
      "m1": "خروشی برآمد ز کشور بدرد",
      "m2": "ازان شهر یاران آزادمرد",
      "verse_number": 135
    },
    {
      "id": "efcbf893-f3c6-4b32-a2cc-004dda95283a",
      "m1": "چو اندر جهان این سخن گشت فاش",
      "m2": "فرود آمد از تخت زرین بلاش",
      "verse_number": 136
    },
    {
      "id": "af9609b6-3cf5-41d3-9434-5d86e18a227d",
      "m1": "همه گوشت بازو به دندان بکند",
      "m2": "همیریخت بر تخت خاک نژند",
      "verse_number": 137
    },
    {
      "id": "8d78b8cf-a6e4-4cb5-9818-d15eb6215e93",
      "m1": "سپاهی و شهری ز ایران بدرد",
      "m2": "زن و مرد و کودک همی مویه کرد",
      "verse_number": 138
    },
    {
      "id": "cb098184-3343-4f69-8e32-94a9f6be2b72",
      "m1": "همه کنده موی و همه خسته روی",
      "m2": "همه شاهجوی و همه راهجوی",
      "verse_number": 139
    },
    {
      "id": "c576c71b-f56c-40c6-baed-0f40d82fea35",
      "m1": "که تا چون گریزند ز ایران زمین",
      "m2": "گرآیند لشکر ازان دشت کین",
      "verse_number": 140
    }
  ],
