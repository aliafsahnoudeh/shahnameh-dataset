      "id": "69a8ee2d-fb9f-49fb-9116-103b076c8905",
      "m1": "بگفت آنچ بشنید وزو گشت شاد",
      "m2": "همانگاه برداشت بند قباد",
      "verse_number": 154
    },
    {
      "id": "3b64296c-6252-47b1-9cfe-1842b3940d80",
      "m1": "همان خواسته سر به سر گرد کرد",
      "m2": "کجا یافت از خاک و دشت نبرد",
      "verse_number": 155
    },
    {
      "id": "3605e415-7c6e-4369-a117-d7576f39dfab",
      "m1": "همان تخت با تاج پیروز شاه",
      "m2": "چو چیز پراگندهٔ آن سپاه",
      "verse_number": 156
    },
    {
      "id": "2ae23873-2fea-4a2b-b813-0ad48fdcdcf0",
      "m1": "فرستاد یکسر سوی سوفزای",
      "m2": "به دست یکی مرد پاکیزهرای",
      "verse_number": 157
    },
    {
      "id": "94d9d90a-0ca0-45de-b51f-05e7a2140478",
      "m1": "چو لشکر بدیدند روی قباد",
      "m2": "ز دیدار او انجمن گشت شاد",
      "verse_number": 158
    },
    {
      "id": "f11fa005-6460-46f4-b261-bd95c2040ece",
      "m1": "بزرگان همه خیمه بگذاشتند",
      "m2": "همه دست بر آسمان داشتند",
      "verse_number": 159
    },
    {
      "id": "df5d0bb0-d457-47ab-a217-0ae64342f26a",
      "m1": "که پور شهنشاه را بیگزند",
      "m2": "بدیدند با هرک بد ارجمند",
      "verse_number": 160
    },
    {
      "id": "f9e59cc2-8c71-4a0f-8690-d90396bd81b6",
      "m1": "همانگه فروهشت پردهسرای",
      "m2": "سپهبد باسب اندر آورد پای",
      "verse_number": 161
    },
    {
      "id": "329787cd-1fdd-4f98-bc26-4fd3841a24a9",
      "m1": "ز جیحون گذر کرد پیروز و شاد",
      "m2": "ابا نامور موبد و کیقباد",
      "verse_number": 162
    },
    {
      "id": "1f3c6702-39e3-4050-bb98-535a32d3592d",
      "m1": "چو آگاهی آمد به ایران زمین",
      "m2": "ازان نیکپی مهتر بفرین",
      "verse_number": 163
    },
    {
      "id": "f369282e-6b4c-4f8d-b2a9-e6fb8e9749ba",
      "m1": "همان جنگ و پیکار با خوشنواز",
      "m2": "ز رای چنان مرد نیرنگساز",
      "verse_number": 164
    },
    {
      "id": "813ced1a-879c-4061-b549-f88b2aba2c19",
      "m1": "همان موبد موبدان اردشیر",
      "m2": "اسیران که بودند برنا و پیر",
      "verse_number": 165
    },
    {
      "id": "18c22a0a-c41e-43a9-a3cb-a9e86b5526b0",
      "m1": "که از جنگ برگشت پیروز و شاد",
      "m2": "گشاده شد از بند پای قباد",
      "verse_number": 166
    },
    {
      "id": "f52f87a8-0537-4634-886b-279c672221ba",
      "m1": "بیاورد و اکنون ز جیحون گذشت",
      "m2": "ز ایران سپاهست بر کوه و دشت",
      "verse_number": 167
    },
    {
      "id": "188c256e-dcda-4d40-9fed-819598b95bb9",
      "m1": "خروشی ز ایران برآمد که گوش",
      "m2": "تو گفتی همی کر شود زان خروش",
      "verse_number": 168
    },
    {
      "id": "38c95928-eccd-4e6e-a852-7b7401e7792a",
      "m1": "بزرگان فرزانه برخاستند",
      "m2": "پذیره شدن را بیاراستند",
      "verse_number": 169
    },
    {
      "id": "5ce60b2f-5bdd-49e7-a54b-43ffc5b61eaa",
      "m1": "بلاش آن زمان تخت زرین نهاد",
      "m2": "که تا برنشیند برو کیقباد",
      "verse_number": 170
    },
    {
      "id": "8e144b72-79fe-4912-8786-db8957e08f6c",
      "m1": "چو آمد به شهر اندرون سوفزای",
      "m2": "بزرگان برفتند یک سر ز جای",
      "verse_number": 171
    },
    {
      "id": "7afca037-048f-4566-8dc2-13f3de3d345c",
      "m1": "پذیره شدن را بیاراست شاه",
      "m2": "همیرفت با آنک بودش سپاه",
      "verse_number": 172
    },
    {
      "id": "f2fc07be-bd21-4339-ba67-ed5b00812fa2",
      "m1": "بلاش آن زمان دید روی قباد",
      "m2": "رها گشته از بند پیروز و شاد",
      "verse_number": 173
    },
    {
      "id": "990fac38-80cf-4f40-8fd3-f92834823f7f",
      "m1": "مر او را سبک شاه در برگرفت",
      "m2": "ز هیتال و چین دست بر سر گرفت",
      "verse_number": 174
    },
    {
      "id": "155e21b9-6ca9-4f18-a704-e4670dc3f3aa",
      "m1": "ز راه اندر ایوان شاه آمدند",
      "m2": "گشادهدل و نیکخواه آمدند",
      "verse_number": 175
    },
    {
      "id": "30a085cf-e578-44d8-a914-3a35f171165a",
      "m1": "بفرمود تا خوان بیاراستند",
      "m2": "می و رود و رامشگران خواستند",
      "verse_number": 176
    },
    {
      "id": "7c18abb8-369e-407e-baeb-d7f9c3171eb1",
      "m1": "همیبود جشنی نه بر آرزوی",
      "m2": "ز تیمار پیروز آزادهخوی",
      "verse_number": 177
    },
    {
      "id": "0847ea7d-afbc-4bfb-abc9-44497af361d3",
      "m1": "همه چامه گر سوفزا را ستود",
      "m2": "ببربط همی رزم ترکان سرود",
      "verse_number": 178
    },
    {
      "id": "69c74609-995e-443e-a4f9-91488114faeb",
      "m1": "مهان را همه چشم بر سوفزای",
      "m2": "ازو گشته شاد و بدو داده رای",
      "verse_number": 179
    },
    {
      "id": "81a939e0-382d-4ed5-9415-bcc17f24264f",
      "m1": "همه شهر ایران بدو گشت باز",
      "m2": "کسی را که بد کینهٔ خوشنواز",
      "verse_number": 180
    },
    {
      "id": "f436a609-5996-43eb-892f-b51edf3ffd66",
      "m1": "بدان پهلوان دل همی شاد کرد",
      "m2": "روان را ز اندیشه آزاد کرد",
      "verse_number": 181
    },
    {
      "id": "dbd9c3d0-7021-4d9f-bb71-bd876ef7dd70",
      "m1": "ببد سوفزای از جهان بیهمال",
      "m2": "همیرفت زین گونه تا چار سال",
      "verse_number": 182
    },
    {
      "id": "7ddcf10a-6ccc-4606-a065-935d57fde09c",
      "m1": "نبودی جز آن چیز کو خواستی",
      "m2": "جهان را به رای خود آراستی",
      "verse_number": 183
    },
    {
      "id": "0d5dc025-b4ea-4f2d-b3d3-bb432eee646b",
      "m1": "چر فرمان او گشت در شهر فاش",
      "m2": "به خوبی بپرداخت گاه از بلاش",
      "verse_number": 184
    },
    {
      "id": "4ded8933-2586-4f93-b049-aadf142bdd7e",
      "m1": "بدو گفت شاهی نرانی همی",
      "m2": "بدان را ز نیکان ندانی همی",
      "verse_number": 185
    },
    {
      "id": "04776b3f-e2c0-40f2-a400-9375af9f1ba5",
      "m1": "همی پادشاهی به بازی کنی",
      "m2": "ز پری وز بینیازی کنی",
      "verse_number": 186
    },
    {
      "id": "46922e6a-4be1-4c98-83f0-c51c2e1ecd9b",
      "m1": "قباد از تو در کار داناترست",
      "m2": "بدین پادشاهی تواناترست",
      "verse_number": 187
    },
    {
      "id": "61de79ae-c707-43ae-a205-4555aa09141b",
      "m1": "به ایوان خویش اندر آمد بلاش",
      "m2": "نیارست گفتن که ایدر مباش",
      "verse_number": 188
    },
    {
      "id": "da240a56-8907-4a05-a79d-1af4a82de0b8",
      "m1": "همیگفت بیرنج تخت این بود",
      "m2": "که بیکوشش و درد و نفرین بود",
      "verse_number": 189
    }
  ],
