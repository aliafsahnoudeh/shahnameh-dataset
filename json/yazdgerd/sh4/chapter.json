{
  "id": "28684b17-8dfb-47f9-ac95-b8eb01d8d141",
  "title": "بخش ۴ - پادشاهی بلاش پیروز چهار سال بود",
  "verses": [
    {
      "id": "8b818917-72a0-43af-a7c5-41b5a87b5f8d",
      "m1": "چو بنشست با سوگ ماهی بلاش",
      "m2": "سرش پر ز گرد و رخش پرخراش",
      "verse_number": 1
    },
    {
      "id": "1ffd60ff-4774-49d4-a0fe-31ae991e6bd5",
      "m1": "سپاه آمد و موبد موبدان",
      "m2": "هر آنکس که بود از رد و بخردان",
      "verse_number": 2
    },
    {
      "id": "8eac2c63-a076-4b84-8cfb-6613b027610e",
      "m1": "فراوان بگفتند با او ز پند",
      "m2": "سخنها که بودی ورا سودمند",
      "verse_number": 3
    },
    {
      "id": "9f9fb6c6-18b3-454b-afb5-c615f221be0d",
      "m1": "بران تخت شاهیش بنشاندند",
      "m2": "بسی زر و گوهر برافشاندند",
      "verse_number": 4
    },
    {
      "id": "d9ee3837-ddd1-4c8a-9ca7-33db3a2b8ab8",
      "m1": "چو بنشست بر گاه گفت ای ردان",
      "m2": "بجویید رای و دل بخردان",
      "verse_number": 5
    },
    {
      "id": "0c220484-a5b6-4ed5-9b1d-aff9d43cb8fa",
      "m1": "شما را بزرگیست نزدیک من",
      "m2": "چو روشن شود رای تاریک من",
      "verse_number": 6
    },
    {
      "id": "a36fb2ec-93e8-4281-aa0b-64068d0cfac3",
      "m1": "به گیتی هر آنکس که نیکی کند",
      "m2": "بکوشد که تا رای ما نشکند",
      "verse_number": 7
    },
    {
      "id": "51e5c95e-0937-4888-87e3-280bbe80219a",
      "m1": "هر آنکس کجا باشد او بدسگال",
      "m2": "که خواهد همی کار خود را همال",
      "verse_number": 8
    },
    {
      "id": "b7afff4c-4105-4533-b8f0-ad575977b36d",
      "m1": "نخستین به پندش توانگر کنم",
      "m2": "چو نپذیرد از خونش افسر کنم",
      "verse_number": 9
    },
    {
      "id": "ec291e7b-b123-4538-acb8-29c217d2a46f",
      "m1": "هرآنگه که زین لشکر دینپرست",
      "m2": "بنالد بر ما یکی زیردست",
      "verse_number": 10
    },
    {
      "id": "0e98eedb-da1a-46b9-9d78-53f077ddb71c",
      "m1": "دل مرد بیدادگر بشکنم",
      "m2": "همه بیخ و شاخش ز بن برکنم",
      "verse_number": 11
    },
    {
      "id": "9fca543b-a227-45e9-8457-1e0c4003de2c",
      "m1": "مباشید گستاخ با پادشا",
      "m2": "بویژه کسی کو بود پارسا",
      "verse_number": 12
    },
    {
      "id": "fdc0b20e-73a4-453d-95f1-388300a34cc1",
      "m1": "که او گاه زهرست و گه پایزهر",
      "m2": "مجویید از زهر تریاک بهر",
      "verse_number": 13
    },
    {
      "id": "e2a7cddc-02f6-42be-a8ec-ccdff0ca7675",
      "m1": "ز گیتی تو خوشنودی شاهجوی",
      "m2": "مشو پیش تختش مگر تازهروی",
      "verse_number": 14
    },
    {
      "id": "f214f855-41a3-4e15-b457-a6153918725f",
      "m1": "چو خشم آورد شاه پوزش گزین",
      "m2": "همی خوان به بیداد و دادآفرین",
      "verse_number": 15
    },
    {
      "id": "d363b6d9-5fcf-4664-bb6c-9056704ee18e",
      "m1": "هرآنگه که گویی که دانا شدم",
      "m2": "به هر دانشی بر توانا شدم",
      "verse_number": 16
    },
    {
      "id": "cc084bdc-7c7c-4db0-97bb-0c23e4415aaa",
      "m1": "چنان دان که نادانتری آن زمان",
      "m2": "مشو بر تن خویش بر بدگمان",
      "verse_number": 17
    },
    {
      "id": "322374ec-d251-4be3-b35b-73f93a4ea6fc",
      "m1": "وگر کار بندید پند مرا",
      "m2": "سخن گفتن سودمند مرا",
      "verse_number": 18
    },
    {
      "id": "530b6da3-57f9-4745-a532-c43477531c78",
      "m1": "ز شاهان داننده یابید گنج",
      "m2": "کسی را ز دانش ندیدم به رنج",
      "verse_number": 19
    },
    {
      "id": "5c546250-f109-482c-83d8-3872040a4f1d",
      "m1": "برو مهتران آفرین خواندند",
      "m2": "ز دانایی او فرو ماندند",
      "verse_number": 20
    },
    {
      "id": "2c07ba04-70a6-48bd-bcc2-cb215c42ac04",
      "m1": "برفتند خشنود ز ایوان اوی",
      "m2": "به یزدان سپرده تن و جان اوی",
      "verse_number": 21
    },
    {
      "id": "a90d200b-5c9c-4a9d-8dae-77bf90becda2",
      "m1": "بدآنگه که پیروز شد سوی جنگ",
      "m2": "یکی پهلوان جست با رای و سنگ",
      "verse_number": 22
    },
    {
      "id": "0f2312b4-48fc-46f2-b8e7-531608e6d1b7",
      "m1": "که باشد نگهبان تخت و کلاه",
      "m2": "بلاش جوان را بود نیکخواه",
      "verse_number": 23
    },
    {
      "id": "4be36ac0-7a3f-4c9c-ae6a-868ccd7defb4",
      "m1": "بدان کار شایسته بد سوفزای",
      "m2": "یکی نامور بود پاکیزهرای",
      "verse_number": 24
    },
    {
      "id": "ac9b679a-794b-4532-96d4-ccec27f67c07",
      "m1": "جهاندیده از شهر شیراز بود",
      "m2": "سپهبددل و گردنافراز بود",
      "verse_number": 25
    },
    {
      "id": "70fb61e2-5b6e-4596-8b54-4fc3111aff4b",
      "m1": "هم او مرزبان بد بزابلستان",
      "m2": "ببست و بغزنین و کابلستان",
      "verse_number": 26
    },
    {
      "id": "09ad1d35-6fd1-4922-806e-0e9fd405df1b",
      "m1": "چو آگاهی آمد سوی سوفزای",
      "m2": "ز پیروز بیرای و بیرهنمای",
      "verse_number": 27
    },
    {
      "id": "738fc933-fba8-4ebb-b698-d80e792ec32c",
      "m1": "ز مژگان سرشکش به رخ برچکید",
      "m2": "همه جامهٔ پهلوی بردرید",
      "verse_number": 28
    },
    {
      "id": "55cc4705-f899-41b8-ac8e-62093744c02b",
      "m1": "ز سر برگرفتند گردان کلاه",
      "m2": "به ماتم نشستند با سوگ شاه",
      "verse_number": 29
    },
    {
      "id": "f7629155-9305-4dc4-887e-639de3f49019",
      "m1": "همیگفت بر کینهٔ شهریار",
      "m2": "بلاش جوان چون بود خواستار",
      "verse_number": 30
    },
    {
      "id": "a6148720-081c-406f-9d55-46a14f91e6a9",
      "m1": "بدانست کان کار بیسود شد",
      "m2": "سر تاج شاهی پر از دود شد",
      "verse_number": 31
    },
    {
      "id": "779afbe2-b285-4f32-885f-9e979e6b8cf6",
      "m1": "سپاه پراگنده را گرد کرد",
      "m2": "بزد کوس وز دشت برخاست گرد",
      "verse_number": 32
    },
    {
      "id": "e6bdfab4-ce5b-494a-9aa2-9e5611008d29",
      "m1": "فراز آمدش تیغزن صد هزار",
      "m2": "همه جنگجوی از در کارزار",
      "verse_number": 33
    },
    {
      "id": "47335e28-a125-48cb-a58f-f5a7c8380707",
      "m1": "درم داد و آن لشکر آباد کرد",
      "m2": "دل مردم کینهور شاد کرد",
      "verse_number": 34
    },
    {
      "id": "d3960e42-45e2-45c1-8097-5deb4e74ac0d",
      "m1": "فرستادهای خواند شیرینزبان",
      "m2": "خردمند و بیدار و روشنروان",
      "verse_number": 35
    },
    {
      "id": "f5b021b6-4d50-45f9-9ad6-27e6affb9482",
      "m1": "یکی نامه بنوشت پر داغ و درد",
      "m2": "دو دیده پر از آب و رخسار زرد",
      "verse_number": 36
    },
    {
      "id": "0d257f14-122f-4058-8a36-d607ada9cb2e",
      "m1": "به نامه درون پندها یاد داد",
      "m2": "ز جمشید و کیخسرو کیقباد",
      "verse_number": 37
    },
    {
      "id": "a7b0b00d-12e3-4935-9329-ec57a99f500c",
      "m1": "وزان پس فرستاد نزد بلاش",
      "m2": "که شاها تو از مرگ غمگین مباش",
      "verse_number": 38
    },
    {
      "id": "93248d1a-2965-431e-86dd-72129c85db81",
      "m1": "که این مرگ هر کس نخواهد چشید",
      "m2": "شکیبایی و نام باید گزید",
      "verse_number": 39
    },
    {
      "id": "2cd04f92-1abc-4e2c-b63d-816107bd8ce7",
      "m1": "ز باد آمده باز گردد بدم",
      "m2": "یکی داد خواندش و دیگر ستم",
      "verse_number": 40
    },
    {
      "id": "6169e03d-b149-473a-b51b-526e75e84d72",
      "m1": "کنون من به دستوری شهریار",
      "m2": "بسیجم برین گونه بر کارزار",
      "verse_number": 41
    },
    {
      "id": "72f880e5-3641-4f87-a285-4c7a11da9c81",
      "m1": "کزین کینه و خون پیروز شاه",
      "m2": "بنالد ز چرخ روان هور و ماه",
      "verse_number": 42
    },
    {
      "id": "a566eb0a-a09e-40a7-ab3b-f9afd7669f20",
      "m1": "فرستاده زین روی برداشت پای",
      "m2": "وزان سوی گریان بشد باز جای",
      "verse_number": 43
    },
    {
      "id": "da8c9ded-bcd0-40ba-9877-5a046e8385d7",
      "m1": "بیاراست لشکر چو پر تذرو",
      "m2": "بیامد ز زاولستان سوی مرو",
      "verse_number": 44
    },
    {
      "id": "ded87fb1-907d-4f67-a8a4-429f23c2341b",
      "m1": "یکی مرد بگزید بیداردل",
      "m2": "که آهسته دارد به گفتار دل",
      "verse_number": 45
    },
    {
      "id": "cfd83076-b7ac-4304-a60f-36816e89dfac",
      "m1": "نویسندهٔ نامه را گفت خیز",
      "m2": "که آمد سر خامه را رستخیز",
      "verse_number": 46
    },
    {
      "id": "b1fba76d-c32b-4b7a-88b8-c4b5ea9f7a92",
      "m1": "یکی نامه بنویس زی خوشنواز",
      "m2": "که ای بیخرد روبه دیوساز",
      "verse_number": 47
    },
    {
      "id": "376241ff-61c5-4432-adab-7466133ff026",
      "m1": "گنهکار کردی به یزدان تنت",
      "m2": "شود مویه گر بر تو پیراهنت",
      "verse_number": 48
    },
    {
      "id": "9f3b8ef0-71e6-4612-88cf-967d34b87a33",
      "m1": "به شاه آنک تو کردی ای بیوفا",
      "m2": "ببینی کنون زور تیغ جفا",
      "verse_number": 49
    },
    {
      "id": "e2d82e0f-ef96-42f4-b3e6-35aa62b0daf5",
      "m1": "به کشتی شهنشاه را بیگناه",
      "m2": "نبیره جهاندار بهرام شاه",
      "verse_number": 50
    },
    {
      "id": "dcdc2983-cfb4-4c59-bc33-d64515736073",
      "m1": "یکی کین نو ساختی در جهان",
      "m2": "که آن کینه هرگز نگردد نهان",
      "verse_number": 51
    },
    {
      "id": "de0c35b6-1cd9-4287-9219-bcd2d76a8cec",
      "m1": "چرا پیش او چون یکی چابلوس",
      "m2": "نرفتی چو برخاست آوای کوس",
      "verse_number": 52
    },
    {
      "id": "bcf479e5-d0fe-4dd1-b01a-cf04b6cd77d0",
      "m1": "نیای تو زین خاندان زنده بود",
      "m2": "پدر پیش بهرام پاینده بود",
      "verse_number": 53
    },
    {
      "id": "72e0b84a-ff75-42d9-a372-e5283ea5fb74",
      "m1": "من اینک به مرو آمدم کینهخواه",
      "m2": "نماند به هیتالیان تاج و گاه",
      "verse_number": 54
    },
    {
      "id": "0547c7ae-5328-4ef3-9071-69106ba4e3df",
      "m1": "اسیران و آن خواسته هرچ هست",
      "m2": "که از رزمگاه آمدستت بدست",
      "verse_number": 55
    },
    {
      "id": "bb80445f-8c86-4dc4-8546-b25d8c6291cf",
      "m1": "همه بازخواهم به شمشیر کین",
      "m2": "به مرو آورم خاک توران زمین",
      "verse_number": 56
    },
    {
      "id": "f27f7a19-773e-4688-baa7-39b5a73164c0",
      "m1": "نمانم جهان را بفرزند تو",
      "m2": "نه بر دوده و خویش و پیوند تو",
      "verse_number": 57
    },
    {
      "id": "a1e6877f-4060-4b02-a4c2-c402227ad70f",
      "m1": "بفرمان یزدان ببرم سرت",
      "m2": "ز خون همچو دریا کنم کشورت",
      "verse_number": 58
    },
    {
      "id": "5fbe23c5-b337-4cdc-b3fe-4d278d768660",
      "m1": "نه کین باشد این چند گویم دراز",
      "m2": "که از کین پیروز با خوشنواز",
      "verse_number": 59
    },
    {
      "id": "ccfef69c-c682-4886-bc81-a10da9b47c94",
      "m1": "شود زیر خاک پی من تباه",
      "m2": "به یزدان روانش بود دادخواه",
      "verse_number": 60
    },
    {
      "id": "29cba632-5180-4c7e-b676-b3e375af1fc1",
      "m1": "فرستاده با نامهٔ سوفزای",
      "m2": "بیامد چو شیر دلاور ز جای",
      "verse_number": 61
    },
    {
      "id": "0013d119-0274-43c6-817f-c7a5caf51eba",
      "m1": "چو آشفته آمد بر خوشنواز",
      "m2": "بشد پیش تخت و ببردش نماز",
      "verse_number": 62
    },
    {
      "id": "f095b4f1-01ca-4311-b01d-49699976dfab",
      "m1": "بدو داد پس نامهٔ سوفزای",
      "m2": "همیبود یک چند پیشش بپای",
      "verse_number": 63
    },
    {
      "id": "c1bc9634-149c-4617-844d-748ff4acb1aa",
      "m1": "نویسندهٔ نامه را داد و گفت",
      "m2": "که پنهان بگوی آنچ نرمست و زفت",
      "verse_number": 64
    },
    {
      "id": "6ca6a288-31ef-4898-bd61-dfc91ce81e27",
      "m1": "به مهتر چنین گفت مرد دبیر",
      "m2": "که این نامه پر گرز و تیغست و تیر",
      "verse_number": 65
    },
    {
      "id": "922b5d6f-00c1-4517-955d-23e64a17373e",
      "m1": "شکسته شد آن مرد جنگآزمای",
      "m2": "ازان پر سخن نامهٔ سوفزار",
      "verse_number": 66
    },
    {
      "id": "e954ed34-c066-44a9-ba8c-93feb2198cdb",
      "m1": "هم اندر زمان زود پاسخ نبشت",
      "m2": "سخن هرچ بود اندرو خوب و زشت",
      "verse_number": 67
    },
    {
      "id": "ba7da52f-fdd8-40f9-871b-939588d5f633",
      "m1": "نخستین چنین گفت کز کردگار",
      "m2": "بترسیم وز گردش روزگار",
      "verse_number": 68
    },
    {
      "id": "d2b4a5d2-71e9-4572-a44d-46c0778d6db3",
      "m1": "که هر کس که بودست یزدانپرست",
      "m2": "نیاورد در عهد شاهان شکست",
      "verse_number": 69
    },
    {
      "id": "c67da070-567c-4bca-9425-fdc32baa6628",
      "m1": "فرستادمش نامهٔ پندمند",
      "m2": "دگر عهد آن شهریار بلند",
      "verse_number": 70
    },
    {
      "id": "6e3158d1-a2fa-484e-92b8-e435752355e4",
      "m1": "برو خوار بود آنچ گفتم سخن",
      "m2": "هم اندیشهٔ روزگار کهن",
      "verse_number": 71
    },
    {
      "id": "122ecd15-579a-4bca-a181-3b421ca2d389",
      "m1": "چو او کینهور گشت و من چارهجوی",
      "m2": "سپه را چو روی اندر آمد به روی",
      "verse_number": 72
    },
    {
      "id": "7dd8543a-a615-4422-9440-555491e737d5",
      "m1": "به پیروز بر اختر آشفته شد",
      "m2": "نه برکام من شاه تو کشته شد",
      "verse_number": 73
    },
    {
      "id": "f3039676-7a17-4ef7-af05-8690f4d82c7b",
      "m1": "چو بشکست پیمان شاهان داد",
      "m2": "نبود از جوانیش یک روز شاد",
      "verse_number": 74
    },
    {
      "id": "bd229a47-f946-4195-857b-5b04518a76ca",
      "m1": "نیامد پسند جهانآفرین",
      "m2": "تو گویی که بگرفت پایش زمین",
      "verse_number": 75
    },
    {
      "id": "ae8dd5fd-1d28-4963-8ddc-5db127eb1434",
      "m1": "هر آنکس که عهد نیا بشکند",
      "m2": "سر راستی را بپای افگند",
      "verse_number": 76
    },
