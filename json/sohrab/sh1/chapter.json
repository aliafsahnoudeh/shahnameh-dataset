{
  "id": "b50b1ae7-d20c-4c20-98f5-9210a5f0f481",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "94abdc00-6aaf-4f30-b8eb-0e5ed5d6a0d4",
      "m1": "اگر تندبادی براید ز کنج",
      "m2": "بخاک افگند نارسیده ترنج",
      "verse_number": 1
    },
    {
      "id": "9eab7b8f-c1cf-45fc-a191-c92550709eed",
      "m1": "ستمکاره خوانیمش ار دادگر",
      "m2": "هنرمند دانیمش ار بیهنر",
      "verse_number": 2
    },
    {
      "id": "88bb79a2-afe1-47f7-bc51-f8256d400505",
      "m1": "اگر مرگ دادست بیداد چیست",
      "m2": "ز داد این همه بانگ و فریاد چیست",
      "verse_number": 3
    },
    {
      "id": "1ff1f99e-9f97-4c54-8010-8ab0a3f0c12b",
      "m1": "ازین راز جان تو آگاه نیست",
      "m2": "بدین پرده اندر ترا راه نیست",
      "verse_number": 4
    },
    {
      "id": "1045c6cc-2e11-4099-b2ec-3987f3dfb6c5",
      "m1": "همه تا در آز رفته فراز",
      "m2": "به کس بر نشد این در راز باز",
      "verse_number": 5
    },
    {
      "id": "6ed6300e-19eb-4615-9c57-484e4915b3e0",
      "m1": "برفتن مگر بهتر آیدش جای",
      "m2": "چو آرام یابد به دیگر سرای",
      "verse_number": 6
    },
    {
      "id": "7d0df735-1515-469d-9718-4567ff1cca5f",
      "m1": "دم مرگ چون آتش هولناک",
      "m2": "ندارد ز برنا و فرتوت باک",
      "verse_number": 7
    },
    {
      "id": "dd6c4f1d-0567-43de-a415-28df07cdab85",
      "m1": "درین جای رفتن نه جای درنگ",
      "m2": "بر اسپ فنا گر کشد مرگ تنگ",
      "verse_number": 8
    },
    {
      "id": "f9d9c3fe-c5db-4364-915d-bf10d947f4d6",
      "m1": "چنان دان که دادست و بیداد نیست",
      "m2": "چو داد آمدش جای فریاد نیست",
      "verse_number": 9
    },
    {
      "id": "26b001e5-5e75-4dbd-b43f-92ad40f37c23",
      "m1": "جوانی و پیری به نزدیک مرگ",
      "m2": "یکی دان چو اندر بدن نیست برگ",
      "verse_number": 10
    },
    {
      "id": "39e5fdf8-5d14-4206-89e8-408bfd7eac76",
      "m1": "دل از نور ایمان گر آگندهای",
      "m2": "ترا خامشی به که تو بندهای",
      "verse_number": 11
    },
    {
      "id": "4d0b5987-e0bd-483b-b35f-80ede8df0fd1",
      "m1": "برین کار یزدان ترا راز نیست",
      "m2": "اگر جانت با دیو انباز نیست",
      "verse_number": 12
    },
    {
      "id": "fcabff47-8f82-431a-a369-b2bdf9402b9c",
      "m1": "به گیتی دران کوش چون بگذری",
      "m2": "سرانجام نیکی بر خود بری",
      "verse_number": 13
    },
    {
      "id": "a4da0584-a1f6-422e-9d22-3023cde1012a",
      "m1": "کنون رزم سهراب رانم نخست",
      "m2": "ازان کین که او با پدر چون بجست",
      "verse_number": 14
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 14
  }
}