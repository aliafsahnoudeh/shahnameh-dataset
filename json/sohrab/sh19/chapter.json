{
  "id": "3a46a321-4f40-4bd6-879f-5fbc8c3a7295",
  "title": "بخش ۱۹",
  "verses": [
    {
      "id": "9b489e84-f945-4571-b810-ed716af2b709",
      "m1": "به گودرز گفت آن زمان پهلوان",
      "m2": "کز ایدر برو زود روشن روان",
      "verse_number": 1
    },
    {
      "id": "12cbf70d-f53d-4259-9249-cf9162aeee2b",
      "m1": "پیامی ز من پیش کاووس بر",
      "m2": "بگویش که مارا چه آمد به سر",
      "verse_number": 2
    },
    {
      "id": "56ecec58-4370-48ea-8377-f94ba2450ab2",
      "m1": "به دشنه جگرگاه پور دلیر",
      "m2": "دریدم که رستم مماناد دیر",
      "verse_number": 3
    },
    {
      "id": "70dd1831-62d9-480a-9539-af43c7d71541",
      "m1": "گرت هیچ یادست کردار من",
      "m2": "یکی رنجه کن دل به تیمار من",
      "verse_number": 4
    },
    {
      "id": "94438a66-a67d-4734-961d-130e408abc9e",
      "m1": "ازان نوشدارو که در گنج تست",
      "m2": "کجا خستگان را کند تن درست",
      "verse_number": 5
    },
    {
      "id": "b61e3059-ba1b-4490-a3e1-8b0cd19e18ec",
      "m1": "به نزدیک من با یکی جام می",
      "m2": "سزد گر فرستی هم اکنون به پی",
      "verse_number": 6
    },
    {
      "id": "80d8a4fd-626f-4eed-89af-3f01241313e7",
      "m1": "مگر کاو ببخت تو بهتر شود",
      "m2": "چو من پیش تخت تو کهتر شود",
      "verse_number": 7
    },
    {
      "id": "9d2b22ac-64b9-4672-8a76-eed6e4fb776f",
      "m1": "بیامد سپهبد بکردار باد",
      "m2": "به کاووس یکسر پیامش بداد",
      "verse_number": 8
    },
    {
      "id": "a120bf2d-3bbe-4b8d-92d7-20a842978537",
      "m1": "بدو گفت کاووس کز انجمن",
      "m2": "اگر زنده ماند چنان پیلتن",
      "verse_number": 9
    },
    {
      "id": "eedb6760-ca14-426d-93a4-dd16145ccce1",
      "m1": "شود پشت رستم به نیرو ترا",
      "m2": "هلاک آورد بیگمانی مرا",
      "verse_number": 10
    },
    {
      "id": "50e9bf3c-2fed-457e-8477-b3bfb6892da3",
      "m1": "اگر یک زمان زو به من بد رسد",
      "m2": "نسازیم پاداش او جز به بد",
      "verse_number": 11
    },
    {
      "id": "79568299-0ea2-4166-a814-825e6ff46ade",
      "m1": "کجا گنجد او در جهان فراخ",
      "m2": "بدان فر و آن برز و آن یال و شاخ",
      "verse_number": 12
    },
    {
      "id": "d8d46e78-adad-402f-96ca-30d759f1576c",
      "m1": "شنیدی که او گفت کاووس کیست",
      "m2": "گر او شهریارست پس طوس کیست",
      "verse_number": 13
    },
    {
      "id": "cab880c1-6a38-4504-ab7d-4f29aae0b365",
      "m1": "کجا باشد او پیش تختم به پای",
      "m2": "کجا راند او زیر فر همای",
      "verse_number": 14
    },
    {
      "id": "8928f375-a4ba-4e68-8f52-ebaf36efd196",
      "m1": "چو بشنید گودرز برگشت زود",
      "m2": "بر رستم آمد به کردار دود",
      "verse_number": 15
    },
    {
      "id": "1701a549-c82c-4176-9905-1244a3c6cb2d",
      "m1": "بدو گفت خوی بد شهریار",
      "m2": "درختیست خنگی همیشه به بار",
      "verse_number": 16
    },
    {
      "id": "9fb0dddc-e125-467c-86f2-c9d3647cc470",
      "m1": "ترا رفت باید به نزدیک او",
      "m2": "درفشان کنی جان تاریک او",
      "verse_number": 17
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 17
  }
}