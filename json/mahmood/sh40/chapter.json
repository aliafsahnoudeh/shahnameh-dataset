{
  "id": "30a0447c-8460-4860-b40f-e2e068652d3a",
  "title": "بخش ۴۰",
  "verses": [
    {
      "id": "2f328b05-c221-4595-b019-b5078b1954f8",
      "m1": "بفرمود تا رفت پیشش دبیر",
      "m2": "بیاورد قرطاس و مشک و عبیر",
      "verse_number": 1
    },
    {
      "id": "64d3b6ae-bbfe-4a61-bf51-3ad0067e4e0e",
      "m1": "نبشتند عهدی ز شاه زمین",
      "m2": "سرافراز کیخسرو پاکدین",
      "verse_number": 2
    },
    {
      "id": "b08dec87-90e8-4cfb-80af-30c384b8fba4",
      "m1": "ز بهر سپهبد گو پیلتن",
      "m2": "ستوده به مردی به هر انجمن",
      "verse_number": 3
    },
    {
      "id": "dae5e264-0bd7-49b2-a34e-312e18f417ab",
      "m1": "که او باشد اندر جهان پیشرو",
      "m2": "جهاندار و بیدار و سالار و گو",
      "verse_number": 4
    },
    {
      "id": "cee1e3b3-8504-49bc-a8ba-854c9fed2ced",
      "m1": "هم او را بود کشور نیمروز",
      "m2": "سپهدار پیروز لشکر فروز",
      "verse_number": 5
    },
    {
      "id": "7e778845-9a4c-423d-b860-cc7d2d042bcd",
      "m1": "نهادند بر عهد بر مهر زر",
      "m2": "بر آیین کیخسرو دادگر",
      "verse_number": 6
    },
    {
      "id": "e8ec3223-9da1-413a-8aab-f9f898c96270",
      "m1": "بدو داد منشور و کرد آفرین",
      "m2": "که آباد بادا به رستم زمین",
      "verse_number": 7
    },
    {
      "id": "75d8e3fb-8093-4b07-8fbc-6555add782e1",
      "m1": "مهانی که با زال سام سوار",
      "m2": "برفتند با زیجها بر کنار",
      "verse_number": 8
    },
    {
      "id": "3c680a17-1cfd-4f09-8105-5796625fafb4",
      "m1": "ببخشیدشان خلعت و سیم و زر",
      "m2": "یکی جام مر هر یکی را گهر",
      "verse_number": 9
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 9
  }
}