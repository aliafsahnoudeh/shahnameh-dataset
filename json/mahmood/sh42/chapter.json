{
  "id": "b2ecc60b-0191-4aa6-82dc-fa256f68e981",
  "title": "بخش ۴۲",
  "verses": [
    {
      "id": "535d7a90-9c48-46a8-a6f9-ed9e821c8653",
      "m1": "چو گودرز بنشست برخاست طوس",
      "m2": "بشد پیش خسرو زمین داد بوس",
      "verse_number": 1
    },
    {
      "id": "baccae71-94bb-407f-a869-e65a6f817ba1",
      "m1": "بدو گفت شاها انوشه بدی",
      "m2": "همیشه ز تو دور دست بدی",
      "verse_number": 2
    },
    {
      "id": "dc00209d-2b87-46dd-b981-ee3aa89db5c7",
      "m1": "منم ز این بزرگان فریدون نژاد",
      "m2": "ز نام آوران تا بیامد قباد",
      "verse_number": 3
    },
    {
      "id": "4eb9fe73-4af7-43ef-856e-1a9c6be2a681",
      "m1": "کمر بستهام پیش ایرانیان",
      "m2": "که نگشادم از بند هرگز میان",
      "verse_number": 4
    },
    {
      "id": "ba2f310d-22aa-4838-8897-eb63ee669a0b",
      "m1": "به کوه هماون ز جوشن تنم",
      "m2": "بخست و همان بود پیراهنم",
      "verse_number": 5
    },
    {
      "id": "c07c6e98-61f0-48a9-bedc-665edd601c4f",
      "m1": "به کین سیاوش بر آن رزمگاه",
      "m2": "بدم هر شبی پاسبان سپاه",
      "verse_number": 6
    },
    {
      "id": "f3af31ff-5e25-4fea-bbad-d8d11c82bef1",
      "m1": "به لاون سپه را نکردم رها",
      "m2": "همی بودم اندر دم اژدها",
      "verse_number": 7
    },
    {
      "id": "ec7c8227-fc30-4ca0-98bc-a4d5dceeec39",
      "m1": "به مازندران بسته کاووس بود",
      "m2": "دگر بند بر گردن طوس بود",
      "verse_number": 8
    },
    {
      "id": "7f1dd589-2459-4e28-b401-7c1f40c4a6af",
      "m1": "نکردم سپه را به جایی یله",
      "m2": "نه از من کسی کرد هرگز گله",
      "verse_number": 9
    },
    {
      "id": "5aa9ecb0-2994-44c8-bee9-a1cae72a9257",
      "m1": "کنون شاه سیر آمد از تاج و گنج",
      "m2": "همی بگذرد ز این سرای سپنج",
      "verse_number": 10
    },
    {
      "id": "80bf78de-5121-4cb5-a751-401191636e01",
      "m1": "چه فرمایدم چیست نیروی من",
      "m2": "تو دانی هنرها و آهوی من",
      "verse_number": 11
    },
    {
      "id": "90b14900-8710-4941-ac21-312a5e11d72e",
      "m1": "چنین داد پاسخ بدو شهریار",
      "m2": "که بیش است رنج تو از روزگار",
      "verse_number": 12
    },
    {
      "id": "d5982d32-c9fb-4bd3-ad99-00efdd891699",
      "m1": "همی باش با کاویانی درفش",
      "m2": "تو باشی سپهدار زرینه کفش",
      "verse_number": 13
    },
    {
      "id": "99d09eb9-58d5-461c-affe-62af1f5f141b",
      "m1": "بدین مرز گیتی خراسان تراست",
      "m2": "از این نامداران تنآسان تراست",
      "verse_number": 14
    },
    {
      "id": "afe5d649-9a51-435e-a1d8-757b0051a301",
      "m1": "نبشتند عهدی بر آن هم نشان",
      "m2": "به پیش بزرگان گردنکشان",
      "verse_number": 15
    },
    {
      "id": "f65d7a89-cc6f-4239-8b00-dc4604e8f162",
      "m1": "نهادند بر عهد بر مهر زر",
      "m2": "یکی طوق زرین و زرین کمر",
      "verse_number": 16
    },
    {
      "id": "2bbef3ff-10ec-4652-b722-6f7dec2d393b",
      "m1": "بدو داد و کردش بسی آفرین",
      "m2": "که از تو مبادا دلی پر ز کین",
      "verse_number": 17
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 17
  },
  "order_in_parent": 42
}