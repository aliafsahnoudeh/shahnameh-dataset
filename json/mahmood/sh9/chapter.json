{
  "id": "3d977d37-b83b-45a9-982c-ed70d9c7103e",
  "title": "بخش ۹",
  "verses": [
    {
      "id": "04d7406a-5584-4c59-9eff-aea9bb7586e4",
      "m1": "چو نیمی ز تیره شب اندر گذشت",
      "m2": "سپهر از بر کوه ساکن بگشت",
      "verse_number": 1
    },
    {
      "id": "abdb0479-5403-4459-a6da-a00804cb6588",
      "m1": "سپهدار ترکان بنه بر نهاد",
      "m2": "سپه را همه ترگ و جوشن بداد",
      "verse_number": 2
    },
    {
      "id": "bd85751f-3788-41ae-a7a8-4ebe02d922da",
      "m1": "طلایه بفرمود تا ده هزار",
      "m2": "بود ترک برگستوان ور سوار",
      "verse_number": 3
    },
    {
      "id": "4c15f101-e4a9-4a68-bc31-8d0536ab2959",
      "m1": "چنین گفت با لشکر افراسیاب",
      "m2": "که من چون گذر یابم از رود آب",
      "verse_number": 4
    },
    {
      "id": "851183f8-e6b0-4b58-94c0-8d0aa694cf88",
      "m1": "دمادم شما از پسم بگذرید",
      "m2": "به جیحون و زورق زمان مشمرید",
      "verse_number": 5
    },
    {
      "id": "89b19a2b-4566-4cf6-8dec-2e3ea369e15a",
      "m1": "شب تیره با لشکر افراسیاب",
      "m2": "گذر کرد از آموی و بگذاشت آب",
      "verse_number": 6
    },
    {
      "id": "4d7139e9-dd66-4c18-b8a6-f0e1aba06a6b",
      "m1": "همه روی کشور به بی راه و راه",
      "m2": "سراپرده و خیمه بد بی سپاه",
      "verse_number": 7
    },
    {
      "id": "3f779143-494b-4a9d-ba61-c9aed24811a8",
      "m1": "سپیده چو از باختر بردمید",
      "m2": "طلایه سپه را به هامون ندید",
      "verse_number": 8
    },
    {
      "id": "17113530-2afe-42bb-bb6b-2cebae07d92c",
      "m1": "بیامد به مژده بر شهریار",
      "m2": "که پردخته شد شاه ز این کارزار",
      "verse_number": 9
    },
    {
      "id": "3d0d6e18-68c6-4e01-8dd5-b22fdc0be0bf",
      "m1": "همه دشت خیمهست و پردهسرای",
      "m2": "ز دشمن سواری نبینم به جای",
      "verse_number": 10
    },
    {
      "id": "fe79c47e-14eb-4e3f-bc48-c95a637a5478",
      "m1": "چو بشنید خسرو دوان شد به خاک",
      "m2": "نیایش کنان پیش یزدان پاک",
      "verse_number": 11
    },
    {
      "id": "afe602d3-36a9-4c43-b754-20d923d778da",
      "m1": "همی گفت کای روشن کردگار",
      "m2": "جهاندار و بیدار و پروردگار",
      "verse_number": 12
    },
    {
      "id": "316f2143-129c-4bf9-b500-84b448ab8ba3",
      "m1": "تو دادی مرا فر و دیهیم و زور",
      "m2": "تو کردی دل و چشم بدخواه کور",
      "verse_number": 13
    },
    {
      "id": "8e0093da-974b-43c4-ac2c-a8f58dd20fc0",
      "m1": "ز گیتی ستمکاره را دور کن",
      "m2": "ز بیمش همه ساله رنجور کن",
      "verse_number": 14
    },
    {
      "id": "d82d8fb9-23c2-4bc0-afbb-8342a11a9c2f",
      "m1": "چو خورشید زرین سپر برگرفت",
      "m2": "شب آن شعر پیروزه بر سر گرفت",
      "verse_number": 15
    },
    {
      "id": "fa1ed561-c854-4271-895b-68d6c1d78c78",
      "m1": "جهاندار بنشست بر تخت عاج",
      "m2": "به سر بر نهاد آن دل افروز تاج",
      "verse_number": 16
    },
    {
      "id": "08752ead-bc8c-4f30-99b8-c48e0475d1b8",
      "m1": "نیایش کنان پیش او شد سپاه",
      "m2": "که جاوید باد این سزاوار گاه",
      "verse_number": 17
    },
    {
      "id": "f5d0bbfb-7f1f-4421-bd1c-e0e92d639210",
      "m1": "شد این لشکر از خواسته بینیاز",
      "m2": "که از لشکر شاه چین ماند باز",
      "verse_number": 18
    },
    {
      "id": "4f4fa721-9918-4bd6-aafb-507297c74986",
      "m1": "همی گفت هر کس که اینت فسوس",
      "m2": "که او رفت با لشکر و بوق و کوس",
      "verse_number": 19
    },
    {
      "id": "2ddb260d-ba37-4d6d-b541-eb837e38c9a3",
      "m1": "شب تیره از دست پرمایگان",
      "m2": "بشد نامداری چنین رایگان",
      "verse_number": 20
    },
    {
      "id": "6ab6632f-24e2-419b-af57-583a6382c7fc",
      "m1": "بدیشان چنین گفت بیدار شاه",
      "m2": "که ای نامداران ایران سپاه",
      "verse_number": 21
    },
    {
      "id": "6d3df5c8-bdf0-4207-b476-b6431d18a4bb",
      "m1": "چو دشمن بود شاه را کشته به",
      "m2": "گر آواره از جنگ برگشته به",
      "verse_number": 22
    },
    {
      "id": "445d378a-3ad3-4e1d-9f05-91fe1764a957",
      "m1": "چو پیروزگر دادمان فرهی",
      "m2": "بزرگی و دیهیم شاهنشهی",
      "verse_number": 23
    },
    {
      "id": "fe2265f9-49dd-4d94-8fb3-895a1c486292",
      "m1": "ز گیتی ستایش مر او را کنید",
      "m2": "شب آید نیایش مر او را کنید",
      "verse_number": 24
    },
    {
      "id": "8aa6a5c9-b427-48e9-8470-aabf4368c217",
      "m1": "که آن را که خواهد کند شوربخت",
      "m2": "یکی بی هنر برنشاند به تخت",
      "verse_number": 25
    },
    {
      "id": "e57d5997-82a2-4057-8c7f-f19945e0bc69",
      "m1": "از این کوشش و پرسشت رای نیست",
      "m2": "که با داد او بنده را پای نیست",
      "verse_number": 26
    },
    {
      "id": "fd72f016-1f47-4c85-ad46-d922f1ea1134",
      "m1": "بباشم بدین رزمگه پنج روز",
      "m2": "ششم روز هرمزد گیتی فروز",
      "verse_number": 27
    },
    {
      "id": "d921c294-4ce6-4303-8747-679f97c85f4f",
      "m1": "برآید برانیم ز ایدر سپاه",
      "m2": "که او کین فزایست و ما کینه خواه",
      "verse_number": 28
    },
    {
      "id": "3a7c8678-d465-44f8-96f3-4dd107bc2c7d",
      "m1": "بدین پنج روز اندر این رزمگاه",
      "m2": "همی کشته جستند ز ایران سپاه",
      "verse_number": 29
    },
    {
      "id": "a675ba6d-7b9c-433d-9952-eec625399db3",
      "m1": "بشستند ایرانیان را ز گرد",
      "m2": "سزاوار هر یک یکی دخمه کرد",
      "verse_number": 30
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 30
  }
}