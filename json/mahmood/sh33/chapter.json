{
  "id": "e5a6088f-78c3-47db-aa65-1b1dabcd3020",
  "title": "بخش ۳۳",
  "verses": [
    {
      "id": "7c2aae1a-9d78-4639-a6b9-6e694a095a13",
      "m1": "چو بشنید خسرو ز دستان سخن",
      "m2": "یکی دانشی پاسخ افگند بن",
      "verse_number": 1
    },
    {
      "id": "40aa1d03-f05a-4f5a-99b6-23edfb88a22d",
      "m1": "بدو گفت کای پیر پاکیزه مغز",
      "m2": "همه رای و گفتارهای تو نغز",
      "verse_number": 2
    },
    {
      "id": "757eceb5-3ce1-4863-8e43-e49e76412242",
      "m1": "ز گاه منوچهر تا این زمان",
      "m2": "نهای جز بیآزار و نیکی گمان",
      "verse_number": 3
    },
    {
      "id": "d7779cdd-3b99-4377-931c-9a8716caed3c",
      "m1": "همان نامور رستم پیلتن",
      "m2": "ستون کیان نازش انجمن",
      "verse_number": 4
    },
    {
      "id": "743e31eb-9776-4775-991f-d05f91422929",
      "m1": "سیاووش را پروراننده اوست",
      "m2": "بدو نیکویها رساننده اوست",
      "verse_number": 5
    },
    {
      "id": "e4266b56-07e4-4875-bb85-a651a6665646",
      "m1": "سپاهی که دیدند گوپال او",
      "m2": "سر ترگ و برز و فر و یال او",
      "verse_number": 6
    },
    {
      "id": "8dab118e-977a-400d-87ee-1593548b649d",
      "m1": "بسی جنگ ناکرده بگریختند",
      "m2": "همه دشت تیر و کمان ریختند",
      "verse_number": 7
    },
    {
      "id": "c1626598-b1d3-4796-a7b0-29c9d027506a",
      "m1": "به پیش نیاکان من کینهخواه",
      "m2": "چو دستور فرخ نماینده راه",
      "verse_number": 8
    },
    {
      "id": "b1084662-85e5-4f56-baa2-d3d20e520506",
      "m1": "وگر نام و رنج تو گیرم به یاد",
      "m2": "بماند سخن تازه تا صد نژاد",
      "verse_number": 9
    },
    {
      "id": "6e62cfc4-593c-49cd-a673-590e80dc01fc",
      "m1": "ز گفتار چرب ار پژوهش کنم",
      "m2": "ترا این ستایش نکوهش کنم",
      "verse_number": 10
    },
    {
      "id": "2f1c3248-2466-46cb-9edc-008e913d3df5",
      "m1": "دگر هرچه پرسیدی از کار من",
      "m2": "ز نادادن بار و آزار من",
      "verse_number": 11
    },
    {
      "id": "cc56164a-7940-4c6a-afda-2d089bc50b98",
      "m1": "به یزدان یکی آرزو داشتم",
      "m2": "جهان را همه خوار بگذاشتم",
      "verse_number": 12
    },
    {
      "id": "4746d2dc-19e2-4de3-a201-c116a7159b40",
      "m1": "کنون پنج هفتست تا من به پای",
      "m2": "همی خواهم از داور رهنمای",
      "verse_number": 13
    },
    {
      "id": "9c89048c-3684-4077-8154-0a812530e410",
      "m1": "که بخشد گذشته گناه مرا",
      "m2": "درخشان کند تیره گاه مرا",
      "verse_number": 14
    },
    {
      "id": "0131af36-bbbc-4ae7-bbdd-a593e907fb51",
      "m1": "برد مر مرا ز این سپنجی سرای",
      "m2": "بود در همه نیکوی رهنمای",
      "verse_number": 15
    },
    {
      "id": "e19187dc-af43-4231-bdfc-fd301dbdfc64",
      "m1": "نماند کز این راستی بگذرم",
      "m2": "چو شاهان پیشین یپیچد سرم",
      "verse_number": 16
    },
    {
      "id": "16a61749-efa9-4241-80ad-8d0480959e0c",
      "m1": "کنون یافتم هرچه جستم ز کام",
      "m2": "بباید پسیچید کآمد خرام",
      "verse_number": 17
    },
    {
      "id": "e23f2663-34d9-42f6-bbc6-ce6b3b399bc4",
      "m1": "سحرگه مرا چشم بغنود دوش",
      "m2": "ز یزدان بیامد خجسته سروش",
      "verse_number": 18
    },
    {
      "id": "24acae42-8303-42d5-9fd9-6986121c3a95",
      "m1": "که برساز کآمد گه رفتنت",
      "m2": "سرآمد نژندی و ناخفتنت",
      "verse_number": 19
    },
    {
      "id": "325c935f-7d9c-49a2-be49-ad29c4d600c0",
      "m1": "کنون بارگاه من آمد به سر",
      "m2": "غم لشکر و تاج و تخت و کمر",
      "verse_number": 20
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 20
  },
