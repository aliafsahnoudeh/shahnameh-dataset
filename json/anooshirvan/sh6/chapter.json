      "id": "75e48f91-a9e6-453a-bc20-0471d13a2c28",
      "m1": "بیاورد زان پس شتر دو هزار",
      "m2": "همه گنج قنوح کردند بار",
      "verse_number": 154
    },
    {
      "id": "67211d75-0990-41ec-92e7-63d58ce01c36",
      "m1": "ز عود و ز عنبر ز کافور و زر",
      "m2": "همه جامه وجام پیکر گهر",
      "verse_number": 155
    },
    {
      "id": "06c9b089-7919-48b7-9522-7146bda8cb8d",
      "m1": "ابا باژ یکساله از پیشگاه",
      "m2": "فرستاد یک سر به درگاه شاه",
      "verse_number": 156
    },
    {
      "id": "c709b527-62dd-40b9-8303-ed28d0195535",
      "m1": "یکی افسری خواست از گنج رای",
      "m2": "همان جامهٔ زر ز سر تا به پای",
      "verse_number": 157
    },
    {
      "id": "1e891181-40b6-40f5-b3df-c74ffb2c9b24",
      "m1": "بدو داد وچند آفرین کرد نیز",
      "m2": "بیارانش بخشید بسیار چیز",
      "verse_number": 158
    },
    {
      "id": "33f41c88-ece2-41ac-a53e-048bb37da73a",
      "m1": "شتر دو هزار آنک از پیش برد",
      "m2": "ابا باژ و هدیه مر او را سپرد",
      "verse_number": 159
    },
    {
      "id": "eaa06a47-eb92-43ab-9e72-22ae57facc71",
      "m1": "یکی کاروان بد که کس پیش ازان",
      "m2": "نراند و نبد خواسته بیش ازان",
      "verse_number": 160
    },
    {
      "id": "2b646d73-075d-41dd-94a8-c50016f2797d",
      "m1": "بیامد ز قنوج بوزرجمهر",
      "m2": "برافراخته سر بگردان سپهر",
      "verse_number": 161
    },
    {
      "id": "7eaa5c4e-c8a7-4d50-b319-7009f620f79d",
      "m1": "دلی شاد با نامه شاه هند",
      "m2": "نبشته به هندی خطی بر پرند",
      "verse_number": 162
    },
    {
      "id": "a2c58bd3-e943-42d0-af62-a496841662bf",
      "m1": "که رای و بزرگان گوایی دهند",
      "m2": "نه از بیم کزنیک رایی دهند",
      "verse_number": 163
    },
    {
      "id": "326f50ab-35a9-4bb6-9563-50ba162413d2",
      "m1": "که چون شاه نوشینروان کس ندید",
      "m2": "نه از موبد سالخورده شنید",
      "verse_number": 164
    },
    {
      "id": "4f151151-8b67-4964-a341-c3c92ce50752",
      "m1": "نه کس دانشی تر ز دستور اوی",
      "m2": "ز دانش سپهرست گنجور اوی",
      "verse_number": 165
    },
    {
      "id": "e8ad13ea-4da5-427b-aa02-ee5b5b13919e",
      "m1": "فرستاده شد باژ یک ساله پیش",
      "m2": "اگر بیش باید فرستیم بیش",
      "verse_number": 166
    },
    {
      "id": "10b3c095-7c7c-46df-bc9d-ca5d53002e23",
      "m1": "ز باژی که پیمان نهادیم نیز",
      "m2": "فرستاده شد هرچ بایست چیز",
      "verse_number": 167
    },
    {
      "id": "bc9d1d8e-2def-4dcd-9f4b-d4da21fd27d9",
      "m1": "چو آگاهی آمد ز دانا به شاه",
      "m2": "که با کام و با خوبی آمد ز راه",
      "verse_number": 168
    },
    {
      "id": "743b7277-4045-4e92-be7a-ce8c79c7b03c",
      "m1": "ازان آگهی شاد شد شهریار",
      "m2": "بفرمود تاهرک بد نامدار",
      "verse_number": 169
    },
    {
      "id": "91c6b32b-4d56-44a6-a1a7-959622bce5c3",
      "m1": "ز شهر و ز لشکر خبیره شدند",
      "m2": "همه نامداران پذیره شدند",
      "verse_number": 170
    },
    {
      "id": "b4a85880-7116-4442-9179-4646bf59ccdb",
      "m1": "به شهر اندر آمد چنان ارجمند",
      "m2": "به پیروزی شهریار بلند",
      "verse_number": 171
    },
    {
      "id": "a45b7601-f0d1-4111-b403-50c04a864606",
      "m1": "به ایوان چو آمد به نزدیک تخت",
      "m2": "برو شهریار آفرین کرد سخت",
      "verse_number": 172
    },
    {
      "id": "6000a285-db81-49ca-b683-48364daa57f5",
      "m1": "ببر در گرفتش جهاندار شاه",
      "m2": "بپرسیدش از رای وز رنج راه",
      "verse_number": 173
    },
    {
      "id": "9c858708-eac2-4ec4-bc5e-19909a146eb8",
      "m1": "بگفت آنکجا رفت بوزرجمهر",
      "m2": "ازان بخت بیدار و مهر سپهر",
      "verse_number": 174
    },
    {
      "id": "4f18d99e-3368-4840-a7fe-bb9f1273cf1c",
      "m1": "پس آن نامه رای پیروزبخت",
      "m2": "بیاورد و بنهاد در پیش تخت",
      "verse_number": 175
    },
    {
      "id": "4a14775a-8e1a-4378-930c-023df289e5e2",
      "m1": "بفرمود تا یزدگرد دبیر",
      "m2": "بیامد بر شاه دانشپذیر",
      "verse_number": 176
    },
    {
      "id": "edfaca6c-f644-4979-972a-1537e68547c7",
      "m1": "چو آن نامه رای هندی بخواند",
      "m2": "یکی انجمن درشگفتی بماند",
      "verse_number": 177
    },
    {
      "id": "12410ae2-ae8a-4035-83bc-b9d1b62740c4",
      "m1": "هم از دانش و رای بوزرجمهر",
      "m2": "ازان بخت سالار خورشید چهر",
      "verse_number": 178
    },
    {
      "id": "12fb00b5-173d-4163-8619-6208405273ab",
      "m1": "چنین گفت کسری که یزدان سپاس",
      "m2": "که هستم خردمند و نیکیشناس",
      "verse_number": 179
    },
    {
      "id": "8f37a91d-c303-41af-904a-45d5ef0304c5",
      "m1": "مهان تاج وتخت مرا بندهاند",
      "m2": "دل وجان به مهر من آگندهاند",
      "verse_number": 180
    },
    {
      "id": "a85bac43-6834-4ea0-bb07-1a3b01826289",
      "m1": "شگفتیتر از کار بوزرجمهر",
      "m2": "که دانش بدو داد چندین سپهر",
      "verse_number": 181
    },
    {
      "id": "a656bdd7-4bd6-4bba-881f-4b982538cc04",
      "m1": "سپاس از خداوند خورشید وماه",
      "m2": "کزویست پیروزی و دستگاه",
      "verse_number": 182
    },
    {
      "id": "93485848-8df5-4a41-afe1-a9aa2c21f298",
      "m1": "برین داستان برسخن ساختم",
      "m2": "به طلخند و شطرنج پرداختم",
      "verse_number": 183
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 183
  }
}