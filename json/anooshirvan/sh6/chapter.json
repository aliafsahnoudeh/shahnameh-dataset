{
  "id": "cde8184e-71dd-4ec6-854e-c0d7e1e948c5",
  "title": "بخش ۶ - داستان درنهادن شطرنج",
  "verses": [
    {
      "id": "ccd29f11-233d-45dc-a6e7-d65fe1bf1bd9",
      "m1": "چنین گفت موبد که یک روز شاه",
      "m2": "به دیبای رومی بیاراست گاه",
      "verse_number": 1
    },
    {
      "id": "bbf5c9c9-1a23-46af-a921-93678a875ba7",
      "m1": "بیاویخت تاج از بر تخت عاج",
      "m2": "همه جای عاج و همه جای تاج",
      "verse_number": 2
    },
    {
      "id": "51689a39-b3b9-4d8e-ab91-c017a967a80c",
      "m1": "همه کاخ پر موبد و مرزبان",
      "m2": "ز بلخ و ز بامین و ز کرزبان",
      "verse_number": 3
    },
    {
      "id": "459a9128-6928-4c7d-b43d-8d98b5b9541b",
      "m1": "چنین آگهی یافت شاه جهان",
      "m2": "ز گفتار بیدار کارآگهان",
      "verse_number": 4
    },
    {
      "id": "365b53cf-c305-43fd-a6e1-9744a0f9f7c2",
      "m1": "که آمد فرستادهٔ شاه هند",
      "m2": "ابا پیل و چتر و سواران سند",
      "verse_number": 5
    },
    {
      "id": "598ec310-8dd7-4235-903b-ecb208ef3262",
      "m1": "شتروار بارست با او هزار",
      "m2": "همی راه جوید بر شهریار",
      "verse_number": 6
    },
    {
      "id": "35589024-3a22-428d-9541-cd5560604693",
      "m1": "همانگه چو بشنید بیدار شاه",
      "m2": "پذیره فرستاد چندی سپاه",
      "verse_number": 7
    },
    {
      "id": "320082a6-6e87-42bf-bd3b-3b7b4aec7a7f",
      "m1": "چو آمد بر شهریار بزرگ",
      "m2": "فرستادهٔ نامدار و سترگ",
      "verse_number": 8
    },
    {
      "id": "caf57501-57c3-4637-90e4-d6ac8b57a800",
      "m1": "برسم بزرگان نیایش گرفت",
      "m2": "جهان آفرین را ستایش گرفت",
      "verse_number": 9
    },
    {
      "id": "c82bd2b5-7938-40e6-a528-117d8c74bdd5",
      "m1": "گهرکرد بسیار پیشش نثار",
      "m2": "یکی چتر و ده پیل با گوشوار",
      "verse_number": 10
    },
    {
      "id": "a8b16b04-8614-485c-9ae9-86ea951772d5",
      "m1": "بیاراسته چتر هندی به زر",
      "m2": "بدو بافته چند گونه گهر",
      "verse_number": 11
    },
    {
      "id": "f94fef9e-d290-4e76-8c10-12f9ccc9256d",
      "m1": "سر بار بگشاد در بارگاه",
      "m2": "بیاورد یک سر همه نزد شاه",
      "verse_number": 12
    },
    {
      "id": "501ebbd1-2745-487b-be33-eec51cf04f62",
      "m1": "فراوان ببار اندرون سیم و زر",
      "m2": "چه از مشک و عنبر چه از عود تر",
      "verse_number": 13
    },
    {
      "id": "e521d7e5-7a25-4e76-a7b8-f6fb9a5e7249",
      "m1": "ز یاقوت والماس وز تیغ هند",
      "m2": "همه تیغ هندی سراسر پرند",
      "verse_number": 14
    },
    {
      "id": "cd4bb68f-1c6b-4734-b080-3e4db2bf0484",
      "m1": "ز چیزی که خیزد ز قنوج و رای",
      "m2": "زده دست و پای آوریده به جای",
      "verse_number": 15
    },
    {
      "id": "7ce54a9e-d0ed-4c55-9f89-c2e681606cd9",
      "m1": "ببردند یک سر همه پیش تخت",
      "m2": "نگه کرد سالار خورشید بخت",
      "verse_number": 16
    },
    {
      "id": "5c59acd8-352f-4c0f-988e-9c89f29dc6bf",
      "m1": "ز چیزی که برد اندران رای رنج",
      "m2": "فرستاد کسری سراسر به گنج",
      "verse_number": 17
    },
    {
      "id": "6004a144-fb84-42ff-8f97-96803c407da5",
      "m1": "بیاورد پس نامهای بر پرند",
      "m2": "نبشته بنوشینروان رای هند",
      "verse_number": 18
    },
    {
      "id": "7429c66e-bc07-4411-bbe9-ff1a07d0c296",
      "m1": "یکی تخت شطرنج کرده به رنج",
      "m2": "تهی کرده از رنج شطرنج گنج",
      "verse_number": 19
    },
    {
      "id": "31008aee-e137-473f-b8f1-3ba91afd7b7f",
      "m1": "بیاورد پیغام هندی ز رای",
      "m2": "که تا چرخ باشد تو بادی به جای",
      "verse_number": 20
    },
    {
      "id": "7ba788d9-a04a-4b96-a280-f2a71c9c5a56",
      "m1": "کسی کو بدانش برد رنج بیش",
      "m2": "بفرمای تا تخت شطرنج پیش",
      "verse_number": 21
    },
    {
      "id": "9c372598-c2b7-4ebe-9272-9f27b30334d8",
      "m1": "نهند و ز هر گونه رای آورند",
      "m2": "که این نغز بازی به جای آورند",
      "verse_number": 22
    },
    {
      "id": "1aeaa325-5572-4019-bb9f-e2e3a2576b86",
      "m1": "بدانند هرمهرهای را به نام",
      "m2": "که گویند پس خانهٔ او کدام",
      "verse_number": 23
    },
    {
      "id": "2f6d5ab6-e7dd-4bd5-bd9e-22a12af301f3",
      "m1": "پیاده بدانند و پیل و سپاه",
      "m2": "رخ واسب و رفتار فرزین و شاه",
      "verse_number": 24
    },
    {
      "id": "88e0507a-d050-4aaa-9636-5733113343f2",
      "m1": "گراین نغز بازی به جای آورند",
      "m2": "درین کار پاکیزه رای آورند",
      "verse_number": 25
    },
    {
      "id": "96b34270-786a-4cad-9335-3dec62a0afdd",
      "m1": "همان باژ و ساوی که فرمودشاه",
      "m2": "به خوبی فرستم بران بارگاه",
      "verse_number": 26
    },
    {
      "id": "248cc9e5-adf3-48b5-b17d-7c1de91891b0",
      "m1": "وگر نامداران ایران گروه",
      "m2": "ازین دانش آیند یک سر ستوه",
      "verse_number": 27
    },
    {
      "id": "3ba6215d-36f2-4ad3-9c39-dca116b0c03e",
      "m1": "چو با دانش ما ندارند تاو",
      "m2": "نخواهند زین بوم و بر باژ و ساو",
      "verse_number": 28
    },
    {
      "id": "10ad7fec-ac53-439a-88cb-60f371cfebae",
      "m1": "همان باژ باید پذیرفت نیز",
      "m2": "که دانش به از نامبردار چیز",
      "verse_number": 29
    },
    {
      "id": "15dca1db-ca0f-45a5-95d2-d33ca779b42f",
      "m1": "دل و گوش کسری بگوینده داد",
      "m2": "سخنها برو کرد گوینده یاد",
      "verse_number": 30
    },
    {
      "id": "7250a69e-369b-4781-9793-02bbfb70103e",
      "m1": "نهادند شطرنج نزدیک شاه",
      "m2": "به مهره درون کرد چندی نگاه",
      "verse_number": 31
    },
    {
      "id": "38f1aded-51fc-4e93-ad0f-4d7073b81367",
      "m1": "ز تختش یکی مهره از عاج بود",
      "m2": "پر از رنگ پیکر دگر ساج بود",
      "verse_number": 32
    },
    {
      "id": "e06df35b-bec1-4ccc-a892-fe6a7e531f39",
      "m1": "بپرسید ازو شاه پیروزبخت",
      "m2": "ازان پیکر ومهره ومشک وتخت",
      "verse_number": 33
    },
    {
      "id": "9c6a8d08-c0b6-40fc-a070-e50c274dad2d",
      "m1": "چنین داد پاسخ که ای شهریار",
      "m2": "همه رسم و راه از در کارزار",
      "verse_number": 34
    },
    {
      "id": "9a64a9dc-a94c-4917-93fb-6bd2fbcb444c",
      "m1": "ببینی چویابی به بازیش راه",
      "m2": "رخ و پیل و آرایش رزمگاه",
      "verse_number": 35
    },
    {
      "id": "2fd7b6bd-cfc1-4084-830b-29e1a0275ba5",
      "m1": "بدو گفت یک هفته ما را زمان",
      "m2": "ببازیم هشتم به روشنروان",
      "verse_number": 36
    },
    {
      "id": "1489340b-f940-4604-b1a4-b52c98b2a375",
      "m1": "یکی خرم ایوان بپرداختند",
      "m2": "فرستاده را پایگه ساختند",
      "verse_number": 37
    },
    {
      "id": "5cdbb02d-554b-406d-b21d-e39807868156",
      "m1": "رد وموبدان نماینده راه",
      "m2": "برفتند یک سر به نزدیک شاه",
      "verse_number": 38
    },
    {
      "id": "ab158bd8-f4e8-427a-bfa5-48c6443e8f9f",
      "m1": "نهادند پس تخت شطرنج پیش",
      "m2": "نگه کرد هریک ز اندازه بیش",
      "verse_number": 39
    },
    {
      "id": "41b3ed58-7137-4385-8924-f2c0a05343c0",
      "m1": "بجستند و هر گونهای ساختند",
      "m2": "ز هر دست یکبارش انداختند",
      "verse_number": 40
    },
    {
      "id": "b78b353b-4674-4cf0-bf17-4e0a1fe505d3",
      "m1": "یکی گفت وپرسید و دیگر شنید",
      "m2": "نیاورد کس راه بازی پدید",
      "verse_number": 41
    },
    {
      "id": "c4332e2b-8ab9-4fdc-8940-0c97f95408ba",
      "m1": "برفتند یکسر پرآژنگ چهر",
      "m2": "بیامد برشاه بوزرجمهر",
      "verse_number": 42
    },
    {
      "id": "93426484-ae07-4368-ae0f-55204ff9ea63",
      "m1": "ورا زان سخن نیک ناکام دید",
      "m2": "به آغاز آن رنج فرجام دید",
      "verse_number": 43
    },
    {
      "id": "19cf0434-a00d-4e62-8956-725d696819d7",
      "m1": "به کسری چنین گفت کای پادشا",
      "m2": "جهاندار و بیدار و فرمانروا",
      "verse_number": 44
    },
    {
      "id": "8a58dd05-0093-49f5-b97e-de443417463f",
      "m1": "من این نغز بازی به جای آورم",
      "m2": "خرد را بدین رهنمای آورم",
      "verse_number": 45
    },
    {
      "id": "912e5b1e-002a-403e-85ad-b3dc431db690",
      "m1": "بدو گفت شاه این سخن کارتست",
      "m2": "که روشنروان بادی وتندرست",
      "verse_number": 46
    },
    {
      "id": "331cd30f-72b8-4e49-8863-19b4ba4aca0b",
      "m1": "کنون رای قنوج گوید که شاه",
      "m2": "ندارد یکی مرد جوینده راه",
      "verse_number": 47
    },
    {
      "id": "1d9a0cdc-ac02-4d91-b7a3-fa49783194ce",
      "m1": "شکست بزرگ است بر موبدان",
      "m2": "به در گاه و بر گاه و بر بخردان",
      "verse_number": 48
    },
    {
      "id": "087ac9f6-05ed-4f4e-8cef-2bbca56101a2",
      "m1": "بیاورد شطرنج بوزرجمهر",
      "m2": "پراندیشه بنشست و بگشاد چهر",
      "verse_number": 49
    },
    {
      "id": "d5a50b36-b01b-4536-954b-5869159882bc",
      "m1": "همیجست بازی چپ و دست راست",
      "m2": "همیراند تا جای هریک کجاست",
      "verse_number": 50
    },
    {
      "id": "ae54ac9c-1de8-4ae3-bfdd-d06d461fa2b5",
      "m1": "به یک روز و یک شب چو بازیش یافت",
      "m2": "از ایوان سوی شاه ایران شتافت",
      "verse_number": 51
    },
    {
      "id": "35fce7ed-32b2-45a3-9b54-18a9a17ecfdb",
      "m1": "بدو گفت کای شاه پیروزبخت",
      "m2": "نگه کردم این مهره و مشک و تخت",
      "verse_number": 52
    },
    {
      "id": "d8013228-4969-4802-8dd8-df539d4008f3",
      "m1": "به خوبی همه بازی آمد به جای",
      "m2": "به بخت بلند جهان کدخدای",
      "verse_number": 53
    },
    {
      "id": "f6f3638d-0a67-45a0-98a3-709abe377ac9",
      "m1": "فرستادهٔ شاه را پیش خواه",
      "m2": "کسی را که دارند ما را نگاه",
      "verse_number": 54
    },
    {
      "id": "b766fb2b-0475-42a9-9ed5-bd92c524a8a2",
      "m1": "شهنشاه باید که بیند نخست",
      "m2": "یکی رزمگاهست گویی درست",
      "verse_number": 55
    },
    {
      "id": "d1850a04-c6e6-46e0-ac77-24a3cc41f1d8",
      "m1": "ز گفتار او شاد شد شهریار",
      "m2": "ورا نیک پی خواند و به روزگار",
      "verse_number": 56
    },
    {
      "id": "ef325b58-871c-42da-9250-ce90c5e1174d",
      "m1": "بفرمود تا موبدان و ردان",
      "m2": "برفتند با نامور بخردان",
      "verse_number": 57
    },
    {
      "id": "77fd66d8-4cb9-40e3-bfbe-4ad84d0df0dc",
      "m1": "فرستاده رای را پیش خواند",
      "m2": "بران نامور پیشگاهش نشاند",
      "verse_number": 58
    },
    {
      "id": "a36b9ca9-f8bc-40b8-bd4f-8731e54b6da3",
      "m1": "بدو گفت گوینده بوزرجمهر",
      "m2": "که ای موبد رای خورشید چهر",
      "verse_number": 59
    },
    {
      "id": "fbfd604b-76c6-4d5f-93dc-5d1030e7037b",
      "m1": "ازین مهرها رای با توچه گفت",
      "m2": "که همواره با توخرد باد جفت",
      "verse_number": 60
    },
    {
      "id": "d7918847-c3c2-4155-bae4-b735987f90dc",
      "m1": "چنین داد پاسخ که فرخندهرای",
      "m2": "چو از پیش او من برفتم ز جای",
      "verse_number": 61
    },
    {
      "id": "31061488-f574-41aa-a577-996d95f7fb5b",
      "m1": "مرا گفت کین مهرهٔ ساج و عاج",
      "m2": "ببر پیش تخت خداوند تاج",
      "verse_number": 62
    },
    {
      "id": "d10b9714-fe9a-4e5b-a086-e7a042cd52e4",
      "m1": "بگویش که با موبد و رایزن",
      "m2": "بنه پیش و بنشان یکی انجمن",
      "verse_number": 63
    },
    {
      "id": "97a68b98-8769-4638-8f3c-24fa33cfde25",
      "m1": "گر این نغز بازی به جای آورند",
      "m2": "پسندیده و دلربای آورند",
      "verse_number": 64
    },
    {
      "id": "84cb589a-f323-4a53-b3c7-ec47038b7c32",
      "m1": "همین بدره و برده و باژ و ساو",
      "m2": "فرستیم چندانک داریم تاو",
      "verse_number": 65
    },
    {
      "id": "bca905b5-c7ae-43c9-b1ce-f1d4be130550",
      "m1": "و گر شاه و فرزانگان این به جای",
      "m2": "نیارند روشن ندارند رای",
      "verse_number": 66
    },
    {
      "id": "755019df-6676-4990-aa53-9089d9a64bf2",
      "m1": "نباید که خواهد ز ما باژ و گنج",
      "m2": "دریغ آیدش جان دانا به رنج",
      "verse_number": 67
    },
    {
      "id": "0e0ced6e-9893-4e44-995f-7f703ce08bd0",
      "m1": "چو بیند دل و رای باریک ما",
      "m2": "فزونتر فرستد به نزدیک ما",
      "verse_number": 68
    },
    {
      "id": "7a55504f-06de-4b01-8624-a4bb21094657",
      "m1": "برتخت آن شاه بیداربخت",
      "m2": "بیاورد و بنهاد شطرنج وتخت",
      "verse_number": 69
    },
    {
      "id": "99a09e13-10a3-490c-bd44-00642c016e10",
      "m1": "چنین گفت با موبدان و ردان",
      "m2": "کهای نامور پاک دل بخردان",
      "verse_number": 70
    },
    {
      "id": "eece4958-b552-47ca-b504-6379a9973a08",
      "m1": "همه گوش دارید گفتار اوی",
      "m2": "هم آن را هشیار سالار اوی",
      "verse_number": 71
    },
    {
      "id": "fa07ed5e-0031-430a-880b-e67a560bc75c",
      "m1": "بیاراست دانا یکی رزمگاه",
      "m2": "به قلب اندرون ساخته جای شاه",
      "verse_number": 72
    },
    {
      "id": "c81e1b2c-d5fd-4bb1-8aa8-b13b6fd6072a",
      "m1": "چپ و راست صف برکشیده سوار",
      "m2": "پیاده به پیش اندرون نیزه دار",
      "verse_number": 73
    },
    {
      "id": "60dd771c-bd38-43b0-9fd3-0686c7a04464",
      "m1": "هشیوار دستور در پیش شاه",
      "m2": "به رزم اندرونش نماینده راه",
      "verse_number": 74
    },
    {
      "id": "ea1d57b9-0b7f-4954-bd10-c9f5811f5b83",
      "m1": "مبارز که اسب افگند بر دو روی",
      "m2": "به دست چپش پیل پرخاشجوی",
      "verse_number": 75
    },
    {
      "id": "d416d305-a5ab-4b97-aa6e-279a39bf7b00",
      "m1": "وزو برتر اسبان جنگی به پای",
      "m2": "بدان تاکه آید به بالای رای",
      "verse_number": 76
    },
    {
