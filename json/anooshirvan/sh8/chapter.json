{
  "id": "c9cca38c-3ca4-4572-9fe5-4c3cc394383a",
  "title": "بخش ۸ - داستان کلیله ودمنه",
  "verses": [
    {
      "id": "3ea6f50e-3e41-416d-b34f-c1957625f3eb",
      "m1": "نگه کن که شادان برزین چه گفت",
      "m2": "بدانگه که بگشاد راز از نهفت",
      "verse_number": 1
    },
    {
      "id": "7c14c85f-5a2b-4786-b331-84b3ededb4bb",
      "m1": "بدرگه شهنشاه نوشینروان",
      "m2": "که نامش بماناد تا جاودان",
      "verse_number": 2
    },
    {
      "id": "c9e62074-1650-4666-bd43-f3bad58f517a",
      "m1": "ز هر دانشی موبدی خواستی",
      "m2": "که درگه بدیشان بیاراستی",
      "verse_number": 3
    },
    {
      "id": "83b5baee-ccbd-4952-ab17-00eb00c775fa",
      "m1": "پزشک سخنگوی و کنداوران",
      "m2": "بزرگان وکارآزموده سران",
      "verse_number": 4
    },
    {
      "id": "83255d80-3693-4feb-8055-fefee25af01f",
      "m1": "ابر هر دری ناموَر مهتری",
      "m2": "کجا هر سری را بُدی افسری",
      "verse_number": 5
    },
    {
      "id": "15789d13-fbae-4252-abae-055e17e63566",
      "m1": "پزشک سراینده برزوی بود",
      "m2": "به نیرو رسیده سخنگوی بود",
      "verse_number": 6
    },
    {
      "id": "b595e1e6-3c3b-4ae3-9255-4888fd07ce76",
      "m1": "ز هر دانشی داشتی بهرهای",
      "m2": "به هر بهرهای در جهان شهرهای",
      "verse_number": 7
    },
    {
      "id": "04f9fcde-4a00-4efb-a449-a6f02c180156",
      "m1": "چنان بد که روزی به هنگام بار",
      "m2": "بیامد بر نامور شهریار",
      "verse_number": 8
    },
    {
      "id": "a40cb136-cba1-424d-8047-bd6e6d234e1b",
      "m1": "چنین گفت کای شاه دانشپذیر",
      "m2": "پژوهنده و یافته یادگیر",
      "verse_number": 9
    },
    {
      "id": "83ebeb69-af17-4f6f-880a-0f7cd639c071",
      "m1": "من امروز در دفتر هندوان",
      "m2": "همی بنگریدم به روشن روان",
      "verse_number": 10
    },
    {
      "id": "88baa6e1-5977-4ea0-a6f5-ea0851a67a09",
      "m1": "چنین بُد نبشته که بر کوه هند",
      "m2": "گیاییست چینی چو رومی پرند",
      "verse_number": 11
    },
    {
      "id": "306d0403-7c1a-4d92-a83e-08785c26deeb",
      "m1": "که آن را چو گرد آورد رهنمای",
      "m2": "بیامیزد و دانش آرد بجای",
      "verse_number": 12
    },
    {
      "id": "89424afa-64aa-410d-9b4e-06b20be54b41",
      "m1": "چو بر مُرده بپراگند بیگمان",
      "m2": "سخنگوی گردد هم اندر زمان",
      "verse_number": 13
    },
    {
      "id": "06b47296-1aa4-4344-a92c-5695211c492d",
      "m1": "کنون من بدستوری شهریار",
      "m2": "بپیمایم این راه دشوار خوار",
      "verse_number": 14
    },
    {
      "id": "7219f09e-cf4a-43db-9ba5-302d582cad52",
      "m1": "بسی دانشی رهنمای آورم",
      "m2": "مگر کین شگفتی بجای آورم",
      "verse_number": 15
    },
    {
      "id": "707ead55-81c2-4dbc-92f8-5887473c2965",
      "m1": "تن مرده گر زنده گردد رواست",
      "m2": "که نوشینروان بر جهان پادشاست",
      "verse_number": 16
    },
    {
      "id": "c909feb8-0a2a-4989-b048-b4c660e25041",
      "m1": "بدو گفت شاه این نشاید بُدن",
      "m2": "مگر آزمون را بباید شدن",
      "verse_number": 17
    },
    {
      "id": "a1271a62-2dab-4b17-8f2a-73b03501d74a",
      "m1": "ببر نامهٔ من برِ رای هند",
      "m2": "نگر تا که باشد بتآرای هند",
      "verse_number": 18
    },
    {
      "id": "df2d1185-bda8-425b-acbe-520d426c7fb2",
      "m1": "بدین کار با خویشتن یارخواه",
      "m2": "همه یاری از بختِ بیدار خواه",
      "verse_number": 19
    },
    {
      "id": "c23e2a80-4fa7-4343-878f-768eebd1f854",
      "m1": "اگر نوشگفتی شود در جهان",
      "m2": "که این گفته رمزی بود درنهان",
      "verse_number": 20
    },
    {
      "id": "1af95266-8028-47ac-b11f-b17faa760eec",
      "m1": "ببر هرچ باید به نزدیک رای",
      "m2": "کزو بایدت بیگمان رهنمای",
      "verse_number": 21
    },
    {
      "id": "5b099aff-9f83-4dff-a303-31b96c4d8409",
      "m1": "در گنج بگشاد نوشینروان",
      "m2": "ز چیزی که بُد درخور خسروان",
      "verse_number": 22
    },
    {
      "id": "2beabdd8-529d-4a88-8968-5f4f05406d1a",
      "m1": "ز دینار و دیبا و خز و حریر",
      "m2": "ز مهر و ز افسر ز مشک و عبیر",
      "verse_number": 23
    },
    {
      "id": "a691b7d0-e10e-4db5-a97d-d01d5fe912f0",
      "m1": "شتروار سیصد بیاراست شاه",
      "m2": "فرستاده برداشت آمد به راه",
      "verse_number": 24
    },
    {
      "id": "de198e8a-b223-41dd-858a-d5c424dbf4fd",
      "m1": "بیامد برِ رای و نامه بداد",
      "m2": "سرِ بارها پیش او برگشاد",
      "verse_number": 25
    },
    {
      "id": "5f35b036-5be8-479f-bcfa-4abb5f369c4c",
      "m1": "چو برخواند آن نامهٔ شاه رای",
      "m2": "بدو گفت کای مرد پاکیزه رای",
      "verse_number": 26
    },
    {
      "id": "3da3a334-e5b3-40e4-a346-8fd83ca9d70d",
      "m1": "ز کسری مرا گنج بخشیده نیست",
      "m2": "همه لشکر و پادشاهی یکیست",
      "verse_number": 27
    },
    {
      "id": "e0bc8bbb-84f1-47f6-9bdf-0ac923893cb5",
      "m1": "ز داد و ز فرّ و ز اورند شاه",
      "m2": "وزان روشنی بخت و آن دستگاه",
      "verse_number": 28
    },
    {
      "id": "f5a40638-e6bc-4808-9086-a9c3b6379a53",
      "m1": "نباشد شگفت از جهاندار پاک",
      "m2": "که گر مردگان را برآرد ز خاک",
      "verse_number": 29
    },
    {
      "id": "b085ab55-dfe2-41c9-8af7-7ebadd23b090",
      "m1": "برهمن بکوه اندرون هرک هست",
      "m2": "یکی دارد این رای را با تو دست",
      "verse_number": 30
    },
    {
      "id": "94967508-f23c-4012-b9db-f8b4a475116b",
      "m1": "بتآرای و فرخنده دستور من",
      "m2": "هم آن گنج و پرمایه گنجور من",
      "verse_number": 31
    },
    {
      "id": "56ac129a-e2f1-4d81-b8f7-0e8fe7d324c6",
      "m1": "بد و نیک هندوستان پیش تست",
      "m2": "بزرگی مرا در کم و بیش تست",
      "verse_number": 32
    },
    {
      "id": "5b2ed25e-2e3b-405b-aeba-4218c21b5fe8",
      "m1": "بیاراستندش به نزدیک رای",
      "m2": "یکی نامور چون ببایست جای",
      "verse_number": 33
    },
    {
      "id": "f2927510-75b3-49ba-9646-2e75a552298b",
      "m1": "خورشگر فرستاد هم خوردنی",
      "m2": "همان پوشش نغز و گستردنی",
      "verse_number": 34
    },
    {
      "id": "b0182756-259c-4342-b872-5df15aad3a3d",
      "m1": "برفت آن شب و رای زد با ردان",
      "m2": "بزرگان قنّوج با بخردان",
      "verse_number": 35
    },
    {
      "id": "c952a02d-962d-407a-aaa0-aa57f9891d70",
      "m1": "چو برزد سر از کوه رخشنده روز",
      "m2": "پدید آمد آن شمع گیتیفروز",
      "verse_number": 36
    },
    {
      "id": "7d315819-5157-413c-b83d-20febc27eb8a",
      "m1": "پزشکان فرزانه را خواند رای",
      "m2": "کسی کو بدانش بُدی رهنمای",
      "verse_number": 37
    },
    {
      "id": "e93e3b18-5848-43f2-a1bb-27fec47d35f6",
      "m1": "چو برزوی بنهاد سر سوی کوه",
      "m2": "برفتند با او پزشکان گروه",
      "verse_number": 38
    },
    {
      "id": "2a61deab-1856-4c66-8ae3-5106b6f1cb7d",
      "m1": "پیاده همه کوهساران بپای",
      "m2": "بپیمود با دانشی رهنمای",
      "verse_number": 39
    },
    {
      "id": "f9510a91-adcc-4872-a666-b14aad744ae9",
      "m1": "گیاها ز خشک و ز تر برگزید",
      "m2": "ز پژمرده و آنچ رخشنده دید",
      "verse_number": 40
    },
    {
      "id": "bdb2e6d8-29b0-43fc-8077-b61bad184c2e",
      "m1": "ز هرگونه دارو ز خشک و ز تر",
      "m2": "همی بر پراگند بر مرده بر",
      "verse_number": 41
    },
    {
      "id": "6e992d06-1604-487d-b2f6-08d92c211d54",
      "m1": "یکی مرده زنده نگشت از گیا",
      "m2": "همانا که سست آمد آن کیمیا",
      "verse_number": 42
    },
    {
      "id": "fde2fd34-e14d-4b21-9562-ae554145a877",
      "m1": "همه کوه بسپرد یک یک بپای",
      "m2": "ابر رنج او بر نیامد بجای",
      "verse_number": 43
    },
    {
      "id": "15bbee38-4442-49ab-9313-73bc3f4ba011",
      "m1": "بدانست کان کار آن پادشاست",
      "m2": "که زنده است جاوید و فرمانرواست",
      "verse_number": 44
    },
    {
      "id": "c7f75634-8372-40d7-9f73-a301bc3940db",
      "m1": "دلش گشت سوزان ز تشویر شاه",
      "m2": "هم از نامداران هم از رنج راه",
      "verse_number": 45
    },
    {
      "id": "87b8d3be-4e76-4d4e-8e25-8af1e5b123f7",
      "m1": "وزان خواسته نیز کاورده بود",
      "m2": "ز گفتار بیهوده آزرده بود",
      "verse_number": 46
    },
    {
      "id": "56412ed8-738e-4426-937b-247693af1d93",
      "m1": "ز کارِ نبشته ببُد تنگدل",
      "m2": "که آن مرد بیدانش و سنگدل",
      "verse_number": 47
    },
    {
      "id": "b9bc66c8-84b2-4d19-bf75-0b92aebd9045",
      "m1": "چرا خیره بر باد چیزی نبشت",
      "m2": "که بد بار آن رنج گفتار زشت",
      "verse_number": 48
    },
    {
      "id": "599073a2-c862-4530-810a-4f063d5345a9",
      "m1": "چنین گفت زان پس بران بخردان",
      "m2": "که ای کاردیده ستوده ردان",
      "verse_number": 49
    },
    {
      "id": "02f6c7e2-8972-43f7-9217-ec0429f76553",
      "m1": "که دانید داناتر از خویشتن",
      "m2": "کجا سرفرازد بدین انجمن",
      "verse_number": 50
    },
    {
      "id": "47a1a016-a0f2-4e44-8d72-e027ee3dfa51",
      "m1": "به پاسخ شدند انجمن همسخن",
      "m2": "که داننده پیرست ایدر کهن",
      "verse_number": 51
    },
    {
      "id": "f55e949b-d18f-4bcb-a83d-02f0208cdb24",
      "m1": "به سال و خرد او ز ما مهترست",
      "m2": "به دانش ز هر مهتری بهترست",
      "verse_number": 52
    },
    {
      "id": "2f8ee8ce-4277-4826-a30b-eed4f97579cd",
      "m1": "چنین گفت برزوی با هندوان",
      "m2": "که ای نامداران روشنروان",
      "verse_number": 53
    },
    {
      "id": "8ab379c9-1d22-4fb6-bd5e-3471392b6501",
      "m1": "برین رنجها بر فزونی کنید",
      "m2": "مرا سوی او رهنمونی کنید",
      "verse_number": 54
    },
    {
      "id": "10bcb637-f893-448b-8e47-99fbb9fa33a4",
      "m1": "مگر کان سخنگوی دانای پیر",
      "m2": "بدین کار باشد مرا دستگیر",
      "verse_number": 55
    },
    {
      "id": "8589bf58-f300-42f2-b56f-23fe9bddb5ba",
      "m1": "ببردند برزوی را نزد اوی",
      "m2": "پراندیشه دل سر پر از گفت و گوی",
      "verse_number": 56
    },
    {
      "id": "2f8e0ae2-17f3-470a-8205-5fb79e2c9605",
      "m1": "چو نزدیک او شد سخنگوی مرد",
      "m2": "همه رنجها پیش او یاد کرد",
      "verse_number": 57
    },
    {
      "id": "069a1538-9cf1-47f4-a3f7-c49737880664",
      "m1": "ز کار نبشته که آمد پدید",
      "m2": "سخنها که از کاردانان شنید",
      "verse_number": 58
    },
    {
      "id": "73ac0d60-e2e3-4905-91a3-49947f9bf6e9",
      "m1": "بدو پیر دانا زبان برگشاد",
      "m2": "ز هر دانشی پیش او کرد یاد",
      "verse_number": 59
    },
    {
      "id": "9df3497e-21c6-4e41-827b-84e88eb2d322",
      "m1": "که من در نبشته چنین یافتم",
      "m2": "بدان آرزو تیز بشتافتم",
      "verse_number": 60
    },
    {
      "id": "4a707f4a-3bdf-44aa-83bc-f97ef4075b88",
      "m1": "چو زان رنجها برنیامد پدید",
      "m2": "ببایست ناچار دیگر شنید",
      "verse_number": 61
    },
    {
      "id": "18771acd-4c50-464c-bb4b-b6da9a34d862",
      "m1": "گیا چون سخندان و دانش چو کوه",
      "m2": "که همواره باشد مر او را شکوه",
      "verse_number": 62
    },
    {
      "id": "340b2fa0-7cd8-4134-9abb-16bb8f4b2c38",
      "m1": "تن مرده چون مرد بیدانشست",
      "m2": "که دانا به هر جای با رامشست",
      "verse_number": 63
    },
    {
      "id": "f5768a68-9031-480c-9fd5-8e650f24d625",
      "m1": "به دانش بود بیگمان زنده مرد",
      "m2": "چو دانش نباشد ب گِردش مگرد",
      "verse_number": 64
    },
    {
      "id": "48358b53-79bf-4ca8-bae6-063e674cdb85",
      "m1": "چو مردم ز دانایی آید ستوه",
      "m2": "گیا چون کلیلهست و دانش چو کوه",
      "verse_number": 65
    },
    {
      "id": "5ea19104-cf91-4c94-bb4e-576fcdeb55e3",
      "m1": "کتابی به دانش نماینده راه",
      "m2": "بیابی چو جویی تو از گنج شاه",
      "verse_number": 66
    },
    {
      "id": "f40272a8-f8e6-41ac-9cf4-ec0dce156b0b",
      "m1": "چو بشنید برزوی زو شاد شد",
      "m2": "همه رنج بر چشم او باد شد",
      "verse_number": 67
    },
    {
      "id": "e501f4f5-2888-486d-ae33-396a9fa19f7d",
      "m1": "بر او آفرین کرد و شد نزد شاه",
      "m2": "به کردار آتش بپیمود راه",
      "verse_number": 68
    },
    {
      "id": "552d05a7-d944-492c-a380-db92b393b14d",
      "m1": "بیامد نیایشکنان پیش رای",
      "m2": "که تا جای باشد تو بادی به جای",
      "verse_number": 69
    },
    {
      "id": "6bf22714-c9d8-410b-9d67-88b27d716479",
      "m1": "کتابیست ای شاه گسترده کام",
      "m2": "که آن را به هندی کلیلهست نام",
      "verse_number": 70
    },
    {
      "id": "b7b7168b-1b12-4c4c-8413-e356879539a7",
      "m1": "به مهرست تا درج در گنج شاه",
      "m2": "به رای و به دانش نماینده راه",
      "verse_number": 71
    },
    {
      "id": "9998ccc2-1682-4763-913d-3ee4d80d6660",
      "m1": "به گنجور فرمان دهد تا ز گنج",
      "m2": "سپارد به من گر ندارد به رنج",
      "verse_number": 72
    },
    {
      "id": "61dd4fad-5e76-4125-96cf-1e4481c7e39a",
      "m1": "دژم گشت زان آرزو جان شاه",
      "m2": "بپیچید برخویشتن چندگاه",
      "verse_number": 73
    },
    {
      "id": "73acebb3-824e-46a4-af38-dfeaed045440",
      "m1": "به برزوی گفت این کس از ما نجُست",
      "m2": "نه اکنون نه از روزگار نخست",
      "verse_number": 74
    },
    {
      "id": "d75922a1-28e7-41fe-b404-6d54b4967c43",
      "m1": "ولیکن جهاندار نوشینروان",
      "m2": "اگر تن بخواهد ز ما یا روان",
      "verse_number": 75
    },
    {
      "id": "6c21cee7-cd3a-48f3-aa97-883fe3637f6c",
      "m1": "نداریم ازو باز چیزی که هست",
      "m2": "اگر سرفرازست اگر زیردست",
      "verse_number": 76
    },
    {
      "id": "135d1c37-7fd3-4383-b997-2552ce5dd2b6",
      "m1": "ولیکن بخوانی مگر پیش ما",
      "m2": "بدان تا روان بداندیش ما",
      "verse_number": 77
    },
    {
      "id": "bbc57f61-af6b-4b89-a425-23d1dfce5624",
      "m1": "نگوید به دل کان نبشتست کس",
      "m2": "بخوان و بدان و ببین پیش و پس",
      "verse_number": 78
    },
    {
      "id": "dd085a06-9391-4f4d-8c4d-a2a0e16b78aa",
      "m1": "بدو گفت برزوی کای شهریار",
      "m2": "ندارم فزون ز آنچ گویی مدار",
      "verse_number": 79
    },
    {
      "id": "740afc82-f8da-4243-aa25-2a5c566c4cdc",
      "m1": "کلیله بیاورد گنجور شاه",
      "m2": "همیبود او را نماینده راه",
      "verse_number": 80
    },
    {
      "id": "2bdfd93d-8298-4ed6-89ae-e9f2761ccc57",
      "m1": "هران دُر که از نامه برخواندی",
      "m2": "همه روز بر دل همیراندی",
      "verse_number": 81
    },
    {
      "id": "16c0577f-a6f7-41fe-aecf-9795e56828e2",
      "m1": "ز نامه فزون ز آنک بودیش یاد",
      "m2": "ز برخواندی نیز تا بامداد",
      "verse_number": 82
    },
    {
      "id": "f6dac08d-9d14-47fb-8e72-0efefa176a74",
      "m1": "همیبود شادان دل و تن درست",
      "m2": "بدانش همی جان روشن بشست",
      "verse_number": 83
    },
    {
      "id": "a76e27d7-34ac-4373-bc9c-d1e1578a9bdb",
      "m1": "چو زو نامه رفتی به شاه جهان",
      "m2": "دری از کلیله نبشتی نهان",
      "verse_number": 84
    },
    {
      "id": "e58d0e9f-50a0-4486-9a98-7b18b42676c3",
      "m1": "بدین چاره تا نامهٔ هندوان",
      "m2": "فرستاد نزدیک نوشینروان",
      "verse_number": 85
    },
    {
      "id": "c17f6fd6-75ae-4d79-8112-962ff792bd08",
      "m1": "بدین گونه تا پاسخ نامه دید",
      "m2": "که دریای دانش بر ما رسید",
      "verse_number": 86
    },
    {
      "id": "8f80f4d6-6b5a-423f-8631-2ad281769cdb",
      "m1": "ز ایوان بیامد به نزدیک رای",
      "m2": "بدستوری بازگشتن به جای",
      "verse_number": 87
    },
    {
      "id": "408fd216-0635-46ee-a6a1-a568369b699e",
      "m1": "چو بگشاد دل رای بنواختش",
      "m2": "یکی خلعت هندویی ساختش",
      "verse_number": 88
    },
    {
      "id": "60f7574f-03b3-4904-8deb-693408c30167",
      "m1": "دو یاره بهاگیر و دو گوشوار",
      "m2": "یکی طوق پرگوهر شاهوار",
      "verse_number": 89
    },
    {
      "id": "581d593c-f460-450d-affb-42a99939117a",
      "m1": "هم از شارهٔ هندی و تیغ هند",
      "m2": "همه روی آهن سراسر پرند",
      "verse_number": 90
    },
    {
      "id": "ff1860e4-0e06-41ba-90d0-026c28eac241",
      "m1": "بیامد ز قنّوج برزوی شاد",
      "m2": "بسی دانشِ نو گرفته به یاد",
      "verse_number": 91
    },
    {
      "id": "81da0f85-11a4-4399-9874-ff28959f47e3",
      "m1": "ز ره چون رسید اندر آن بارگاه",
      "m2": "نیایش کنان رفت نزدیک شاه",
      "verse_number": 92
    },
    {
      "id": "5275c500-3b2a-4d0b-a52d-7bc6f8980cc7",
      "m1": "بگفت آنچ از رای دید و شنید",
      "m2": "بجای گیا دانش آمد پدید",
      "verse_number": 93
    },
    {
      "id": "4103b40b-d4bc-465f-ad63-1604c8ec9aef",
      "m1": "بدو گفت شاه ای پسندیده مرد",
      "m2": "کلیله روان مرا زنده کرد",
      "verse_number": 94
    },
    {
      "id": "351f297e-0cfc-4092-a092-68eadd95bf8e",
      "m1": "تو اکنون ز گنجور بستان کلید",
      "m2": "ز چیزی که باید بباید گزید",
      "verse_number": 95
    },
    {
      "id": "1a21f8ff-3fa6-4898-99d9-13fe98929b25",
      "m1": "بیامد خرد یافته سوی گنج",
      "m2": "به گنجور بسیار ننمود رنج",
      "verse_number": 96
    },
    {
      "id": "b926101b-90e9-49be-b17b-d475391c4b21",
      "m1": "درم بود و گوهر چپ و دست راست",
      "m2": "جز از جامهٔ شاه چیزی نخواست",
      "verse_number": 97
    },
    {
      "id": "d54065a0-e3b7-4315-ad0c-f615bdc432c9",
      "m1": "گرانمایه دستی بپوشید و رفت",
      "m2": "بر گاه کسری خرامید تفت",
      "verse_number": 98
    },
    {
      "id": "23969991-b3f2-4e4a-b1b1-75e16dbcc2b2",
      "m1": "چو آمد به نزدیک تختش فراز",
      "m2": "برو آفرین کرد و بردش نماز",
      "verse_number": 99
    },
    {
      "id": "cf2bf94a-c700-4ff1-a2b5-a86782758e82",
      "m1": "بدو گفت پس نامور شهریار",
      "m2": "که بی بدره و گوهر شاهوار",
      "verse_number": 100
    },
    {
      "id": "2d30df00-4dc1-474a-9edf-1d12fbd88847",
      "m1": "چرا رفتی ای رنج دیده ز گنج",
      "m2": "کسی را سزد گنج کو دید رنج",
      "verse_number": 101
    },
    {
      "id": "b07d800a-efe6-45ae-a6ab-7f41943d6446",
      "m1": "چنین پاسخ آورد برزو بشاه",
      "m2": "که ای تاج تو برتر از چرخ ماه",
      "verse_number": 102
    },
    {
      "id": "ad5f6168-66fc-4be0-856f-f6226210409d",
      "m1": "هرآنکس که او پوشش شاه یافت",
      "m2": "به بخت و به تخت مهی راه یافت",
      "verse_number": 103
    },
    {
      "id": "7fdf35e7-b28b-496d-9ea7-ec77e29db2be",
      "m1": "دگر آنک با جامهٔ شهریار",
      "m2": "ببیند مرا مرد ناسازگار",
      "verse_number": 104
    },
    {
      "id": "a9db2385-cc0b-4254-b963-5932b830f65c",
      "m1": "دل بدسگالان شود تار و تنگ",
      "m2": "بماند رخ دوست با آب و رنگ",
      "verse_number": 105
    },
    {
      "id": "02814e3c-174d-4a73-b2a1-fc61a588e550",
      "m1": "یکی آرزو خواهم از شهریار",
      "m2": "که ماند ز من در جهان یادگار",
      "verse_number": 106
    },
    {
      "id": "cb9a62c2-a12b-4c3c-99f5-7fe8f36878ba",
      "m1": "چو بنویسد این نامه بوزرجمهر",
      "m2": "گشاید برین رنج برزوی چهر",
      "verse_number": 107
    },
    {
      "id": "fce86e2d-94bb-47aa-accc-34a243274830",
      "m1": "نخستین در از من کند یادگار",
      "m2": "به فرمان پیروزگر شهریار",
      "verse_number": 108
    },
    {
      "id": "f46575e0-86d2-404a-9321-ce8f7165e96b",
      "m1": "بدان تا پس از مرگ من در جهان",
      "m2": "ز داننده رنجم نگردد نهان",
      "verse_number": 109
    },
    {
      "id": "e456ef27-1f14-4a8c-a438-d586edb5126f",
      "m1": "بدو گفت شاه این بزرگ آرزوست",
      "m2": "بر اندازهٔ مرد آزادهخوست",
      "verse_number": 110
    },
    {
      "id": "381c7537-b9a0-4664-a994-ec549ed1bd80",
      "m1": "ولیکن به رنج تو اندر خورست",
      "m2": "سخن گرچه از پایگه برترست",
      "verse_number": 111
    },
    {
      "id": "de99042f-85f8-4303-a1b6-796ae5a4e1b1",
      "m1": "به بوزرجمهر آن زمان شاه گفت",
      "m2": "که این آرزو را نشاید نهفت",
      "verse_number": 112
    },
    {
      "id": "6c4ca136-86de-46d4-9fe8-af9f90205f56",
      "m1": "نویسنده از کلک چون خامه کرد",
      "m2": "ز برزوی یک در سرِ نامه کرد",
      "verse_number": 113
    },
    {
      "id": "917427de-1a1e-481c-bb5e-2c265890b102",
      "m1": "نبشت او بران نامهٔ خسروی",
      "m2": "نبود آن زمان خط جز پهلوی",
      "verse_number": 114
    },
    {
      "id": "89f147f4-1a4a-48d5-b3bd-1af68fd5af84",
      "m1": "همیبود با ارج در گنج شاه",
      "m2": "بدو ناسزا کس نکردی نگاه",
      "verse_number": 115
    },
    {
      "id": "b254a780-38a4-46b8-a65a-3f6470076d20",
      "m1": "چنین تا بتازی سخن راندند",
      "m2": "ورا پهلوانی همیخواندند",
      "verse_number": 116
    },
    {
      "id": "f63cccb4-f5fa-4ca5-a373-1bdc4b39d890",
      "m1": "چو مامون روشن روان تازه کرد",
      "m2": "خور روز بر دیگر اندازه کرد",
      "verse_number": 117
    },
    {
      "id": "ac972089-c4b4-4cd8-ac9c-d8a147a6111a",
      "m1": "دل موبدان داشت و رای کیان",
      "m2": "ببسته به هر دانشی بر میان",
      "verse_number": 118
    },
    {
      "id": "f3dababb-e4be-4496-9855-4084e13a4f64",
      "m1": "کلیله به تازی شد از پهلوی",
      "m2": "بدین سان که اکنون همیبشنوی",
      "verse_number": 119
    },
    {
      "id": "07cb47ab-d823-45b8-9521-fafc3c242441",
      "m1": "بتازی همیبود تا گاه نصر",
      "m2": "بدانگه که شد در جهان شاه نصر",
      "verse_number": 120
    },
    {
      "id": "69c42a63-ab09-46cf-8e30-bf640d9cae77",
      "m1": "گرانمایه بوالفضل دستور اوی",
      "m2": "که اندر سخن بود گنجور اوی",
      "verse_number": 121
    },
    {
      "id": "49665d93-04ef-4fb5-86ee-86ff3ff42f1a",
      "m1": "بفرمود تا پارسی و دری",
      "m2": "نبشتند و کوتاه شد داوری",
      "verse_number": 122
    },
    {
      "id": "bef2ff7d-9052-43fd-b271-ce8e9c2e80bd",
      "m1": "وزان پس چو پیوسته رای آمدش",
      "m2": "بدانش خرد رهنمای آمدش",
      "verse_number": 123
    },
    {
      "id": "80dab213-1e13-4361-8ee1-ec8e58551928",
      "m1": "همیخواست تا آشکار و نهان",
      "m2": "ازو یادگاری بود در جهان",
      "verse_number": 124
    },
    {
      "id": "2d3fff26-4599-49aa-8b52-d9b599413f85",
      "m1": "گزارنده را پیش بنشاندند",
      "m2": "همه نامه بر رودکی خواندند",
      "verse_number": 125
    },
    {
      "id": "80dd55e4-59f9-4350-a685-fd7858ee6e60",
      "m1": "بپیوست گویا پراگنده را",
      "m2": "بسفت اینچنین در آگنده را",
      "verse_number": 126
    },
    {
      "id": "2eace151-de69-4265-92d6-f172f90b7006",
      "m1": "بدان کو سخن راند آرایشست",
      "m2": "چو ابله بود جای بخشایشست",
      "verse_number": 127
    },
    {
      "id": "eba47b67-1741-4abf-91ff-da08bbe25345",
      "m1": "حدیث پراگنده بپراگند",
      "m2": "چو پیوسته شد جان و مغز آگند",
      "verse_number": 128
    },
    {
      "id": "19f376c8-fef8-4eb7-89cd-77fda993f423",
      "m1": "جهاندار تا جاودان زنده باد",
      "m2": "زمان و زمین پیش او بنده باد",
      "verse_number": 129
    },
    {
      "id": "ba2a6b68-cedd-4a7d-a70b-a33032ab5326",
      "m1": "از اندیشه دل را مدار ایچ تنگ",
      "m2": "که دوری تو از روزگار درنگ",
      "verse_number": 130
    },
    {
      "id": "e3d30199-06bf-4114-96a9-3303a45b9e28",
      "m1": "گهی بر فراز و گهی بر نشیب",
      "m2": "گهی با مراد و گهی با نهیب",
      "verse_number": 131
    },
    {
      "id": "a410302e-4348-4d83-b5cf-aaec3d5bee2e",
      "m1": "ازین دو یکی نیز جاوید نیست",
      "m2": "به بودن تو را راه امید نیست",
      "verse_number": 132
    },
    {
      "id": "4f8ee171-6329-41d1-b4dc-16c293dc7d55",
      "m1": "نگه کن کنون کار بوزرجمهر",
      "m2": "که از خاک بر شد به گردان سپهر",
      "verse_number": 133
    },
    {
      "id": "0ecd14fa-2cd3-497f-8fd8-776ccd792ccb",
      "m1": "فراز آوریدش به خاک نژند",
      "m2": "همان کس که بردش به ابر بلند",
      "verse_number": 134
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 134
  }
}