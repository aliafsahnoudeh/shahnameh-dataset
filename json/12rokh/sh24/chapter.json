{
  "id": "afd4ab84-fa33-4f3a-b4e8-c7fedfba8af1",
  "title": "بخش ۲۴",
  "verses": [
    {
      "id": "0e53c9e9-4d68-4325-9815-442c7237c925",
      "m1": "چهارم فروهل بد و زنگله",
      "m2": "دو جنگی به کردار شیر یله",
      "verse_number": 1
    },
    {
      "id": "950bbee6-7b05-46a5-acee-d557150794d8",
      "m1": "به ایران نبرده به تیر و کمان",
      "m2": "نبد چون فروهل دگر بدگمان",
      "verse_number": 2
    },
    {
      "id": "5ca031b8-c88c-497c-a4e7-1f4c237e27e7",
      "m1": "چو از دور ترک دژم را بدید",
      "m2": "کمان را به زه کرد و اندر کشید",
      "verse_number": 3
    },
    {
      "id": "8dcfc9a7-4bfa-4679-bd9f-5f7b9b32d6f8",
      "m1": "برآورد زان تیرهای خدنگ",
      "m2": "گرفته کمان رفت پیشش به جنگ",
      "verse_number": 4
    },
    {
      "id": "beeb4fd7-889b-4883-9b92-fc45314d341c",
      "m1": "ابر زنگله تیرباران گرفت",
      "m2": "ز هر سو کمین سواران گرفت",
      "verse_number": 5
    },
    {
      "id": "5257a206-dfc4-4e7d-98d3-9c48bc85ae8d",
      "m1": "خدنگی به رانش برآمد چو باد",
      "m2": "که بگذشت بر مرد و بر اسب شاد",
      "verse_number": 6
    },
    {
      "id": "18cfd1d7-88c7-43de-a78a-43cb41469896",
      "m1": "به روی اندر آمد تگاور ز درد",
      "m2": "جدا شد از او زنگله روی زرد",
      "verse_number": 7
    },
    {
      "id": "53a551ba-a8ef-4790-8b0f-275c8bc07215",
      "m1": "نگون شد سر زنگله جان بداد",
      "m2": "تو گفتی همانا ز مادر نزاد",
      "verse_number": 8
    },
    {
      "id": "9f932976-25b7-40b2-a435-4e19c6155308",
      "m1": "فروهل فروجست و ببرید سر",
      "m2": "برون کرد خفتان رومی ز بر",
      "verse_number": 9
    },
    {
      "id": "8fff373f-f51f-4656-ac73-3dfe2ba8027b",
      "m1": "سرش را به فتراک زین برببست",
      "m2": "بیامد گرفت اسب او را به دست",
      "verse_number": 10
    },
    {
      "id": "24963bac-68dc-4d97-b07b-6065bb141efb",
      "m1": "به بالا برآمد به سان پلنگ",
      "m2": "به خون غرقه گشته بر و تیغ و چنگ",
      "verse_number": 11
    },
    {
      "id": "a6908d45-8329-47f6-8081-2230e3fe1a83",
      "m1": "درفش خجسته برآورد راست",
      "m2": "شده شادمان یافته هرچ خواست",
      "verse_number": 12
    },
    {
      "id": "c0dd3903-9dae-4906-aa43-5b413ec96ce6",
      "m1": "خروشید زان پس که پیروز باد",
      "m2": "سر خسروان شاه فرخ نژاد",
      "verse_number": 13
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 13
  }
}