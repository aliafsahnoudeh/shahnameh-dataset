{
  "id": "357c4ef6-175a-4b52-a9da-1a4657c5e082",
  "title": "بخش ۲۵",
  "verses": [
    {
      "id": "10546c05-bddf-40da-80a2-6f14b687405f",
      "m1": "به پنجم چو رهام گودرز بود",
      "m2": "که با بارمان او نبرد آزمود",
      "verse_number": 1
    },
    {
      "id": "ea7e1525-a290-4f81-a8ee-1f9364bb5471",
      "m1": "کمان برگرفتند و تیر خدنگ",
      "m2": "برآمد خروش سواران جنگ",
      "verse_number": 2
    },
    {
      "id": "8e010c3a-9510-4589-a26f-e2aef200497d",
      "m1": "کمانها همه پاک بر هم شکست",
      "m2": "سوی نیزه بردند چون باد دست",
      "verse_number": 3
    },
    {
      "id": "2939dbaf-d5e2-4297-af63-eb61d4560367",
      "m1": "دو جنگی و هر دو دلیر و سوار",
      "m2": "هشیوار و دیده بسی کارزار",
      "verse_number": 4
    },
    {
      "id": "efdf809c-2c34-41a5-8e07-6a413c892c6c",
      "m1": "بگشتند بسیار یک با دگر",
      "m2": "بپیچید رهام پرخاشخر",
      "verse_number": 5
    },
    {
      "id": "0b19c24c-9454-4170-98af-95e639144ed2",
      "m1": "یکی نیزه انداخت بر ران اوی",
      "m2": "کز اسب اندر آمد به فرمان اوی",
      "verse_number": 6
    },
    {
      "id": "b3d808de-4712-41a1-86da-1eab7b327e5f",
      "m1": "جدا شد ز باره هم آنگاه ترک",
      "m2": "ز اسب اندر افتاد ترک سترگ",
      "verse_number": 7
    },
    {
      "id": "48fcfe69-9711-4573-9704-9c68fe39df71",
      "m1": "به پشت اندرش نیزهای زد دگر",
      "m2": "سنان اندر آمد میان جگر",
      "verse_number": 8
    },
    {
      "id": "d2009edb-fe63-4494-8c40-e4b638d93854",
      "m1": "فرود آمد از باره کرد آفرین",
      "m2": "ز دادار بر بخت شاه زمین",
      "verse_number": 9
    },
    {
      "id": "733d5d06-e37d-42ab-a0c5-86d9ed6eefbd",
      "m1": "به کین سیاوش کشیدش نگون",
      "m2": "ز کینه بمالید بر روی خون",
      "verse_number": 10
    },
    {
      "id": "f2eaa270-f889-42b6-95af-0fac04b5ec2c",
      "m1": "به زین اندر آهخت و بستش چو سنگ",
      "m2": "سر آویخته پایها زیر تنگ",
      "verse_number": 11
    },
    {
      "id": "5ee8faae-51ed-45d0-aa6b-6cf21855b6f3",
      "m1": "نشست از بر زین و اسبش کشان",
      "m2": "بیامد دوان تا به جای نشان",
      "verse_number": 12
    },
    {
      "id": "3fe62b71-d941-45d2-b210-ccdbfa4da5ed",
      "m1": "به بالا برآمد شده شاد دل",
      "m2": "ز درد و غمان گشته آزاددل",
      "verse_number": 13
    },
    {
      "id": "6ae630be-f7dd-47b6-99da-84a4ba627015",
      "m1": "به پیروزی شاه و تخت بلند",
      "m2": "به کام آمده زیر بخت بلند",
      "verse_number": 14
    },
    {
      "id": "c6e82d15-ed6f-465a-8020-814ccdf55cca",
      "m1": "همی آفرین خواند سالار شاه",
      "m2": "ابر شاه کیخسرو و تاج و گاه",
      "verse_number": 15
    },
    {
      "id": "83c45c6e-36fa-4ac6-ba4c-eb98bfc99816",
      "m1": "که پیروزگر شاه پیروز باد",
      "m2": "همه روزگارانش نوروز باد",
      "verse_number": 16
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 16
  }
}