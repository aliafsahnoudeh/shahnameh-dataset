{
  "id": "9e19a96e-c257-4ff8-9c2f-7a5c91b8e675",
  "title": "بخش ۲۳",
  "verses": [
    {
      "id": "e904bafd-fcfb-43d7-94af-1bbdec7b84a1",
      "m1": "سه دیگر سیامک ز توران سپاه",
      "m2": "بشد با گرازه به آوردگاه",
      "verse_number": 1
    },
    {
      "id": "925ec4f5-2440-40d7-879c-441cc735fe77",
      "m1": "برفتند و نیزه گرفته به دست",
      "m2": "خروشان به کردار پیلان مست",
      "verse_number": 2
    },
    {
      "id": "42a7b8ca-ca1d-43d8-9012-2f1b51ab2efc",
      "m1": "پر از جنگ و پر خشم کینهوران",
      "m2": "گرفتند زان پس عمود گران",
      "verse_number": 3
    },
    {
      "id": "27845467-b548-43e9-9f7f-4a6799bf1890",
      "m1": "چو شیران جنگی برآشوفتند",
      "m2": "همی بر سر یکدگر کوفتند",
      "verse_number": 4
    },
    {
      "id": "b2837a41-2ee1-4201-9563-a52965256708",
      "m1": "زبانشان شد از تشنگی لخت لخت",
      "m2": "به تنگی فراز آمد آن کار سخت",
      "verse_number": 5
    },
    {
      "id": "2294244c-68dd-4475-ab03-6e7ff309676f",
      "m1": "پیاده شدند و برآویختند",
      "m2": "همی گرد کینه برانگیختند",
      "verse_number": 6
    },
    {
      "id": "0095d84a-2916-4abc-8efe-935a0c190485",
      "m1": "گرازه بزد دست بر سان شیر",
      "m2": "مر او را چو باد اندر آورد زیر",
      "verse_number": 7
    },
    {
      "id": "bd47d0bf-7f61-41ae-8935-33ef4732feb5",
      "m1": "چنان سخت زد بر زمین کاستخوانش",
      "m2": "شکست و برآمد ز تن نیز جانش",
      "verse_number": 8
    },
    {
      "id": "8b1c7935-b183-474b-bb41-c84b8518a10c",
      "m1": "گرازه هم آنگه ببستش به اسب",
      "m2": "نشست از بر زین چو آذرگشسب",
      "verse_number": 9
    },
    {
      "id": "7c6a5c6e-01ab-43bb-b791-8d8c53d8b1dd",
      "m1": "گرفت آنگه اسب سیامک به دست",
      "m2": "به بالا برآمد بکردار مست",
      "verse_number": 10
    },
    {
      "id": "6e49de92-1fcd-4029-b3f6-efe1729f1b0c",
      "m1": "درفش خجسته به دست اندرون",
      "m2": "گرازان و شادان و دشمن نگون",
      "verse_number": 11
    },
    {
      "id": "56df9299-28c9-4c7a-8a89-e0c18cd34e4f",
      "m1": "خروشان و جوشان و نعره زنان",
      "m2": "ابر پهلوان آفرین برکنان",
      "verse_number": 12
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 12
  }
}