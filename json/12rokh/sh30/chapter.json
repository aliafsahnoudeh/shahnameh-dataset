{
  "id": "cb2243f4-4037-4feb-a846-e9904d5d267d",
  "title": "بخش ۳۰",
  "verses": [
    {
      "id": "b100b2cb-951e-401e-8238-397dc65a011e",
      "m1": "دهم برته با کهرم تیغزن",
      "m2": "دو خونی و هر دو سر انجمن",
      "verse_number": 1
    },
    {
      "id": "0f7fe084-dcd2-414c-b1b2-e67094c02c1c",
      "m1": "همی آزمودند هرگونه جنگ",
      "m2": "گرفتند پس تیغ هندی به چنگ",
      "verse_number": 2
    },
    {
      "id": "6e410885-18f6-4034-a639-b8f58a0be6f7",
      "m1": "درفش همایون به دست اندرون",
      "m2": "تو گفتی بجنبد که بیستون",
      "verse_number": 3
    },
    {
      "id": "236f9245-d6f8-45e8-8ff5-5b8bf56ec1e6",
      "m1": "یکایک بپیچید از او برته روی",
      "m2": "یکی تیغ زد بر سر و ترگ اوی",
      "verse_number": 4
    },
    {
      "id": "6aa2c934-016a-4515-892a-f6fcfff9b161",
      "m1": "که تا سینه کهرم بدو نیم گشت",
      "m2": "ز دشمن دل برته بیبیم گشت",
      "verse_number": 5
    },
    {
      "id": "e48c32a9-b199-452a-bd16-074924365d88",
      "m1": "فرود آمد از اسب و او را ببست",
      "m2": "بر آن زین توزی و خود برنشست",
      "verse_number": 6
    },
    {
      "id": "49792b39-1715-44e5-9756-1396273c0429",
      "m1": "برآمد به بالا چو شرزه پلنگ",
      "m2": "خروشان یکی تیغ هندی به چنگ",
      "verse_number": 7
    },
    {
      "id": "8f504c62-c1b5-44ae-9a4e-202b6f6afa64",
      "m1": "درفش همایون به دست اندرون",
      "m2": "فگنده بر آن باره کهرم نگون",
      "verse_number": 8
    },
    {
      "id": "4958a7b0-2408-48d2-acbb-3fd80da544e0",
      "m1": "همی گفت شاهست پیروزگر",
      "m2": "همیشه کلاهش به خورشید بر",
      "verse_number": 9
    },
    {
      "id": "6afbd974-cd6d-4ec5-95ce-06c8b8171166",
      "m1": "چو از روز نه ساعت اندر گذشت",
      "m2": "ز ترکان نبد کس بر آن پهندشت",
      "verse_number": 10
    },
    {
      "id": "d880599d-7db3-4aed-9f6e-1d405c16f949",
      "m1": "کسی را کجا پروراند به ناز",
      "m2": "برآید بر او روزگار دراز",
      "verse_number": 11
    },
    {
      "id": "2adfb358-8248-437a-8428-13f8c4d09e14",
      "m1": "شبیخون کند گاه شادی بر اوی",
      "m2": "همی خواری و سختی آرد به روی",
      "verse_number": 12
    },
    {
      "id": "f2793495-883e-4bcf-8b6c-cba727294971",
      "m1": "ز باد اندر آرد دهدمان به دم",
      "m2": "همی داد خوانیم و پیدا ستم",
      "verse_number": 13
    },
    {
      "id": "dad12bae-e7d6-478b-aa14-189b8b7ad159",
      "m1": "به تورانیان بر بد آن جنگ شوم",
      "m2": "به آوردگه کردن آهنگ شوم",
      "verse_number": 14
    },
    {
      "id": "99fcc13a-ee40-4649-80bd-aae1ad0bb33a",
      "m1": "چنان شد که پیران ز توران سپاه",
      "m2": "سواری ندید اندر آوردگاه",
      "verse_number": 15
    },
    {
      "id": "2cd51e06-b142-41a9-93fa-290130f0f33d",
      "m1": "روانها گسسته ز تنشان به تیغ",
      "m2": "جهان را تو گفتی نیامد دریغ",
      "verse_number": 16
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 16
  },
  "order_in_parent": 30
}