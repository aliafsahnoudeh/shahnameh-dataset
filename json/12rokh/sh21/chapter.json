{
  "id": "4ffa83c3-94f0-4e79-8e6e-a2c64c030a57",
  "title": "بخش ۲۱",
  "verses": [
    {
      "id": "414d8c64-b2b7-4ae1-bba4-91d958d0c447",
      "m1": "نخستین فریبرز نیو دلیر",
      "m2": "ز لشکر برون تاخت بر سان شیر",
      "verse_number": 1
    },
    {
      "id": "daa8dfb1-b2d0-4208-96c4-df718a8c30b6",
      "m1": "به نزدیک کلباد ویسه دمان",
      "m2": "بیامد به زه برنهاده کمان",
      "verse_number": 2
    },
    {
      "id": "41793a9e-f42c-4cd4-bc9a-94171ee288dd",
      "m1": "همی گشت و تیرش نیامد چو خواست",
      "m2": "کشید آن پرنداور از دست راست",
      "verse_number": 3
    },
    {
      "id": "cbbbefcd-f697-45a0-93f8-10d29b5552fc",
      "m1": "برآورد و زد تیغ بر گردنش",
      "m2": "به دو نیم شد تا کمرگه تنش",
      "verse_number": 4
    },
    {
      "id": "d802b18d-7b58-4721-847e-d413d8f6a509",
      "m1": "فرود آمد از اسب و بگشاد بند",
      "m2": "ز فتراک خویش آن کیانی کمند",
      "verse_number": 5
    },
    {
      "id": "ff7698dd-1a10-4425-886a-0d623c982327",
      "m1": "ببست از بر باره کلباد را",
      "m2": "گشاد از برش بند پولاد را",
      "verse_number": 6
    },
    {
      "id": "79e2a9bc-a90d-43a8-aa50-928120a8dcfa",
      "m1": "به بالا برآمد به پیروز نام",
      "m2": "خروشی برآورد و بگذارد گام",
      "verse_number": 7
    },
    {
      "id": "350aead8-8d39-4d42-a280-ca6b0ff10bd2",
      "m1": "که سالار ما باد پیروزگر",
      "m2": "همه دشمن شاه خستهجگر",
      "verse_number": 8
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 8
  }
}