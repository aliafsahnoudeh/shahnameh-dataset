{
  "id": "c048eb1e-168f-461b-af32-3d9fb5ffc164",
  "title": "بخش ۲۷",
  "verses": [
    {
      "id": "a09a3783-a33c-484f-8e0f-202d65db0f89",
      "m1": "برون تاخت هفتم ز گردان هجیر",
      "m2": "یکی نامداری سواری هژیر",
      "verse_number": 1
    },
    {
      "id": "b5e639b2-a1e6-4ed4-90f2-383c432fae04",
      "m1": "سپهرم ز خویشان افراسیاب",
      "m2": "یکی نامور بود با جاه و آب",
      "verse_number": 2
    },
    {
      "id": "f78c7a70-1e81-43a9-9c7a-37e1f23b943c",
      "m1": "ابا پور گودرز رزم آزمود",
      "m2": "که چون او به لشکر سواری نبود",
      "verse_number": 3
    },
    {
      "id": "d56e61ca-6bd8-4517-ab94-834e96111145",
      "m1": "برفتند هر دو به جای نبرد",
      "m2": "برآمد ز آوردگه تیره گرد",
      "verse_number": 4
    },
    {
      "id": "6076d0f8-b036-44c4-978b-e4ab11ae5b6e",
      "m1": "به شمشیر هر دو برآویختند",
      "m2": "همی زآهن آتش فروریختند",
      "verse_number": 5
    },
    {
      "id": "9dd798a6-0c69-4c7b-82fc-4d61bb05ad5d",
      "m1": "هجیر دلاور به کردار شیر",
      "m2": "به روی سپهرم درآمد دلیر",
      "verse_number": 6
    },
    {
      "id": "0ff51f10-3a00-4fcb-9dbd-6a66f9687936",
      "m1": "به نام جهانآفرین کردگار",
      "m2": "به بخت جهاندار با شهریار",
      "verse_number": 7
    },
    {
      "id": "4af3db1f-91ef-4c88-a178-4cb423e93173",
      "m1": "یکی تیغ زد بر سر و ترگ اوی",
      "m2": "که آمد هم اندر زمان مرگ اوی",
      "verse_number": 8
    },
    {
      "id": "a8708b44-5dc9-4b3d-bc76-2bb816ce2c28",
      "m1": "درافتاد ز اسبش هم آنگه نگون",
      "m2": "به زاری و خواری دهن پر ز خون",
      "verse_number": 9
    },
    {
      "id": "cd6a8249-9ab5-4322-ae24-909c56e19d8a",
      "m1": "فرود آمد از باره فرخ هجیر",
      "m2": "مر او را ببست از بر زین چو شیر",
      "verse_number": 10
    },
    {
      "id": "3e5992f8-a3a6-46a7-9f1e-17f605475ec2",
      "m1": "نشست از بر اسب و آن اسب اوی",
      "m2": "گرفته عنان و درآورده روی",
      "verse_number": 11
    },
    {
      "id": "103896c9-f4cc-48dd-9516-82e5176202c5",
      "m1": "برآمد به بالا و کرد آفرین",
      "m2": "بر آن اختر نیک و فرخ زمین",
      "verse_number": 12
    },
    {
      "id": "b08a4741-4cd5-4830-b517-07f2480321cf",
      "m1": "همی زور و بخت از جهاندار دید",
      "m2": "وز آن گردش بخت بیدار دید",
      "verse_number": 13
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 13
  },
  "order_in_parent": 27
}