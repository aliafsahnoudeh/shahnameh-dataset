{
  "id": "b4c51c83-dbad-4525-890f-0b90e706806f",
  "title": "بخش ۲۹",
  "verses": [
    {
      "id": "f25b6b0d-d10b-4f38-a573-cb55e5c8a8d0",
      "m1": "برون رفت گرگین نهم کینهخواه",
      "m2": "ابا اندریمان ز توران سپاه",
      "verse_number": 1
    },
    {
      "id": "1e5a6562-42fb-4565-9210-3162dd8ae7ca",
      "m1": "جهاندیده و کارکرده دو مرد",
      "m2": "برفتند و جستند جای نبرد",
      "verse_number": 2
    },
    {
      "id": "649bbb96-653b-4ad5-bbe2-0002e5405c8c",
      "m1": "به نیزه بگشتند و بشکست پست",
      "m2": "کمان برگرفتند هر دو به دست",
      "verse_number": 3
    },
    {
      "id": "bbbaaf93-ee17-4fe2-aa2e-15976772c67f",
      "m1": "ببارید تیر از کمان سران",
      "m2": "به روی اندر آورده کرگ اسپران",
      "verse_number": 4
    },
    {
      "id": "b26a79ae-1b9a-4b33-a8aa-86b6ce8543c5",
      "m1": "همی تیر بارید همچون تگرگ",
      "m2": "بر آن اسپر کرگ و بر ترک و ترگ",
      "verse_number": 5
    },
    {
      "id": "6906b16d-7e9c-4105-83e1-55267f25ade9",
      "m1": "یکی تیر گرگین بزد بر سرش",
      "m2": "که بردوخت با ترگ رومی برش",
      "verse_number": 6
    },
    {
      "id": "ee5b26f1-f336-430a-a099-49930b97c6b6",
      "m1": "بلرزید بر زین ز سختی سوار",
      "m2": "یکی تیر دیگر بزد نامدار",
      "verse_number": 7
    },
    {
      "id": "16f6d943-155a-4b3e-972f-13fa4960a42b",
      "m1": "هم آنگاه ترک اندر آمد نگون",
      "m2": "ز چشمش برون آمد از درد خون",
      "verse_number": 8
    },
    {
      "id": "ed3d85d1-b660-458e-ac86-c31ad485258f",
      "m1": "فرود آمد از باره گرگین چو گرد",
      "m2": "سر اندریمان ز تن دور کرد",
      "verse_number": 9
    },
    {
      "id": "41a06202-bd6e-4a49-a807-b4046d3dd9b5",
      "m1": "به فتراک بربست و خود برنشست",
      "m2": "نوند سوار نبرده به دست",
      "verse_number": 10
    },
    {
      "id": "85931fd0-7d56-495b-8e0b-bd460239712b",
      "m1": "بر آن تند بالا برآمد دمان",
      "m2": "همیدون به بازو به زه بر کمان",
      "verse_number": 11
    },
    {
      "id": "5f8ae196-30e9-4767-86c6-fe1ed1367361",
      "m1": "به نیروی یزدان که او بد پناه",
      "m2": "به پیروز بخت جهاندار شاه",
      "verse_number": 12
    },
    {
      "id": "4c8b583f-ba2d-4869-b009-76e6c8a364cd",
      "m1": "چو پیروز برگشت مرد از نبرد",
      "m2": "درفش دلفروز بر پای کرد",
      "verse_number": 13
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 13
  }
}