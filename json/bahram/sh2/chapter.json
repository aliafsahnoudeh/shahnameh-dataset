{
  "id": "0c8afb16-1b2a-45f3-90f5-6a56a2a35477",
  "title": "بخش ۲",
  "verses": [
    {
      "id": "bc46669b-0b62-4355-8c13-7938d98a5124",
      "m1": "برو نیز بگذشت سال دراز",
      "m2": "سر تاجور اندر آمد به گاز",
      "verse_number": 1
    },
    {
      "id": "9782e8ee-2825-47c0-9ce8-231b8661456a",
      "m1": "یکی پور بودش دلارام بود",
      "m2": "ورا نام بهرام بهرام بود",
      "verse_number": 2
    },
    {
      "id": "a8ca1902-971d-4985-a1e6-c7c5c95e81fd",
      "m1": "بیاورد و بنشاندش زیر تخت",
      "m2": "بدو گفت کای سبز شاخ درخت",
      "verse_number": 3
    },
    {
      "id": "238e5e27-d512-4809-9cba-3fb86e6bb2c8",
      "m1": "نبودم فراوان من از تخت شاد",
      "m2": "همه روزگار تو فرخنده باد",
      "verse_number": 4
    },
    {
      "id": "4cf25c18-7d74-407d-afa4-49a22c4a2d5d",
      "m1": "سراینده باش و فزاینده باش",
      "m2": "شب و روز بارامش و خنده باش",
      "verse_number": 5
    },
    {
      "id": "283e7ba5-94d1-4dc2-853d-2cbd46fd7f94",
      "m1": "چنان رو که پرسند روز شمار",
      "m2": "نپیچی سر از شرم پروردگار",
      "verse_number": 6
    },
    {
      "id": "45634256-877d-4b7e-8498-f9e94dc0760f",
      "m1": "به داد و دهش گیتی آباد دار",
      "m2": "دل زیردستان خود شاد دار",
      "verse_number": 7
    },
    {
      "id": "b0a7efe3-748e-4e63-b47c-0def7fe9fe8a",
      "m1": "که برکس نماند جهان جاودان",
      "m2": "نه بر تاجدار و نه بر موبدان",
      "verse_number": 8
    },
    {
      "id": "fd334311-b88b-4457-92fe-e64d46ba1bbf",
      "m1": "تو از چرخ گردان مدان این ستم",
      "m2": "چو از باد چندی گذاری به دم",
      "verse_number": 9
    },
    {
      "id": "5145e223-6e00-4e85-9dbb-7c0b464efa21",
      "m1": "به سه سال و سه ماه و بر سر سه روز",
      "m2": "تهی ماند زو تخت گیتی فروز",
      "verse_number": 10
    },
    {
      "id": "c2cf60b3-94a2-4d1d-a70f-1081404ede16",
      "m1": "چو بهرام گیتی به بهرام داد",
      "m2": "پسر مر ورا دخمه آرام داد",
      "verse_number": 11
    },
    {
      "id": "46c2dcda-fedb-44b0-b89b-0c96228c07a6",
      "m1": "چنین بود تا بود چرخ بلند",
      "m2": "به انده چه داری دلت را نژند",
      "verse_number": 12
    },
    {
      "id": "e5b13ead-4bf8-4a03-a698-0d335be22ce5",
      "m1": "چه گویی چه جویی چه شاید بدن",
      "m2": "برین داستانی نشاید زدن",
      "verse_number": 13
    },
    {
      "id": "b4aceefd-dc82-4b65-9a9f-005a4ee42557",
      "m1": "روانت گر از آز فرتوت نیست",
      "m2": "نشست تو جز تنگ تابوت نیست",
      "verse_number": 14
    },
    {
      "id": "61e28e1c-a99b-45f5-8cfb-7ccb38fc163a",
      "m1": "اگر مرگ دارد چنین طبع گرگ",
      "m2": "پر از می یکی جام خواهم بزرگ",
      "verse_number": 15
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 15
  }
}