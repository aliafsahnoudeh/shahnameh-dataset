{
  "id": "d8513d0b-5cf3-4b61-89d6-3ee8424f6156",
  "title": "بخش ۷",
  "verses": [
    {
      "id": "7b8149a6-8aaf-44a9-b796-6bfa8a262b77",
      "m1": "یکی خویش کاموس بد ساوه نام",
      "m2": "سرافراز و هر جای گسترده کام",
      "verse_number": 1
    },
    {
      "id": "49b9c62e-043a-440e-bb35-7f1d3ec9f27f",
      "m1": "بیامد بپیش تهمتن بجنگ",
      "m2": "یکی تیغ هندی گرفته بچنگ",
      "verse_number": 2
    },
    {
      "id": "5e9169d2-13ab-464b-bfd0-20f1b0edb684",
      "m1": "بگردید گرد چپ و دست راست",
      "m2": "ز رستم همی کین کاموس خواست",
      "verse_number": 3
    },
    {
      "id": "631d9948-5644-4078-9d82-016b2f33067f",
      "m1": "برستم چنین گفت کای ژنده پیل",
      "m2": "ببینی کنون موج دریای نیل",
      "verse_number": 4
    },
    {
      "id": "35667583-689d-4a87-8017-f52e27571184",
      "m1": "بخواهم کنون کین کاموس خوار",
      "m2": "اگر باشدم زین سپس کارزار",
      "verse_number": 5
    },
    {
      "id": "82edb879-5876-4755-964b-8c2618ccb3cc",
      "m1": "چو گفتار ساوه برستم رسید",
      "m2": "بزد دست و گرز گران برکشید",
      "verse_number": 6
    },
    {
      "id": "74513125-2aa1-457e-aaee-0aa3626aa884",
      "m1": "بزد بر سرش گرز را پیلتن",
      "m2": "که جانش برون شد بزاری ز تن",
      "verse_number": 7
    },
    {
      "id": "e8e99bf7-ff14-4640-ba9f-0e12c69809b1",
      "m1": "برآورد و زد بر سر و مغفرش",
      "m2": "ندیدست گفتی تنش را سرش",
      "verse_number": 8
    },
    {
      "id": "4789e551-f066-466b-9603-2d0c3d5ca49d",
      "m1": "بیفگند و رخش از بر او براند",
      "m2": "ز ساوه بگیتی نشانی نماند",
      "verse_number": 9
    },
    {
      "id": "561a7e98-e87d-47e9-8286-a2940ee58306",
      "m1": "درفش کشانی نگونسار کرد",
      "m2": "و زو جان لشکر پرآزار کرد",
      "verse_number": 10
    },
    {
      "id": "7f0e969d-f181-403e-b6ac-545be4aa95e1",
      "m1": "نبد نیز کس پیش او پایدار",
      "m2": "همه خاک مغز سر آورد بار",
      "verse_number": 11
    },
    {
      "id": "427b7db2-7c7b-4cad-837a-40d96f1809e4",
      "m1": "پس از میمنه شد سوی میسره",
      "m2": "غمی گشت لشکر همه یکسره",
      "verse_number": 12
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 12
  },
  "order_in_parent": 7
}