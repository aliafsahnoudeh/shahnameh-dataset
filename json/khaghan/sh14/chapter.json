    },
    {
      "id": "03225ba7-36e4-456d-8bd2-228006dda631",
      "m1": "نیامد سلیحم بدو کارگر",
      "m2": "بران ببر و آن خود و چینی سپر",
      "verse_number": 232
    },
    {
      "id": "8a680736-e467-4340-b337-f82b396f1e38",
      "m1": "بیابان سپردی و راه دراز",
      "m2": "کنون چارهٔ کار او را بساز",
      "verse_number": 233
    },
    {
      "id": "42dee812-f535-4a82-98c0-a0695fb6de26",
      "m1": "پر اندیشه شد جان پولادوند",
      "m2": "که آن بند را چون شود کاربند",
      "verse_number": 234
    },
    {
      "id": "6e593422-0831-43f1-acb9-1d2af5595126",
      "m1": "چنین داد پاسخ بافراسیاب",
      "m2": "که در جنگ چندین نباید شتاب",
      "verse_number": 235
    },
    {
      "id": "24e177a3-49c0-4ebd-8036-89dacd5459c3",
      "m1": "گر آنست رستم که مازندران",
      "m2": "تبه کرد و بستد بگرز گران",
      "verse_number": 236
    },
    {
      "id": "efe01ef0-4a1d-48ac-838d-0dd21ea99b62",
      "m1": "بدرید پهلوی دیو سپید",
      "m2": "جگرگاه پولاد غندی و بید",
      "verse_number": 237
    },
    {
      "id": "b729716c-4e42-4cb1-9ecd-3b99f45058c8",
      "m1": "مرا نیست پایاب با جنگ اوی",
      "m2": "نیارم ببد کردن آهنگ اوی",
      "verse_number": 238
    },
    {
      "id": "69b3745d-9b3f-4227-a71e-f83db98cfa81",
      "m1": "تن و جان من پیش رای تو باد",
      "m2": "همیشه خرد رهنمای تو باد",
      "verse_number": 239
    },
    {
      "id": "794bed35-3f7f-4706-b316-34199d657df4",
      "m1": "من او را بر اندیشه دارم بجنگ",
      "m2": "بگردش بگردم بسان پلنگ",
      "verse_number": 240
    },
    {
      "id": "e00be0e0-7047-4291-86b7-db7b0cab5ab9",
      "m1": "تو لشکر برآغال بر لشکرش",
      "m2": "بانبوه تا خیره گردد سرش",
      "verse_number": 241
    },
    {
      "id": "061cbc1c-64d1-4a82-a699-ff5c82c53ba2",
      "m1": "مگر چاره سازم و گر نی بدست",
      "m2": "بر و یال او را نشاید شکست",
      "verse_number": 242
    },
    {
      "id": "4f5520af-ac0a-48a4-8bab-b27333c94a31",
      "m1": "ازو شاد شد جان افراسیاب",
      "m2": "می روشن آورد و چنگ و رباب",
      "verse_number": 243
    },
    {
      "id": "3b7d1a29-277d-4f3c-a56b-2ec73caf8734",
      "m1": "بدانگه که شد مست پولادوند",
      "m2": "چنین گفت با او ببانگ بلند",
      "verse_number": 244
    },
    {
      "id": "7a29ae78-5352-46e0-9aff-5970bbdb8918",
      "m1": "که من بر فریدون و ضحاک و جم",
      "m2": "خور و خواب و آرام کردم دژم",
      "verse_number": 245
    },
    {
      "id": "532c3463-3680-49e4-abcf-23e9b8d17528",
      "m1": "برهمن بترسد ز آواز من",
      "m2": "وزین لشکر گردنافراز من",
      "verse_number": 246
    },
    {
      "id": "9c129cf9-2035-42b5-9161-9216a522c8ce",
      "m1": "من این زابلی را بشمشیر تیز",
      "m2": "برآوردگه بر کنم ریز ریز",
      "verse_number": 247
    }
  ],
