{
  "id": "dd1f0bc1-06ed-4c1b-b7b6-57815b171996",
  "title": "بخش ۱۴",
  "verses": [
    {
      "id": "0deb6366-c243-4f6c-b425-8a74473fcf56",
      "m1": "وزان جایگه لشکر اندر کشید",
      "m2": "بیک منزلی بر یکی شهر دید",
      "verse_number": 1
    },
    {
      "id": "4afeaa78-b520-4e66-a12c-e922bac8f6c4",
      "m1": "کجا نام آن شهر بیداد بود",
      "m2": "دژی بود وز مردم آباد بود",
      "verse_number": 2
    },
    {
      "id": "cbfdd5ab-6faa-440c-9676-ae6dc3877386",
      "m1": "همه خوردنیشان ز مردم بدی",
      "m2": "پری چهرهای هر زمان گم بدی",
      "verse_number": 3
    },
    {
      "id": "43964e79-e475-4c14-a6b6-7ca1345d4323",
      "m1": "بخوان چنان شهریار پلید",
      "m2": "نبودی جز از کودک نارسید",
      "verse_number": 4
    },
    {
      "id": "a6b2baa4-b69c-453c-a722-608cca227d51",
      "m1": "پرستندگانی که نیکو بدی",
      "m2": "به دیدار و بالا بیآهو بدی",
      "verse_number": 5
    },
    {
      "id": "aa2c1f4e-a575-416c-8125-64448f399926",
      "m1": "از آن ساختندی بخوان بر خورش",
      "m2": "بدین گونه بد شاه را پرورش",
      "verse_number": 6
    },
    {
      "id": "86ac0d6f-65cf-4179-aa6c-e0a1bffc8948",
      "m1": "تهمتن بفرمود تا سه هزار",
      "m2": "زرهدار برگستوانور سوار",
      "verse_number": 7
    },
    {
      "id": "643e5e9b-a869-46c2-97c2-cd1e9ccf3a91",
      "m1": "بدان دژ فرستاد با گستهم",
      "m2": "دو گرد خردمند با اوبهم",
      "verse_number": 8
    },
    {
      "id": "c3b88f0a-3e50-41ce-b7a9-f51bb74e4b16",
      "m1": "مرین مرد را نام کافور بود",
      "m2": "که او را بران شهر منشور بود",
      "verse_number": 9
    },
    {
      "id": "36050ea1-272d-447e-a889-65553ead6fec",
      "m1": "بپوشید کافور خفتان جنگ",
      "m2": "همه شهر با او بسان پلنگ",
      "verse_number": 10
    },
    {
      "id": "c855c046-7c0b-4ffd-ad96-8b6c802cfc06",
      "m1": "کمندافگن و زورمندان بدند",
      "m2": "برزم اندرون پیل دندان بدند",
      "verse_number": 11
    },
    {
      "id": "512da736-d80f-40ab-aa22-60a98d76cee7",
      "m1": "چو گستهم گیتی بران گونه دید",
      "m2": "جهان در کف دیو وارونه دید",
      "verse_number": 12
    },
    {
      "id": "a192c668-ee4c-4d81-a233-b63458b1b09b",
      "m1": "بفرمود تا تیر باران کنند",
      "m2": "بریشان کمین سواران کنند",
      "verse_number": 13
    },
    {
      "id": "ee675646-f104-435a-9445-4c53f2108cc5",
      "m1": "چنین گفت کافور با سرکشان",
      "m2": "که سندان نگیرد ز پیکان نشان",
      "verse_number": 14
    },
    {
      "id": "70713d08-c7ac-49ae-929b-a58a6e394160",
      "m1": "همه تیغ و گرز و کمند آورید",
      "m2": "سر سرکشان را ببند آورید",
      "verse_number": 15
    },
    {
      "id": "97c49e53-6047-41ee-9118-62a1377d317a",
      "m1": "زمانی بران سان برآویختند",
      "m2": "که آتش ز دریا برانگیختند",
      "verse_number": 16
    },
    {
      "id": "f6ddd8dd-b353-4fcd-8e43-89f7cb48da6d",
      "m1": "فراوان ز ایرانیان کشته شد",
      "m2": "بسر بر سپهر بلا گشته شد",
      "verse_number": 17
    },
    {
      "id": "514c5701-95a5-4b5c-86c4-9776cfafbc55",
      "m1": "ببیژن چنین گفت گستهم زود",
      "m2": "که لختی عنانت بباید بسود",
      "verse_number": 18
    },
    {
      "id": "dc9d85ec-020a-4316-b132-df4bb52f4c80",
      "m1": "برستم بگویی که چندین مایست",
      "m2": "بجنبان عنان با سواری دویست",
      "verse_number": 19
    },
    {
      "id": "6cdcf6c3-b1d5-4d44-bf34-a81ac7669746",
      "m1": "بشد بیژن گیو برسان باد",
      "m2": "سخن بر تهمتن همه کرد یاد",
      "verse_number": 20
    },
    {
      "id": "b47263ff-6d5a-4d43-881e-e376975a1c99",
      "m1": "گران کرد رستم زمانی رکیب",
      "m2": "ندانست لشکر فراز از نشیب",
      "verse_number": 21
    },
    {
      "id": "289c01db-866d-4e5c-999f-1c3a38841e8e",
      "m1": "بدانسان بیامد بدان رزمگاه",
      "m2": "که باد اندر آید ز کوه سیاه",
      "verse_number": 22
    },
    {
      "id": "52d3342d-133e-4dfa-a19b-f43865c57d0b",
      "m1": "فراوان ز ایرانیان کشته دید",
      "m2": "بسی سرکش از جنگ برگشته دید",
      "verse_number": 23
    },
    {
      "id": "71bc5583-1688-4d85-acc4-2eb5f1d83cb1",
      "m1": "بکافور گفت ای سگ بدگهر",
      "m2": "کنون رزم و رنج تو آمد بسر",
      "verse_number": 24
    },
    {
      "id": "740fbc3e-5c76-414d-98e4-8fdea1314fa6",
      "m1": "یکی حمله آورد کافور سخت",
      "m2": "بران بارور خسروانی درخت",
      "verse_number": 25
    },
    {
      "id": "fdf41bb3-d7c0-410e-b424-9ec66b2718e5",
      "m1": "بینداخت تیغی بکردار تیر",
      "m2": "که آید مگر بر یل شیرگیر",
      "verse_number": 26
    },
    {
      "id": "86af7e66-090c-4ae2-a5c1-b27928202f7e",
      "m1": "بپیش اندر آورد رستم سپر",
      "m2": "فرو ماند کافور پرخاشخر",
      "verse_number": 27
    },
    {
      "id": "675cc812-34a5-4cac-8e23-1215dddfa501",
      "m1": "کمندی بینداخت بر سوی طوس",
      "m2": "بسی کرد رستم برو بر فسوس",
      "verse_number": 28
    },
    {
      "id": "7395e5d2-38f2-4496-bb49-f9f8024da1e6",
      "m1": "عمودی بزد بر سرش پور زال",
      "m2": "که بر هم شکستش سر و ترگ و یال",
      "verse_number": 29
    },
    {
      "id": "de0be0cc-8409-44a0-b700-fd625c329fbe",
      "m1": "چنین تا در دژ یکی حمله برد",
      "m2": "بزرگان نبودند پیدا ز خرد",
      "verse_number": 30
    },
    {
      "id": "174e0b5e-c3eb-4398-8670-d10b8f3bc5cf",
      "m1": "در دژ ببستند وز باره تیز",
      "m2": "برآمد خروشیدن رستخیز",
      "verse_number": 31
    },
    {
      "id": "c703fac4-cc38-425c-92ac-da143bd53a9c",
      "m1": "بگفتند کای مرد بازور و هوش",
      "m2": "برین گونه با ما بکینه مکوش",
      "verse_number": 32
    },
    {
      "id": "7a6698a7-618e-40ab-99ca-872130a8a286",
      "m1": "پدر نام تو چون بزادی چه کرد",
      "m2": "کمندافگنی گر سپهر نبرد",
      "verse_number": 33
    },
    {
      "id": "a3c47ed1-c867-4daf-b19c-964ab0fa4b67",
      "m1": "دریغست رنج اندرین شارستان",
      "m2": "که داننده خواند ورا کارستان",
      "verse_number": 34
    },
    {
      "id": "44eff186-b4e9-42d7-9a25-dd8652399615",
      "m1": "چو تور فریدون ز ایران براند",
      "m2": "ز هر گونه دانندگان را بخواند",
      "verse_number": 35
    },
    {
      "id": "2f8ea0a6-ddbb-49ba-b9a4-5899fc22f533",
      "m1": "یکی باره افگند زین گونه پی",
      "m2": "ز سنگ و ز خشت و ز چوب و ز نی",
      "verse_number": 36
    },
    {
      "id": "639cb41a-3a7a-4f13-baff-e1656e0e5a96",
      "m1": "برآورد ازینسان بافسون و رنج",
      "m2": "بپالود رنج و تهی کرد گنج",
      "verse_number": 37
    },
    {
      "id": "5c0679cf-24dd-4969-81b6-b408dfa5a7e5",
      "m1": "بسی رنج بردند مردان مرد",
      "m2": "کزین بارهٔ دژ برآرند گرد",
      "verse_number": 38
    },
    {
      "id": "a0e60f3e-bffe-4074-a169-bf1b7131bf0b",
      "m1": "نبدکس بدین شارستان پادشا",
      "m2": "بدین رنج بردن نیارد بها",
      "verse_number": 39
    },
    {
      "id": "29e2b386-b842-4eb4-b9b1-d83c13c9a218",
      "m1": "سلیحست و ایدر بسی خوردنی",
      "m2": "بزیر اندرون راه آوردنی",
      "verse_number": 40
    },
    {
      "id": "238e3973-aada-417d-b8ba-421063d3e8fa",
      "m1": "اگر سالیان رنج و رزم آوری",
      "m2": "نباشد بدستت جز از داوری",
      "verse_number": 41
    },
    {
      "id": "e9492249-1b9d-4137-a6b8-65bbae051d1b",
      "m1": "نیاید برین باره بر منجنیق",
      "m2": "از افسون سلم و دم جاثلیق",
      "verse_number": 42
    },
    {
      "id": "f139382a-a286-4eb7-8454-edf404535559",
      "m1": "چو بشنید رستم پر اندیشه شد",
      "m2": "دلش از غم و درد چون بیشه شد",
      "verse_number": 43
    },
    {
      "id": "ab8ca358-a88d-4f9b-9f22-620054c4a4f3",
      "m1": "یکی رزم کرد آن نه بر آرزوی",
      "m2": "سپاه اندر آورد بر چار سوی",
      "verse_number": 44
    },
    {
      "id": "bbc2e40e-e8f4-4671-a2ac-24aea34ed4b1",
      "m1": "بیک روی گودرز و یک روی طوس",
      "m2": "پس پشت او پیل با بوق و کوس",
      "verse_number": 45
    },
    {
      "id": "30b5c80a-e57c-4ae0-802c-dbd5274780e8",
      "m1": "بیک روی بر لشکر زابلی",
      "m2": "زرهدار با خنجر کابلی",
      "verse_number": 46
    },
    {
      "id": "2e236177-1413-4511-b015-e26f28c82fcc",
      "m1": "چو آن دید رستم کمان برگرفت",
      "m2": "همه دژ بدو ماند اندر شگفت",
      "verse_number": 47
    },
    {
      "id": "5c6cf4a8-7b78-40cc-a0d4-c9098f20a491",
      "m1": "هر آنکس که از باره سر بر زدی",
      "m2": "زمانه سرش را بهم در زدی",
      "verse_number": 48
    },
    {
      "id": "767d6ff5-7867-4587-b086-bfcec6fdea6c",
      "m1": "ابا مغز پیکان همی راز گفت",
      "m2": "ببدسازگاری همی گشت جفت",
      "verse_number": 49
    },
    {
      "id": "25d21974-75dc-45e8-897c-935ceef73e2c",
      "m1": "بن باره زان پس بکندن گرفت",
      "m2": "ز دیوار مردم فگندن گرفت",
      "verse_number": 50
    },
    {
      "id": "f656fc12-9d37-4047-ad1c-b8c52d2e93b8",
      "m1": "ستونها نهادند زیر اندرش",
      "m2": "بیالود نفط سیاه از برش",
      "verse_number": 51
    },
    {
      "id": "bee14133-8fb0-4969-b274-9180e8c38ec3",
      "m1": "چو نیمی ز دیوار دژکنده شد",
      "m2": "بچوب اندر آتش پراگنده شد",
      "verse_number": 52
    },
    {
      "id": "c067a2ee-0463-42e4-aff3-e4bb1d1b8bc8",
      "m1": "فرود آمد آن بارهٔ تور گرد",
      "m2": "ز هر سو سپاه اندر آمد بگرد",
      "verse_number": 53
    },
    {
      "id": "2f900853-a47f-4b65-b6dd-97c40e795ce3",
      "m1": "بفرمود رستم که جنگ آورید",
      "m2": "کمانها و تیر خدنگ آورید",
      "verse_number": 54
    },
    {
      "id": "1ad2c831-e8b1-45d7-bc50-a2d269240ff9",
      "m1": "گوان از پی گنج و فرزند خویش",
      "m2": "همان از پی بوم و پیوند خویش",
      "verse_number": 55
    },
    {
      "id": "19e54251-0889-4e00-8552-5daedc4dccb2",
      "m1": "همه سر بدادند یکسر بباد",
      "m2": "گرامیتر آنکو ز مادر نزاد",
      "verse_number": 56
    },
    {
      "id": "38a48993-243e-4db5-aab3-e0b8d5e4d01b",
      "m1": "دلیران پیاده شدند آن زمان",
      "m2": "سپرهای چینی و تیر و کمان",
      "verse_number": 57
    },
    {
      "id": "d6c11725-d7a5-4442-91ed-b8d672157f03",
      "m1": "برفتند با نیزهداران بهم",
      "m2": "بپیش اندرون بیژن و گستهم",
      "verse_number": 58
    },
    {
      "id": "ba286583-dbae-4b0d-ba34-30ce816c3205",
      "m1": "دم آتش تیز و باران تیر",
      "m2": "هزیمت بود زان سپس ناگزیر",
      "verse_number": 59
    },
    {
      "id": "47ef11c0-58b9-4218-ae90-dcfe33fd6084",
      "m1": "چو از بارهٔ دژ بیرون شدند",
      "m2": "گریزان گریزان بهامون شدند",
      "verse_number": 60
    },
    {
      "id": "4225580b-112a-4783-95fb-90960632fcf1",
      "m1": "در دژ ببست آن زمان جنگجوی",
      "m2": "بتاراج و کشتن نهادند روی",
      "verse_number": 61
    },
    {
      "id": "7f6bb3e8-0454-4dbf-a3d1-1d0ca864e277",
      "m1": "چه مایه بکشتند و چندی اسیر",
      "m2": "ببردند زان شهر برنا و پیر",
      "verse_number": 62
    },
    {
      "id": "21489a2a-296c-4316-ae04-8860b2b7b202",
      "m1": "بسی سیم و زر و گرانمایه چیز",
      "m2": "ستور و غلام و پرستار نیز",
      "verse_number": 63
    },
    {
      "id": "36772f9c-9b0b-4e2c-992a-0f1936a4022f",
      "m1": "تهمتن بیامد سر و تن بشست",
      "m2": "بپیش جهانداور آمد نخست",
      "verse_number": 64
    },
    {
      "id": "208a38e4-1b38-46db-ac82-97da041964b3",
      "m1": "ز پیروز گشتن نیایش گرفت",
      "m2": "جهان آفرین را ستایش گرفت",
      "verse_number": 65
    },
    {
      "id": "47513c65-fd86-452f-b97b-157157141b38",
      "m1": "بایرانیان گفت با کردگار",
      "m2": "بیامد نهانی هم از آشکار",
      "verse_number": 66
    },
    {
      "id": "e8b55b2b-84e2-407f-ad01-7f0dbbbeca6c",
      "m1": "بپیروزی اندر نیایش کنید",
      "m2": "جهان آفرین را ستایش کنید",
      "verse_number": 67
    },
    {
      "id": "813537d6-e81e-4d4e-82fd-5affcf6c44ef",
      "m1": "بزرگان بپیش جهانآفرین",
      "m2": "نیایش گرفتند سر بر زمین",
      "verse_number": 68
    },
    {
      "id": "dbe0fd13-458e-46c0-855a-ce36a186acc1",
      "m1": "چو از پاک یزدان بپرداختند",
      "m2": "بران نامدار آفرین ساختند",
      "verse_number": 69
    },
    {
      "id": "2f798af4-b0ab-425b-bf23-2a739d9e18da",
      "m1": "که هر کس که چون تو نباشد بجنگ",
      "m2": "نشستن به آید بنام و بننگ",
      "verse_number": 70
    },
    {
      "id": "d7349fe6-c7bb-4dc2-aa48-502b885f240c",
      "m1": "تن پیل داری و چنگال شیر",
      "m2": "زمانی نباشی ز پیگار سیر",
      "verse_number": 71
    },
    {
      "id": "d78b7f19-3e4e-49dc-b921-5fbb6889fbab",
      "m1": "تهمتن چنین گفت کین زور و فر",
      "m2": "یکی خلعتی باشد از دادگر",
      "verse_number": 72
    },
    {
      "id": "0dc46f0c-6176-4c34-ac87-960ade6c00bd",
      "m1": "شما سربسر بهره دارید زین",
      "m2": "نه جای گلهست از جهان آفرین",
      "verse_number": 73
    },
    {
      "id": "a675c7af-f0b0-4446-b358-dcf5c9bc7c4d",
      "m1": "بفرمود تا گیو با ده هزار",
      "m2": "سپردار و بر گستوان ور سوار",
      "verse_number": 74
    },
    {
      "id": "ce85f083-95fa-4361-b722-6e4592e7a13c",
      "m1": "شود تازیان تا بمرز ختن",
      "m2": "نماند که ترکان شوند انجمن",
      "verse_number": 75
    },
    {
      "id": "13d9fad8-9d8a-4844-9140-4162b9d71c22",
      "m1": "چو بنمود شب جعد زلف سیاه",
      "m2": "از اندیشه خمیده شد پشت ماه",
      "verse_number": 76
    },
    {
      "id": "858f89d3-337a-4c63-92e3-488f403f8882",
      "m1": "بشد گیو با آن سواران جنگ",
      "m2": "سه روز اندر آن تاختن شد درنگ",
      "verse_number": 77
