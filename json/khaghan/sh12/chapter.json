{
  "id": "04f40fea-0c7e-40e2-b94c-b82bf94e95a4",
  "title": "بخش ۱۲",
  "verses": [
    {
      "id": "e3a3e24f-d0b0-4cc3-9013-66d7784d06fa",
      "m1": "بایوان شد و نامه پاسخ نوشت",
      "m2": "بباغ بزرگی درختی بکشت",
      "verse_number": 1
    },
    {
      "id": "502faef7-2ba3-488e-af7e-956061f52b33",
      "m1": "نخست آفرین کرد بر کردگار",
      "m2": "کزو بود روشن دل و بختیار",
      "verse_number": 2
    },
    {
      "id": "343d998b-0b83-46cd-8fe8-a5c12c833694",
      "m1": "خداوند ناهید و گردان سپهر",
      "m2": "کزویست پرخاش و آرام و مهر",
      "verse_number": 3
    },
    {
      "id": "7b2522f8-c1aa-4e5f-8505-d23b8de8fdb8",
      "m1": "سپهری برین گونه بر پای کرد",
      "m2": "شب و روز را گیتی آرای کرد",
      "verse_number": 4
    },
    {
      "id": "6ef13f47-ed6e-4540-ad8f-c09741f505c7",
      "m1": "یکی را چنین تیرهبخت آفرید",
      "m2": "یکی را سزاوار تخت آفرید",
      "verse_number": 5
    },
    {
      "id": "3f012ad9-067e-441f-b03a-97894ac4759c",
      "m1": "غم و شادمانی ز یزدان شناس",
      "m2": "کزویست هر گونه بر ما سپاس",
      "verse_number": 6
    },
    {
      "id": "8f0462cc-3965-4b20-b734-4d9c422e40a7",
      "m1": "رسید آنچ دادی بدین بارگاه",
      "m2": "اسیران و پیلان و تخت و کلاه",
      "verse_number": 7
    },
    {
      "id": "de4b890b-b2b5-4171-b86d-71f24d5dcd2c",
      "m1": "هیونان بسیار و افگندنی",
      "m2": "ز پوشیدنی هم ز گستردنی",
      "verse_number": 8
    },
    {
      "id": "60ce69ad-f1d9-4fa4-8cbe-3b37307eb294",
      "m1": "همه آلت ناز و سورست و بزم",
      "m2": "بپیش تو زین سان که آید برزم",
      "verse_number": 9
    },
    {
      "id": "4c20b859-a9c2-4fdf-862b-bf065c79cf09",
      "m1": "مگر آنکسی کش سرآید بپیش",
      "m2": "بدین گونه سیر آید از جان خویش",
      "verse_number": 10
    },
    {
      "id": "793efb8c-9d0a-483b-82ae-2156f0bb3903",
      "m1": "وزان رنج بردن ز توران سپاه",
      "m2": "شب و روز بودن به آوردگاه",
      "verse_number": 11
    },
    {
      "id": "8b291db2-849a-4cec-a852-1454380668dd",
      "m1": "ز کارت خبر بد مرا روز و شب",
      "m2": "گشاده نکردم به بیگانه لب",
      "verse_number": 12
    },
    {
      "id": "7bb9ae5b-d621-4a92-a292-bfd209187f2b",
      "m1": "شب و روز بر پیش یزدان پاک",
      "m2": "نوان بودم و دل شده چاک چاک",
      "verse_number": 13
    },
    {
      "id": "df99771d-d241-497b-9506-82e873724191",
      "m1": "کسی را که رستم بود پهلوان",
      "m2": "سزد گر بماند همیشه جوان",
      "verse_number": 14
    },
    {
      "id": "1a2df8ba-8a74-4cd2-9f5e-88464735a70e",
      "m1": "پرستنده چون تو ندارد سپهر",
      "m2": "ز تو بخت هرگز مبراد مهر",
      "verse_number": 15
    },
    {
      "id": "eed50d36-efbc-406d-8c42-9185b53b0201",
      "m1": "نویسنده پردخته شد ز آفرین",
      "m2": "نهاد از بر نامه خسرو نگین",
      "verse_number": 16
    },
    {
      "id": "6c63e0a1-1cb9-49bc-843f-7ecd0dc52072",
      "m1": "بفرمود تا خلعت آراستند",
      "m2": "ستام و کمرها بپیراستند",
      "verse_number": 17
    },
    {
      "id": "40dfeb46-0a59-4c99-a55a-2d716e0b9ec5",
      "m1": "صد از جعد مویان زرین کمر",
      "m2": "صد اسپ گرانمایه با زین زر",
      "verse_number": 18
    },
    {
      "id": "95825661-eefa-4c0e-a225-726482f7981e",
      "m1": "صد اشتر همه بار دیبای چین",
      "m2": "صد اشتر ز افگندنی هم چنین",
      "verse_number": 19
    },
    {
      "id": "71d488c1-4b41-48b0-9481-ab0d84e92bac",
      "m1": "ز یاقوت رخشان دو انگشتری",
      "m2": "ز خوشاب و در افسری بر سری",
      "verse_number": 20
    },
    {
      "id": "ccbf613f-22d6-486d-a11d-b40cf754649e",
      "m1": "ز پوشیدن شاه دستی بزر",
      "m2": "همان یاره و طوق و زرین کمر",
      "verse_number": 21
    },
    {
      "id": "eb58292b-6295-415d-bcd4-0f85666ceb42",
      "m1": "سران را همه هدیهها ساختند",
      "m2": "یکی گنج زین سان بپرداختند",
      "verse_number": 22
    },
    {
      "id": "e896cfdd-6b2a-402f-b157-0bea927188fc",
      "m1": "فریبرز با تاج و گرز و درفش",
      "m2": "یکی تخت زرین و زرینه کفش",
      "verse_number": 23
    },
    {
      "id": "e347bd5a-5b0a-4963-a63d-10a129bf1d51",
      "m1": "فرستاد و فرمود تا بازگشت",
      "m2": "از ایران بسوی سپهبد گذشت",
      "verse_number": 24
    },
    {
      "id": "ad38923b-36fb-4a3c-9557-c0131249436a",
      "m1": "چنین گفت کز جنگ افراسیاب",
      "m2": "نه آرام باید نه خورد و نه خواب",
      "verse_number": 25
    },
    {
      "id": "b3789798-36c9-47d7-922c-fa5ddf659563",
      "m1": "مگر کان سر شهریار گزند",
      "m2": "بخم کمند تو آید ببند",
      "verse_number": 26
    },
    {
      "id": "3096c1b5-12ce-4e6f-9b96-680705522b33",
      "m1": "فریبرز برگشت زان بارگاه",
      "m2": "بکام دل شاه ایران سپاه",
      "verse_number": 27
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 27
  },
  "order_in_parent": 12
}