{
  "id": "73e8c801-e2ef-456c-89dd-005a3cf1bb49",
  "title": "بخش ۴",
  "verses": [
    {
      "id": "0d6b1823-7afa-4018-8ad5-943b7e497ee9",
      "m1": "به رستم چنین گفت فرخنده زال",
      "m2": "که برگیر کوپال و بفراز یال",
      "verse_number": 1
    },
    {
      "id": "e83044f3-c791-45a2-9a55-7d1198723530",
      "m1": "برو تازیان تا به البرز کوه",
      "m2": "گزین کن یکی لشکر همگروه",
      "verse_number": 2
    },
    {
      "id": "e3dbbd8c-4ff6-4f01-9d7a-7c7e1635e801",
      "m1": "ابر کیقباد آفرین کن یکی",
      "m2": "مکن پیش او بر درنگ اندکی",
      "verse_number": 3
    },
    {
      "id": "7b9ef14f-f064-4103-bc3a-573fd46d1c35",
      "m1": "به دو هفته باید که ایدر بوی",
      "m2": "گه و بیگه از تاختن نغنوی",
      "verse_number": 4
    },
    {
      "id": "1aac1bc9-3f6c-4687-8b68-4c7bdcc8e366",
      "m1": "بگویی که لشکر ترا خواستند",
      "m2": "همی تخت شاهی بیاراستند",
      "verse_number": 5
    },
    {
      "id": "8e673ec3-2106-4455-81f5-e591462ecb52",
      "m1": "که در خورد تاج کیان جز تو کس",
      "m2": "نبینیم شاها تو فریادرس",
      "verse_number": 6
    },
    {
      "id": "bdb40952-9c4f-4568-b286-31cb062d6dde",
      "m1": "تهمتن زمین را به مژگان برفت",
      "m2": "کمر برمیان بست و چون باد تفت",
      "verse_number": 7
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 7
  }
}