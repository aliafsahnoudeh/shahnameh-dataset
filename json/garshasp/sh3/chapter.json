{
  "id": "598aa4c7-1b3a-4b42-8778-b98761b80462",
  "title": "بخش ۳",
  "verses": [
    {
      "id": "05abbf17-360f-4d5a-b151-54329d6104c3",
      "m1": "بزد مهره در جام بر پشت پیل",
      "m2": "ازو برشد آواز تا چند میل",
      "verse_number": 1
    },
    {
      "id": "25421b99-66ac-44e9-9158-d4bdad255dc1",
      "m1": "خروشیدن کوس با کرنای",
      "m2": "همان ژنده پیلان و هندی درای",
      "verse_number": 2
    },
    {
      "id": "8d894386-5eb0-4ada-9151-47ca93c79126",
      "m1": "برآمد ز زاولستان رستخیز",
      "m2": "زمین خفته را بانگ برزد که خیز",
      "verse_number": 3
    },
    {
      "id": "59eaa46e-1018-45b1-9ddc-ca056ec6352a",
      "m1": "به پیش اندرون رستم پهلوان",
      "m2": "پس پشت او سالخورده گوان",
      "verse_number": 4
    },
    {
      "id": "a975da1c-7e02-4776-a9e6-9007cc700f83",
      "m1": "چنان شد ز لشکر در و دشت و راغ",
      "m2": "که بر سر نیارست پرید زاغ",
      "verse_number": 5
    },
    {
      "id": "efdad209-e474-4782-a1cf-1479d6ce28e2",
      "m1": "تبیره زدندی همی شست جای",
      "m2": "جهان را نه سر بود پیدا نه پای",
      "verse_number": 6
    },
    {
      "id": "eab313ec-4b12-4db5-b950-1c538206b36f",
      "m1": "به هنگام بشکوفهٔ گلستان",
      "m2": "بیاورد لشکر ز زابلستان",
      "verse_number": 7
    },
    {
      "id": "bb0890a3-65f0-43a9-ac06-ce2d8a36f315",
      "m1": "ز زال آگهی یافت افراسیاب",
      "m2": "برآمد ز آرام و از خورد و خواب",
      "verse_number": 8
    },
    {
      "id": "dff16f62-2907-4887-b14c-030bb29f272e",
      "m1": "بیاورد لشکر سوی خوار ری",
      "m2": "بران مرغزاری که بد آب و نی",
      "verse_number": 9
    },
    {
      "id": "e78f5b39-f2c4-429e-b806-34742efc495a",
      "m1": "ز ایران بیامد دمادم سپاه",
      "m2": "ز راه بیابان سوی رزمگاه",
      "verse_number": 10
    },
    {
      "id": "0198b762-b35b-48f9-a86d-99fdb93a701c",
      "m1": "ز لشکر به لشکر دو فرسنگ ماند",
      "m2": "سپهبد جهاندیدگان را بخواند",
      "verse_number": 11
    },
    {
      "id": "c8548ffb-1a42-404e-b2bd-a1aae35e0718",
      "m1": "بدیشان چنین گفت کای بخردان",
      "m2": "جهاندیده و کارکرده ردان",
      "verse_number": 12
    },
    {
      "id": "24fd90dc-22a3-43df-9545-7fd841cae6c8",
      "m1": "هم ایدر من این لشکر آراستم",
      "m2": "بسی سروری و مهی خواستم",
      "verse_number": 13
    },
    {
      "id": "89485e1d-c25d-4c23-9557-e88a8f1bf34c",
      "m1": "پراگنده شد رای بی تخت شاه",
      "m2": "همه کار بیروی و بیسر سپاه",
      "verse_number": 14
    },
    {
      "id": "397118cf-8d2d-4bba-842e-0e76b24c2909",
      "m1": "چو بر تخت بنشست فرخنده زو",
      "m2": "ز گیتی یکی آفرین خاست نو",
      "verse_number": 15
    },
    {
      "id": "a1431c3c-ae09-4dbe-a8e0-e260ad862d36",
      "m1": "شهی باید اکنون ز تخم کیان",
      "m2": "به تخت کیی بر کمر بر میان",
      "verse_number": 16
    },
    {
      "id": "c500524c-9e65-43aa-9989-47553e5fac3f",
      "m1": "شهی کاو باورنگ دارد ز می",
      "m2": "که بیسر نباشد تن آدمی",
      "verse_number": 17
    },
    {
      "id": "ebc1ff8d-0716-472e-84c7-509671415b78",
      "m1": "نشان داد موبد مرا در زمان",
      "m2": "یکی شاه با فر و بخت جوان",
      "verse_number": 18
    },
    {
      "id": "de0d321c-5496-41f2-9273-e5ab77103bf3",
      "m1": "ز تخم فریدون یل کیقباد",
      "m2": "که با فر و برزست و با رای و داد",
      "verse_number": 19
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 19
  }
}