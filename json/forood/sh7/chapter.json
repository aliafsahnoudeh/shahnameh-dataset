{
  "id": "600cc883-5549-4d35-b235-55f2d29e24bc",
  "title": "بخش ۷",
  "verses": [
    {
      "id": "a6cdf44b-76b5-4b69-ad41-83d20b8eca1e",
      "m1": "چو بهرام برگشت با طوس گفت",
      "m2": "که با جان پاکت خرد باد جفت",
      "verse_number": 1
    },
    {
      "id": "74c3cf3a-94b8-4668-a4da-c8de1fc94e4c",
      "m1": "بدان کان فرودست فرزند شاه",
      "m2": "سیاوش که شد کشته بر بی گناه",
      "verse_number": 2
    },
    {
      "id": "9808f33f-0dda-48c4-81e9-b56d2fdb5545",
      "m1": "نمود آن نشانی که اندر نژاد",
      "m2": "ز کاوس دارند و ز کیقباد",
      "verse_number": 3
    },
    {
      "id": "2408aa84-dcb6-4e05-a318-22b12ddabd30",
      "m1": "ترا شاه کیخسرو اندرز کرد",
      "m2": "که گرد فرود سیاوش مگرد",
      "verse_number": 4
    },
    {
      "id": "d123e895-c50e-4765-9f6b-6bb4456fbf88",
      "m1": "چنین داد پاسخ ستمکاره طوس",
      "m2": "که من دارم این لشکر و بوق و کوس",
      "verse_number": 5
    },
    {
      "id": "17113ef2-288f-445c-b6f8-8ef41c0a88fe",
      "m1": "ترا گفتم او را بنزد من آر",
      "m2": "سخن هیچگونه مکن خواستار",
      "verse_number": 6
    },
    {
      "id": "450a3925-675b-4a15-b49e-76226be0b4d4",
      "m1": "گر او شهریارست پس من کیم",
      "m2": "برین کوه گوید ز بهر چیم",
      "verse_number": 7
    },
    {
      "id": "73d05305-72a3-4f15-827b-976dffc3e13a",
      "m1": "یکی ترکزاده چو زاغ سیاه",
      "m2": "برین گونه بگرفت راه سپاه",
      "verse_number": 8
    },
    {
      "id": "11a8c28b-585c-4ddf-9b43-1fdc47eb9e9b",
      "m1": "نبینم ز خودکامه گودرزیان",
      "m2": "مگر آنک دارد سپه را زیان",
      "verse_number": 9
    },
    {
      "id": "f733db58-5e57-4ca5-940b-eeb7f7a7af37",
      "m1": "بترسیدی از بیهنر یک سوار",
      "m2": "نه شیر ژیان بود بر کوهسار",
      "verse_number": 10
    },
    {
      "id": "31f0d2ab-a9b6-4ce4-9ae5-88fd0a3d46fe",
      "m1": "سپه دید و برگشت سوی فریب",
      "m2": "بخیره سپردی فراز و نشیب",
      "verse_number": 11
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 11
  }
}