{
  "id": "6f50b7d7-3982-49d7-9915-4eb1c73150f9",
  "title": "بخش ۲۲",
  "verses": [
    {
      "id": "6ee4d61c-e40d-497b-93b2-a10a12b9fdd5",
      "m1": "چو آمد سر ماه هنگام جنگ",
      "m2": "ز پیمان بگشتند و از نام و ننگ",
      "verse_number": 1
    },
    {
      "id": "2b7897ad-8343-4e57-be96-fb6d06e0f188",
      "m1": "خروشی برآمد ز هر دو سپاه",
      "m2": "برفتند یکسر سوی رزمگاه",
      "verse_number": 2
    },
    {
      "id": "98805770-596e-4d2c-9034-ace5adb0daed",
      "m1": "ز بس ناله بوق و هندی درای",
      "m2": "همی آسمان اندر آمد ز جای",
      "verse_number": 3
    },
    {
      "id": "5e4ffc47-1bf8-47a1-b47e-5920646dd2c7",
      "m1": "هم از یال اسپان و دست و عنان",
      "m2": "ز گوپال و تیغ و کمان و سنان",
      "verse_number": 4
    },
    {
      "id": "aaf12ce3-94bc-4d26-84e9-fb17002f59d6",
      "m1": "تو گفتی جهان دام نر اژدهاست",
      "m2": "وگر آسمان بر زمین گشت راست",
      "verse_number": 5
    },
    {
      "id": "85ffddd6-d19d-422c-912f-2942f83b044b",
      "m1": "نبد پشه را روزگار گذر",
      "m2": "ز بس گرز و تیغ و سنان و سپر",
      "verse_number": 6
    },
    {
      "id": "aade3597-d2a9-41bc-8f3e-8b664ebc733c",
      "m1": "سوی میمنه گیو گودرز بود",
      "m2": "رد و موبد و مهتر مرز بود",
      "verse_number": 7
    },
    {
      "id": "6b377928-d396-46a7-bc8a-f8c7c38c9b23",
      "m1": "سوی میسره اشکش تیزچنگ",
      "m2": "که دریای خون راند هنگام جنگ",
      "verse_number": 8
    },
    {
      "id": "a6bc9d03-771a-41a2-8031-a26ffe2eccef",
      "m1": "یلان با فریبرز کاوس شاه",
      "m2": "درفش از پس پشت در قلبگاه",
      "verse_number": 9
    },
    {
      "id": "0a4b159a-e5d6-41f4-a5ac-4990bbc1b7d6",
      "m1": "فریبرز با لشکر خویش گفت",
      "m2": "که ما را هنرها شد اندر نهفت",
      "verse_number": 10
    },
    {
      "id": "9bbe053e-a610-4f1e-ac6a-174fd91146e1",
      "m1": "یک امروز چون شیر جنگ آوریم",
      "m2": "جهان بر بداندیش تنگ آوریم",
      "verse_number": 11
    },
    {
      "id": "d1dbf813-b587-475d-9480-9ce02f25dd96",
      "m1": "کزین ننگ تا جاودان بر سپاه",
      "m2": "بخندد همی گرز و رومی کلاه",
      "verse_number": 12
    },
    {
      "id": "260a929d-ec51-426e-8873-bf9fef14ac67",
      "m1": "یکی تیرباران بکردند سخت",
      "m2": "چو باد خزانی که ریزد درخت",
      "verse_number": 13
    },
    {
      "id": "bc0164fb-cc52-44e5-8c7c-623bd37bdd8c",
      "m1": "تو گفتی هوا پر کرگس شدست",
      "m2": "زمین از پی پیل پامس شدست",
      "verse_number": 14
    },
    {
      "id": "305d266a-68a2-4927-900c-d77c9380ff10",
      "m1": "نبد بر هوا مرغ را جایگاه",
      "m2": "ز تیر و ز گرز و ز گرد سپاه",
      "verse_number": 15
    },
    {
      "id": "56519e36-6f07-400b-bcc0-f4ef41fb1d1a",
      "m1": "درفشیدن تیغ الماس گون",
      "m2": "بکردار آتش بگرد اندرون",
      "verse_number": 16
    },
    {
      "id": "9ae5f5b3-da04-4f8c-93b4-2325fba8aba6",
      "m1": "تو گفتی زمین روی زنگی شدست",
      "m2": "ستاره دل پیل جنگی شدست",
      "verse_number": 17
    },
    {
      "id": "f95fc09b-c39d-42dd-b39f-4b46bf710638",
      "m1": "ز بس نیزه و گرز و شمشیر تیز",
      "m2": "برآمد همی از جهان رستخیز",
      "verse_number": 18
    },
    {
      "id": "6ca15925-7bb3-4064-a417-ca27d48a9028",
      "m1": "ز قلب سپه گیو شد پیش صف",
      "m2": "خروشان و بر لب برآورده کف",
      "verse_number": 19
    },
    {
      "id": "ce51f533-a325-4dba-82a7-9d3caa1df1d5",
      "m1": "ابا نامداران گودرزیان",
      "m2": "کزیشان بدی راه سود و زیان",
      "verse_number": 20
    },
    {
      "id": "6d788558-53f4-4c67-a114-8bd3bfa95f7f",
      "m1": "بتیغ و بنیزه برآویختند",
      "m2": "همی ز آهن آتش فرو ریختند",
      "verse_number": 21
    },
    {
      "id": "b34bfc59-8d20-48b5-b8d7-bece97d86703",
      "m1": "چو شد رزم گودرز و پیران درشت",
      "m2": "چو نهصد تن از تخم پیران بکشت",
      "verse_number": 22
    },
    {
      "id": "9797ace8-c139-4d92-a23d-94bdbbab89d7",
      "m1": "چو دیدند لهاک و فرشیدورد",
      "m2": "کزان لشکر گشن برخاست گرد",
      "verse_number": 23
    },
    {
      "id": "9a98d032-22ee-484a-81ab-ad666fcdf1cb",
      "m1": "یکی حمله بردند برسوی گیو",
      "m2": "بران گرزداران و شیران نیو",
      "verse_number": 24
    },
    {
      "id": "778d6537-3d61-440b-aa5b-4a96f4899c03",
      "m1": "ببارید تیر از کمان سران",
      "m2": "بران نامداران جوشنوران",
      "verse_number": 25
    },
    {
      "id": "fe1c96af-ade0-41f1-b8dd-aac7e3aace06",
      "m1": "چنان شد که کس روی کشور ندید",
      "m2": "ز بس کشتگان شد زمین ناپدید",
      "verse_number": 26
    },
    {
      "id": "d1f57880-2524-460b-b7cf-69de8da32af8",
      "m1": "یکی پشت بر دیگری برنگاشت",
      "m2": "نه بگذاشت آن جایگه را که داشت",
      "verse_number": 27
    },
    {
      "id": "c46ed817-ff36-4ceb-b340-600fa0d31bc7",
      "m1": "چنین گفت هومان به فرشیدورد",
      "m2": "که با قلبگه جست باید نبرد",
      "verse_number": 28
    },
    {
      "id": "c4be6abd-0edc-496f-9d60-eba2dced20e0",
      "m1": "فریبرز باید کزان قلبگاه",
      "m2": "گریزان بیاید ز پشت سپاه",
      "verse_number": 29
    },
    {
      "id": "ec0bed08-f6ea-4153-887c-3912cfecf5a8",
      "m1": "پس آسان بود جنگ با میمنه",
      "m2": "بچنگ آید آن رزمگاه و بنه",
      "verse_number": 30
    },
    {
      "id": "77fc0893-dcfc-41e2-91ca-15b19379bd4b",
      "m1": "برفتند پس تا بقلب سپاه",
      "m2": "بجنگ فریبرز کاوس شاه",
      "verse_number": 31
    },
    {
      "id": "0eeecfa8-0e5f-46b2-823d-cb3685eb5251",
      "m1": "ز هومان گریزان بشد پهلوان",
      "m2": "شکست اندر آمد برزم گوان",
      "verse_number": 32
    },
    {
      "id": "cd714eae-a88e-445f-a2a0-e504b81c31d9",
      "m1": "بدادند گردنکشان جای خویش",
      "m2": "نبودند گستاخ با رای خویش",
      "verse_number": 33
    },
    {
      "id": "203881a0-504b-4b1f-96ce-f3c7beda6e54",
      "m1": "یکایک بدشمن سپردند جای",
      "m2": "ز گردان ایران نبد کس بپای",
      "verse_number": 34
    },
    {
      "id": "cf285414-6c07-4be4-b268-260b3380b3ae",
      "m1": "بماندند بر جای کوس و درفش",
      "m2": "ز پیکارشان دیدهها شد بنفش",
      "verse_number": 35
    },
    {
      "id": "02d8e93a-28a5-4914-baba-7ee0090271a3",
      "m1": "دلیران بدشمن نمودند پشت",
      "m2": "ازان کارزار انده آمد بمشت",
      "verse_number": 36
    },
    {
      "id": "13eb6e74-6b97-4634-92d4-6b4f781d5dd3",
      "m1": "نگون گشته کوس و درفش و سنان",
      "m2": "نبود ایچ پیدا رکیب از عنان",
      "verse_number": 37
    },
    {
      "id": "7959affe-86cd-42f8-bdef-2f2a19c68dd6",
      "m1": "چو دشمن ز هر سو بانبوه شد",
      "m2": "فریبرز بر دامن کوه شد",
      "verse_number": 38
    },
    {
      "id": "004646b1-3965-4c9f-bdd2-e9e547533eac",
      "m1": "برفتند ز ایرانیان هرک زیست",
      "m2": "بران زندگانی بباید گریست",
      "verse_number": 39
    },
    {
      "id": "4bfef582-609a-4c6e-82f8-b5098a75d804",
      "m1": "همی بود بر جای گودرز و گیو",
      "m2": "ز لشکر بسی نامبردار نیو",
      "verse_number": 40
    },
    {
      "id": "3850a2b1-3bb3-4089-872f-b969b55bc205",
      "m1": "چو گودرز کشواد بر قلبگاه",
      "m2": "درفش فریبرز کاوس شاه",
      "verse_number": 41
    },
    {
      "id": "c26dabdf-2fe0-4f9e-b0a0-6aa7bb734e6c",
      "m1": "ندید و یلان سپه را ندید",
      "m2": "بکردار آتش دلش بردمید",
      "verse_number": 42
    },
    {
      "id": "0da9a7be-41d5-413e-a4b3-d0f6a91fb0b4",
      "m1": "عنان کرد پیچان براه گریز",
      "m2": "برآمد ز گودرزیان رستخیز",
      "verse_number": 43
    },
    {
      "id": "5879f973-51d0-4079-8aab-72b3fedba8e6",
      "m1": "بدو گفت گیو ای سپهدار پیر",
      "m2": "بسی دیدهای گرز و گوپال و تیر",
      "verse_number": 44
    },
    {
      "id": "2ef61333-d8e8-4c23-8fc2-dc17a6adc2f3",
      "m1": "اگر تو ز پیران بخواهی گریخت",
      "m2": "بباید بسر بر مرا خاک ریخت",
      "verse_number": 45
    },
    {
      "id": "bb24c20e-16ea-4e09-8928-f79e08c0fe5e",
      "m1": "نماند کسی زنده اندر جهان",
      "m2": "دلیران و کارآزموده مهان",
      "verse_number": 46
    },
    {
      "id": "cde659f8-1e63-4ce3-802a-4ba5e9e21386",
      "m1": "ز مردن مرا و ترا چاره نیست",
      "m2": "درنگی تر از مرگ پتیاره نیست",
      "verse_number": 47
    },
    {
      "id": "e55b17d8-df69-4858-b3a5-c2831b452821",
      "m1": "چو پیش آمد این روزگار درشت",
      "m2": "ترا روی بینند بهتر که پشت",
      "verse_number": 48
    },
    {
      "id": "ca98b7af-efd6-4bc9-a1c4-7097c84f933b",
      "m1": "بپیچیم زین جایگه سوی جنگ",
      "m2": "نیاریم بر خاک کشواد ننگ",
      "verse_number": 49
    },
    {
      "id": "c343ed08-8b16-4ea3-b201-397ba8469de0",
      "m1": "ز دانا تو نشنیدی آن داستان",
      "m2": "که برگوید از گفتهٔ باستان",
      "verse_number": 50
    },
    {
      "id": "61c68bbb-09be-4fdc-857f-96869a3eaec3",
      "m1": "که گر دو برادر نهد پشت پشت",
      "m2": "تن کوه را سنگ ماند بمشت",
      "verse_number": 51
    },
    {
      "id": "99c4df68-fd02-43b0-afbc-1bf12fce7c06",
      "m1": "تو باشی و هفتاد جنگی پسر",
      "m2": "ز دوده ستوده بسی نامور",
      "verse_number": 52
    },
    {
      "id": "ff79e49e-c7c3-4ee0-8cb3-f50fdffcb65f",
      "m1": "بخنجر دل دشمنان بشکنیم",
      "m2": "وگر کوه باشد ز بن برکنیم",
      "verse_number": 53
    },
    {
      "id": "497c2e3a-8466-4fa5-9679-eabf224859bb",
      "m1": "چو گودرز بشنید گفتار گیو",
      "m2": "بدید آن سر و ترگ بیدار نیو",
      "verse_number": 54
    },
    {
      "id": "3534e30e-c46a-49d4-8f1f-c6221a4fd8ba",
      "m1": "پشیمان شد از دانش و رای خویش",
      "m2": "بیفشارد بر جایگه پای خویش",
      "verse_number": 55
    },
    {
      "id": "63301cae-2291-49ea-af85-1fe2c13672eb",
      "m1": "گرازه برون آمد و گستهم",
      "m2": "ابا برته و زنگهٔ یل بهم",
      "verse_number": 56
    },
    {
      "id": "e37ddd9d-4612-4f61-8a6a-15a9b5781b57",
      "m1": "بخوردند سوگندهای گران",
      "m2": "که پیمان شکستن نبود اندران",
      "verse_number": 57
    },
    {
      "id": "1f5d00f0-fb5b-470d-ad3a-3dc98e19f75c",
      "m1": "کزین رزمگه برنتابیم روی",
      "m2": "گر از گرز خون اندر آید بجوی",
      "verse_number": 58
    },
    {
      "id": "6ea68038-05a3-483d-9729-502aea867963",
      "m1": "وزان جایگه ران بیفشاردند",
      "m2": "برزم اندرون گرز بگذاردند",
      "verse_number": 59
    },
    {
      "id": "401402c3-820b-4c27-bddc-9499c8cdad6e",
      "m1": "ز هر سو سپه بیکران کشته شد",
      "m2": "زمانه همی بر بدی گشته شد",
      "verse_number": 60
    },
    {
      "id": "07b5fbae-ad75-4423-abf0-f71a6485981b",
      "m1": "به بیژن چنین گفت گودرز پیر",
      "m2": "کز ایدر برو زود برسان تیر",
      "verse_number": 61
    },
    {
      "id": "324f5213-f01b-4104-940d-847fdd5293c6",
      "m1": "بسوی فریبرز برکش عنان",
      "m2": "بپیش من آر اختر کاویان",
      "verse_number": 62
    },
    {
      "id": "f8d9c14d-9c0c-49d4-a71a-ddd4b74b17b0",
      "m1": "مگر خود فریبرز با آن درفش",
      "m2": "بیاید کند روی دشمن بنفش",
      "verse_number": 63
    },
    {
      "id": "1a8860d5-08f2-4420-b995-f47426a3ee75",
      "m1": "چو بشنید بیژن برانگیخت اسپ",
      "m2": "بیامد بکردار آذرگشسپ",
      "verse_number": 64
    },
    {
      "id": "b652fee3-38a8-4f38-8334-c7668ba5bf3e",
      "m1": "بنزد فریبرز و با او بگفت",
      "m2": "که ایدر چه داری سپه در نهفت",
      "verse_number": 65
    },
    {
      "id": "92954f2e-9f93-46bd-abcc-a8f9c95913e7",
      "m1": "عنان را چو گردان یکی برگرای",
      "m2": "برین کوه سر بر فزون زین مپای",
      "verse_number": 66
    },
    {
      "id": "6d70b22c-45b4-47cb-8d52-24d8ffabbad6",
      "m1": "اگر تو نیایی مرا ده درفش",
      "m2": "سواران و این تیغهای بنفش",
      "verse_number": 67
    },
    {
      "id": "8e103a59-b38f-4d15-8074-ecb7474d7277",
      "m1": "چو بیژن سخن با فریبرز گفت",
      "m2": "نکرد او خرد با دل خویش جفت",
      "verse_number": 68
    },
    {
      "id": "b6f99017-edb7-4db2-9e44-34322084201c",
      "m1": "یکی بانگ برزد به بیژن که رو",
      "m2": "که در کار تندی و در جنگ نو",
      "verse_number": 69
    },
    {
      "id": "b80606b7-d11f-4768-a808-fe59262174ff",
      "m1": "مرا شاه داد این درفش و سپاه",
      "m2": "همین پهلوانی و تخت و کلاه",
      "verse_number": 70
    },
    {
      "id": "cf58fb6e-1602-457c-8b49-78be75a76c29",
      "m1": "درفش از در بیژن گیو نیست",
      "m2": "نه اندر جهان سربسر نیو نیست",
      "verse_number": 71
    },
    {
      "id": "4ae56c65-52d7-4345-9993-13c751da91e8",
      "m1": "یکی تیغ بگرفت بیژن بنفش",
      "m2": "بزد ناگهان بر میان درفش",
      "verse_number": 72
    },
    {
      "id": "3ce26fa0-d0bd-4e4e-bb8e-3ec6c0498dfc",
      "m1": "بدو نیمه کرد اختر کاویان",
      "m2": "یکی نیمه برداشت گرد از میان",
      "verse_number": 73
    },
    {
      "id": "584e1ead-99ab-4747-a941-6a3f8a3d9bc7",
      "m1": "بیامد که آرد بنزد سپاه",
      "m2": "چو ترکان بدیدند اختر براه",
      "verse_number": 74
    },
    {
      "id": "d396869d-a0c5-478c-bfbc-7fb845269c11",
      "m1": "یکی شیردل لشکری جنگجوی",
      "m2": "همه سوی بیژن نهادند روی",
      "verse_number": 75
    },
    {
      "id": "537e3d2e-6d62-431f-a641-8d240b2b7a62",
      "m1": "کشیدند گوپال و تیغ بنفش",
      "m2": "به پیکار آن کاویانی درفش",
      "verse_number": 76
    },
    {
      "id": "bb4d088a-5f02-4047-988f-a876e490a718",
      "m1": "چنین گفت هومان که آن اخترست",
      "m2": "که نیروی ایران بدو اندر است",
      "verse_number": 77
    },
    {
      "id": "48407b07-e779-4ccd-82fe-fd4dd05ad221",
      "m1": "درفش بنفش ار بچنگ آوریم",
      "m2": "جهان جمله بر شاه تنگ آوریم",
      "verse_number": 78
    },
    {
      "id": "b616115b-d797-4925-9ee4-dfdc8cca728f",
      "m1": "کمان را بزه کرد بیژن چو گرد",
      "m2": "بریشان یکی تیرباران بکرد",
      "verse_number": 79
    },
    {
      "id": "f8f814b5-7f0e-4a8f-8ddc-bce72e733647",
      "m1": "سپه یکسر از تیر او دور شد",
      "m2": "همی گرگ درنده را سور شد",
      "verse_number": 80
    },
    {
      "id": "ba4b8f89-1202-428c-95ae-f525ca50a6a1",
      "m1": "بگفتند با گیو و با گستهم",
      "m2": "سواران که بودند با او بهم",
      "verse_number": 81
    },
    {
      "id": "82fd37eb-8038-44d0-9edf-532d0268a673",
      "m1": "که مان رفت باید بتوران سپاه",
      "m2": "ربودن ازیشان همی تاج و گاه",
      "verse_number": 82
    },
    {
      "id": "a25284c0-115a-4600-a894-aee2f97a3510",
      "m1": "ز گردان ایران دلاور سران",
      "m2": "برفتند بسیار نیزهوران",
      "verse_number": 83
    },
    {
      "id": "2dc94790-17ef-41ed-bec9-e8682bac139c",
      "m1": "بکشتند زیشان فراوان سوار",
      "m2": "بیامد ز ره بیژن نامدار",
      "verse_number": 84
    },
    {
      "id": "07b91271-e371-404a-92c8-cb71edca6d6f",
      "m1": "سپاه اندر آمد بگرد درفش",
      "m2": "هوا شد ز گرد سواران بنفش",
      "verse_number": 85
    },
    {
      "id": "07692d52-e4e7-4bb7-9b8d-2aea275b3459",
      "m1": "دگر باره از جای برخاستند",
      "m2": "بران دشت رزمی نو آراستند",
      "verse_number": 86
    },
    {
      "id": "7a315014-0791-4273-9174-459e5726fcaf",
      "m1": "به پیش سپه کشته شد ریونیز",
      "m2": "که کاوس را بد چو جان عزیز",
      "verse_number": 87
    },
    {
      "id": "b639d024-cb23-4f59-bcf5-02b4ca8bd33b",
      "m1": "یکی تاجور شاه کهتر پسر",
      "m2": "نیاز فریبرز و جان پدر",
      "verse_number": 88
    },
    {
      "id": "395d7a8f-e7df-4b89-9394-136fba05dcee",
      "m1": "سر و تاج او اندر آمد بخاک",
      "m2": "بسی نامور جامه کردند چاک",
      "verse_number": 89
    },
    {
      "id": "1d3242a5-3310-4001-9670-6a142e355e17",
      "m1": "ازان پس خروشی برآورد گیو",
      "m2": "که ای نامداران و گردان نیو",
      "verse_number": 90
    },
    {
      "id": "6aaaed32-d983-4501-82d8-14be1304b74d",
      "m1": "چنویی نبود اندرین رزمگاه",
      "m2": "جوان و سرافراز و فرزند شاه",
      "verse_number": 91
    },
    {
      "id": "b5af24ef-45f5-41be-8eb7-1361d1d5991c",
      "m1": "نبیره جهاندار کاوس پیر",
      "m2": "سه تن کشته شد زار بر خیره خیر",
      "verse_number": 92
    },
    {
      "id": "a7c8a4d4-cfdd-45e5-8c81-b14ff2717e8a",
      "m1": "فرود سیاوش چون ریونیز",
      "m2": "بگیتی فزون زین شگفتی چه چیز",
      "verse_number": 93
    },
    {
      "id": "dafd1712-306e-4249-bb42-6c8d56a73110",
      "m1": "اگر تاج آن نارسیده جوان",
      "m2": "بدشمن رسد شرم دارد روان",
      "verse_number": 94
    },
    {
      "id": "99537951-cfc8-4d97-bf08-889a14c1a68f",
      "m1": "اگر من بجنبم ازین رزمگاه",
      "m2": "شکست اندر آید بایران سپاه",
      "verse_number": 95
    },
    {
      "id": "2861e526-e626-4b45-856a-6f2f765f4566",
      "m1": "نباید که آن افسر شهریار",
      "m2": "بترکان رسد در صف کارزار",
      "verse_number": 96
    },
    {
      "id": "4afa23d3-2c5f-40e5-8e02-375988569d5a",
      "m1": "فزاید بر این ننگها ننگ نیز",
      "m2": "ازین افسر و کشتن ریو نیز",
      "verse_number": 97
    },
    {
      "id": "a6b258e7-0249-4a34-baad-4de17383f03f",
      "m1": "چنان بد که بشنید آواز گیو",
      "m2": "سپهبد سرافراز پیران نیو",
      "verse_number": 98
    },
    {
      "id": "0f73294e-ced6-4257-ad0a-a92e6c64fc6f",
      "m1": "برامد بنوی یکی کارزار",
      "m2": "ز لشکر بران افسر نامدار",
      "verse_number": 99
    },
    {
      "id": "6fa1278c-4c7e-4295-8ebb-87026b6b9e6f",
      "m1": "فراوان ز هر سو سپه کشته شد",
      "m2": "سربخت گردنکشان گشته شد",
      "verse_number": 100
    },
    {
      "id": "61931a42-7bf6-498c-9de9-7a8f2ccd7866",
      "m1": "برآویخت چون شیر بهرام گرد",
      "m2": "بنیزه بریشان یکی حمله برد",
      "verse_number": 101
    },
    {
      "id": "d1476d3a-9b32-4cce-a3c4-eb2655eaac04",
      "m1": "بنوک سنان تاج را برگرفت",
      "m2": "دو لشکر بدو مانده اندر شگفت",
      "verse_number": 102
    },
    {
      "id": "3f1f8c90-113e-4210-ba20-4457b1eaaff3",
      "m1": "همی بود زان گونه تا تیره گشت",
      "m2": "همی دیده از تیرگی خیره گشت",
      "verse_number": 103
    },
    {
      "id": "ddd2c939-2565-4c2c-bcf8-13c10307f44f",
      "m1": "چنین هر زمانی برآشوفتند",
      "m2": "همی بر سر یکدگر کوفتند",
      "verse_number": 104
    },
    {
      "id": "3823c2cf-8d1c-4ac0-bbd6-d83920f37cfc",
      "m1": "ز گودرزیان هشت تن زنده بود",
      "m2": "بران رزمگه دیگر افگنده بود",
      "verse_number": 105
    },
    {
      "id": "cfd697b2-ab30-4288-8e18-92cb53c11a25",
      "m1": "هم از تخمهٔ گیو چون بیست و پنج",
      "m2": "که بودند زیبای دیهیم و گنج",
      "verse_number": 106
    },
    {
      "id": "f044af6f-0c75-44c4-bdb4-65b80f4760d4",
      "m1": "هم از تخم کاوس هفتاد مرد",
      "m2": "سواران و شیران روز نبرد",
      "verse_number": 107
    },
    {
      "id": "c343450b-30e6-4670-83cb-f2172991090d",
      "m1": "جز از ریونیز آن سر تاجدار",
      "m2": "سزد گر نیاید کسی در شمار",
      "verse_number": 108
    },
    {
      "id": "989cec87-3ba0-4f51-9e92-a3090f358d7f",
      "m1": "چو سیصد تن از تخم افراسیاب",
      "m2": "کجا بختشان اندر آمد بخواب",
      "verse_number": 109
    },
    {
      "id": "b98e4b44-5abf-47da-97f3-8e8f2ed20a04",
      "m1": "ز خویشان پیران چو نهصد سوار",
      "m2": "کم آمد برین روز در کارزار",
      "verse_number": 110
    },
    {
      "id": "6c96a174-7513-4040-ae18-35da6ffe3b07",
      "m1": "همان دست پیران بد و روز اوی",
      "m2": "ازان اختر گیتیافروز اوی",
      "verse_number": 111
    },
    {
      "id": "4b8e12b5-b1f5-4a32-a93b-c9efd9577b81",
      "m1": "نبد روز پیکار ایرانیان",
      "m2": "ازان جنگ جستن سرآمد زمان",
      "verse_number": 112
    },
    {
      "id": "f2335c55-a935-47cc-bb0f-8d768cd0f486",
      "m1": "از آوردگه روی برگاشتند",
      "m2": "همی خستگان خوار بگذاشتند",
      "verse_number": 113
    },
    {
      "id": "c8b9ab8f-565b-4354-8529-9dcc43e7a174",
      "m1": "بدانگه کجا بخت برگشته بود",
      "m2": "دمان بارهٔ گستهم کشته بود",
      "verse_number": 114
    },
    {
      "id": "c57a604a-0d95-498c-94c5-1d86243aba59",
      "m1": "پیاده همی رفت نیزه بدست",
      "m2": "ابا جوشن و خود برسان مست",
      "verse_number": 115
    },
    {
      "id": "0c23a89b-01b9-4523-8e73-40bc3915ed79",
      "m1": "چو بیژن بگستهم نزدیک شد",
      "m2": "شب آمد همی روز تاریک شد",
      "verse_number": 116
    },
    {
      "id": "30f2849a-d31c-4dae-8cb1-fa0bbe2d6b57",
      "m1": "بدو گفت هین برنشین از پسم",
      "m2": "گرامیتر از تو نباشد کسم",
      "verse_number": 117
    },
    {
      "id": "29014757-d45a-47b4-a41b-c9c0031549d3",
      "m1": "نشستند هر دو بران بارگی",
      "m2": "چو خورشید شد تیره یکبارگی",
      "verse_number": 118
    },
    {
      "id": "613ef6b9-26ae-4963-b18a-9fa01a8419ce",
      "m1": "همه سوی آن دامن کوهسار",
      "m2": "گریزان برفتند برگشته کار",
      "verse_number": 119
    },
    {
      "id": "6fe3138a-9a0d-4dc3-9267-a61ab81d45bc",
      "m1": "سواران ترکان همه شاددل",
      "m2": "ز رنج و ز غم گشته آزاددل",
      "verse_number": 120
    },
    {
      "id": "21682233-3a51-4a6d-ad31-51b42e2586de",
      "m1": "بلشکرگه خویش بازآمدند",
      "m2": "گرازنده و بزم ساز آمدند",
      "verse_number": 121
    },
    {
      "id": "33b7ee02-c99f-42f0-b983-d057ce49d680",
      "m1": "ز گردان ایران برآمد خروش",
      "m2": "همی کر شد از نالهٔ کوس گوش",
      "verse_number": 122
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 122
  },
