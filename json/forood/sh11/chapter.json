{
  "id": "7299ac53-4798-40a9-bd34-9f93f51e0f9d",
  "title": "بخش ۱۱",
  "verses": [
    {
      "id": "46e78545-3461-4634-a121-59d6a9376392",
      "m1": "فرود جوان را دژ آباد بود",
      "m2": "بدژ درپرستنده هفتاد بود",
      "verse_number": 1
    },
    {
      "id": "fb58c2cb-94b9-4aac-a00e-53a983508a0b",
      "m1": "همه ماهرویان بباره بدند",
      "m2": "چو دیبای چینی نظاره بدند",
      "verse_number": 2
    },
    {
      "id": "661dc696-8215-4b86-bfb2-5d69398e877d",
      "m1": "ازان بازگشتن فرود جوان",
      "m2": "ازیشان همی بود تیرهروان",
      "verse_number": 3
    },
    {
      "id": "75298ec8-493f-42da-b07f-9cc5fb41869c",
      "m1": "چنین گفت با شاهزاده تخوار",
      "m2": "که گر جست خواهی همی کارزار",
      "verse_number": 4
    },
    {
      "id": "ebcff2a8-0325-495c-8895-28b8d4ee2850",
      "m1": "نگر نامور طوس را نشکنی",
      "m2": "ترا آن به آید که اسپ افگنی",
      "verse_number": 5
    },
    {
      "id": "4bda6c67-b8cd-40b2-bfef-bf7f0d6a27eb",
      "m1": "و دیگر که باشد مر او را زمان",
      "m2": "نیاید به یک چوبه تیر از کمان",
      "verse_number": 6
    },
    {
      "id": "228fed68-c719-4284-8633-a2af589b6d6e",
      "m1": "چو آمد سپهبد بر این تیغ کوه",
      "m2": "بیاید کنون لشکرش همگروه",
      "verse_number": 7
    },
    {
      "id": "cf12cd65-fbe2-4426-a80f-94459e55b502",
      "m1": "ترا نیست در جنگ پایاب اوی",
      "m2": "ندیدی بروهای پرتاب اوی",
      "verse_number": 8
    },
    {
      "id": "74e9b361-6d5c-4299-a47f-95e52b56adba",
      "m1": "فرود از تخوار این سخنها شنید",
      "m2": "کمان را بزه کرد و اندر کشید",
      "verse_number": 9
    },
    {
      "id": "e8f35a5b-689c-4524-a0de-6013ea2123e5",
      "m1": "خدنگی بر اسپ سپهبد بزد",
      "m2": "چنان کز کمان سواران سزد",
      "verse_number": 10
    },
    {
      "id": "a8d39401-b5e9-412f-9ca6-119b8b4afb48",
      "m1": "نگون شد سر تازی و جان بداد",
      "m2": "دل طوس پرکین و سر پر ز باد",
      "verse_number": 11
    },
    {
      "id": "52b2bcd9-98ac-4020-addf-57b5d681e953",
      "m1": "بلشکر گه آمد بگردن سپر",
      "m2": "پیاده پر از گرد و آسیمه سر",
      "verse_number": 12
    },
    {
      "id": "cd61c18d-d005-475d-a1ff-7b9a24b3f3da",
      "m1": "گواژه همی زد پس او فرود",
      "m2": "که این نامور پهلوان را چه بود",
      "verse_number": 13
    },
    {
      "id": "feb6fe24-6462-4bc3-a01b-a3ef8698c2f1",
      "m1": "که ایدون ستوه آمد از یک سوار",
      "m2": "چگونه چمد در صف کارزار",
      "verse_number": 14
    },
    {
      "id": "cbdabc73-f2e7-4914-8877-9eab1a4df808",
      "m1": "پرستندگان خنده برداشتند",
      "m2": "همی از چرم نعره برداشتند",
      "verse_number": 15
    },
    {
      "id": "18b7ae77-6bfc-4f1a-8c70-19de64aa42f2",
      "m1": "که پیش جوانی یکی مرد پیر",
      "m2": "ز افراز غلتان شد از بیم تیر",
      "verse_number": 16
    },
    {
      "id": "bcc74436-c3d4-4084-80ca-03f104967b9c",
      "m1": "سپهبد فرود آمد از کوه سر",
      "m2": "برفتند گردان پر اندوه سر",
      "verse_number": 17
    },
    {
      "id": "caa96230-62ab-40dc-babe-f5af024dc3c0",
      "m1": "که اکنون تو بازآمدی تندرست",
      "m2": "بآب مژه رخ نبایست شست",
      "verse_number": 18
    },
    {
      "id": "4f01d272-2d1f-4644-8572-7e49e050e57f",
      "m1": "بپیچید زان کار پرمایه گیو",
      "m2": "که آمد پیاده سپهدار نیو",
      "verse_number": 19
    },
    {
      "id": "0a440958-86ef-4bae-bbac-2984924749ec",
      "m1": "چنین گفت کین را خود اندازه نیست",
      "m2": "رخ نامداران برین تازه نیست",
      "verse_number": 20
    },
    {
      "id": "0923e1c0-481d-43d1-a4a9-23373f654a30",
      "m1": "اگر شهریارست با گوشوار",
      "m2": "چه گیرد چنین لشکر گشن خوار",
      "verse_number": 21
    },
    {
      "id": "eda5ada0-b202-4501-8f5d-635a9729de4c",
      "m1": "نباید که باشیم همداستان",
      "m2": "به هر گونهای کو زند داستان",
      "verse_number": 22
    },
    {
      "id": "41822648-8c55-47ca-8748-1037ce512225",
      "m1": "اگر طوس یک بار تندی نمود",
      "m2": "زمانه پرآزار گشت از فرود",
      "verse_number": 23
    },
    {
      "id": "8c67604d-4ea8-40e4-acb5-17f2f2ab1be8",
      "m1": "همه جان فدای سیاوش کنیم",
      "m2": "نباید که این بد فرامش کنیم",
      "verse_number": 24
    },
    {
      "id": "17080384-0777-473e-958f-44611f468f91",
      "m1": "زرسپ گرانمایه زو شد بباد",
      "m2": "سواری سرافراز نوذرنژاد",
      "verse_number": 25
    },
    {
      "id": "8cf07299-2d0f-4091-8e03-c772a794ad1d",
      "m1": "بخونست غرقه تن ریونیز",
      "m2": "ازین بیش خواری چه بینیم نیز",
      "verse_number": 26
    },
    {
      "id": "a1c2ab12-ae2b-4c9c-81aa-bf6cebd4e53d",
      "m1": "گرو پور جمست و مغز قباد",
      "m2": "بنادانی این جنگ را برگشاد",
      "verse_number": 27
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 27
  },
  "order_in_parent": 11
}