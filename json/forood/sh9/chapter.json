{
  "id": "86107ae7-dcce-4b35-917a-06b1a7965665",
  "title": "بخش ۹",
  "verses": [
    {
      "id": "ee7425d1-9f66-4943-b90a-bdfadb23c0a3",
      "m1": "چنین گفت پس پهلوان با زرسپ",
      "m2": "که بفروز دل را چو آذرگشسپ",
      "verse_number": 1
    },
    {
      "id": "4a5e708e-e804-4a4e-93dd-a6405c0d9351",
      "m1": "سلیح سواران جنگی بپوش",
      "m2": "بجان و تن خویشتن دار گوش",
      "verse_number": 2
    },
    {
      "id": "005f0c64-34d5-4582-9e21-9644450fdc33",
      "m1": "تو خواهی مگر کین آن نامدار",
      "m2": "وگرنه نبینم کسی خواستار",
      "verse_number": 3
    },
    {
      "id": "23d9258d-6efc-4b55-84d7-5a0c31a3be0b",
      "m1": "زرسپ آمد و ترگ بر سر نهاد",
      "m2": "دلی پر ز کین و لبی پر ز باد",
      "verse_number": 4
    },
    {
      "id": "7656a23f-9fa6-4035-bb5f-e2504a722c6f",
      "m1": "خروشان باسپ اندر آورد پای",
      "m2": "بکردار آتش درآمد ز جای",
      "verse_number": 5
    },
    {
      "id": "9bb900fa-1468-4b18-a23e-22ec594301c2",
      "m1": "چنین گفت شیر ژیان با تخوار",
      "m2": "که آمد دگرگون یکی نامدار",
      "verse_number": 6
    },
    {
      "id": "1f102e6f-fffb-4e7c-b960-be7b17157a07",
      "m1": "ببین تا شناسی که این مرد کیست",
      "m2": "یکی شهریار است اگر لشکریست",
      "verse_number": 7
    },
    {
      "id": "da7f9925-0f60-42b0-97d2-c0781941eac7",
      "m1": "چنین گفت با شاه جنگی تخوار",
      "m2": "که آمد گه گردش روزگار",
      "verse_number": 8
    },
    {
      "id": "54982c24-b137-4fe3-8fd3-2092cdd264c4",
      "m1": "که این پور طوسست نامش زرسپ",
      "m2": "که از پیل جنگی نگرداند اسپ",
      "verse_number": 9
    },
    {
      "id": "d1d1edad-28d6-4b6e-bdee-4e59fcad112e",
      "m1": "که جفتست با خواهر ریونیز",
      "m2": "بکین آمدست این جهانجوی نیز",
      "verse_number": 10
    },
    {
      "id": "db2a1e93-7b05-4331-986e-66816ff7cd1f",
      "m1": "چو بیند بر و بازوی و مغفرت",
      "m2": "خدنگی بباید گشاد از برت",
      "verse_number": 11
    },
    {
      "id": "40374865-8db9-4cd6-b3da-e5eee609f9c2",
      "m1": "بدان تا بخاک اندر آید سرش",
      "m2": "نگون اندر آید ز باره برش",
      "verse_number": 12
    },
    {
      "id": "b9fc5ae4-77ce-482b-983f-ca6cffee342a",
      "m1": "بداند سپهدار دیوانه طوس",
      "m2": "که ایدر نبودیم ما بر فسوس",
      "verse_number": 13
    },
    {
      "id": "e6b1be94-73e5-4f57-8240-7afedd4834db",
      "m1": "فرود دلاور برانگیخت اسپ",
      "m2": "یکی تیر زد بر میان زرسپ",
      "verse_number": 14
    },
    {
      "id": "9204f97d-e402-415d-a61d-1723829194f1",
      "m1": "که با کوههٔ زین تنش را بدوخت",
      "m2": "روانش ز پیکان او برفروخت",
      "verse_number": 15
    },
    {
      "id": "0bb548c4-e9e2-4c22-bcb0-ce943b344ed7",
      "m1": "بیفتاد و برگشت ازو بادپای",
      "m2": "همی شد دمان و دنان باز جای",
      "verse_number": 16
    },
    {
      "id": "3c0fe4f9-259a-4ea9-bead-60c735de33d6",
      "m1": "خروشی برآمد ز ایران سپاه",
      "m2": "زسر برگرفتند گردان کلاه",
      "verse_number": 17
    },
    {
      "id": "ca885345-88a4-4484-b3a8-037160ffef74",
      "m1": "دل طوس پرخون و دیده پراب",
      "m2": "بپوشید جوشن هم اندر شتاب",
      "verse_number": 18
    },
    {
      "id": "d093179a-fd52-415c-9ed7-3f7d09c411e2",
      "m1": "ز گردان جنگی بنالید سخت",
      "m2": "بلرزید برسان برگ درخت",
      "verse_number": 19
    },
    {
      "id": "66aa05a7-dc24-4248-847f-1653a9d5c7a4",
      "m1": "نشست از بر زین چو کوهی بزرگ",
      "m2": "که بنهند بر پشت پیلی سترگ",
      "verse_number": 20
    },
    {
      "id": "7f4e5689-0c94-4b93-97d1-5759f5410386",
      "m1": "عنان را بپیچید سوی فرود",
      "m2": "دلش پر ز کین و سرش پر ز دود",
      "verse_number": 21
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 21
  },
