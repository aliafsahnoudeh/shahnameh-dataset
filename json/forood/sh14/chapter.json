{
  "id": "0480f373-ed7d-4d3d-ae2b-8c646c28d3c1",
  "title": "بخش ۱۴",
  "verses": [
    {
      "id": "7c630e68-d0bb-4879-85e4-7d509e53d574",
      "m1": "چو خورشید تابنده شد ناپدید",
      "m2": "شب تیره بر چرخ لشکر کشید",
      "verse_number": 1
    },
    {
      "id": "044a3117-aaf7-4773-8666-cca1d9051dd3",
      "m1": "دلیران دژدار مردی هزار",
      "m2": "ز سوی کلات اندر آمد سوار",
      "verse_number": 2
    },
    {
      "id": "f76e2e04-d981-405c-8cdd-540be8986592",
      "m1": "در دژ ببستند زین روی تنگ",
      "m2": "خروش جرس خاست و آوای زنگ",
      "verse_number": 3
    },
    {
      "id": "ed08189f-725a-4b9d-bc22-7020ecba30a0",
      "m1": "جریره بتخت گرامی بخفت",
      "m2": "شب تیره با درد و غم بود جفت",
      "verse_number": 4
    },
    {
      "id": "1afa0b9c-f0af-4729-99a2-a6782cd1a0de",
      "m1": "بخواب آتشی دید کز دژ بلند",
      "m2": "برافروختی پیش آن ارجمند",
      "verse_number": 5
    },
    {
      "id": "3cb0586c-a031-4e14-85c7-edac0a9a9919",
      "m1": "سراسر سپد کوه بفروختی",
      "m2": "پرستنده و دژ همی سوختی",
      "verse_number": 6
    },
    {
      "id": "53f96573-b6db-4ae8-bf2a-cd61d84752ce",
      "m1": "دلش گشت پر درد و بیدار گشت",
      "m2": "روانش پر از درد و تیمار گشت",
      "verse_number": 7
    },
    {
      "id": "f3163738-eaa2-47f3-9927-0b0f95e4da17",
      "m1": "بباره برآمد جهان بنگرید",
      "m2": "همه کوه پرجوشن و نیزه دید",
      "verse_number": 8
    },
    {
      "id": "1748a47e-43f5-455e-aa62-49caa869db36",
      "m1": "رخش گشت پرخون و دل پر ز دود",
      "m2": "بیامد به بالین فرخ فرود",
      "verse_number": 9
    },
    {
      "id": "daf818a4-04be-41f1-8e5e-4e4debcda253",
      "m1": "بدو گفت بیدار گرد ای پسر",
      "m2": "که ما را بد آمد ز اختر بسر",
      "verse_number": 10
    },
    {
      "id": "0476ea6f-f3c1-4c8e-99ca-3c8a88d416d3",
      "m1": "سراسر همه کوه پر دشمنست",
      "m2": "در دژ پر از نیزه و جوشنست",
      "verse_number": 11
    },
    {
      "id": "372de495-83d2-4568-98f4-7736c1251c33",
      "m1": "بمادر چنین گفت جنگی فرود",
      "m2": "که از غم چه داری دلت پر ز دود",
      "verse_number": 12
    },
    {
      "id": "21b52e49-2b32-4d37-ac95-4795a302be7d",
      "m1": "مرا گر زمانه شدست اسپری",
      "m2": "زمانه ز بخشش فزون نشمری",
      "verse_number": 13
    },
    {
      "id": "b769153c-8527-44f0-a010-1ee8d74e12e1",
      "m1": "بروز جوانی پدر کشته شد",
      "m2": "مرا روز چون روز او گشته شد",
      "verse_number": 14
    },
    {
      "id": "f9a1b691-bc65-4abd-83cf-2535a33794e4",
      "m1": "بدست گروی آمد او را زمان",
      "m2": "سوی جان من بیژن آمد دمان",
      "verse_number": 15
    },
    {
      "id": "d5646320-2dda-4cc3-91e9-f3c7f5c73c37",
      "m1": "بکوشم نمیرم مگر غرموار",
      "m2": "نخواهم ز ایرانیان زینهار",
      "verse_number": 16
    },
    {
      "id": "287ba379-5b7b-4401-8e46-eb40a44e4a5e",
      "m1": "سپه را همه ترگ و جوشن بداد",
      "m2": "یکی ترگ رومی بسر برنهاد",
      "verse_number": 17
    },
    {
      "id": "17c7dec6-f1c3-4cdb-a631-ec8959b9a4d4",
      "m1": "میانرا بخفتان رومی ببست",
      "m2": "بیامد کمان کیانی بدست",
      "verse_number": 18
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 18
  }
}