{
  "id": "baf7aae3-8757-481f-890c-c415e08faa27",
  "title": "بخش ۱۹",
  "verses": [
    {
      "id": "46264cce-ce40-4cc3-b8ee-651dbb4ba151",
      "m1": "گزین کرد زان لشکر نامدار",
      "m2": "سواران شمشیرزن سیهزار",
      "verse_number": 1
    },
    {
      "id": "aeb32cbf-4761-43c9-8ad8-3a106f84237f",
      "m1": "برفتند نیمی گذشته ز شب",
      "m2": "نه بانگ تبیره نه بوق و جلب",
      "verse_number": 2
    },
    {
      "id": "ba2b8576-27cf-4ee5-8df6-37aa1f09efe2",
      "m1": "چو پیران سالار لشکر براند",
      "m2": "میان یلان هفت فرسنگ ماند",
      "verse_number": 3
    },
    {
      "id": "36781523-4ea6-41d1-894e-66a9331e6415",
      "m1": "نخستین رسیدند پیش گله",
      "m2": "کجا بود بر دشت توران یله",
      "verse_number": 4
    },
    {
      "id": "0cdcb8b1-bb36-4aff-8b54-6ae9867df379",
      "m1": "گرفتند بسیار و کشتند نیز",
      "m2": "نبود از بد بخت مانند چیز",
      "verse_number": 5
    },
    {
      "id": "746ae491-aaa2-4200-8c61-2cfd408de02b",
      "m1": "گلهدار و چوپان بسی کشته شد",
      "m2": "سر بخت ایرانیان گشته شد",
      "verse_number": 6
    },
    {
      "id": "83afb44e-9b6c-4a4f-8f4f-fac0115412df",
      "m1": "وزان جایگه سوی ایران سپاه",
      "m2": "برفتند برسان گرد سیاه",
      "verse_number": 7
    },
    {
      "id": "233656ff-25a5-4882-8ed5-f67607cce806",
      "m1": "همه مست بودند ایرانیان",
      "m2": "گروهی نشسته گشاده میان",
      "verse_number": 8
    },
    {
      "id": "e068b91f-1d9f-413a-a912-bc94d7d05c59",
      "m1": "بخیمه درون گیو بیدار بود",
      "m2": "سپهدار گودرز هشیار بود",
      "verse_number": 9
    },
    {
      "id": "6bc5ee61-5c89-4818-8e7b-71dd059c068c",
      "m1": "خروش آمد و بانگ زخم تبر",
      "m2": "سراسیمه شد گیو پرخاشخر",
      "verse_number": 10
    },
    {
      "id": "9894acc6-3897-48b9-bcdc-a22122041295",
      "m1": "ستاده ابر پیش پردهسرای",
      "m2": "یکی اسپ برگستوانور به پای",
      "verse_number": 11
    },
    {
      "id": "7269ec48-4a09-413a-981f-49c4a0c9bbce",
      "m1": "برآشفت با خویشتن چون پلنگ",
      "m2": "ز بافیدن پای آمدش ننگ",
      "verse_number": 12
    },
    {
      "id": "8a2aa8d0-1de0-45df-a80b-f0def659ac24",
      "m1": "بیامد باسپ اندر آورد پای",
      "m2": "بکردار باد اندر آمد ز جای",
      "verse_number": 13
    },
    {
      "id": "485352bc-1ec3-4315-9a97-55bbe5e635b7",
      "m1": "بپردهسرای سپهبد رسید",
      "m2": "ز گرد سپه آسمان تیره دید",
      "verse_number": 14
    },
    {
      "id": "33d719db-11b2-4715-a9b1-7943d91f1526",
      "m1": "بدو گفت برخیز کامد سپاه",
      "m2": "یکی گرد برخاست ز اوردگاه",
      "verse_number": 15
    },
    {
      "id": "92d2d749-7dd3-43ba-b621-0f6f876fc659",
      "m1": "وزان جایگه رفت نزد پدر",
      "m2": "به چنگ اندرون گُرزهٔ گاو سر",
      "verse_number": 16
    },
    {
      "id": "0b97a4ac-ccc5-4fc8-a414-85288066c402",
      "m1": "همی گشت بر گرد لشکر چو دود",
      "m2": "برانگیخت آن را که هشیار بود",
      "verse_number": 17
    },
    {
      "id": "65ab427f-0f49-4c41-87c1-fc0b963f5976",
      "m1": "یکی جنگ با بیژن افگند پی",
      "m2": "که این دشت رزم است گر باغ می",
      "verse_number": 18
    },
    {
      "id": "824bc6e6-97d3-467c-9074-3234c01b37be",
      "m1": "وزان پس بیامد سوی کارزار",
      "m2": "بره برشتابید چندی سوار",
      "verse_number": 19
    },
    {
      "id": "bb6a5bc3-3aad-455a-a65f-5855d117007d",
      "m1": "بدان اندکی برکشیدند نخ",
      "m2": "سپاهی ز ترکان چو مور و ملخ",
      "verse_number": 20
    },
    {
      "id": "751a6b9a-b0b6-416a-81a6-76f4ecd625ae",
      "m1": "همی کرد گودرز هر سو نگاه",
      "m2": "سپاه اندر آمد بگرد سپاه",
      "verse_number": 21
    },
    {
      "id": "e78ffa3f-dfb4-4aba-a901-84620375ec97",
      "m1": "سراسیمه شد خفته از داروگیر",
      "m2": "برآمد یکی ابر بارانش تیر",
      "verse_number": 22
    },
    {
      "id": "b1b37ee8-77f8-4e8c-ab82-4e392f0b44c0",
      "m1": "بزیر سر مست بالین نرم",
      "m2": "ز بر گُرز و گوپال و شمشیر گرم",
      "verse_number": 23
    },
    {
      "id": "4705f1a6-3169-4f87-a9fc-25ffb94436c6",
      "m1": "سپیده چو برزد سر از برج شیر",
      "m2": "به لشکر نگه کرد گیو دلیر",
      "verse_number": 24
    },
    {
      "id": "a525c833-7007-46d3-ad5b-9ba780a65b8f",
      "m1": "همه دشت از ایرانیان کشته دید",
      "m2": "سر بخت بیدار برگشته دید",
      "verse_number": 25
    },
    {
      "id": "ac7ca8ad-d160-4b50-a167-b448c5324750",
      "m1": "دریده درفش و نگونسار کوس",
      "m2": "رخ زندگان تیره چون آبنوس",
      "verse_number": 26
    },
    {
      "id": "13a12490-33ff-4aad-97e9-bf9c2e65152e",
      "m1": "سپهبد نگه کرد و گردان ندید",
      "m2": "ز لشکر دلیران و مردان ندید",
      "verse_number": 27
    },
    {
      "id": "04a1b178-e562-427c-a818-0cedd95c0316",
      "m1": "همه رزمگه سربسر کشته بود",
      "m2": "تنانشان بخون اندر آغشته بود",
      "verse_number": 28
    },
    {
      "id": "b5273bff-8e21-49c7-908f-7daef57915ef",
      "m1": "پسر بیپدر شد پدر بیپسر",
      "m2": "همه لشگر گشن زیر و زبر",
      "verse_number": 29
    },
    {
      "id": "3bd1f76d-70af-4ddc-8256-7d80ccfc548b",
      "m1": "به بیچارگی روی برگاشتند",
      "m2": "سراپرده و خیمه بگذاشتند",
      "verse_number": 30
    },
    {
      "id": "71ec053a-31c7-42b2-9c50-33d3ab7901c0",
      "m1": "نه کوس و نه لشکر نه بار و بنه",
      "m2": "همه میسره خسته و میمنه",
      "verse_number": 31
    },
    {
      "id": "8de9e458-f832-4acf-9ef8-7dd95c2e4309",
      "m1": "ازین گونه لشکر سوی کاسهرود",
      "m2": "برفتند بیمایه و تار و پود",
      "verse_number": 32
    },
    {
      "id": "af09eed8-07c0-4194-8ff1-6333bf3ce634",
      "m1": "چنین آمد این گنبد تیزگرد",
      "m2": "گهی شادمانی دهد گاه درد",
      "verse_number": 33
    },
    {
      "id": "6d829a69-4cf2-48f0-819a-9bf7d21991cd",
      "m1": "سواران توران پس پشت طوس",
      "m2": "دلان پر ز کین و سران پر فسوس",
      "verse_number": 34
    },
    {
      "id": "d7abbeb5-3c53-47ed-835a-004335cc6940",
      "m1": "همی گُرز بارید گویی ز ابر",
      "m2": "پس پشت بر جوشن و خود و گبر",
      "verse_number": 35
    },
    {
      "id": "134d030b-5882-4524-bc2a-cab2977f4fbc",
      "m1": "نبد کس برزم اندرون پایدار",
      "m2": "همه کوه کردند گردان حصار",
      "verse_number": 36
    },
    {
      "id": "194efcaf-460a-4ef5-a8c4-01d05b444964",
      "m1": "فرومانده اسپان و مردان جنگ",
      "m2": "یکی را نبد هوش و توش و نه هنگ",
      "verse_number": 37
    },
    {
      "id": "749bb33d-e2a0-46f8-9676-a53e4059b41b",
      "m1": "سپاهی ازین گونه گشتند باز",
      "m2": "شده مانده از رزم و راه دراز",
      "verse_number": 38
    },
    {
      "id": "dc17bf49-6ca7-40a1-854b-b6a7af94e6f9",
      "m1": "ز هامون سپهبد سوی کوه شد",
      "m2": "ز پیکار ترکان بیاندوه شد",
      "verse_number": 39
    },
    {
      "id": "b3eddb52-16d2-46f6-9bfc-002561cbcc7d",
      "m1": "فراوان کم آمد ز ایرانیان",
      "m2": "برآمد خروشی بدرد از میان",
      "verse_number": 40
    },
    {
      "id": "c1a40af6-a955-4980-a2f0-ef3e58609f16",
      "m1": "همه خسته و بسته بد هرک زیست",
      "m2": "شد آن کشته بر خسته باید گریست",
      "verse_number": 41
    },
    {
      "id": "9029160b-ca33-4c76-b9df-df3ed3af0fb9",
      "m1": "نه تاج و نه تخت و نه پردهسرای",
      "m2": "نه اسپ و نه مردان جنگی بپای",
      "verse_number": 42
    },
    {
      "id": "c3957dd5-f02b-41c8-b6e3-bb92587bb810",
      "m1": "نه آباد بوم نه مردان کار",
      "m2": "نه آن خستگانرا کسی خواستار",
      "verse_number": 43
    },
    {
      "id": "de4393c6-cfaf-4cdc-9760-29794dc2053f",
      "m1": "پدر بر پسر چند گریان شده",
      "m2": "وزان خستگان چند بریان شده",
      "verse_number": 44
    },
    {
      "id": "8c212f11-5718-4e0f-a5b9-08e511c0ee76",
      "m1": "چنین است رسم جهان جهان",
      "m2": "که کردار خویش از تو دارد نهان",
      "verse_number": 45
    },
    {
      "id": "f635fe28-dfa1-4def-bc8b-216a4be80796",
      "m1": "همی با تو در پرده بازی کند",
      "m2": "ز بیرون ترا بینیازی کند",
      "verse_number": 46
    },
    {
      "id": "8de343bd-ddf2-41f4-8e51-67b4a0a5d34c",
      "m1": "ز باد آمدی رفت خواهی به گرد",
      "m2": "چه دانی که با تو چه خواهند کرد",
      "verse_number": 47
    },
    {
      "id": "e791ae60-465e-467f-bcbe-eb99210bc167",
      "m1": "ببند درازیم و در چنگ آز",
      "m2": "ندانیم باز آشکارا ز راز",
      "verse_number": 48
    },
    {
      "id": "23d71725-2822-4662-abe1-954d3c40f046",
      "m1": "دو بهره ز ایرانیان کشته بود",
      "m2": "دگر خسته از رزم برگشته بود",
      "verse_number": 49
    },
    {
      "id": "8fa53a9b-75a0-4773-879f-ecc7590e001e",
      "m1": "سپهبد ز پیکار دیوانه گشت",
      "m2": "دلش با خرد همچو بیگانه گشت",
      "verse_number": 50
    },
    {
      "id": "e63bbfa9-1e89-424a-83eb-36aacbe7f2e6",
      "m1": "بلشکرگه اندر می و خوان و بزم",
      "m2": "سپاه آرزو کرد بر جای رزم",
      "verse_number": 51
    },
    {
      "id": "29f8a00e-133b-4206-bcb5-a6b860f914f0",
      "m1": "جهاندیده گودرز با پیر سر",
      "m2": "نه پور و نبیره نه بوم و نه بر",
      "verse_number": 52
    },
    {
      "id": "b5addb95-6a32-4992-b0a5-408e8f0ca586",
      "m1": "نه آن خستگان را خورش نه پزشک",
      "m2": "همه جای غم بود و خونین سرشک",
      "verse_number": 53
    },
    {
      "id": "05311589-9850-4d1c-ac2b-8dac0435dc88",
      "m1": "جهاندیدگان پیش اوی آمدند",
      "m2": "شکسته دل و راهجوی آمدند",
      "verse_number": 54
    },
    {
      "id": "fbc6a15f-3e02-40b7-bf84-33b1130687e7",
      "m1": "یکی دیدبان بر سر کوه کرد",
      "m2": "کجا دیدگان سوی انبوه کرد",
      "verse_number": 55
    },
    {
      "id": "be4d981f-1903-4b4f-9170-a26de4282f3a",
      "m1": "طلایه فرستاد بر هر سویی",
      "m2": "مگر یابد آن درد را دارویی",
      "verse_number": 56
    },
    {
      "id": "4ca7e7dc-0b74-4522-aa54-b801f1a3bc5e",
      "m1": "یکی نامداری ز ایرانیان",
      "m2": "بفرمود تا تنگ بندد میان",
      "verse_number": 57
    },
    {
      "id": "7fcc0d05-03ca-4fba-8296-223bce67700e",
      "m1": "دهد شاه را آگهی زین سخن",
      "m2": "که سالار لشکر چه افگند بن",
      "verse_number": 58
    },
    {
      "id": "f8eb0619-f1a9-4d73-90b2-e9e22fa0ee60",
      "m1": "چه روز بد آمد بایرانیان",
      "m2": "سران را ز بخشش سرآمد زیان",
      "verse_number": 59
    },
    {
      "id": "0c16931e-5254-4e61-864c-da789e72e7d2",
      "m1": "رونده بر شاه برد آگهی",
      "m2": "که تیره شد آن روزگار مهی",
      "verse_number": 60
    },
    {
      "id": "d66fb727-454c-453e-b25b-719ee489c2cf",
      "m1": "چو شاه دلیر این سخنها شنید",
      "m2": "بجوشید وز غم دلش بردمید",
      "verse_number": 61
    },
    {
      "id": "f57277e3-1fd8-4ea8-8bb6-065bed710098",
      "m1": "ز کار برادر پر از درد بود",
      "m2": "بران درد بر درد لشکر فزود",
      "verse_number": 62
    },
    {
      "id": "9a7874d0-e4b2-46a6-9c7e-552588324cac",
      "m1": "زبان کرد گویا بنفرین طوس",
      "m2": "شب تیره تا گاه بانگ خروس",
      "verse_number": 63
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 63
  },
  "order_in_parent": 19
}