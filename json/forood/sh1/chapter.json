{
  "id": "c61ba74a-9c1b-4b09-9b20-9459e443448e",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "e6c98c70-36c6-41aa-9f3d-cc2c47f13024",
      "m1": "جهانجوی چون شد سرافراز و گرد",
      "m2": "سپه را بدشمن نشاید سپرد",
      "verse_number": 1
    },
    {
      "id": "1ad1980c-8fa6-4bd7-a329-050027ef27d2",
      "m1": "سرشک اندر آید بمژگان ز رشک",
      "m2": "سرشکی که درمان نداند پزشک",
      "verse_number": 2
    },
    {
      "id": "83caf6f6-f4f8-48a1-a68b-df6612a76f24",
      "m1": "کسی کز نژاد بزرگان بود",
      "m2": "به بیشی بماند سترگ آن بود",
      "verse_number": 3
    },
    {
      "id": "6a76ff11-0be8-4ad7-9efc-b274ef7af2b3",
      "m1": "چو بیکام دل بنده باید بدن",
      "m2": "بکام کسی داستانها زدن",
      "verse_number": 4
    },
    {
      "id": "2f852f62-8733-488f-b20d-4839877b8f32",
      "m1": "سپهبد چو خواند ورا دوستدار",
      "m2": "نباشد خرد با دلش سازگار",
      "verse_number": 5
    },
    {
      "id": "33813c31-4a1c-4a58-8936-1f87158ad280",
      "m1": "گرش زآرزو بازدارد سپهر",
      "m2": "همان آفرینش نخواند بمهر",
      "verse_number": 6
    },
    {
      "id": "acc28f00-a27b-40aa-853f-51a9d4483e14",
      "m1": "ورا هیچ خوبی نخواهد به دل",
      "m2": "شود آرزوهای او دلگسل",
      "verse_number": 7
    },
    {
      "id": "faf6d193-dc51-465e-b706-813355aab15a",
      "m1": "و دیگر کش از بن نباشد خرد",
      "m2": "خردمندش از مردمان نشمرد",
      "verse_number": 8
    },
    {
      "id": "1fe5839d-3d92-4f58-981e-11db7bcb830d",
      "m1": "چو این داستان سربسر بشنوی",
      "m2": "ببینی سر مایهٔ بدخوی",
      "verse_number": 9
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 9
  },
  "order_in_parent": 1
}