{
  "id": "7f163e6e-1a70-4a8d-b2d2-bfb78e15c292",
  "title": "بخش ۵",
  "verses": [
    {
      "id": "76c251f8-7f62-4440-9d18-1e0fefbf3b7b",
      "m1": "سواران رسیدند بر تیغ کوه",
      "m2": "سپاه اندر آمد گروها گروه",
      "verse_number": 1
    },
    {
      "id": "7b4168b0-9054-4e70-985f-6d900492ae1c",
      "m1": "سپردار با نیزهور سی هزار",
      "m2": "همه رزمجوی از در کارزار",
      "verse_number": 2
    },
    {
      "id": "6046a021-a464-4408-9aec-3c5369700eac",
      "m1": "سوار و پیاده بزرین کمر",
      "m2": "همه تیغ دار و همه نیزهور",
      "verse_number": 3
    },
    {
      "id": "1dd96304-5cdc-42da-b7a1-89bc262fe982",
      "m1": "ز بس ترگ زرین و زرین درفش",
      "m2": "ز گوپال زرین و زرینه کفش",
      "verse_number": 4
    },
    {
      "id": "ca4282ba-54e1-46db-83ad-b62b7742ac6b",
      "m1": "تو گفتی به کان اندرون زر نماند",
      "m2": "برآمد یکی ابر و گوهر فشاند",
      "verse_number": 5
    },
    {
      "id": "644e1df2-d41f-46a2-8841-6d4f913469a3",
      "m1": "ز بانگ تبیره میان دو کوه",
      "m2": "دل کرگس اندر هوا شد ستوه",
      "verse_number": 6
    },
    {
      "id": "ec5a8524-aaba-4342-a980-4733d746a318",
      "m1": "چنین گفت کاکنون درفش مهان",
      "m2": "بگو و مدار ایچ گونه نهان",
      "verse_number": 7
    },
    {
      "id": "a6e9e097-7947-4cb1-8a8a-e219e9e00dac",
      "m1": "بدو گفت کان پیل پیکر درفش",
      "m2": "سواران و آن تیغهای بنفش",
      "verse_number": 8
    },
    {
      "id": "6761662e-9242-4b21-a471-12e2dbe9dce1",
      "m1": "کرا باشد اندر میان سپاه",
      "m2": "چنین آلت ساز و این دستگاه",
      "verse_number": 9
    },
    {
      "id": "000a9d39-1909-40cd-9653-f1d106084a6f",
      "m1": "چو بشنید گفتار او را تخوار",
      "m2": "چنین داد پاسخ که ای شهریار",
      "verse_number": 10
    },
    {
      "id": "3b258f89-9d1f-45fb-83c9-f83e21587045",
      "m1": "پس پشت طوس سپهبد بود",
      "m2": "که در کینه پیکار او بد بود",
      "verse_number": 11
    },
    {
      "id": "64956e8e-a935-4891-a6e1-02737017ed64",
      "m1": "درفشی پس پشت او دیگرست",
      "m2": "چو خورشید تابان بدو پیکرست",
      "verse_number": 12
    },
    {
      "id": "cb1879a8-bb2d-4b8f-a51a-61d2ee731eb3",
      "m1": "برادر پدر تست با فر و کام",
      "m2": "سپهبد فریبرز کاوس نام",
      "verse_number": 13
    },
    {
      "id": "d0414185-4e9e-405e-8e17-0842898e0697",
      "m1": "پسش ماه پیکر درفشی بزرگ",
      "m2": "دلیران بسیار و گردی سترگ",
      "verse_number": 14
    },
    {
      "id": "92631695-949b-4b56-a701-6ec0be8b0447",
      "m1": "ورانام گستهم گژدهم خوان",
      "m2": "که لرزان بود پیل ازو ز استخوان",
      "verse_number": 15
    },
    {
      "id": "38b045d7-4cb7-4bfa-a417-f58ff6c145d1",
      "m1": "پسش گرگ پیکر درفشی دراز",
      "m2": "بگردش بسی مردم رزمساز",
      "verse_number": 16
    },
    {
      "id": "f05acaa3-9338-46c5-9f9a-dd0a06db48bd",
      "m1": "بزیر اندرش زنگهٔ شاوران",
      "m2": "دلیران و گردان و کنداوران",
      "verse_number": 17
    },
    {
      "id": "4611f57c-5e5d-4500-8bcf-b9a272f4c6e8",
      "m1": "درفشی پرستار پیکر چو ماه",
      "m2": "تنش لعل و جعد از حریر سیاه",
      "verse_number": 18
    },
    {
      "id": "1ca0f784-502c-4f82-a166-921f0cfe836d",
      "m1": "ورا بیژن گیو راند همی",
      "m2": "که خون بآسمان برفشاند همی",
      "verse_number": 19
    },
    {
      "id": "21b101fb-b5e2-477f-8a2b-355421a9316b",
      "m1": "درفشی کجا پیکرش هست ببر",
      "m2": "همی بشکند زو میان هژبر",
      "verse_number": 20
    },
    {
      "id": "678b67da-c0da-4a60-accb-0df3a348368f",
      "m1": "ورا گرد شیدوش دارد بپای",
      "m2": "چو کوهی همی اندر آید ز جای",
      "verse_number": 21
    },
    {
      "id": "f9d37412-07e9-4cb8-8ed2-5934795f671d",
      "m1": "درفش گرازست پیکر گراز",
      "m2": "سپاهی کمندافگن و رزم ساز",
      "verse_number": 22
    },
    {
      "id": "171f7e2b-3f91-47c9-b8de-2af4886a9d46",
      "m1": "درفشی کجا پیکرش گاومیش",
      "m2": "سپاه از پس و نیزهداران ز پیش",
      "verse_number": 23
    },
    {
      "id": "013feee2-21b8-4af2-a036-0c0c73834198",
      "m1": "چنان دان که آن شهره فرهاد راست",
      "m2": "که گویی مگر با سپهرست راست",
      "verse_number": 24
    },
    {
      "id": "d7276b7e-da4a-4501-a7a2-a972926dfe27",
      "m1": "درفشی کجا پیکرش دیزه گرگ",
      "m2": "نشان سپهدار گیو سترگ",
      "verse_number": 25
    },
    {
      "id": "4eaaaf04-5ccc-4fb1-b7f4-553e40bdfdcb",
      "m1": "درفشی کجا شیر پیکر بزر",
      "m2": "که گودرز کشواد دارد بسر",
      "verse_number": 26
    },
    {
      "id": "023372c9-766e-4d72-aac5-cf4810eaa3fc",
      "m1": "درفشی پلنگست پیکر گراز",
      "m2": "پس ریونیزست با کام و ناز",
      "verse_number": 27
    },
    {
      "id": "9a5f9e23-2ebe-47fa-bd3f-0ebfc4328c42",
      "m1": "درفشی کجا آهویش پیکرست",
      "m2": "که نستوه گودرز با لشکرست",
      "verse_number": 28
    },
    {
      "id": "bcf7b149-7342-4738-9ee0-e8a35a56bb0e",
      "m1": "درفشی کجا غرم دارد نشان",
      "m2": "ز بهرام گودرز کشوادگان",
      "verse_number": 29
    },
    {
      "id": "54bff97c-607b-453f-bbd0-cf4fd7bc75b0",
      "m1": "همه شیرمردند و گرد و سوار",
      "m2": "یکایک بگویم درازست کار",
      "verse_number": 30
    },
    {
      "id": "ef420f8d-fba6-491d-b126-629ad27f1ea0",
      "m1": "چو یکیک بگفت از نشان گوان",
      "m2": "بپیش فرود آن شه خسروان",
      "verse_number": 31
    },
    {
      "id": "dc60cc1a-994d-4edd-a6c2-d874e4b7414f",
      "m1": "مهان و کهان را همه بنگرید",
      "m2": "ز شادی رخش همچو گل بشکفید",
      "verse_number": 32
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 32
  }
}