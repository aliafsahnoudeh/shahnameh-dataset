{
  "id": "9214ccf8-bf05-4dfd-a996-e42e28eb2db4",
  "title": "بخش ۱۰",
  "verses": [
    {
      "id": "3ec87fd8-d5ab-45a1-b54f-2a7c88c6f156",
      "m1": "تخوار سراینده گفت آن زمان",
      "m2": "که آمد بر کوه کوهی دمان",
      "verse_number": 1
    },
    {
      "id": "6dd02811-203d-4843-9b2f-e682b809b4f4",
      "m1": "سپهدار طوسست کامد بجنگ",
      "m2": "نتابی تو با کار دیده نهنگ",
      "verse_number": 2
    },
    {
      "id": "fa706af3-d315-43d3-96d8-a2f92b295fd0",
      "m1": "برو تا در دژ ببندیم سخت",
      "m2": "ببینیم تا چیست فرجام بخت",
      "verse_number": 3
    },
    {
      "id": "2b979a93-5d85-4d5f-ad9f-43b451017f19",
      "m1": "چو فرزند و داماد او را برزم",
      "m2": "تبه کردی اکنون میندیش بزم",
      "verse_number": 4
    },
    {
      "id": "b81c7cb8-d9d3-4975-9594-a72e38ace7bb",
      "m1": "فرود جوان تیز شد با تخوار",
      "m2": "که چون رزم پیش آید و کارزار",
      "verse_number": 5
    },
    {
      "id": "3837d1b2-4092-445a-ab76-f4effad2fc6e",
      "m1": "چه طوس و چه شیر و چه پیل ژیان",
      "m2": "چه جنگی نهنگ و چه ببر بیان",
      "verse_number": 6
    },
    {
      "id": "dfb98388-8761-4f33-8bcd-a79964384a2f",
      "m1": "بجنگ اندرون مرد را دل دهند",
      "m2": "نه بر آتش تیز بر گل نهند",
      "verse_number": 7
    },
    {
      "id": "4912fe68-ca14-48ef-95cd-da82893bdcbe",
      "m1": "چنین گفت با شاهزاده تخوار",
      "m2": "که شاهان سخن را ندارند خوار",
      "verse_number": 8
    },
    {
      "id": "1f51cc71-f2b1-4345-bf28-3259cdbbf11d",
      "m1": "تو هم یک سواری اگر ز آهنی",
      "m2": "همی کوه خارا ز بن برکنی",
      "verse_number": 9
    },
    {
      "id": "c26f832a-c583-49cc-a452-819f694b7965",
      "m1": "از ایرانیان نامور سی هزار",
      "m2": "برزم تو آیند بر کوهسار",
      "verse_number": 10
    },
    {
      "id": "74327158-8955-4803-9a40-20cc17058bae",
      "m1": "نه دژ ماند اینجا نه سنگ و نه خاک",
      "m2": "سراسر ز جا اندر آرند پاک",
      "verse_number": 11
    },
    {
      "id": "653483b7-1f10-479a-92bd-8b4d8f75a618",
      "m1": "وگر طوس را زین گزندی رسد",
      "m2": "به خسرو ز دردش نژندی رسد",
      "verse_number": 12
    },
    {
      "id": "0c14847f-3982-4aef-926a-32c2a5c554b1",
      "m1": "بکین پدرت اندر آید شکست",
      "m2": "شکستی که هرگز نشایدش بست",
      "verse_number": 13
    },
    {
      "id": "128cd256-e7d0-4e61-b018-2577800d9c84",
      "m1": "بگردان عنان و مینداز تیر",
      "m2": "بدژ شو مبر رنج بر خیرهخیر",
      "verse_number": 14
    },
    {
      "id": "58d2e46e-4a22-4a21-883a-11cb69fdec5b",
      "m1": "سخن هرچ از پیش بایست گفت",
      "m2": "نگفت و همی داشت اندر نهفت",
      "verse_number": 15
    },
    {
      "id": "8bbbb816-6d51-4067-a3e4-44dfef1812dd",
      "m1": "ز بیمایه دستور ناکاردان",
      "m2": "ورا جنگ سود آمد و جان زیان",
      "verse_number": 16
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 16
  },
  "order_in_parent": 10
}