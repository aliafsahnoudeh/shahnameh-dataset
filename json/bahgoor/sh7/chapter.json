{
  "id": "88da7f1c-3cb6-49a0-84a7-27f96384ff3d",
  "title": "بخش ۷",
  "verses": [
    {
      "id": "2233cec6-ca3d-498a-946a-9482db2881e2",
      "m1": "چو بنشست می خواست از بامداد",
      "m2": "بزرگان لشکر برفتند شاد",
      "verse_number": 1
    },
    {
      "id": "a3e60c5d-6d2a-4b4c-996f-2b7b22579f32",
      "m1": "بیامد همانگه یکی مرد مه",
      "m2": "ورا میوه آورد چندی ز ده",
      "verse_number": 2
    },
    {
      "id": "b82f5580-6422-4cba-908d-e088055051ec",
      "m1": "شتربارها نار و سیب و بهی",
      "m2": "ز گل دستهها کرده شاهنشهی",
      "verse_number": 3
    },
    {
      "id": "7733dcf3-4023-4122-b48d-1fb63e2674ff",
      "m1": "جهاندار چون دید بنواختش",
      "m2": "میان یلان پایگه ساختش",
      "verse_number": 4
    },
    {
      "id": "4d8a89b2-54d2-452f-917b-f91cdd0cb77e",
      "m1": "همین مه که با میوه و بوی بود",
      "m2": "ورا پهلوی نام کبروی بود",
      "verse_number": 5
    },
    {
      "id": "2c606d82-166a-4450-ace1-94ef38fb4123",
      "m1": "به روی جهاندار جام نبید",
      "m2": "دو من را به یکبار اندر کشید",
      "verse_number": 6
    },
    {
      "id": "8958993c-b991-4917-b64c-739cb77783f4",
      "m1": "چو شد مرد خرم ز دیدار شاه",
      "m2": "ازان نامداران و آن جشنگاه",
      "verse_number": 7
    },
    {
      "id": "7636027e-26c6-4fa3-860a-69c4d5840ab1",
      "m1": "یکی جام دیگر پر از می بلور",
      "m2": "به دلش اندر افتاد زان جام شور",
      "verse_number": 8
    },
    {
      "id": "b8353494-d788-4787-9e5f-0e7e08ce145c",
      "m1": "ز پیش بزرگان بیازید دست",
      "m2": "بدان جام می تاخت و بر پای جست",
      "verse_number": 9
    },
    {
      "id": "3ed6fbf8-c766-4af1-b1bd-c07d4c967f37",
      "m1": "به یاد شهنشاه بگرفت جام",
      "m2": "منم گفت میخواره کبروی نام",
      "verse_number": 10
    },
    {
      "id": "41990a6e-10ed-4359-be35-72b83f4559cf",
      "m1": "به روی شهنشاه جام نبید",
      "m2": "چو من درکشم یار خواهم گزید",
      "verse_number": 11
    },
    {
      "id": "a2411cf5-308e-4549-a81e-857735a97929",
      "m1": "به جام اندرون بود می پنج من",
      "m2": "خورم هفت ازین بر سر انجمن",
      "verse_number": 12
    },
    {
      "id": "e4542854-e18d-493f-a0ee-8b43f011a3bd",
      "m1": "پس انگه سوی ده روم من به هوش",
      "m2": "ز من نشنود کس به مستی خروش",
      "verse_number": 13
    },
    {
      "id": "5be913cf-5261-4516-8b5b-031fa8bd432f",
      "m1": "چنان هفت جام پر از می بخورد",
      "m2": "ازان می پرستان برآورد گرد",
      "verse_number": 14
    },
    {
      "id": "1f8b0343-084a-4fde-8b31-66bd4b8e59f2",
      "m1": "به دستوری شاه بیرون گذشت",
      "m2": "که داند که می در تنش چون گذشت",
      "verse_number": 15
    },
    {
      "id": "68bc8136-35e1-476b-8fa2-33b42e7c1a70",
      "m1": "وزان جای خرم بیامد به دشت",
      "m2": "چو در سینهٔ مرد می گرم گشت",
      "verse_number": 16
    },
    {
      "id": "cfbdfc72-0a84-4541-9501-ce795883f04c",
      "m1": "برانگیخت اسپ از میان گروه",
      "m2": "ز هامون همی تاخت تا پیش کوه",
      "verse_number": 17
    },
    {
      "id": "72a86b48-b630-4018-b0f7-abddeed1895c",
      "m1": "فرود آمد از باره جایی نهفت",
      "m2": "یله کرد و در سایهٔ کوه خفت",
      "verse_number": 18
    },
    {
      "id": "866c4ffb-7604-498d-9f21-0e5c6bc2ebc1",
      "m1": "ز کوه اندرآمد کلاغ سیاه",
      "m2": "دو چشمش بکند اندران خوابگاه",
      "verse_number": 19
    },
    {
      "id": "bddf1e87-66f9-40f7-b24f-ad4e10555296",
      "m1": "همی تاختند از پساندر گروه",
      "m2": "ورا مرده دیدند بر پیش کوه",
      "verse_number": 20
    },
    {
      "id": "9cdab7c8-ebb1-42e0-9077-5f5fb2b0aae9",
      "m1": "دو چشمش ز سر کنده زاغ سیاه",
      "m2": "برش اسپ او ایستاده به راه",
      "verse_number": 21
    },
    {
      "id": "0360d53b-89af-41b8-a607-f8b507ffd2a2",
      "m1": "برو کهترانش خروشان شدند",
      "m2": "وزان مجلس و جام جوشان شدند",
      "verse_number": 22
    },
    {
      "id": "44c27469-8f5d-42a4-ad3a-431e23574b81",
      "m1": "چو بهرام برخاست از خوابگاه",
      "m2": "بیامد بر او یکی نیکخواه",
      "verse_number": 23
    },
    {
      "id": "a06aab46-9b24-48de-aff2-51e6dee5e97a",
      "m1": "که کبروی را چشم روشن کلاغ",
      "m2": "ز مستی بکندست در پیش راغ",
      "verse_number": 24
    },
    {
      "id": "43770be7-85a1-4a2e-807e-8b411ba2e93e",
      "m1": "رخ شهریار جهان زرد شد",
      "m2": "ز تیمار کبروی پر درد شد",
      "verse_number": 25
    },
    {
      "id": "19f6bf8f-603d-42c0-9b89-8041da7cfd39",
      "m1": "همانگه برآمد ز درگه خروش",
      "m2": "که ای نامداران با فر و هوش",
      "verse_number": 26
    },
    {
      "id": "4b362fc4-bff2-4acb-b3da-c5b584b80989",
      "m1": "حرامست می در جهان سربسر",
      "m2": "اگر زیردستت گر نامور",
      "verse_number": 27
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 27
  },
  "order_in_parent": 7
}