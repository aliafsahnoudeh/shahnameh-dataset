{
  "id": "705d9191-5dfc-4bc6-bac4-84335d8cbb2c",
  "title": "بخش ۴۵",
  "verses": [
    {
      "id": "0491fcbc-887e-428f-a6d1-0371bec9faa1",
      "m1": "ازان پس به هرسو یکی نامه کرد",
      "m2": "به جایی که درویش بد جامه کرد",
      "verse_number": 1
    },
    {
      "id": "22e3fe0e-8a73-486c-9d73-b39c10f9b765",
      "m1": "بپرسید هرجا که بیرنج کیست",
      "m2": "به هرجای درویش و بیگنج کیست",
      "verse_number": 2
    },
    {
      "id": "0c4d4f5d-36fc-404a-9ef3-ec044a492979",
      "m1": "ز کار جهان یکسر آگه کنید",
      "m2": "دلم را سوی روشنی ره کنید",
      "verse_number": 3
    },
    {
      "id": "27c13cd0-c711-4ec1-b32c-7ad8a4ad385e",
      "m1": "بیامدش پاسخ ز هر کشوری",
      "m2": "ز هر نامداری و هر مهتری",
      "verse_number": 4
    },
    {
      "id": "d15e3eb4-a80b-4611-962a-be2d4b1ff01f",
      "m1": "که آباد بینیم روی زمین",
      "m2": "به هرجای پیوسته شد آفرین",
      "verse_number": 5
    },
    {
      "id": "923bbe65-5079-4990-aaba-14ae77706551",
      "m1": "مگر مرد درویش کز شهریار",
      "m2": "بنالد همی از بد روزگار",
      "verse_number": 6
    },
    {
      "id": "a7a37c56-2e12-4b87-9517-f0a201c5f9be",
      "m1": "که چون می گسارد توانگر همی",
      "m2": "به سر بر ز گل دارد افسر همی",
      "verse_number": 7
    },
    {
      "id": "5ce0dba9-bd95-40f0-9ca3-f81a332db740",
      "m1": "به آواز رامشگران می خورند",
      "m2": "چو ما مردمان را به کس نشمرند",
      "verse_number": 8
    },
    {
      "id": "fb8f2c11-09c4-4485-bf46-488aa671fa4c",
      "m1": "تهی دست بیرود و گل می خورد",
      "m2": "توانگر همانا ندارد خرد",
      "verse_number": 9
    },
    {
      "id": "3c5d9de5-e62e-4379-a094-443a505cb43d",
      "m1": "بخندید زان نامه بیدار شاه",
      "m2": "هیونی برافگند پویان به راه",
      "verse_number": 10
    },
    {
      "id": "564a9b0c-ca6d-4495-922f-adc1e3de0c5c",
      "m1": "به نزدیک شنگل فرستاد کس",
      "m2": "چنین گفت کای شاه فریادرس",
      "verse_number": 11
    },
    {
      "id": "9dc033d5-4847-4125-b3eb-dc0905263859",
      "m1": "ازان لوریان برگزین ده هزار",
      "m2": "نر و ماده بر زخم بربط سوار",
      "verse_number": 12
    },
    {
      "id": "7c00207a-69e7-4e28-81ef-39ae5abf76f5",
      "m1": "به ایران فرستش که رامشگری",
      "m2": "کند پیش هر کهتری بهتری",
      "verse_number": 13
    },
    {
      "id": "53279633-8468-4055-80c6-aa39c070756d",
      "m1": "چو برخواند آن نامه شنگل تمام",
      "m2": "گزین کرد زان لوریان به نام",
      "verse_number": 14
    },
    {
      "id": "449d9b2f-9878-4cf4-8ded-54ada3ea097c",
      "m1": "به ایران فرستاد نزدیک شاه",
      "m2": "چنان کان بود در خور نیکخواه",
      "verse_number": 15
    },
    {
      "id": "0c36746e-a59a-4f9c-8bca-043224f0af8a",
      "m1": "چو لوری بیامد به درگاه شاه",
      "m2": "بفرمود تا برگشادند راه",
      "verse_number": 16
    },
    {
      "id": "f472dc95-e32f-4e90-9abc-3caeb6d81a71",
      "m1": "به هریک یکی گاو داد و خری",
      "m2": "ز لوری همی ساخت برزیگری",
      "verse_number": 17
    },
    {
      "id": "c50b1ea7-0516-46a8-8892-b84f55d40887",
      "m1": "همان نیز خروار گندم هزار",
      "m2": "بدیشان سپرد آنک بد پایدار",
      "verse_number": 18
    },
    {
      "id": "afb05974-21b3-4a8b-988a-22a0b26b37de",
      "m1": "بدان تا بورزد به گاو و به خر",
      "m2": "ز گندم کند تخم و آرد به بر",
      "verse_number": 19
    },
    {
      "id": "abec3d61-7dac-4885-b636-3d41c0f0dd91",
      "m1": "کند پیش درویش رامشگری",
      "m2": "چو آزادگان را کند کهتری",
      "verse_number": 20
    },
    {
      "id": "6e4cc371-925a-4529-ac12-fbccd798f1e9",
      "m1": "بشد لوری و گاو و گندم بخورد",
      "m2": "بیامد سر سال رخساره زرد",
      "verse_number": 21
    },
    {
      "id": "ba0cc19a-886d-4bff-b0f7-17949272d784",
      "m1": "بدو گفت شاه این نه کار تو بود",
      "m2": "پراگندن تخم و کشت و درود",
      "verse_number": 22
    },
    {
      "id": "2019a191-53e2-4977-8dc9-9cce55eed274",
      "m1": "خری ماند اکنون بنه برنهید",
      "m2": "بسازید رود و بریشم دهید",
      "verse_number": 23
    },
    {
      "id": "083811da-5d2d-4384-85aa-160124084c12",
      "m1": "کنون لوری از پاک گفتار اوی",
      "m2": "همی گردد اندر جهان چارهجوی",
      "verse_number": 24
    },
    {
      "id": "0f856628-d568-4543-a8ed-3d75c49a6e4b",
      "m1": "سگ و کبک بفزود بر گفت شاه",
      "m2": "شب و روز پویان به دزدی به راه",
      "verse_number": 25
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 25
  },
