{
  "id": "66900e7a-948e-4088-88ac-592943d39948",
  "title": "بخش ۴۳",
  "verses": [
    {
      "id": "e131e011-16ba-418b-b46c-48fe412b4fcb",
      "m1": "بیامد ز میدان چو تیر از کمان",
      "m2": "بر دختر خویش رفت آن زمان",
      "verse_number": 1
    },
    {
      "id": "5ede06e2-01fb-4f84-8ee5-d4c7813ffce7",
      "m1": "قلم خواست از ترک و قرطاس خواست",
      "m2": "ز مشک سیه سوده انقاس خواست",
      "verse_number": 2
    },
    {
      "id": "e9c77967-1316-462e-8a91-998fbf4e505a",
      "m1": "سر عهد کرد آفرین از نخست",
      "m2": "بران کو جهان از نژندی بشست",
      "verse_number": 3
    },
    {
      "id": "d1b42bcc-a15a-4ba1-b655-3429b9531d62",
      "m1": "بگسترد هم پاکی و راستی",
      "m2": "سوی دیو شد کژی و کاستی",
      "verse_number": 4
    },
    {
      "id": "4253aa78-f266-40ba-80d4-6707426fd769",
      "m1": "سپینود را جفت بهرامشاه",
      "m2": "سپردم بدین نامور پیشگاه",
      "verse_number": 5
    },
    {
      "id": "1bbd9b1f-9470-4561-ba7c-9a6f294a6e8f",
      "m1": "شهنشاه تا جاودان زنده باد",
      "m2": "بزرگان همه پیش او بنده باد",
      "verse_number": 6
    },
    {
      "id": "c85c2c3f-efe0-4101-b636-9f4bbdb3738f",
      "m1": "چو من بگذرم زین سپنجی سرای",
      "m2": "به قنوج بهرامشاهست رای",
      "verse_number": 7
    },
    {
      "id": "95eefd6f-9859-49fe-b378-2e27b65e4431",
      "m1": "ز فرمان این تاجور مگذرید",
      "m2": "تن مرده را سوی آتش برید",
      "verse_number": 8
    },
    {
      "id": "c7802d04-41d1-4fd8-a7e1-7e3a73ffcd38",
      "m1": "سپارید گنجم به بهرامشاه",
      "m2": "همان کشور و تاج و گاه و سپاه",
      "verse_number": 9
    },
    {
      "id": "5f692d3e-c531-4264-b6ee-89afbc3a6848",
      "m1": "سپینود را داد منشور هند",
      "m2": "نوشته خطی هندوی بر پرند",
      "verse_number": 10
    },
    {
      "id": "5424a519-fac3-4881-8072-5315e3a3af00",
      "m1": "به ایران همی بود شنگل دو ماه",
      "m2": "فرستاد پس مهتری نزد شاه",
      "verse_number": 11
    },
    {
      "id": "d0586303-3212-4304-a13a-645cb410ae3c",
      "m1": "به دستوری بازگشتن به جای",
      "m2": "خود و نامداران فرخندهرای",
      "verse_number": 12
    },
    {
      "id": "dbc11ca4-8970-4093-bc27-898459204f0e",
      "m1": "بدان شد شهنشاه همداستان",
      "m2": "که او بازگردد به هندوستان",
      "verse_number": 13
    },
    {
      "id": "d814f57a-0fd6-41e0-af6d-30247067ef5d",
      "m1": "ز چیزی که باشد به ایران زمین",
      "m2": "بفرمود تا کرد موبد گزین",
      "verse_number": 14
    },
    {
      "id": "d8cf2021-2604-4723-9714-6ac4fa11da82",
      "m1": "ز دینار و ز گوهر شاهوار",
      "m2": "ز تیغ و ز خود و کمر بیشمار",
      "verse_number": 15
    },
    {
      "id": "eb444152-6123-4d73-b75c-9ed34c522dfa",
      "m1": "ز دیبا و از جامهٔ نابسود",
      "m2": "که آن را شمار و کرانه نبود",
      "verse_number": 16
    },
    {
      "id": "cdd04db1-2524-4730-ae7d-bdfc2ef4c785",
      "m1": "به اندازه یارانش را هم چنین",
      "m2": "بیاراست اسپان به دیبای چین",
      "verse_number": 17
    },
    {
      "id": "fb0a4c11-b963-4e0a-8730-cb02ce88623f",
      "m1": "گسی کردشان شاد و خشنود شاه",
      "m2": "سه منزل همی راند با او به راه",
      "verse_number": 18
    },
    {
      "id": "b6a096c8-4b05-40c6-a8db-d917292ef1da",
      "m1": "نبد هم بدین هدیه همداستان",
      "m2": "علف داد تا مرز هندوستان",
      "verse_number": 19
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 19
  },
