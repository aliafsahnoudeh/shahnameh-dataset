{
  "id": "98299fb5-a74d-4742-86f7-e490ae8eec80",
  "title": "بخش ۳۴",
  "verses": [
    {
      "id": "8a0319b1-9cf9-4eb9-8828-f0f948d133a0",
      "m1": "سکندر سوی روشنایی رسید",
      "m2": "یکی بر شد کوه رخشنده دید",
      "verse_number": 1
    },
    {
      "id": "37f56a80-6207-4758-8888-2a8bcf5a1a83",
      "m1": "زده بر سر کوه خارا عمود",
      "m2": "سرش تا به ابر اندر از چوب عود",
      "verse_number": 2
    },
    {
      "id": "7efd66f4-c6ca-47e2-9228-40f18887fde8",
      "m1": "بر هر عمودی کنامی بزرگ",
      "m2": "نشسته برو سبز مرغی سترگ",
      "verse_number": 3
    },
    {
      "id": "f88ca0d0-3edb-474a-8da1-f2b93a56ecb4",
      "m1": "به آواز رومی سخن راندند",
      "m2": "جهاندار پیروز را خواندند",
      "verse_number": 4
    },
    {
      "id": "008fb774-d82d-435e-8071-f784b3853193",
      "m1": "چو آواز بشنید قیصر برفت",
      "m2": "به نزدیک مرغان خرامید تفت",
      "verse_number": 5
    },
    {
      "id": "2c167174-e0aa-4aaa-8186-f49c0e555d1d",
      "m1": "بدو مرغ گفت ای دلارای رنج",
      "m2": "چه جویی همی زین سرای سپنج",
      "verse_number": 6
    },
    {
      "id": "6a99eef2-8d28-4237-9a02-54caa1ad702b",
      "m1": "اگر سر برآری به چرخ بلند",
      "m2": "همان بازگردی ازو مستمند",
      "verse_number": 7
    },
    {
      "id": "504374ac-e40b-481a-a22d-b4e9073feeae",
      "m1": "کنون کامدی هیچ دیدی زنا",
      "m2": "وگر کرده از خشت پخته بنا",
      "verse_number": 8
    },
    {
      "id": "a5d3f246-8ecc-4100-8877-851f42dbfef1",
      "m1": "چنین داد پاسخ کزین هر دو هست",
      "m2": "زنا و برین گونه جای نشست",
      "verse_number": 9
    },
    {
      "id": "3105538a-824c-45e6-af3c-486ea4d491ad",
      "m1": "چو بشنید پاسخ فروتر نشست",
      "m2": "درو خیره شد مرد یزدانپرست",
      "verse_number": 10
    },
    {
      "id": "bf6480e0-3680-42ba-9f07-f9bd985ae224",
      "m1": "بپرسید کاندر جهان بانگ رود",
      "m2": "شنیدی و آوای مست و سرود",
      "verse_number": 11
    },
    {
      "id": "c4e81c0c-1051-486c-a06c-fb130c659f4e",
      "m1": "چنین داد پاسخ که هر کو ز دهر",
      "m2": "ز شادی همی برنگیرند بهر",
      "verse_number": 12
    },
    {
      "id": "93b7783e-9a36-4f22-a1ae-f76a29374f60",
      "m1": "ورا شاد مردم نخواند همی",
      "m2": "وگر جان و دل برفشاند همی",
      "verse_number": 13
    },
    {
      "id": "7c8ee675-8d3c-4eba-97a0-d4ac296eea95",
      "m1": "به خاک آمد از بر شده چوب عمود",
      "m2": "تهی ماند زان مرغ رنگین عمود",
      "verse_number": 14
    },
    {
      "id": "f11de926-6732-4392-9d5d-f84d472b0d94",
      "m1": "بپرسید دانایی و راستی",
      "m2": "فزونست اگر کمی و کاستی",
      "verse_number": 15
    },
    {
      "id": "c16ac185-8f52-45fa-a0ed-1b3bf6d3210a",
      "m1": "چنین داد پاسخ که دانش پژوه",
      "m2": "همی سرفرازد ز هر دو گروه",
      "verse_number": 16
    },
    {
      "id": "054ef95e-34e8-4db4-bc60-6e5fbd4c82df",
      "m1": "به سوی عمود آمد از تیره خاک",
      "m2": "به منقار چنگالها کرد پاک",
      "verse_number": 17
    },
    {
      "id": "2bb916c9-bc9a-4f61-b292-0ebabcff2687",
      "m1": "ز قیصر بپرسید یزدانپرست",
      "m2": "به شهر تو بر کوه دارد نشست",
      "verse_number": 18
    },
    {
      "id": "55c281d5-26ff-4bec-87df-e2d4038d1b98",
      "m1": "بدو گفت چون مرد شد پاکرای",
      "m2": "بیابد پرستنده بر کوه جای",
      "verse_number": 19
    },
    {
      "id": "fa63a0ae-8f0e-49d0-889e-8bb6afad3428",
      "m1": "ازان چوب جوینده شد بر کنام",
      "m2": "جهانجوی روشندل و شادکام",
      "verse_number": 20
    },
    {
      "id": "73c54368-4d4f-4f88-bf18-8d752d573000",
      "m1": "به چنگال میکرد منقار تیز",
      "m2": "چو ایمن شد از گردش رستخیز",
      "verse_number": 21
    },
    {
      "id": "4d8e89b5-f5d4-4cd3-a958-b259fa573a52",
      "m1": "به قیصر بفرمود تا بیگروه",
      "m2": "پیاده شود بر سر تیغ کوه",
      "verse_number": 22
    },
    {
      "id": "30d6c441-4d3b-42ba-82c0-fddf79b371a4",
      "m1": "ببیند که تا بر سر کوه چیست",
      "m2": "کزو شادمان را بباید گریست",
      "verse_number": 23
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 23
  }
}