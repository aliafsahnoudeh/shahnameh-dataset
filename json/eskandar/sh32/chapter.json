{
  "id": "daec3c5f-3d57-4c31-ac7d-99d015091c6b",
  "title": "بخش ۳۲",
  "verses": [
    {
      "id": "bb8160e6-49c4-422f-b4e8-d7d41c89ca86",
      "m1": "بپرسید هرچیز و دریا بدید",
      "m2": "وزان روی لشکر به مغرب کشید",
      "verse_number": 1
    },
    {
      "id": "44981502-54f5-4175-b1d9-3024c6ecffe7",
      "m1": "یکی شارستان پیشش آمد بزرگ",
      "m2": "بدو اندرون مردمانی سترگ",
      "verse_number": 2
    },
    {
      "id": "25507f5a-d745-42f4-b3eb-8df579a19cf9",
      "m1": "همه روی سرخ و همه موی زرد",
      "m2": "همه در خور جنگ روز نبرد",
      "verse_number": 3
    },
    {
      "id": "cbe82fb7-dc7f-45e3-a100-6650cbc4fc35",
      "m1": "به فرمان به پیش سکندر شدند",
      "m2": "دو تا گشته و دست بر سر شدند",
      "verse_number": 4
    },
    {
      "id": "4cf8bf59-1d9e-4696-841d-e9a412fd6f43",
      "m1": "سکندر بپرسید از سرکشان",
      "m2": "که ایدر چه دارد شگفتی نشان",
      "verse_number": 5
    },
    {
      "id": "d7baf1d5-fe13-450f-866f-b5f3df12421a",
      "m1": "چنین گفت با او یکی مرد پیر",
      "m2": "که ای شاه نیکاختر و شهرگیر",
      "verse_number": 6
    },
    {
      "id": "993c0043-af11-48e8-be3f-6c9abf16f819",
      "m1": "یکی آبگیرست زان روی شهر",
      "m2": "کزان آب کس را ندیدیم بهر",
      "verse_number": 7
    },
    {
      "id": "09968b43-11d5-4f39-919c-c51312d63b4a",
      "m1": "چو خورشید تابان بدانجا رسید",
      "m2": "بران ژرف دریا شود ناپدید",
      "verse_number": 8
    },
    {
      "id": "c7c91199-2da6-4884-acfa-947d966c35ea",
      "m1": "پس چشمهدر تیره گردد جهان",
      "m2": "شود آشکارای گیتی نهان",
      "verse_number": 9
    },
    {
      "id": "b9c0bcab-e1e3-459a-9677-bc9661c3afbd",
      "m1": "وزان جای تاریک چندان سخن",
      "m2": "شنیدم که هرگز نیاید به بن",
      "verse_number": 10
    },
    {
      "id": "32409e31-7e65-43f3-97c2-9d19a3caaddb",
      "m1": "خرد یافته مرد یزدانپرست",
      "m2": "بدو در یکی چشمه گوید که هست",
      "verse_number": 11
    },
    {
      "id": "26ac0721-01da-4021-b5f9-0ed90da83b22",
      "m1": "گشاده سخن مرد با رای و کام",
      "m2": "همی آب حیوانش خواند به نام",
      "verse_number": 12
    },
    {
      "id": "bbc40b20-5b36-4b8d-930d-6c9c9dd6de46",
      "m1": "چنین گفت روشندل پر خرد",
      "m2": "که هرک آب حیوان خورد کی مرد",
      "verse_number": 13
    },
    {
      "id": "b98cd0e6-6b98-44ce-82b4-cc8a7e8c56f3",
      "m1": "ز فردوس دارد بران چشمه راه",
      "m2": "بشوید برآن تن بریزد گناه",
      "verse_number": 14
    },
    {
      "id": "adbe7407-a3fb-4876-a9fa-7178b236ec6f",
      "m1": "بپرسید پس شه که تاریک جای",
      "m2": "بدو اندرون چون رود چارپای",
      "verse_number": 15
    },
    {
      "id": "fd2fa06e-643b-4066-a59d-395d2e21f0d7",
      "m1": "چنین پاسخ آورد یزدانپرست",
      "m2": "کزان راه بر کره باید نشست",
      "verse_number": 16
    },
    {
      "id": "497e8613-cb62-4a75-9dd5-844e78ab771b",
      "m1": "به چوپان بفرمود کاسپ یله",
      "m2": "سراسر به لشکرگه آرد گله",
      "verse_number": 17
    },
    {
      "id": "7de1d92f-2403-4281-b224-36dd99894e44",
      "m1": "گزین کرد زو بارگی ده هزار",
      "m2": "همه چار سال از در کارزار",
      "verse_number": 18
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 18
  }
}