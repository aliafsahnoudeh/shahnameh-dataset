{
  "id": "98c96779-728b-4db2-9132-0f29c08fd2f8",
  "title": "بخش ۲",
  "verses": [
    {
      "id": "c3e75a7b-1252-404d-aeb4-9e51c3317e23",
      "m1": "بفرمود تا پیش او شد دبیر",
      "m2": "قلم خواست چینی و رومی حریر",
      "verse_number": 1
    },
    {
      "id": "4472621e-a330-440b-9658-879546544a59",
      "m1": "نویسنده از کلک چون خامه کرد",
      "m2": "سوی مادر روشنک نامه کرد",
      "verse_number": 2
    },
    {
      "id": "705fdff2-428c-4cad-9f4c-78615b7a66b8",
      "m1": "که یزدان ترا مزد نیکان دهاد",
      "m2": "بداندیش را درد پیکان دهاد",
      "verse_number": 3
    },
    {
      "id": "a7595a36-d2a9-4806-b314-13ded4732e89",
      "m1": "نوشتم یکی نامهای پیش ازین",
      "m2": "نوشته درو دردها بیش ازین",
      "verse_number": 4
    },
    {
      "id": "e3f637df-caa7-48ba-ad10-553208919e47",
      "m1": "چو جفت ترا روز برگشته شد",
      "m2": "به دست یکی بندهبر کشته شد",
      "verse_number": 5
    },
    {
      "id": "09435e98-cf00-4f86-84b1-79daf8fc7c64",
      "m1": "بر آیین شاهان کفن ساختم",
      "m2": "ورا زین جهان تیز پرداختم",
      "verse_number": 6
    },
    {
      "id": "7cbf354c-6b7b-40c0-b274-ba61f89a684d",
      "m1": "بسی آشتی خواستم پیش جنگ",
      "m2": "نکرد آشتی چون نبودش درنگ",
      "verse_number": 7
    },
    {
      "id": "23430939-f4a8-4c69-ab99-8f5061666582",
      "m1": "ز خونش بپیچید هم دشمنش",
      "m2": "به مینو رساناد یزدان تنش",
      "verse_number": 8
    },
    {
      "id": "22cc4ffb-c35b-41cf-bcf9-5fdc5bae92a8",
      "m1": "نیابد کسی چاره از چنگ مرگ",
      "m2": "چو باد خزانست و ما همچو برگ",
      "verse_number": 9
    },
    {
      "id": "fdc5e289-d610-41ce-bf28-18e4b3ef84b2",
      "m1": "جهان یکسر اکنون به پیش شماست",
      "m2": "بر اندرز دارا فراوان گواست",
      "verse_number": 10
    },
    {
      "id": "fece2017-c6fe-4ee7-b211-eb2675a2e95a",
      "m1": "که او روشنک را به من داد و گفت",
      "m2": "که چون او بباید ترا در نهفت",
      "verse_number": 11
    },
    {
      "id": "77ee930e-ffc6-435f-a156-9576b446558f",
      "m1": "کنون با پرستنده و دایگان",
      "m2": "از ایران بزرگان پرمایگان",
      "verse_number": 12
    },
    {
      "id": "0043bb74-7341-4acb-8ff6-de8a2e6ca692",
      "m1": "فرستید زودش به نزدیک من",
      "m2": "زداید مگر جان تاریک من",
      "verse_number": 13
    },
    {
      "id": "ff06b2f8-8b0a-4e97-9965-340d7b819a80",
      "m1": "بدارید چون پیش بود اصفهان",
      "m2": "ز هر سو پراگنده کارآگهان",
      "verse_number": 14
    },
    {
      "id": "41ff2d36-1255-4699-aa45-6e3e7e602ff8",
      "m1": "همه کارداران با شرم و داد",
      "m2": "که دارای دارابشان کار داد",
      "verse_number": 15
    },
    {
      "id": "150a2132-0ca2-4fff-84b1-0145d58b3043",
      "m1": "وز آنجا نخواهید فرمان رواست",
      "m2": "همه شهر ایران پیش شماست",
      "verse_number": 16
    },
    {
      "id": "6ef465b5-b522-41ff-aaae-5806aed2983e",
      "m1": "دل خویش را پر مدارا کنید",
      "m2": "مرا در جهان نام دارا کنید",
      "verse_number": 17
    },
    {
      "id": "a33611e2-6601-4571-ab38-c597804d028a",
      "m1": "سوی روشنک همچنین نامهای",
      "m2": "ز شاه جهاندار خودکامهای",
      "verse_number": 18
    },
    {
      "id": "e4bc9fbf-e7a5-43a2-b07d-1dbcbf2c0315",
      "m1": "نخست آفرین کرد بر کردگار",
      "m2": "جهاندار و دانا و پروردگار",
      "verse_number": 19
    },
    {
      "id": "6731228d-3019-4acf-8929-34a120c5dce5",
      "m1": "دگر گفت کز گوهر پادشا",
      "m2": "نزاید مگر مردم پارسا",
      "verse_number": 20
    },
    {
      "id": "a7929dbc-d1f3-4cd9-9d3e-f3a12c865050",
      "m1": "دلارای با نام و با رای و شرم",
      "m2": "سخن گفتن خوب و آوای نرم",
      "verse_number": 21
    },
    {
      "id": "5b95ca4c-8c43-473f-96ee-c28a1d8177ba",
      "m1": "پدر مر ترا پیش ما را سپرد",
      "m2": "وزان پس شد و نام نیکی ببرد",
      "verse_number": 22
    },
    {
      "id": "f064b501-a49a-4a30-bc72-6cbb4eae21e1",
      "m1": "چو آیی شبستان و مشکوی من",
      "m2": "ببینی تو باشی جهانجوی من",
      "verse_number": 23
    },
    {
      "id": "37c8b1e9-fb16-4aa1-ac21-6d75a80ada85",
      "m1": "سر بانوانی و زیبای تاج",
      "m2": "فروزندهٔ یاره و تخت عاج",
      "verse_number": 24
    },
    {
      "id": "e75c8429-b505-4b9d-b06a-899a65841df2",
      "m1": "نوشتیم نامه بر مادرت",
      "m2": "که ایدر فرستد ترا در خورت",
      "verse_number": 25
    },
    {
      "id": "d9393f84-d2dc-42f4-b30d-3221130d8304",
      "m1": "به آیین فرزند شاهنشهان",
      "m2": "به پیش اندرون موبد اصفهان",
      "verse_number": 26
    },
    {
      "id": "2c4454b7-5295-447d-8392-ccfc8f2c134e",
      "m1": "پرستنده و تاج شاهان و مهد",
      "m2": "هم آن را که خوردی ازو شیر و شهد",
      "verse_number": 27
    },
    {
      "id": "d83cbe43-abfc-48bb-b901-b8ad9013c2e4",
      "m1": "به مشکوی ما باش روشنروان",
      "m2": "توی در شبستان سر بانوان",
      "verse_number": 28
    },
    {
      "id": "a1aa6768-0eb7-4bc2-baf6-485f0300e28a",
      "m1": "همیشه دل شرم جفت تو باد",
      "m2": "شبستان شاهان نهفت تو باد",
      "verse_number": 29
    },
    {
      "id": "53b9b6ff-4469-4eeb-84fd-cc76e4d0bb2f",
      "m1": "بیامد یکی فیلسوفی چو گرد",
      "m2": "سخنهای شاه جهان یاد کرد",
      "verse_number": 30
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 30
  }
}