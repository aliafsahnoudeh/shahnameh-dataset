{
  "id": "871502b9-e9ec-408d-8701-c263ab65debb",
  "title": "بخش ۸",
  "verses": [
    {
      "id": "dff83dc1-0cd7-4c0a-9310-d1a626e2d22c",
      "m1": "چو نامه بر کید هندی رسید",
      "m2": "فرستادهٔ پادشا را بدید",
      "verse_number": 1
    },
    {
      "id": "48c4bcac-e893-4d48-8f31-49ee0dee6cb5",
      "m1": "فراوانش بستود و بنواختش",
      "m2": "به نیکی بر خویش بنشاختش",
      "verse_number": 2
    },
    {
      "id": "ff2bdb1c-dfd8-44f2-b0b0-8236ca0f1507",
      "m1": "بدو گفت شادم ز فرمان اوی",
      "m2": "زمانی نگردم ز پیمان اوی",
      "verse_number": 3
    },
    {
      "id": "7e46527e-5b86-4e21-9676-357c6b7b5d6a",
      "m1": "ولیکن برین گونه ناساخته",
      "m2": "بیایم دمان گردن افراخته",
      "verse_number": 4
    },
    {
      "id": "8743f71e-0b0c-48a1-b3c3-9f25c0b33150",
      "m1": "نباشد پسند جهانآفرین",
      "m2": "نه نزدیک آن پادشاه زمین",
      "verse_number": 5
    },
    {
      "id": "4675cb24-b835-4fbd-b862-f93a28639ef2",
      "m1": "همانگه بفرمود تا شد دبیر",
      "m2": "قلم خواست هندی و چینی حریر",
      "verse_number": 6
    },
    {
      "id": "d642bafd-4d61-41e7-9cda-732b525d2a91",
      "m1": "مران نامه را زود پاسخ نوشت",
      "m2": "بیاراست بر سان باغ بهشت",
      "verse_number": 7
    },
    {
      "id": "aa6ebe83-36a0-42d4-8e91-b75b827191cc",
      "m1": "نخست آفرین کرد بر کردگار",
      "m2": "خداوند پیروز و به روزگار",
      "verse_number": 8
    },
    {
      "id": "2e385a78-7c12-484f-a410-834a6c49cc48",
      "m1": "خداوند بخشنده و دادگر",
      "m2": "خداوند مردی و هوش و هنر",
      "verse_number": 9
    },
    {
      "id": "9f430e94-e288-4bf1-934b-0603aaacf91c",
      "m1": "دگر گفت کز نامور پادشا",
      "m2": "نپیچد سر مردم پارسا",
      "verse_number": 10
    },
    {
      "id": "b6ec9ce3-e18f-4d20-b258-b5bf7c4f1cf3",
      "m1": "نشاید که داریم چیزی دریغ",
      "m2": "ز دارندهٔ لشکر و تاج و تیغ",
      "verse_number": 11
    },
    {
      "id": "756b71f6-7192-4c9b-a3f4-e1e77f19acaa",
      "m1": "مرا چار چیزست کاندر جهان",
      "m2": "کسی را نبود آشکار و نهان",
      "verse_number": 12
    },
    {
      "id": "108e526b-a1c7-44b6-8d0d-ced5866e756c",
      "m1": "نباشد کسی را پس از من به نیز",
      "m2": "بدین گونه اندر جهان چار چیز",
      "verse_number": 13
    },
    {
      "id": "43deac84-e15e-40a1-b1b5-e951cfe7bec3",
      "m1": "فرستم چو فرمان دهد پیش اوی",
      "m2": "ازان تازه گردد دل و کیش اوی",
      "verse_number": 14
    },
    {
      "id": "4920a398-0ef2-418f-ade6-c2914a2ea755",
      "m1": "ازان پس چو فرمایدم شهریار",
      "m2": "بیایم پرستش کنم بندهوار",
      "verse_number": 15
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 15
  },
  "order_in_parent": 8
}