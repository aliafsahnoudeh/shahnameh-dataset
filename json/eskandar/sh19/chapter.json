{
  "id": "b5317b07-70e1-405b-a084-e961c55fcb6b",
  "title": "بخش ۱۹",
  "verses": [
    {
      "id": "a7694472-74dc-4b1d-ac89-7aba137b8eda",
      "m1": "چو لشکر شد از خواسته بینیاز",
      "m2": "برو ناگذشته زمانی دراز",
      "verse_number": 1
    },
    {
      "id": "4fdb0440-0e8a-465b-b18b-363e9a1aa4f8",
      "m1": "به شبگیر برخاست آوای کوس",
      "m2": "هوا شد به کردار چشم خروس",
      "verse_number": 2
    },
    {
      "id": "dca01c7f-96be-4c8a-b779-4b354440916d",
      "m1": "ز بس نیزه و پرنیانی درفش",
      "m2": "ستاره شده سرخ و زرد و بنفش",
      "verse_number": 3
    },
    {
      "id": "c03034f1-e51b-45e5-888a-62c6c2ca87a3",
      "m1": "سکندر بیامد به سوی حرم",
      "m2": "گروهی ازو شاد و بهری دژم",
      "verse_number": 4
    },
    {
      "id": "efb40133-7bb5-49b3-8f3a-d6f43ed8580d",
      "m1": "ابا نالهٔ بوق و با کوس تفت",
      "m2": "به خان براهیم آزر برفت",
      "verse_number": 5
    },
    {
      "id": "a9d0fda4-7bd7-42ae-b921-2b5e3ce93165",
      "m1": "که خان حرم را برآورده بود",
      "m2": "بدو اندرون رنجها برده بود",
      "verse_number": 6
    },
    {
      "id": "616263dd-0dc3-4bef-83e2-deba759d515e",
      "m1": "خداوند خواندش بیتالحرام",
      "m2": "بدو شد همه راه یزدان تمام",
      "verse_number": 7
    },
    {
      "id": "3406489a-9342-4c39-b8cf-791cdceedc98",
      "m1": "ز پاکی ورا خانهٔ خویش خواند",
      "m2": "نیایش بران کو ترا پیش خواند",
      "verse_number": 8
    },
    {
      "id": "69ec6d65-296c-4b91-9441-e15a7cb74419",
      "m1": "خدای جهان را نباشد نیاز",
      "m2": "نه جای خور و کام و آرام و ناز",
      "verse_number": 9
    },
    {
      "id": "d06e158a-ab06-4a10-9668-4fdcff0da491",
      "m1": "پرستشگهی بود تا بود جای",
      "m2": "بدو اندرون یاد کرد خدای",
      "verse_number": 10
    },
    {
      "id": "4c929071-13ea-4335-be35-6eece80e867b",
      "m1": "پس آمد سکندر سوی قادسی",
      "m2": "جهانگیر تا جهرم پارسی",
      "verse_number": 11
    },
    {
      "id": "56b2a692-3cba-4d50-98b9-799b82212cd6",
      "m1": "چو آگاهی آمد به نصر قتیب",
      "m2": "کزو بود مر مکه را فر و زیب",
      "verse_number": 12
    },
    {
      "id": "19c6d261-9edd-4646-ad96-1517e59dc070",
      "m1": "پذیره شدش با نبرده سران",
      "m2": "دلاور سواران نیزهوران",
      "verse_number": 13
    },
    {
      "id": "b36195de-3068-43d4-8702-64bc6ce829d1",
      "m1": "سواری بیامد هم اندر زمان",
      "m2": "ز مکه به نزد سکندر دمان",
      "verse_number": 14
    },
    {
      "id": "4b827769-6fd5-4276-822e-91c5f9769206",
      "m1": "که این نامداری که آمد ز راه",
      "m2": "نجوید همی تاج و گنج و سپاه",
      "verse_number": 15
    },
    {
      "id": "6c25c79b-19b3-4c29-8f4f-a3e5528d1d1f",
      "m1": "نبیرهٔ سماعیل نیک اخترست",
      "m2": "که پور براهیم پیغمبرست",
      "verse_number": 16
    },
    {
      "id": "2cd2062b-416c-4f9a-9eea-e382c4d33ade",
      "m1": "چو پیش آمدش نصر بنواختش",
      "m2": "یکی مایهور جایگه ساختش",
      "verse_number": 17
    },
    {
      "id": "187fdc2d-1144-4d3b-981c-6c6e05345554",
      "m1": "بدو شاد شد نصر و گوهر بگفت",
      "m2": "همه رازها برگشاد از نهفت",
      "verse_number": 18
    },
    {
      "id": "7e656f05-3b42-4e0d-9fe1-e0ad61ab3225",
      "m1": "سکندر چنین داد پاسخ بدوی",
      "m2": "که ای پاکدل مهتر راستگوی",
      "verse_number": 19
    },
    {
      "id": "3e569d17-56f7-4834-994f-fe6f4cf66ddf",
      "m1": "بدین دوده اکنون کدامست مه",
      "m2": "جز از تو پسندیده و روزبه",
      "verse_number": 20
    },
    {
      "id": "1dc9b438-f848-4c27-a151-7f4df8d76b18",
      "m1": "بدو گفت نصر ای جهاندار شاه",
      "m2": "خزاعست مهتر بدین جایگاه",
      "verse_number": 21
    },
    {
      "id": "8514d8b2-696c-4d06-af00-06af5fe984cc",
      "m1": "سماعیل چون زین جهان درگذشت",
      "m2": "جهانگیر قحطان بیامد ز دشت",
      "verse_number": 22
    },
    {
      "id": "d2213809-3e38-4a82-a0de-962dbc1bc24a",
      "m1": "ابا لشکر گشن شمشیرزن",
      "m2": "به بیداد بگرفت شهر یمن",
      "verse_number": 23
    },
    {
      "id": "d1f47aa6-81de-40cf-b101-97fd100f1e87",
      "m1": "بسی مردم بیگنه کشته شد",
      "m2": "بدین دودمان روز برگشته شد",
      "verse_number": 24
    },
    {
      "id": "7598ac77-af78-4bfb-9004-787aa3048048",
      "m1": "نیامد جهانآفرین را پسند",
      "m2": "برو تیره شد رای چرخ بلند",
      "verse_number": 25
    },
    {
      "id": "a05a0472-2f22-471a-8864-1728c57d7419",
      "m1": "خزاعه بیامد چو او گشت خاک",
      "m2": "بر رنج و بیداد بدرود پاک",
      "verse_number": 26
    },
    {
      "id": "c726c4de-4108-486d-9984-78e47e18e14b",
      "m1": "حرم تا یمن پاک بر دست اوست",
      "m2": "به دریای مصر اندرون شست اوست",
      "verse_number": 27
    },
    {
      "id": "06bf8c80-2dec-4a74-9736-0b3c9b96f93e",
      "m1": "سر از راه پیچیده و داد نه",
      "m2": "ز یزدان یکی را به دل یاد نه",
      "verse_number": 28
    },
    {
      "id": "f612c27c-79f2-4306-919b-e4bd9ba0f406",
      "m1": "جهانی گرفته به مشت اندرون",
      "m2": "نژاد سماعیل ازو پر ز خون",
      "verse_number": 29
    },
    {
      "id": "db8cc145-0f4f-4347-9e27-c7a8b239a7d4",
      "m1": "سکندر ز نصر این سخنها شنید",
      "m2": "ز تخم خزاعه هرانکس که دید",
      "verse_number": 30
    },
    {
      "id": "f4d448bd-e2c1-4d85-b5ba-04585532af57",
      "m1": "به تن کودکان را نماندش روان",
      "m2": "نماندند زان تخمه کس در جهان",
      "verse_number": 31
    },
    {
      "id": "e85ce6b4-4189-4471-a47d-d505bd7ae573",
      "m1": "ز بیداد بستد حجاز و یمن",
      "m2": "به رای و به مردان شمشیرزن",
      "verse_number": 32
    },
    {
      "id": "dcd860d3-bd48-4bd8-83a2-3693b53d9a98",
      "m1": "نژاد سماعیل را برکشید",
      "m2": "هرانکس که او مهتری را سزید",
      "verse_number": 33
    },
    {
      "id": "bc4895ca-f93c-4595-95a7-55846fa1b504",
      "m1": "پیاده درآمد به بیتالحرام",
      "m2": "سماعیلیان زو شده شادکام",
      "verse_number": 34
    },
    {
      "id": "24a6c1b8-095b-4632-8f31-7a50f292ffb1",
      "m1": "بهر پی که برداشت قیصر ز راه",
      "m2": "همی ریخت دینار گنجور شاه",
      "verse_number": 35
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 35
  },
  "order_in_parent": 19
}