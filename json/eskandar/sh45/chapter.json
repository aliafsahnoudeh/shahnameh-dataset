{
  "id": "4246efeb-8f37-49fc-814b-2b4b5a69c2fd",
  "title": "بخش ۴۵",
  "verses": [
    {
      "id": "78072535-a727-4969-bead-dde714e0f6c0",
      "m1": "چو آمد سکندر به اسکندری",
      "m2": "جهان را دگرگونه شد داوری",
      "verse_number": 1
    },
    {
      "id": "630bb668-d236-4319-b3ef-1f98c9fd226c",
      "m1": "به هامون نهادند صندوق اوی",
      "m2": "زمین شد سراسر پر از گفتوگوی",
      "verse_number": 2
    },
    {
      "id": "83dd1ffb-09ad-4069-bc4e-81d7ad8eeb5b",
      "m1": "به اسکندری کودک و مرد و زن",
      "m2": "به تابوت او بر شدند انجمن",
      "verse_number": 3
    },
    {
      "id": "8ad53a68-9133-46db-9d7b-28d0d356c07e",
      "m1": "اگر برگرفتی ز مردم شمار",
      "m2": "مهندس فزون آمدی صد هزار",
      "verse_number": 4
    },
    {
      "id": "a42ab645-c609-42f7-9e47-8c5473736bf3",
      "m1": "حکیم ارسطالیس پیش اندرون",
      "m2": "جهانی برو دیدگان پر ز خون",
      "verse_number": 5
    },
    {
      "id": "6ecc80a2-fd38-4f8e-8979-502ed512c401",
      "m1": "برآن تنگ صندوق بنهاد دست",
      "m2": "چنین گفت کای شاه یزدان پرست",
      "verse_number": 6
    },
    {
      "id": "713abd01-aa45-42a6-b368-b08dc094e8ef",
      "m1": "کجا آن هش و دانش و رای تو",
      "m2": "که این تنگ تابوت شد جای تو",
      "verse_number": 7
    },
    {
      "id": "8648b0e0-713f-427d-ac28-529242bd88e4",
      "m1": "به روز جوانی برین مایه سال",
      "m2": "چرا خاک را برگزیدی نهال",
      "verse_number": 8
    },
    {
      "id": "fd91b75a-bedb-446b-8ba5-d9720b0d59d1",
      "m1": "حکیمان رومی شدند انجمن",
      "m2": "یکی گفت کای پیل رویینه تن",
      "verse_number": 9
    },
    {
      "id": "d5108f9e-04d0-458e-9239-1a50f11b5d1f",
      "m1": "ز پایت که افگند و جانت که خست",
      "m2": "کجا آن همه حزم و رای و نشست",
      "verse_number": 10
    },
    {
      "id": "8e32fee2-eaf2-4762-9ee6-2c9652efa053",
      "m1": "دگر گفت چندین نهفتی تو زر",
      "m2": "کنون زر دارد تنت را به بر",
      "verse_number": 11
    },
    {
      "id": "5dd862b5-80e2-446d-b10c-122340b5cf89",
      "m1": "دگر گفت کز دست تو کس نرست",
      "m2": "چرا سودی ای شاه با مرگ دست",
      "verse_number": 12
    },
    {
      "id": "1f3242d0-65e5-4229-aacb-5c3bb400e47f",
      "m1": "دگر گفت کسودی از درد و رنج",
      "m2": "هم از جستن پادشاهی و گنج",
      "verse_number": 13
    },
    {
      "id": "60e31c96-c2b1-45f6-ae01-08c298243c82",
      "m1": "دگر گفت چون پیش داور شوی",
      "m2": "همان بر که کشتی همان بدروی",
      "verse_number": 14
    },
    {
      "id": "c4913f39-0e50-4889-a959-576c1d6dce93",
      "m1": "دگر گفت بیدستگاه آن بود",
      "m2": "که ریزندهٔ خون شاهان بود",
      "verse_number": 15
    },
    {
      "id": "716dae63-e222-4755-8f57-f0c4a742a13d",
      "m1": "دگر گفت ما چون تو باشیم زود",
      "m2": "که بودی تو چون گوهر نابسود",
      "verse_number": 16
    },
    {
      "id": "bfa0acaa-fe04-4a98-aa2c-0dd518a5cbac",
      "m1": "دگر گفت چون بیندت اوستاد",
      "m2": "بیاموزد آن چیز کت نیست یاد",
      "verse_number": 17
    },
    {
      "id": "ffe02446-33ef-4f66-9be7-5e0be398b121",
      "m1": "دگر گفت کز مرگ چون تو نرست",
      "m2": "به بیشی سزد گر نیازیم دست",
      "verse_number": 18
    },
    {
      "id": "f632e473-0c3a-4fff-8562-d9f277b4ee04",
      "m1": "دگر گفت کای برتر از ماه و مهر",
      "m2": "چه پوشی همی ز انجمن خوب چهر",
      "verse_number": 19
    },
    {
      "id": "38f60d9d-ca3b-41a2-85e9-ae96e31c4e52",
      "m1": "دگر گفت مرد فراوان هنر",
      "m2": "بکوشد که چهره بپوشد به زر",
      "verse_number": 20
    },
    {
      "id": "ad202ca2-10c3-498a-9c59-cddcb1da8c16",
      "m1": "کنون ای هنرمند مرد دلیر",
      "m2": "ترا زر زرد آوریدست زیر",
      "verse_number": 21
    },
    {
      "id": "20b61bed-50fc-49e8-b512-598793e4a4ed",
      "m1": "دگرگفت دیبا بپوشیدهای",
      "m2": "نپوشیده را نیز رخ دیدهای",
      "verse_number": 22
    },
    {
      "id": "40396ee7-9a57-4479-9ec2-792b8adeb1b1",
      "m1": "کنون سر ز دیبا برآور که تاج",
      "m2": "همی جویدت یاره و تخت عاج",
      "verse_number": 23
    },
    {
      "id": "dde09f05-32a3-4e3a-9fca-f2e6b1927f68",
      "m1": "دگر گفت کز ماهرخ بندگان",
      "m2": "ز چینی و رومی پرستندگان",
      "verse_number": 24
    },
    {
      "id": "f60ba478-30fa-4f47-832a-e4b858d19632",
      "m1": "بریدی و زر داری اندر کنار",
      "m2": "به رسم کیان زر و دیبا مدار",
      "verse_number": 25
    },
    {
      "id": "d6b58102-fff8-4fca-9b66-868daf0140de",
      "m1": "دگر گفت پرسنده پرسد کنون",
      "m2": "چه یاد آیدت پاسخ رهنمون",
      "verse_number": 26
    },
    {
      "id": "d3856c3a-98f7-4171-be4d-38db6322d560",
      "m1": "که خون بزرگان چرا ریختی",
      "m2": "به سختی به گنج اندر آویختی",
      "verse_number": 27
    },
    {
      "id": "48592b77-1005-452a-b983-54b806ad32bf",
      "m1": "خنک آنکسی کز بزرگان بمرد",
      "m2": "ز گیتی جز از نیکنامی نبرد",
      "verse_number": 28
    },
    {
      "id": "bf530e8f-7dad-4276-973b-7e6f2a3c10f8",
      "m1": "دگر گفت روز تو اندرگذشت",
      "m2": "زبانت ز گفتار بیکار گشت",
      "verse_number": 29
    },
    {
      "id": "13aa26db-7fd5-45d6-99c7-999043911969",
      "m1": "هرانکس که او تاج و تخت تو دید",
      "m2": "عنان از بزرگی بباید کشید",
      "verse_number": 30
    },
    {
      "id": "80a419a4-007b-4bd6-841c-93fc92f2b5ed",
      "m1": "که بر کس نماند چو بر تو نماند",
      "m2": "درخت بزرگی چه باید نشاید",
      "verse_number": 31
    },
    {
      "id": "3f395c74-33d8-461a-8d74-ba1bd3f3adcf",
      "m1": "دگر گفت کردار تو بادگشت",
      "m2": "سر سرکشان از تو آزاد گشت",
      "verse_number": 32
    },
    {
      "id": "8de10824-151f-484b-8b07-4de38efaee79",
      "m1": "ببینی کنون بارگاه بزرگ",
      "m2": "جهانی جدا کرده از میش گرگ",
      "verse_number": 33
    },
    {
      "id": "dd32a024-57bf-4090-bcdd-d20cb4411f4c",
      "m1": "دگر گفت کاندر سرای سپنج",
      "m2": "چرا داشتی خویشتن را به رنج",
      "verse_number": 34
    },
    {
      "id": "3d1031b7-7941-408e-bf06-68e2fdea330f",
      "m1": "که بهر تو این آمد از رنج تو",
      "m2": "یکی تنگ تابوت شد گنج تو",
      "verse_number": 35
    },
    {
      "id": "4f9eaa71-7c0c-4df5-a8d4-dc1d31380933",
      "m1": "نجویی همی نالهٔ بوق را",
      "m2": "به سند آمدت بند صندوق را",
      "verse_number": 36
    },
    {
      "id": "61e6cb7c-d93d-4b2b-975b-0f4ebab72433",
      "m1": "دگر گفت چون لشکرت بازگشت",
      "m2": "تو تنها نمانی برین پهن دشت",
      "verse_number": 37
    },
    {
      "id": "4e6253f1-df72-4370-af35-9c30cce09af6",
      "m1": "همانا پس هرکسی بنگری",
      "m2": "فراوان غم زندگانی خوری",
      "verse_number": 38
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 38
  }
}