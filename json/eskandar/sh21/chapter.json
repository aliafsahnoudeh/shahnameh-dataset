{
  "id": "2a166818-d237-4a59-ab88-604c3f53e2e5",
  "title": "بخش ۲۱",
  "verses": [
    {
      "id": "75b2cbd0-9c80-467e-ae69-34e226f86e3e",
      "m1": "سکندر چو بشنید از یادگیر",
      "m2": "بفرمود تا پیش او شد دبیر",
      "verse_number": 1
    },
    {
      "id": "08f5333c-f652-4701-9163-422d2a62a08b",
      "m1": "نوشتند پس نامهای بر حریر",
      "m2": "ز شیراوژن اسکندر شهرگیر",
      "verse_number": 2
    },
    {
      "id": "8fa5f299-568f-4937-a948-63a8a8df3b3e",
      "m1": "به نزدیک قیدافهٔ هوشمند",
      "m2": "شده نام او در بزرگی بلند",
      "verse_number": 3
    },
    {
      "id": "559394ea-6ff8-4d02-826d-4cdae1e9dcb6",
      "m1": "نخست آفرین خداوند مهر",
      "m2": "فروزندهٔ ماه و گردان سپهر",
      "verse_number": 4
    },
    {
      "id": "7e097f20-f79c-4975-b01a-6cddc2378da3",
      "m1": "خداوند بخشنده داد و راست",
      "m2": "فزونی کسی را دهد کش سزاست",
      "verse_number": 5
    },
    {
      "id": "b5ae9c51-f989-4a1b-88bf-780d3b589383",
      "m1": "به تندی نجستیم رزم ترا",
      "m2": "گراینده گشتیم بزم ترا",
      "verse_number": 6
    },
    {
      "id": "c5367358-1a8e-4bb8-8f3e-f34c326e71dc",
      "m1": "چو این نامه آرند نزدیک تو",
      "m2": "درخشان شود رای تاریک تو",
      "verse_number": 7
    },
    {
      "id": "f5b90476-6e3d-45f2-b479-ae485a8ee03a",
      "m1": "فرستی به فرمان ما باژ و ساو",
      "m2": "بدانی که با ما ترا نیست تاو",
      "verse_number": 8
    },
    {
      "id": "979f9c1e-778f-48df-ba04-00163dfcbecd",
      "m1": "خردمندی و پیشبینی کنی",
      "m2": "توانایی و پاک دینی کنی",
      "verse_number": 9
    },
    {
      "id": "17be9639-1b0c-4ba0-bd58-f02023a4f38d",
      "m1": "وگر هیچ تاب اندر آری به کار",
      "m2": "نبینی جز از گردش روزگار",
      "verse_number": 10
    },
    {
      "id": "fca78480-9947-46d1-911b-7d238518082c",
      "m1": "چو اندازه گیری ز دارا و فور",
      "m2": "خود آموزگارت نباید ز دور",
      "verse_number": 11
    },
    {
      "id": "c93a99a8-489e-4c56-b1de-20ae233e8f42",
      "m1": "چو از باد عنوان او گشت خشک",
      "m2": "نهادند مهری بروبر ز مشک",
      "verse_number": 12
    },
    {
      "id": "d58076dd-4988-455e-85a0-bb8e5aa23ae5",
      "m1": "بیامد هیون تگاور به راه",
      "m2": "به فرمان آن نامبردار شاه",
      "verse_number": 13
    },
    {
      "id": "d97fb985-72d1-4dd6-b6b3-7fd0410b792e",
      "m1": "چو قیدافه آن نامهٔ او بخواند",
      "m2": "ز گفتار او در شگفتی بماند",
      "verse_number": 14
    },
    {
      "id": "dbc35754-24d7-4388-9aaa-1f9fdb427190",
      "m1": "به پاسخ نخست آفرین گسترید",
      "m2": "بدان دادگر کو زمین گسترید",
      "verse_number": 15
    },
    {
      "id": "a6f5312a-95f2-4a74-9d46-301b737a9ac2",
      "m1": "ترا کرد پیروز بر فور هند",
      "m2": "به دارا و بر نامداران سند",
      "verse_number": 16
    },
    {
      "id": "669a2b9f-14f9-42c4-a894-8e1440abed13",
      "m1": "مرا با چو ایشان برابر نهی",
      "m2": "به سر بر ز پیروزه افسر نهی",
      "verse_number": 17
    },
    {
      "id": "915ff8bc-34f3-4751-bc9e-1867fdd58756",
      "m1": "مرا زان فزونست فر و مهی",
      "m2": "همان لشکر و گنج شاهنشهی",
      "verse_number": 18
    },
    {
      "id": "a9845072-b99a-4720-aeb8-8b9854e92ce5",
      "m1": "که من قیصران را به فرمان شوم",
      "m2": "بترسم ز تهدید و پیچان شوم",
      "verse_number": 19
    },
    {
      "id": "bceff2b2-aca6-493b-9b4d-09992fa6480d",
      "m1": "هزاران هزارم فزون لشکرست",
      "m2": "که بر هر سری شهریاری سرست",
      "verse_number": 20
    },
    {
      "id": "004290c5-572c-45c1-9374-a41feafaca69",
      "m1": "وگر خوانم از هر سوی زیردست",
      "m2": "نماند برین بوم جای نشست",
      "verse_number": 21
    },
    {
      "id": "d24249b3-2686-485d-bdc3-0b005189b6f6",
      "m1": "یکی گنج در پیش هر مهتری",
      "m2": "چو آید ازین مرز با لشکری",
      "verse_number": 22
    },
    {
      "id": "19d52b15-5d1c-4636-94d7-9ed87ff74a41",
      "m1": "تو چندین چه رانی زبان بر گزاف",
      "m2": "ز دارا شدستی خداوند لاف",
      "verse_number": 23
    },
    {
      "id": "d5d85635-c74d-4d6c-9627-8f2dc8ed2eb6",
      "m1": "بران نامه بر مهر زرین نهاد",
      "m2": "هیونی برافگند بر سان باد",
      "verse_number": 24
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 24
  }
}