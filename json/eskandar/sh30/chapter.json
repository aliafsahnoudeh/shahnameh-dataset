{
  "id": "8133aae8-45de-40cf-8420-2c4c7d128ce0",
  "title": "بخش ۳۰",
  "verses": [
    {
      "id": "ccb6d2ea-2a81-4f19-996c-570d6f301249",
      "m1": "وزان جایگه رفت خورشیدفش",
      "m2": "بیامد دمان تا زمین حبش",
      "verse_number": 1
    },
    {
      "id": "757d8708-f607-41f2-bf86-82a87b75c53f",
      "m1": "ز مردم زمین بود چون پر زاغ",
      "m2": "سیه گشته و چشمها چون چراغ",
      "verse_number": 2
    },
    {
      "id": "4114bbc4-4a63-42d9-bffd-2fea2fa6c802",
      "m1": "تناور یکی لشکری زورمند",
      "m2": "برهنه تن و پوست و بالابلند",
      "verse_number": 3
    },
    {
      "id": "43eee010-bf00-402e-a2e2-82dd786030a6",
      "m1": "چو از دور دیدند گرد سپاه",
      "m2": "خروشی برآمد ز ابر سیاه",
      "verse_number": 4
    },
    {
      "id": "98374157-cd2b-489a-b8a3-4e9abf0cdcda",
      "m1": "سپاه انجمن شد هزاران هزار",
      "m2": "وران تیره شد دیدهٔ شهریار",
      "verse_number": 5
    },
    {
      "id": "7741d49f-f7ed-4525-adf2-156701997f03",
      "m1": "به سوی سکندر نهادند سر",
      "m2": "بکشتند بسیار پرخاشخر",
      "verse_number": 6
    },
    {
      "id": "de8954e9-8265-4f15-8be3-0450efb0c8e1",
      "m1": "به جای سنان استخوان داشتند",
      "m2": "همی بر تن مرد بگذاشتند",
      "verse_number": 7
    },
    {
      "id": "7bb596f6-c34d-47ca-863d-091ed150fb66",
      "m1": "به لشکر بفرمود پس شهریار",
      "m2": "که برداشتند آلت کارزار",
      "verse_number": 8
    },
    {
      "id": "6611b53c-584f-4c58-8404-bf9d0cc56108",
      "m1": "برهنه به جنگ اندر آمد حبش",
      "m2": "غمی گشت زان لشکر شیرفش",
      "verse_number": 9
    },
    {
      "id": "b62745e8-0d51-4b8c-a879-eb79ebfa5e37",
      "m1": "بکشتند زیشان فزون از شمار",
      "m2": "بپیچید دیگر سر از کارزار",
      "verse_number": 10
    },
    {
      "id": "2b1bcb41-0b7e-4c16-9ba9-757f665f0323",
      "m1": "ز خون ریختن گشت روی زمین",
      "m2": "سراسر به کردار دریای چین",
      "verse_number": 11
    },
    {
      "id": "dd8eeb28-643a-424f-9d96-a6fc3bcd0640",
      "m1": "چو از خون در و دشت آلوده شد",
      "m2": "ز کشته به هر جای بر توده شد",
      "verse_number": 12
    },
    {
      "id": "8ab6ea4c-a46d-4b39-8f61-e45508538a7f",
      "m1": "چو بر توده خاشاکها برزدند",
      "m2": "بفرمود تا آتش اندر زدند",
      "verse_number": 13
    },
    {
      "id": "41521203-05ce-4cf0-ac0b-839d8029e9ce",
      "m1": "چو شب گشت بشنید آواز گرگ",
      "m2": "سکندر بپوشید خفتان و ترگ",
      "verse_number": 14
    },
    {
      "id": "da709561-a987-451b-a683-b2de258fb6e8",
      "m1": "یکی پیش رو بود مهتر ز پیل",
      "m2": "به سر بر سرو داشت همرنگ نیل",
      "verse_number": 15
    },
    {
      "id": "65a40d80-21ce-41e1-bbff-b6ff5ae4023e",
      "m1": "ازین نامداران فراوان بکشت",
      "m2": "بسی حمله بردند و ننمود پشت",
      "verse_number": 16
    },
    {
      "id": "5b3afe2d-5d79-4b59-bcb2-64167742ab23",
      "m1": "بکشتند فرجام کارش به تیر",
      "m2": "یکی آهنین کوه بد پیل گیر",
      "verse_number": 17
    },
    {
      "id": "300ed007-81f1-47c7-a09d-2121605dfecb",
      "m1": "وزان جایگه تیز لشکر براند",
      "m2": "بسی نام دادار گیهان بخواند",
      "verse_number": 18
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 18
  },
