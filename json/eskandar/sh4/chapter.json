{
  "id": "3ce73775-a008-49c6-854a-147db6bcbfcb",
  "title": "بخش ۴",
  "verses": [
    {
      "id": "595206f8-24fb-40b8-9258-2098b736e83d",
      "m1": "ز عموریه مادرش را بخواند",
      "m2": "چو آمد سخنهای دارا براند",
      "verse_number": 1
    },
    {
      "id": "a04c408c-a0da-4cbe-ba15-e5bedfe8c7e0",
      "m1": "بدو گفت نزد دلارای شو",
      "m2": "به خوبی به پیوند گفتار نو",
      "verse_number": 2
    },
    {
      "id": "4d69a2ca-1abd-499f-87ca-513cd538f603",
      "m1": "به پرده درون روشنک را ببین",
      "m2": "چو دیدی ز ما کن برو آفرین",
      "verse_number": 3
    },
    {
      "id": "fcc0043d-84aa-4444-8c13-22c26d1f559d",
      "m1": "ببر طوق با یاره و گوشوار",
      "m2": "یکی تاج پر گوهر شاهوار",
      "verse_number": 4
    },
    {
      "id": "34602efa-8d08-41ff-af29-4217d28dc8ab",
      "m1": "صد اشتر ز گستردنیها ببر",
      "m2": "صد اشتر ز هر گونه دیبا به زر",
      "verse_number": 5
    },
    {
      "id": "7679960a-4fb8-4d0d-a38e-95e6bcf702d5",
      "m1": "هم از گنج دینار چو سی هزار",
      "m2": "به بدره درون کن ز بهر نثار",
      "verse_number": 6
    },
    {
      "id": "120a4f69-263b-4d69-b8e3-66d4165b77e5",
      "m1": "ز رومی کنیزک چو سیصد ببر",
      "m2": "دگر هرچ باید همه سر به سر",
      "verse_number": 7
    },
    {
      "id": "ecea7ce4-dfe7-496c-9821-a9c9a8512c14",
      "m1": "یکی جام زر هر یکی را به دست",
      "m2": "بر آیین خوبان خسروپرست",
      "verse_number": 8
    },
    {
      "id": "cc32c017-15f4-41ee-8a51-3265f9c4e204",
      "m1": "ابا خویشتن خادمان بر براه",
      "m2": "ز راه و ز آیین شاهان مکاه",
      "verse_number": 9
    },
    {
      "id": "01a9c7b4-6adb-485f-a59b-b62bb5b3e8e5",
      "m1": "بشد مادر شاه با ترجمان",
      "m2": "ده از فیلسوفان شیرینزبان",
      "verse_number": 10
    },
    {
      "id": "ed6be5c8-d9d4-40c1-97ef-db7d0214262d",
      "m1": "چو آمد به نزدیکی اصفهان",
      "m2": "پذیره شدندش فراوان مهان",
      "verse_number": 11
    },
    {
      "id": "bae52329-3e55-4415-b8e7-08e35770b574",
      "m1": "بیامد ز ایوان دلارای پیش",
      "m2": "خود و نامداران به آیین خویش",
      "verse_number": 12
    },
    {
      "id": "fa94e474-2460-479b-b125-3aa0f2354aff",
      "m1": "به دهلیز کردند چندان نثار",
      "m2": "که بر چشم گنج درم گشت خوار",
      "verse_number": 13
    },
    {
      "id": "58991ec8-2d21-4831-9ec2-55b290289bfb",
      "m1": "به ایوان نشستند با رایزن",
      "m2": "همه نامداران شدند انجمن",
      "verse_number": 14
    },
    {
      "id": "0497d799-7b59-4ce6-890d-ba7d3cb3ef79",
      "m1": "دلارای برداشت چندان جهیز",
      "m2": "که شد در جهان روی بازار تیز",
      "verse_number": 15
    },
    {
      "id": "00ab96e6-287a-428f-b29d-bccf0fb6c1d6",
      "m1": "شتر در شتر رفت فرسنگها",
      "m2": "ز زرین و سیمین وز رنگها",
      "verse_number": 16
    },
    {
      "id": "c63a3d61-ca70-4022-a319-8fe527e9b6a6",
      "m1": "ز پوشیدنی و ز گستردنی",
      "m2": "ز افگندنی و پراگندنی",
      "verse_number": 17
    },
    {
      "id": "7400426f-3cfd-44e5-9ed9-885caa2259a5",
      "m1": "ز اسپان تازی به زرین ستام",
      "m2": "ز شمشیر هندی به زرین نیام",
      "verse_number": 18
    },
    {
      "id": "976580de-2d91-4f8b-9397-8e8acd94165a",
      "m1": "ز خفتان و از خود و برگستوان",
      "m2": "ز گوپال و ز خنجر هندوان",
      "verse_number": 19
    },
    {
      "id": "b71d536f-48d0-443b-b780-dd21b5f7c84c",
      "m1": "چه مایه بریده چه از نابرید",
      "m2": "کسی در جهان بیشتر زان ندید",
      "verse_number": 20
    },
    {
      "id": "446a7048-c222-4018-b3d6-1ef040ce57e4",
      "m1": "ز ایوان پرستندگان خواستند",
      "m2": "چهل مهد زرین بیاراستند",
      "verse_number": 21
    },
    {
      "id": "6d278bde-9b8f-43e5-a937-1a897e470189",
      "m1": "یکی مهد با چتر و با خادمان",
      "m2": "نشست اندرو روشنک شادمان",
      "verse_number": 22
    },
    {
      "id": "1e0e059e-597d-4649-b4ac-69a09cd59cb1",
      "m1": "ز کاخ دلارای تا نیم راه",
      "m2": "درم بود و دینار و اسپ و سپاه",
      "verse_number": 23
    },
    {
      "id": "5432c995-4a2a-4a30-9d03-e86c635d2f93",
      "m1": "ببستند آذین به شهر اندرون",
      "m2": "پر از خنده لبها و دل پر ز خون",
      "verse_number": 24
    },
    {
      "id": "a0fe17d3-b05d-42cd-8e48-40e24d3d58e3",
      "m1": "بران چتر دیبا درم ریختند",
      "m2": "ز بر مشک سارا همی بیختند",
      "verse_number": 25
    },
    {
      "id": "8e17e4ec-7fba-4daa-bc87-fa8a513ffad2",
      "m1": "چو ماه اندر آمد به مشکوی شاه",
      "m2": "سکندر بدو کرد چندی نگاه",
      "verse_number": 26
    },
    {
      "id": "8c768b92-809b-4db9-9bee-2507fe57ec74",
      "m1": "بران برز و بالا و آن خوب چهر",
      "m2": "تو گفتی خرد پروریدش به مهر",
      "verse_number": 27
    },
    {
      "id": "0272ba17-84d0-483d-8e2e-d2e7791f0633",
      "m1": "چو مادرش بر تخت زرین نشاند",
      "m2": "سکندر بروبر همی جان فشاند",
      "verse_number": 28
    },
    {
      "id": "1e1642f2-da89-472f-afd1-a4410b970863",
      "m1": "نشستند یک هفته با او به هم",
      "m2": "همی رای زد شاه بر بیش و کم",
      "verse_number": 29
    },
    {
      "id": "1568cc88-8fd5-4946-b13f-778e68d99c70",
      "m1": "نبد جز بزرگی و آهستگی",
      "m2": "خردمندی و شرم و شایستگی",
      "verse_number": 30
    },
    {
      "id": "c7be485f-34c1-494b-8394-b19de9cab94a",
      "m1": "ببردند ز ایران فراوان نثار",
      "m2": "ز دینار وز گوهر شاهوار",
      "verse_number": 31
    },
    {
      "id": "1fee99a9-ce84-48a8-b21f-51422cccaeaa",
      "m1": "همه شهر ایران و توران و چین",
      "m2": "به شاهی برو خواندند آفرین",
      "verse_number": 32
    },
    {
      "id": "4e1f79b9-41a6-4fd6-9a9f-83feaf7b2f26",
      "m1": "همه روی گیتی پر از داد شد",
      "m2": "به هر جای ویرانی آباد شد",
      "verse_number": 33
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 33
  },
  "order_in_parent": 4
}