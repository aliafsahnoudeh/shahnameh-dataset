{
  "id": "dd38be0c-ef4e-4f17-be3c-51e0754ebb6e",
  "title": "بخش ۲۳",
  "verses": [
    {
      "id": "7d2ef523-b858-4c70-be93-3201edd2a9ca",
      "m1": "جهانجوی ده نامور برگزید",
      "m2": "ز مردان رومی چنانچون سزید",
      "verse_number": 1
    },
    {
      "id": "59eb1ebc-b363-4d20-bd22-4ef31b67518e",
      "m1": "که بودند یکسر همآواز اوی",
      "m2": "نگه داشتندی همه راز اوی",
      "verse_number": 2
    },
    {
      "id": "7dcc2177-2929-4e80-bbd4-9c1fe44db244",
      "m1": "چنین گفت کاکنون به راه اندرون",
      "m2": "مخوانید ما را جز از بیقطون",
      "verse_number": 3
    },
    {
      "id": "8ed078e1-b085-4fa2-9573-9043f165e77a",
      "m1": "همی رفت پیش اندرون قیدروش",
      "m2": "سکندر سپرده بدو چشم و گوش",
      "verse_number": 4
    },
    {
      "id": "edff665f-243f-4db9-b9b9-f2943f52ae44",
      "m1": "چو آتش همی راند مهتر ستور",
      "m2": "به کوهی رسیدند سنگش بلور",
      "verse_number": 5
    },
    {
      "id": "a9f8a98e-dc65-4e4b-ae51-9d4095a40a2b",
      "m1": "بدودر ز هرگونهای میوهدار",
      "m2": "فراوان گیا بود بر کوهسار",
      "verse_number": 6
    },
    {
      "id": "60fdf9d9-8aaa-415f-9142-a49a267a9484",
      "m1": "برفتند زانگونه پویان به راه",
      "m2": "برآن بوم و بر کاندرو بود شاه",
      "verse_number": 7
    },
    {
      "id": "c65fdb7e-0a9b-452d-a515-a4fdcde166df",
      "m1": "چو قیدافه آگه شد از قیدروش",
      "m2": "ز بهر پسر پهن بگشاد گوش",
      "verse_number": 8
    },
    {
      "id": "cd42a12d-7424-4b1d-81cc-f6a5d391b88d",
      "m1": "پذیره شدش با سپاهی گران",
      "m2": "همه نامداران و نیک اختران",
      "verse_number": 9
    },
    {
      "id": "b913dd01-1d4d-4223-95c0-6d09d8042d0c",
      "m1": "پسر نیز چون مادرش را بدید",
      "m2": "پیاده شد و آفرین گسترید",
      "verse_number": 10
    },
    {
      "id": "c6b4dc3e-d497-4b53-897f-fcaec5eb5e59",
      "m1": "بفرمود قیدافه تا برنشست",
      "m2": "همی راند و دستش گرفته به دست",
      "verse_number": 11
    },
    {
      "id": "e8ac7085-ec79-4e12-a954-e2cdf3ab30c2",
      "m1": "بدو قیدروش آنچ دید و شنید",
      "m2": "همی گفت و رنگ رخش ناپدید",
      "verse_number": 12
    },
    {
      "id": "bbc1954e-67c4-49d7-9936-d8cf4e7c4b55",
      "m1": "که بر شهر فریان چه آمد ز رنج",
      "m2": "نماند افسر و تخت و لشکر نه گنج",
      "verse_number": 13
    },
    {
      "id": "fc5e7be5-6854-4a37-b81f-22d3dd693893",
      "m1": "مرا این که آمد همی با عروس",
      "m2": "رها کرد ز اسکندر فیلقوس",
      "verse_number": 14
    },
    {
      "id": "2ae0a156-3160-4f3e-8c7b-a5fc2459c142",
      "m1": "وگرنه بفرمود تا گردنم",
      "m2": "زنند و به آتش بسوزد تنم",
      "verse_number": 15
    },
    {
      "id": "c3595f43-244a-4ff3-9d18-d2c2f94ff2e0",
      "m1": "کنون هرچ باید به خوبی بکن",
      "m2": "برو هیچ مشکن بخواهش سخن",
      "verse_number": 16
    },
    {
      "id": "97e2b805-e8d2-41f8-93e6-b29e9b263056",
      "m1": "چو بشنید قیدافه این از پسر",
      "m2": "دلش گشت زان درد زیر و زبر",
      "verse_number": 17
    },
    {
      "id": "91adb037-7295-4665-8558-b05a0da09ba4",
      "m1": "از ایوان فرستاده را پیش خواند",
      "m2": "به تخت گرانمایگان برنشاند",
      "verse_number": 18
    },
    {
      "id": "d0c4d538-1381-4992-affd-df1c68c26291",
      "m1": "فراوان بپرسید و بنواختش",
      "m2": "یکی مایهور جایگه ساختش",
      "verse_number": 19
    },
    {
      "id": "b3ba4e56-cc6a-4631-999d-e8842e4982f6",
      "m1": "فرستاد هرگونهای خوردنی",
      "m2": "ز پوشیدنی هم ز گستردنی",
      "verse_number": 20
    },
    {
      "id": "e758e31c-8ed3-4c64-9c5e-0d429e23ee47",
      "m1": "بشد آن شب و بامداد پگاه",
      "m2": "به پرسش بیامد به درگاه شاه",
      "verse_number": 21
    },
    {
      "id": "967de84b-4c8c-41fe-aa81-b6798f8e42d6",
      "m1": "پرستندگان پرده برداشتند",
      "m2": "بر اسپش ز درگاه بگذاشتند",
      "verse_number": 22
    },
    {
      "id": "27dd8b59-56a2-40fa-9bf9-bbb30c3e4a25",
      "m1": "چو قیدافه را دید بر تخت عاج",
      "m2": "ز یاقوت و پیروزه بر سرش تاج",
      "verse_number": 23
    },
    {
      "id": "edd444fe-e1eb-49ec-bc62-a0af6a4f6281",
      "m1": "ز زربفت پوشیده چینی قبای",
      "m2": "فراوان پرستنده گردش به پای",
      "verse_number": 24
    },
    {
      "id": "f3ef1a1b-7568-4ac5-b1a1-a10e32b61cef",
      "m1": "رخ شاه تابان به کردار هور",
      "m2": "نشستن گهش را ستونها بلور",
      "verse_number": 25
    },
    {
      "id": "ee5c6eaa-ede1-4885-915f-b59193ff09ca",
      "m1": "زبر پوششی جزع بسته به زر",
      "m2": "برو بافته دانههای گهر",
      "verse_number": 26
    },
    {
      "id": "5a4bf570-515f-412f-96ee-b490f89743d0",
      "m1": "پرستنده با طوق و با گوشوار",
      "m2": "به پای اندر آن گلشن زرنگار",
      "verse_number": 27
    },
    {
      "id": "1f924af1-ccc4-49b9-ae18-cf0089d9c498",
      "m1": "سکندر بدان درشگفتی بماند",
      "m2": "فراوان نهان نام یزدان بخواند",
      "verse_number": 28
    },
    {
      "id": "3d9238ff-4ffe-44fe-b839-527703756da1",
      "m1": "نشستن گهی دید مهتر که نیز",
      "m2": "نیامد ورا روم و ایران به چیز",
      "verse_number": 29
    },
    {
      "id": "c12e2f86-65bb-4cd6-8e46-4f4b9c1b28f6",
      "m1": "بر مهتر آمد زمین داد بوس",
      "m2": "چنانچون بود مردم چاپلوس",
      "verse_number": 30
    },
    {
      "id": "828b874c-b409-4b04-802e-8f7e1f7ba7fd",
      "m1": "ورا دید قیدافه بنواختش",
      "m2": "بپرسید بسیار و بنشاختش",
      "verse_number": 31
    },
    {
      "id": "04b5d83d-4839-4a05-b736-57f52ae90b5a",
      "m1": "چو خورشید تابان ز گنبد بگشت",
      "m2": "گه بار بیگانه اندر گذشت",
      "verse_number": 32
    },
    {
      "id": "1a818efd-199e-4b32-bf89-6767137c8431",
      "m1": "بفرمود تا خوان بیاراستند",
      "m2": "پرستندهٔ رود و می خواستند",
      "verse_number": 33
    },
    {
      "id": "6d846bbf-6647-4c58-a662-a433b974a476",
      "m1": "نهادند یک خانه خوانهای ساج",
      "m2": "همه پیکرش زر و کوکبش عاج",
      "verse_number": 34
    },
    {
      "id": "543f135d-b4ca-4b5a-97b2-6d1fe3296995",
      "m1": "خورشهای بسیار آورده شد",
      "m2": "می آورد و چون خوردنی خورده شد",
      "verse_number": 35
    },
    {
      "id": "b0571292-2226-4c4d-b6d0-238a8c79f0a5",
      "m1": "طبقهای زرین و سیمین نهاد",
      "m2": "نخستین ز قیدافه کردند یاد",
      "verse_number": 36
    },
    {
      "id": "e0aa9e65-d006-408e-ad52-47c5f5335a24",
      "m1": "به می خوردن اندر گرانمایه شاه",
      "m2": "فزون کرد سوی سکندر نگاه",
      "verse_number": 37
    },
    {
      "id": "81ce1b76-624d-4ad1-951d-85ee8be0a548",
      "m1": "به گنجور گفت آن درخشان حریر",
      "m2": "نوشته برو صورت دلپذیر",
      "verse_number": 38
    },
    {
      "id": "68190829-e595-4f7c-a810-e393b6d1c156",
      "m1": "به پیش من آور چنان هم که هست",
      "m2": "به تندی برو هیچ مبسای دست",
      "verse_number": 39
    },
    {
      "id": "9555562f-d96a-45aa-ac41-8182cc25408a",
      "m1": "بیاورد گنجور و بنهاد پیش",
      "m2": "چو دیدش نگه کرد ز اندازه بیش",
      "verse_number": 40
    },
    {
      "id": "5fdf28fb-94c1-4365-b2a6-e430181159d0",
      "m1": "بدانست قیدافه کو قیصرست",
      "m2": "بران لشکر نامور مهترست",
      "verse_number": 41
    },
    {
      "id": "8aaf7d16-b00c-42a9-95f1-b72a3a1e1882",
      "m1": "فرستادهای کرده از خویشتن",
      "m2": "دلیر آمدست اندرین انجمن",
      "verse_number": 42
    },
    {
      "id": "8aa355b1-0ea4-469a-8019-2251c4fb3415",
      "m1": "بدو گفت کای مرد گسترده کام",
      "m2": "بگو تا سکندر چه دادت پیام",
      "verse_number": 43
    },
    {
      "id": "79e3d9e4-abbd-4302-8d3e-014edad8f0ab",
      "m1": "چنین داد پاسخ که شاه جهان",
      "m2": "سخن گفت با من میان مهان",
      "verse_number": 44
    },
    {
      "id": "1575d9c4-5025-400b-af5a-73bbf6ead182",
      "m1": "که قیدافهٔ پاکدل را بگوی",
      "m2": "که جز راستی در زمانه مجوی",
      "verse_number": 45
    },
    {
      "id": "057e64e4-eec0-4c28-8d7e-dc5743ef517c",
      "m1": "نگر سر نپیچی ز فرمان من",
      "m2": "نگه دار بیدار پیمان من",
      "verse_number": 46
    },
    {
      "id": "bc1f7b32-7103-4855-9dfb-452f0adea996",
      "m1": "وگر هیچ تاب اندر آری به دل",
      "m2": "بیارم یکی لشکری دل گسل",
      "verse_number": 47
    },
    {
      "id": "1285f362-96f2-417a-ba7a-727772ff7229",
      "m1": "نشان هنرهای تو یافتم",
      "m2": "به جنگ آمدن تیز نشتافتم",
      "verse_number": 48
    },
    {
      "id": "3a645223-e976-474e-850d-afd7c353b6cd",
      "m1": "خردمندی و شرم نزدیک تست",
      "m2": "جهان ایمن از رای باریک تست",
      "verse_number": 49
    },
    {
      "id": "235ea344-9fc9-4956-809a-d4c9a519d0be",
      "m1": "کنون گر نتابی سر از باژ و ساو",
      "m2": "بدانی که با ما نداری تو تاو",
      "verse_number": 50
    },
    {
      "id": "8052c7af-dceb-4688-a932-845edd1d51c3",
      "m1": "نبینی به جز خوبی و راستی",
      "m2": "چو پیچی سر از کژی و کاستی",
      "verse_number": 51
    },
    {
      "id": "919c1c3a-73cb-47f1-b6fc-5e31c695a545",
      "m1": "برآشفت قیدافه چون این شنید",
      "m2": "بجز خامشی چارهٔ آن ندید",
      "verse_number": 52
    },
    {
      "id": "f69a0921-c973-49f5-b31f-9c2ee436b33a",
      "m1": "بدو گفت کاکنون ره خانه گیر",
      "m2": "بیاسای با مردم دلپذیر",
      "verse_number": 53
    },
    {
      "id": "bf826a96-d20e-4c59-9463-48b35efe10b0",
      "m1": "چو فردا بیایی تو پاسخ دهم",
      "m2": "به بر گشتنت رای فرخ نهم",
      "verse_number": 54
    },
    {
      "id": "98dd1f07-d98e-4b2a-9129-5fff2829e8db",
      "m1": "سکندر بیامد سوی خان خویش",
      "m2": "همه شب همی ساخت درمان خویش",
      "verse_number": 55
    },
    {
      "id": "dcbe4676-2a44-4732-85ec-b52742196eb0",
      "m1": "چو بر زد سر از کوه روشن چراغ",
      "m2": "چو دیبا فروزنده شد دشت و راغ",
      "verse_number": 56
    },
    {
      "id": "431c51fe-381d-4c43-85a3-314b8e0172e4",
      "m1": "سکندر بیامد بران بارگاه",
      "m2": "دو لب پر ز خنده دل از غم تباه",
      "verse_number": 57
    },
    {
      "id": "b67e39e1-cd5d-4b53-9577-163a8c6f4e48",
      "m1": "فرستاده را دید سالار بار",
      "m2": "بپرسید و بردش بر شهریار",
      "verse_number": 58
    },
    {
      "id": "f60dcee8-91af-4042-a996-ee4a708e40ad",
      "m1": "همه کاخ او پر ز بیگانه بود",
      "m2": "نشستن بلورین یکی خانه بود",
      "verse_number": 59
    },
    {
      "id": "58d519e4-41b5-4733-a7e8-c79adbcfe22b",
      "m1": "عقیق و زبرجد بروبر نگار",
      "m2": "میان اندرون گوهر شاهوار",
      "verse_number": 60
    },
    {
      "id": "0ee0d190-0aa1-4112-bde9-08620d6a431a",
      "m1": "زمینش همه صندل و چوب عود",
      "m2": "ز جزع و ز پیروزه او را عمود",
      "verse_number": 61
    },
    {
      "id": "517ac058-007d-4bec-b9aa-0dee58187ee2",
      "m1": "سکندر فروماند زان جایگاه",
      "m2": "ازان فر و اورنگ و آن دستگاه",
      "verse_number": 62
    },
    {
      "id": "fbd6a2bb-7549-48b1-9f57-4c0fd6661901",
      "m1": "همی گفت کاینت سرای نشست",
      "m2": "نبیند چنین جای یزدان پرست",
      "verse_number": 63
    },
    {
      "id": "6dfe4cb9-4bd5-43d3-a8ae-2a64ee692ba1",
      "m1": "خرامان بیامد به نزدیک شاه",
      "m2": "نهادند زرین یکی زیرگاه",
      "verse_number": 64
    },
    {
      "id": "65c8c5f9-a5b6-4b2e-a525-64ec59515eba",
      "m1": "بدو گفت قیدافه ای بیطقون",
      "m2": "چرا خیره ماندی به جزع اندرون",
      "verse_number": 65
    },
    {
      "id": "b1f1a861-b780-4ecc-b45a-ec6ff22c08e5",
      "m1": "همانا که چونین نباشد به روم",
      "m2": "که آسیمه گشتی بدین مایه بوم",
      "verse_number": 66
    },
    {
      "id": "da9acc2c-506f-48e9-a331-71d5f17e5ae8",
      "m1": "سکندر بدو گفت کای شهریار",
      "m2": "تو این خانه را خوارمایه مدار",
      "verse_number": 67
    },
    {
      "id": "c5aade19-75d0-4cf2-ac9d-96d683ddec86",
      "m1": "ز ایوان شاهان سرش برترست",
      "m2": "که ایوان تو معدن گوهرست",
      "verse_number": 68
    },
    {
      "id": "a5719f3e-19b1-4a12-8abd-be6c4b756ea5",
      "m1": "بخندید قیدافه از کار اوی",
      "m2": "دلش گشت خرم به بازار اوی",
      "verse_number": 69
    },
    {
      "id": "8dcc08be-3457-4739-87a1-f0be5afed507",
      "m1": "ازان پس بدر کرد کسهای خویش",
      "m2": "فرستاده را تنگ بنشاند پیش",
      "verse_number": 70
    },
    {
      "id": "5b8395c6-1e45-478e-b967-0544ae82ddb2",
      "m1": "بدو گفت کای زادهٔ فیلقوس",
      "m2": "همت بزم و رزمست و هم نعم و بوس",
      "verse_number": 71
    },
    {
      "id": "2af0fbad-bfa7-4ffc-a311-82b819dece68",
      "m1": "سکندر ز گفتار او گشت زرد",
      "m2": "روان پر ز درد و رخان لاژورد",
      "verse_number": 72
    },
    {
      "id": "7b989546-e5d3-46c1-a4b6-a221ce9a08b4",
      "m1": "بدو گفت کای مهتر پرخرد",
      "m2": "چنین گفتن از تو نه اندر خورد",
      "verse_number": 73
    },
    {
      "id": "b96ab8cd-70e4-4956-b67e-79e3d947461b",
      "m1": "منم بیطقون کدخدای جهان",
      "m2": "چنین تخمهٔ فیلقوسم مخوان",
      "verse_number": 74
    },
    {
      "id": "77e474bb-22ea-4e92-a2cb-545ff128806a",
      "m1": "سپاسم ز یزدان پروردگار",
      "m2": "که با من نبد مهتری نامدار",
      "verse_number": 75
    },
    {
      "id": "9a480b0a-0c62-43f8-ba3e-1119cbddaf64",
      "m1": "که بردی به شاه جهان آگهی",
      "m2": "تنم را ز جان زود کردی تهی",
      "verse_number": 76
    },
    {
      "id": "173cdccc-3d62-4ba2-843e-7c09fe1d5959",
      "m1": "بدو گفت قیدافه کز داوری",
      "m2": "لبت را بپرداز کاسکندری",
      "verse_number": 77
    },
    {
      "id": "13e5a5b9-961c-4a95-9274-1ab0c7854d10",
      "m1": "اگر چهرهٔ خویش بینی به چشم",
      "m2": "ز چاره بیاسای و منمای خشم",
      "verse_number": 78
    },
    {
      "id": "b5b6263b-bcb1-4764-8394-65dea98a69b3",
      "m1": "بیاورد و بنهاد پیشش حریر",
      "m2": "نوشته برو صورت دلپذیر",
      "verse_number": 79
    },
    {
      "id": "7f03ceb2-f076-4f04-ad91-b6245e804765",
      "m1": "که گر هیچ جنبش بدی در نگار",
      "m2": "نبودی جز اسکندر شهریار",
      "verse_number": 80
    },
    {
      "id": "5dbf0379-1aaf-4a75-b8db-9792ed5ad7fc",
      "m1": "سکندر چو دید آن بخایید لب",
      "m2": "برو تیره شد روز چون تیره شب",
      "verse_number": 81
    },
    {
      "id": "38a78882-3568-4e88-99fd-8ac0ba17b448",
      "m1": "چنین گفت بیخنجری در نهان",
      "m2": "مبادا که باشد کس اندر جهان",
      "verse_number": 82
    },
    {
      "id": "7d0527ef-a937-46ac-b667-b4ac1a2f3418",
      "m1": "بدو گفت قیدافه گر خنجرت",
      "m2": "حمایل بدی پیش من بر برت",
      "verse_number": 83
    },
    {
      "id": "45cf3534-e1b5-4153-95a0-fd111fc6c55d",
      "m1": "نه نیروت بودی نه شمشیر تیز",
      "m2": "نه جای نبرد و نه راه گریز",
      "verse_number": 84
    },
    {
      "id": "89f895ad-7ec2-4b98-ad4a-d8824fa6206e",
      "m1": "سکندر بدو گفت هر کز مهان",
      "m2": "به مردی بود خواستار جهان",
      "verse_number": 85
    },
    {
      "id": "105c5d29-e5e5-4145-9266-9d0b5f6e52fa",
      "m1": "نباید که پیچد ز راه گزند",
      "m2": "که بد دل به گیتی نگردد بلند",
      "verse_number": 86
    },
    {
      "id": "81cce807-3b18-4431-8c99-61a4f038d432",
      "m1": "اگر با منستی سلیحم کنون",
      "m2": "همه خانه گشتی چو دریای خون",
      "verse_number": 87
    },
    {
      "id": "91cd84d6-7291-43f4-ab98-6c6aeda1d25e",
      "m1": "ترا کشتمی گر جگرگاه خویش",
      "m2": "بدریدمی پیش بدخواه خویش",
      "verse_number": 88
    }
  ],
