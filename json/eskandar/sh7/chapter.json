{
  "id": "11893a40-0452-4509-9003-a33c29d9941e",
  "title": "بخش ۷",
  "verses": [
    {
      "id": "0bde44b7-4845-4dbe-b4fb-643927be0a3d",
      "m1": "سکندر چو کرد اندر ایران نگاه",
      "m2": "بدانست کو را شد آن تاج و گاه",
      "verse_number": 1
    },
    {
      "id": "644bd938-db29-403a-9421-76481b864465",
      "m1": "همی راه و بیراه لشکر کشید",
      "m2": "سوی کید هندی سپه برکشید",
      "verse_number": 2
    },
    {
      "id": "deff86aa-ab9e-4c8a-9c50-28bed0631fd1",
      "m1": "به جایی که آمد سکندر فراز",
      "m2": "در شارستانها گشادند باز",
      "verse_number": 3
    },
    {
      "id": "0d3d0dae-9eee-4156-89c8-7979127bed04",
      "m1": "ازان مرز کس را به مردم نداشت",
      "m2": "ز ناهید مغفر همی برگذاشت",
      "verse_number": 4
    },
    {
      "id": "51289b56-0de1-4dde-bdea-b0ef5dcf992e",
      "m1": "چو آمد بران شارستان بزرگ",
      "m2": "که میلاد خواندیش کید سترگ",
      "verse_number": 5
    },
    {
      "id": "15fba7a2-ba2d-43a5-bc74-6dd613a5a85e",
      "m1": "بران مرز لشکر فرود آورید",
      "m2": "همه بوم ایشان سپه گسترید",
      "verse_number": 6
    },
    {
      "id": "0c098c44-71d6-410d-ab77-c55cfef8dbb3",
      "m1": "نویسندهٔ نامه را خواندند",
      "m2": "به پیش سکندرش بنشاندند",
      "verse_number": 7
    },
    {
      "id": "42892586-b84b-4020-bb50-d3688f34f01b",
      "m1": "یکی نامه بنوشت نزدیک کید",
      "m2": "چو شیری که ارغنده گردد به صید",
      "verse_number": 8
    },
    {
      "id": "0ea2f928-516c-434a-a226-f572bf4aedce",
      "m1": "ز اسکندر راد پیروزگر",
      "m2": "خداوند شمشیر و تاج و کمر",
      "verse_number": 9
    },
    {
      "id": "5f0226f8-af2a-454b-94b7-f7963561ad42",
      "m1": "سر نامه بود آفرین از نخست",
      "m2": "بدانکس که دل را به دانش بشست",
      "verse_number": 10
    },
    {
      "id": "cbdb43db-4daa-4e14-82e0-b0b461baafc4",
      "m1": "ز کار آن گزیند که بیرنجتر",
      "m2": "چو خواهد که بردارد از گنج بر",
      "verse_number": 11
    },
    {
      "id": "20c88cee-68d0-495d-bec4-9190cb559f97",
      "m1": "گراینده باشد به یزدان پاک",
      "m2": "بدو دارد امید و زو ترس و باک",
      "verse_number": 12
    },
    {
      "id": "344d8998-6339-4dcf-a262-ec24ee7f9c43",
      "m1": "بداند که ما تخت را مایهایم",
      "m2": "جهاندار پیروز را سایهایم",
      "verse_number": 13
    },
    {
      "id": "e27f4051-55c3-4691-a2b9-e2045057c1d2",
      "m1": "نوشتم یکی نامه نزدیک تو",
      "m2": "که روشن کند جان تاریک تو",
      "verse_number": 14
    },
    {
      "id": "d1baaf8f-a4d6-4595-b153-22c38eeddee1",
      "m1": "همآنگه که بر تو بخواند دبیر",
      "m2": "منه پیش و این را سگالش مگیر",
      "verse_number": 15
    },
    {
      "id": "6dc15270-a9c8-4271-b351-0bce2b18fd5b",
      "m1": "اگر شب رسد روشنی را مپای",
      "m2": "هماندر زمان سوی فرمان گرای",
      "verse_number": 16
    },
    {
      "id": "7f9fdde0-e6dc-4fc5-989b-6cad4e1724ff",
      "m1": "وگر بگذری زین سخن نگذرم",
      "m2": "سر و تاج و تختت به پی بسپرم",
      "verse_number": 17
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 17
  }
}