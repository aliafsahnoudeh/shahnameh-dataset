{
  "id": "50e95156-fb33-4462-9970-32a7f39d130a",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "1ae837be-3cd4-4378-8d70-1c869d40416c",
      "m1": "سکندر چو بر تخت بنشست گفت",
      "m2": "که با جان شاهان خرد باد جفت",
      "verse_number": 1
    },
    {
      "id": "fea42c5e-8b17-4d2e-9d93-3a91d32db19a",
      "m1": "که پیروزگر در جهان ایزدست",
      "m2": "جهاندار کز وی نترسد بدست",
      "verse_number": 2
    },
    {
      "id": "a2540f7d-bd79-4dba-8f02-14623573da5f",
      "m1": "بد و نیک هم بگذرد بیگمان",
      "m2": "رهایی نباشد ز چنگ زمان",
      "verse_number": 3
    },
    {
      "id": "666c6f87-499f-4ab4-8ca3-0aea26e1a9a1",
      "m1": "هرانکس که آید بدین بارگاه",
      "m2": "که باشد ز ما سوی ما دادخواه",
      "verse_number": 4
    },
    {
      "id": "57688ca1-2d91-4842-88bc-ae296df8807f",
      "m1": "اگر گاه بار آید ار نیمشب",
      "m2": "به پاسخ رسد چون گشاید دو لب",
      "verse_number": 5
    },
    {
      "id": "b7068ab4-d4d2-4ee6-8690-912ef1673e95",
      "m1": "چو پیروزگر فرهی دادمان",
      "m2": "در بخت پیروز بگشادمان",
      "verse_number": 6
    },
    {
      "id": "4a414221-64c0-4883-b3d1-38dfca380ae2",
      "m1": "همه زیردستان بیابند بهر",
      "m2": "به کوه و بیابان و دریا و شهر",
      "verse_number": 7
    },
    {
      "id": "b745d781-6c5f-437f-b44c-701675d15d0f",
      "m1": "نخواهیم باژ از جهان پنج سال",
      "m2": "جز آنکس که گوید که هستم همال",
      "verse_number": 8
    },
    {
      "id": "9cff5412-eb31-4b8d-b8d4-43bbad94b86b",
      "m1": "به درویش بخشیم بسیار چیز",
      "m2": "ز دارنده چیزی نخواهیم نیز",
      "verse_number": 9
    },
    {
      "id": "e60e8a67-7f3f-4a22-b892-90bb8598ba30",
      "m1": "چو اسکندر این نیکویها بگفت",
      "m2": "دل پادشا گشت با داد جفت",
      "verse_number": 10
    },
    {
      "id": "224630d4-e290-4795-a6fe-5fc458f23638",
      "m1": "ز ایوان برآمد یکی آفرین",
      "m2": "بران دادگر شهریار زمین",
      "verse_number": 11
    },
    {
      "id": "0b28c873-8ece-4e90-90ab-d83a28369ee4",
      "m1": "ازان پس پراگنده شد انجمن",
      "m2": "جهاندار بنشست با رایزن",
      "verse_number": 12
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 12
  },
  "order_in_parent": 1
}