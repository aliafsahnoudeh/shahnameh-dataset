{
  "id": "c4669b4a-2970-4851-9832-e94a74ac6626",
  "title": "بخش ۷",
  "verses": [
    {
      "id": "aa9628a6-9240-4792-9b28-9e28e19c81d4",
      "m1": "چنین گفت رودابه روزی به زال",
      "m2": "که از داغ و سوگ تهمتن بنال",
      "verse_number": 1
    },
    {
      "id": "c90af147-a28a-41e1-8952-087a7a415718",
      "m1": "همانا که تا هست گیتی فروز",
      "m2": "ازین تیرهتر کس ندیدست روز",
      "verse_number": 2
    },
    {
      "id": "799573a1-57da-49c8-914e-f0262575e64c",
      "m1": "بدو گفت زال ای زن کم خرد",
      "m2": "غم ناچریدن بدین بگذرد",
      "verse_number": 3
    },
    {
      "id": "d3986d7b-7c60-4948-8786-a2140858a2f2",
      "m1": "برآشفت رودابه سوگند خورد",
      "m2": "که هرگز نیابد تنم خواب و خورد",
      "verse_number": 4
    },
    {
      "id": "258e453a-856f-4187-b65a-4e586c26074b",
      "m1": "روانم روان گو پیلتن",
      "m2": "مگر باز بیند بران انجمن",
      "verse_number": 5
    },
    {
      "id": "25c06b57-c91e-46a3-acaa-0768fd6c3f32",
      "m1": "ز خوردن یکی هفته تن باز داشت",
      "m2": "که با جان رستم به دل راز داشت",
      "verse_number": 6
    },
    {
      "id": "6054caac-e6f4-4f98-b762-646d1335f4cf",
      "m1": "ز ناخوردنش چشم تاریک شد",
      "m2": "تن نازکش نیز باریک شد",
      "verse_number": 7
    },
    {
      "id": "f5925378-cfbb-4658-ad12-9c6c381cf532",
      "m1": "ز هر سو که رفتی پرستنده چند",
      "m2": "همی رفت با او ز بیم گزند",
      "verse_number": 8
    },
    {
      "id": "48861005-bce5-4e7d-b1ba-967d96c4bebc",
      "m1": "سر هفته را زو خرد دور شد",
      "m2": "ز بیچارگی ماتمش سور شد",
      "verse_number": 9
    },
    {
      "id": "b528a653-5a32-4723-916e-7f0f242625e7",
      "m1": "بیامد به بستان به هنگام خواب",
      "m2": "یکی مرده ماری بدید اندر آب",
      "verse_number": 10
    },
    {
      "id": "6618dbf5-631c-4200-b7f9-ebeea3d305a6",
      "m1": "بزد دست و بگرفت پیچان سرش",
      "m2": "همی خواست کز مار سازد خورش",
      "verse_number": 11
    },
    {
      "id": "61f762fa-8a52-47ed-aa3b-0e06c84dac1c",
      "m1": "پرستنده از دست رودابه مار",
      "m2": "ربود و گرفتندش اندر کنار",
      "verse_number": 12
    },
    {
      "id": "6dd01b3d-c05a-4e7d-9e21-37fc06326f83",
      "m1": "کشیدند از جای ناپاک دست",
      "m2": "به ایوانش بردند و جای نشست",
      "verse_number": 13
    },
    {
      "id": "ef76bea2-a898-40ab-8c44-45d3025bacee",
      "m1": "به جایی که بودیش بنشاختند",
      "m2": "ببردند خوان و خورش ساختند",
      "verse_number": 14
    },
    {
      "id": "71805c0c-6aea-414d-b204-c458a98b0135",
      "m1": "همی خورد هرچیز تا گشت سیر",
      "m2": "فگندند پس جامهٔ نرم زیر",
      "verse_number": 15
    },
    {
      "id": "937b8418-569a-426d-98c4-e813da749a54",
      "m1": "چو باز آمدش هوش با زال گفت",
      "m2": "که گفتار تو با خرد بود جفت",
      "verse_number": 16
    },
    {
      "id": "7e20ab42-295b-497f-b42c-4266e768b561",
      "m1": "هرانکس که او را خور و خواب نیست",
      "m2": "غم مرگ با جشن و سورش یکیست",
      "verse_number": 17
    },
    {
      "id": "51db42ae-a91c-4192-ab8d-49cadcc8138f",
      "m1": "برفت او و ما از پس او رویم",
      "m2": "به داد جهانآفرین بگرویم",
      "verse_number": 18
    },
    {
      "id": "5b23ef17-fd00-467a-b150-8c38b28973d4",
      "m1": "به درویش داد آنچ بودش نهان",
      "m2": "همی گفت با کردگار جهان",
      "verse_number": 19
    },
    {
      "id": "43a603cf-ae67-44c0-b62a-bde3e1a2b9b7",
      "m1": "که ای برتر از نام وز جایگاه",
      "m2": "روان تهمتن بشوی از گناه",
      "verse_number": 20
    },
    {
      "id": "80153715-9b7c-410a-82cb-971ec3deb685",
      "m1": "بدان گیتیش جای ده در بهشت",
      "m2": "برش ده ز تخمی که ایدر بکشت",
      "verse_number": 21
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 21
  }
}