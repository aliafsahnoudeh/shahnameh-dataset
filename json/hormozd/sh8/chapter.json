{
  "id": "9d781a4a-1907-48bc-a406-38978e4bb7f7",
  "title": "بخش ۸",
  "verses": [
    {
      "id": "94c9590f-c3a4-4358-9140-bb4dfaee1817",
      "m1": "برآشفت بهرام و شد شوخ چشم",
      "m2": "زگفتار پرموده آمد بخشم",
      "verse_number": 1
    },
    {
      "id": "c6da397a-bee5-4ee3-b70f-5b8820f4b591",
      "m1": "بتندیش یک تازیانه بزد",
      "m2": "بران سان که از ناسزایان سزد",
      "verse_number": 2
    },
    {
      "id": "72e30d0c-9816-4dbb-9303-b9394c4a17a3",
      "m1": "ببستند هم در زمان پای اوی",
      "m2": "یکی تنگ خرگاه شد جای اوی",
      "verse_number": 3
    },
    {
      "id": "9d80a82a-203e-4095-8915-72366d61b8da",
      "m1": "چو خراد برزین چنان دید گفت",
      "m2": "که این پهلوان را خرد نیست جفت",
      "verse_number": 4
    },
    {
      "id": "38fcbc12-b8d0-4107-bae4-ba7a2577a917",
      "m1": "بیامد بنزد دبیر بزرگ",
      "m2": "بدو گفت کین پهلوان سترگ",
      "verse_number": 5
    },
    {
      "id": "18d725d6-2e75-4dc7-b7db-7e3f721cc476",
      "m1": "بیک پر پشه ندارد خرد",
      "m2": "ازیرا کسی را بکس نشمرد",
      "verse_number": 6
    },
    {
      "id": "5ec3f3b8-00ed-46ee-ab71-e71e34b24864",
      "m1": "ببایدش گفتن کزین چاره نیست",
      "m2": "ورا بتر از خشم پتیاره نیست",
      "verse_number": 7
    },
    {
      "id": "c2643290-664b-406f-a5ef-6e663002eaec",
      "m1": "به نزدیک بهرام رفت آن دو مرد",
      "m2": "زبانها پراز بند و رخ لاژورد",
      "verse_number": 8
    },
    {
      "id": "d1830189-2866-4129-8605-c5ba47c0fcbf",
      "m1": "بگفتند کین رنج دادی بباد",
      "m2": "سر نامور پر ز آتش مباد",
      "verse_number": 9
    },
    {
      "id": "d25cc3de-5da6-41a1-b075-394f916b3ecd",
      "m1": "بدانست بهرام کان بود زشت",
      "m2": "باب اندرافگند و تر گشت خشت",
      "verse_number": 10
    },
    {
      "id": "8bb92bcd-038a-44fd-bdfa-c6e671cf984a",
      "m1": "پشیمان شد وبند او برگرفت",
      "m2": "ز کردار خود دست بر سرگرفت",
      "verse_number": 11
    },
    {
      "id": "b9b9b0dd-40a0-406e-9058-b90a7fa81738",
      "m1": "فرستاد اسبی بزرین ستام",
      "m2": "یکی تیغ هندی بزرین نیام",
      "verse_number": 12
    },
    {
      "id": "460591f1-5077-465e-9e5b-9e43351b9a97",
      "m1": "هم اندر زمان شد به نزدیک اوی",
      "m2": "که روشن کند جان تاریک اوی",
      "verse_number": 13
    },
    {
      "id": "702f2a67-716e-4ec0-82a1-e6e118234929",
      "m1": "همیبود تا او میان را ببست",
      "m2": "یکی بارهٔ تیزتگ برنشست",
      "verse_number": 14
    },
    {
      "id": "44717325-cbea-4d97-a671-b3640df174c5",
      "m1": "سپهبد همیراند با اوبه راه",
      "m2": "بدید آنک تازه نبد روی شاه",
      "verse_number": 15
    },
    {
      "id": "c4e86878-2137-4eda-8b56-af4f3777947e",
      "m1": "بهنگام پدرود کردنش گفت",
      "m2": "که آزار داری ز من درنهفت",
      "verse_number": 16
    },
    {
      "id": "7172bfd7-9696-4f08-b305-29921c57ddd9",
      "m1": "گرت هست با شاه ایران مگوی",
      "m2": "نیاید تو را نزد او آب روی",
      "verse_number": 17
    },
    {
      "id": "5325da9b-baca-45d0-ade9-ed017c3d8495",
      "m1": "بدو گفت خاقان که ما راگله",
      "m2": "زبختست و کردم به یزدان یله",
      "verse_number": 18
    },
    {
      "id": "e76d0361-5739-45f8-9f35-48eab313e9d6",
      "m1": "نه من زان شمارم که از هرکسی",
      "m2": "سخنها همیراند خواهم بسی",
      "verse_number": 19
    },
    {
      "id": "779db8fb-178a-47e9-9fd7-797fa9b5ba41",
      "m1": "اگر شهریار تو زین آگهی",
      "m2": "نیابد نزیبد برو بر مهی",
      "verse_number": 20
    },
    {
      "id": "faf91015-a7b0-4122-b8ae-b21dbbbd725d",
      "m1": "مرا بند گردون گردنده کرد",
      "m2": "نگویم که با من بدی بنده کرد",
      "verse_number": 21
    },
    {
      "id": "f7b0218e-f6bc-46df-b900-c521c1581dcc",
      "m1": "ز گفتار اوگشت بهرام زرد",
      "m2": "بپیچید و خشم از دلیری بخورد",
      "verse_number": 22
    },
    {
      "id": "4587d7b0-4edc-436f-a8ae-c41c5d6f7ca7",
      "m1": "چنین داد پاسخ که آمد نشان",
      "m2": "ز گفتار آن مهتر سرکشان",
      "verse_number": 23
    },
    {
      "id": "b7211fca-efe5-45f4-acb5-64007b8e4eec",
      "m1": "که تخم بدی تا توانی مکار",
      "m2": "چوکاری برت بر دهد روزگار",
      "verse_number": 24
    },
    {
      "id": "61d501b0-f027-4c44-af54-7b7f95946d90",
      "m1": "بدو گفت بهرام کای نامجوی",
      "m2": "سخنها چنین تا توانی مگوی",
      "verse_number": 25
    },
    {
      "id": "90597247-fcb2-4b27-a679-4ebec86a5f06",
      "m1": "چرا من بتو دل بیاراستم",
      "m2": "ز گیتی تو را نیکویی خواستم",
      "verse_number": 26
    },
    {
      "id": "01ca20c5-cb37-46c7-a58a-417bee09b00e",
      "m1": "ز تو نامه کردم بشاه جهان",
      "m2": "همی زشت تو داشتم در نهان",
      "verse_number": 27
    },
    {
      "id": "08238ce4-9563-41c8-9d74-6589b8d132ea",
      "m1": "بدو گفت خاقان که آن بد گذشت",
      "m2": "گذشته سخنها همه باد گشت",
      "verse_number": 28
    },
    {
      "id": "35c81556-b163-4df1-8ab3-d49dc264aa28",
      "m1": "ولیکن چو در جنگ خواری بود",
      "m2": "گه آشتی بردباری بود",
      "verse_number": 29
    },
    {
      "id": "e124cee4-3adc-4cf2-b76a-ec7f52848c09",
      "m1": "تو راخشم با آشتی گر یکیست",
      "m2": "خرد بیگمان نزد تواندکیست",
      "verse_number": 30
    },
    {
      "id": "fe7468b4-b5b1-4540-85b7-dc705e51f58d",
      "m1": "چو سالار راه خداوند خویش",
      "m2": "بگیرد نیفتد بهرکار پیش",
      "verse_number": 31
    },
    {
      "id": "66f934da-a5b7-483f-95e0-eb860bd664a3",
      "m1": "همان راه یزدان بباید سپرد",
      "m2": "ز دل تیرگیها بباید سترد",
      "verse_number": 32
    },
    {
      "id": "851d04ba-4396-4ad4-9cc7-0cb989f684e3",
      "m1": "سخن گر نیفزایی اکنون رواست",
      "m2": "که آن بد که شد گشت با باد راست",
      "verse_number": 33
    },
    {
      "id": "93ec32b4-9b4f-4d23-b9d2-3e388fac5195",
      "m1": "زخاقان چوبشنید بهرام گفت",
      "m2": "که پنداشتم کین بماند نهفت",
      "verse_number": 34
    },
    {
      "id": "38701b87-aa9d-4144-bdfb-7a52afe434d1",
      "m1": "کنون زان گنه گر بیاید زیان",
      "m2": "نپوشم برو چادر پرنیان",
      "verse_number": 35
    },
    {
      "id": "73c1ec09-cdce-4888-84e5-fe1fbec0dfc6",
      "m1": "چوآنجارسی هرچ باید بگوی",
      "m2": "نه زان مر مراکم شود آب روی",
      "verse_number": 36
    },
    {
      "id": "e30c72f9-5d3e-4d4d-802e-14f2cf1f3562",
      "m1": "بدو گفت خاقان که هرشهریار",
      "m2": "که ازنیک وبد برنگیرد شمار",
      "verse_number": 37
    },
    {
      "id": "aad035f7-af50-477a-8cc4-97a0b565913b",
      "m1": "ببد کردن بنده خامش بود",
      "m2": "برمن چنان دان که بیهش بود",
      "verse_number": 38
    },
    {
      "id": "e18d5cb4-ed26-4515-9bc2-e283768f338c",
      "m1": "چواز دور بیند ورا بدسگال",
      "m2": "وگر نیک خواهی بود گر همال",
      "verse_number": 39
    },
    {
      "id": "e2677ec1-eaae-4094-b076-e832f62eff26",
      "m1": "تو را ناسزا خواند وسرسبک",
      "m2": "ورا شاه ایران ومغزی تنگ",
      "verse_number": 40
    },
    {
      "id": "caca192e-e22c-4cc5-897b-42c5462299c4",
      "m1": "بجوشید بهرام وشد زردروی",
      "m2": "نگه کرد خراد برزین بروی",
      "verse_number": 41
    },
    {
      "id": "87ae800e-666e-4195-9802-b8a709026ec1",
      "m1": "بترسید زان تیزخونخوار مرد",
      "m2": "که اورا زباد اندرآرد بگرد",
      "verse_number": 42
    },
    {
      "id": "f6e7f889-9b55-4009-9a4a-3def0314a5cb",
      "m1": "ببهرام گفت ای سزاوار گاه",
      "m2": "بخور خشم وسر بازگردان ز راه",
      "verse_number": 43
    },
    {
      "id": "8e752184-decb-403d-a990-4a3c52925bc1",
      "m1": "که خاقان همی راست گوید سخن",
      "m2": "توبنیوش واندیشه بدمکن",
      "verse_number": 44
    },
    {
      "id": "7e658c34-fc2a-4878-934c-4aadec6794d7",
      "m1": "سخن گر نرفتی بدین گونه سرد",
      "m2": "تو را نیستی دل پرآزار و درد",
      "verse_number": 45
    },
    {
      "id": "7f3830d5-b0eb-4253-8f60-c0e452cc317e",
      "m1": "بدو گفت کین بدرگ بیهنر",
      "m2": "بجوید همی خاک وخون پدر",
      "verse_number": 46
    },
    {
      "id": "d2511fb3-519f-4c5b-90c6-7b2512074668",
      "m1": "بدو گفت خاقان که این بد مکن",
      "m2": "بتیزی بزرگی بگردد کهن",
      "verse_number": 47
    },
    {
      "id": "1e9a88b1-9321-4816-82c4-8433258da3d7",
      "m1": "بگیتی هرآنکس که او چون تو بود",
      "m2": "سرش پر ز گرد ودلش پر ز دود",
      "verse_number": 48
    },
    {
      "id": "dc24b8e7-8d65-426a-99da-a8f41b342ee6",
      "m1": "همه بد سگالید وباکس نساخت",
      "m2": "بکژی ونابخردی سر فراخت",
      "verse_number": 49
    },
    {
      "id": "977c7d69-6c34-4c4c-a0da-24dab2f5c601",
      "m1": "همی ازشهنشاه ترسانییم",
      "m2": "سزا زو بود رنج وآسانییم",
      "verse_number": 50
    },
    {
      "id": "82bcb5c7-17c0-4e16-9689-6882493dba4a",
      "m1": "زگردنکشان اوهمال منست",
      "m2": "نه چون بنده اوبدسگال منست",
      "verse_number": 51
    },
    {
      "id": "d3c20acf-de65-49e6-8273-310d10dba170",
      "m1": "هشیوار وآهسته و با نژاد",
      "m2": "بسی نامبردار دارد بیاد",
      "verse_number": 52
    },
    {
      "id": "e65b2314-ca4b-477d-9462-f3ebc01277ea",
      "m1": "به جان و سرشاه ایران سپاه",
      "m2": "کز ایدر کنون بازگردی به راه",
      "verse_number": 53
    },
    {
      "id": "35a05c2b-03f5-478a-af65-f55ecabde33a",
      "m1": "بپاسخ نیفزایی وبدخوی",
      "m2": "نگویی سخن نیز تا نشنوی",
      "verse_number": 54
    },
    {
      "id": "fad7532a-a041-4e1e-aae2-20e3a5e44677",
      "m1": "چوبشنید بهرام زوگشت باز",
      "m2": "بلشکر گه آمد گو رزمساز",
      "verse_number": 55
    },
    {
      "id": "4120a8ad-6622-4e74-b0db-e87f8fb7db90",
      "m1": "چو خراد برزین وآن بخردان",
      "m2": "دبیر بزرگ ودگر موبدان",
      "verse_number": 56
    },
    {
      "id": "26921ceb-e64b-49be-ac9c-d91477dc0c33",
      "m1": "نبشتند نامه بشاه جهان",
      "m2": "سخن هرچ بد آشکار ونهان",
      "verse_number": 57
    },
    {
      "id": "b3412451-4fe5-41ad-aabe-7b796fb9c72c",
      "m1": "سپهدار با موبد موبدان",
      "m2": "بخشم آن زمان گفت کای بخردان",
      "verse_number": 58
    },
    {
      "id": "831badcd-8afb-45ab-ae77-d472e08317cb",
      "m1": "هم اکنون از ایدر بدز درشوید",
      "m2": "بکوشید و با باد همبر شوید",
      "verse_number": 59
    },
    {
      "id": "15cf8991-a8ef-4be2-8b67-2c3fb23bda5b",
      "m1": "بدز بر ببیند تا خواسته",
      "m2": "چه مایه بود گنج آراسته",
      "verse_number": 60
    },
    {
      "id": "33f0c15c-60fc-43ca-91bd-497c2786a4a0",
      "m1": "دبیران برفتند دل پرهراس",
      "m2": "ز شبگیر تاشب گذشته سه پاس",
      "verse_number": 61
    },
    {
      "id": "5118e019-f3d7-4f63-ad80-3c99af676801",
      "m1": "سیه شد بسی یازگار از شمار",
      "m2": "نبشته نشد هم بفرجام کار",
      "verse_number": 62
    },
    {
      "id": "318e9f01-1231-4631-b509-0fc9d3ba6b1b",
      "m1": "بدز بر نبد راه زان خواسته",
      "m2": "گذشته بدو سال و ناکاسته",
      "verse_number": 63
    },
    {
      "id": "c30ff66e-f413-46bc-9580-ea43dc6f0364",
      "m1": "ز هنگام ارجاسب و افراسیاب",
      "m2": "ز دینار و گوهر که خیزد ز آب",
      "verse_number": 64
    },
    {
      "id": "920d2169-09ee-4a55-bfdc-cd779f7864c7",
      "m1": "همان نیز چیزی که کانی بود",
      "m2": "کجا رستنش آسمانی بود",
      "verse_number": 65
    },
    {
      "id": "c8c9f76d-282f-445c-8a63-40393875929d",
      "m1": "همه گنجها اندر آورده بود",
      "m2": "کجا نام او در جهان برده بود",
      "verse_number": 66
    },
    {
      "id": "2a58ccbb-42fa-468a-a223-581472ef1497",
      "m1": "زچیز سیاوش نخستین کمر",
      "m2": "بهرمهرهای در سه یاره گهر",
      "verse_number": 67
    },
    {
      "id": "15e6aff3-78eb-4362-bc0a-8f3195aa275c",
      "m1": "همان گوشوارش که اندر جهان",
      "m2": "کسی را نبود ازکهان ومهان",
      "verse_number": 68
    },
    {
      "id": "45b2db4e-0544-4dda-b778-72f78d8194ff",
      "m1": "که کیخسرو آن رابه لهراسب داد",
      "m2": "که لهراسب زان پس بگشتاسب داد",
      "verse_number": 69
    },
    {
      "id": "f9ab3143-8ef4-4123-b17a-458ed06dd493",
      "m1": "که ارجاسب آن را بدز درنهاد",
      "m2": "که هنگام آنکس ندارد بیاد",
      "verse_number": 70
    },
    {
      "id": "0a087fca-db01-446a-a763-47345c572e9e",
      "m1": "شمارش ندانست کس در جهان",
      "m2": "ستاره شناسان و فرخ مهان",
      "verse_number": 71
    },
    {
      "id": "2da0d718-2b81-4fcc-9054-dfaaeac5016a",
      "m1": "نبشتند یک یک همه خواسته",
      "m2": "که بود اندر آن گنج آراسته",
      "verse_number": 72
    },
    {
      "id": "effdcec5-16fb-46f0-a8c6-5b405d2ded94",
      "m1": "فرستاد بهرام مردی دبیر",
      "m2": "سخن گوی و روشن دل و یادگیر",
      "verse_number": 73
    },
    {
      "id": "82f73df0-b592-447b-ae74-a94111f622fc",
      "m1": "بیامد همه خواسته گرد کرد",
      "m2": "که بد در دز وهم به دشت نبرد",
      "verse_number": 74
    },
    {
      "id": "26be757d-2b32-431a-8296-25a9efb663e7",
      "m1": "ابا خواسته بود دو گوشوار",
      "m2": "دو موزه درو بود گوهرنگار",
      "verse_number": 75
    },
    {
      "id": "be972f9c-d91f-4665-8c92-1b073516680a",
      "m1": "همان شوشه زر وبرو بافته",
      "m2": "بگوهر سر شوشه برتافته",
      "verse_number": 76
    },
    {
      "id": "0d7b3341-ba91-42cf-b85d-9bb3e847c051",
      "m1": "دو برد یمانی همه زربفت",
      "m2": "بسختند هر یک بمن بود هفت",
      "verse_number": 77
    },
    {
      "id": "b529fd0d-c562-4529-8d41-a5d0b6c763d2",
