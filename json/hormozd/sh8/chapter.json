      "id": "474ef70a-0a66-4976-aa25-0bb5ff4abea2",
      "m1": "همیبود دربلخ چندی دژم",
      "m2": "زکرده پشیمان ودل پر زغم",
      "verse_number": 155
    },
    {
      "id": "9d51cd07-e26c-4abc-9d8b-6a7af12b4f79",
      "m1": "جهاندار زو هم نه خشنودبود",
      "m2": "زتیزی روانش پراز دود بود",
      "verse_number": 156
    },
    {
      "id": "dbd26280-18f4-4815-a4ce-2e45c5b9f99e",
      "m1": "از آزار خاقان چینی نخست",
      "m2": "که بهرام آزار او را بجست",
      "verse_number": 157
    },
    {
      "id": "562fd387-0203-4e76-adef-b84e4be6b515",
      "m1": "دگر آنک چیزی که فرمان نبود",
      "m2": "ببرداشتن چون دلیری نمود",
      "verse_number": 158
    },
    {
      "id": "cd6f0bc3-46f1-490b-b74b-7e28f0dfe2d1",
      "m1": "یکی نامه بنوشت پس شهریار",
      "m2": "ببهرام کای دیو ناسازگار",
      "verse_number": 159
    },
    {
      "id": "c658a8fa-9e99-4acd-8d2c-a13b1fe76fb3",
      "m1": "ندانی همی خویشتن راتوباز",
      "m2": "چنین رابزرگان شدی بینیاز",
      "verse_number": 160
    },
    {
      "id": "1a60d6f9-a7ab-46aa-ba60-b73592abc6da",
      "m1": "هنرها ز یزدان نبینی همی",
      "m2": "به چرخ فلک برنشینی همی",
      "verse_number": 161
    },
    {
      "id": "c62932c6-6770-442e-9d6c-8bdecffb356b",
      "m1": "زفرمان من سربپیچیدهای",
      "m2": "دگرگونه کاری بسیجیدهای",
      "verse_number": 162
    },
    {
      "id": "962a89a5-aacc-4825-afec-5c669973fab4",
      "m1": "نیاید همی یادت از رنج من",
      "m2": "سپاه من و کوشش وگنج من",
      "verse_number": 163
    },
    {
      "id": "e73494ad-a31c-4f17-aac1-29575742cc4d",
      "m1": "ره پهلوانان نسازی همی",
      "m2": "سرت به آسمان برفرازی همی",
      "verse_number": 164
    },
    {
      "id": "1cbf01ae-feac-48ea-a8d0-b81d624bae6c",
      "m1": "کنون خلعت آمد سزاوار تو",
      "m2": "پسندیده و در خور کار تو",
      "verse_number": 165
    },
    {
      "id": "c8f4f8d6-b396-4418-a750-fe1b060a39c0",
      "m1": "چوبنهاد برنامه برمهرشاه",
      "m2": "بفرمود تا دو کدانی سیاه",
      "verse_number": 166
    },
    {
      "id": "5b550966-471f-4031-916e-0a45cf60337c",
      "m1": "بیارند با دوک و پنبه دروی",
      "m2": "نهاده بسی ناسزا رنگ وبوی",
      "verse_number": 167
    },
    {
      "id": "85b736d5-f8b0-4a0d-bfff-7526457536bb",
      "m1": "هم از شعر پیراهن لاژورد",
      "m2": "یکی سرخ مقناع و شلوار زرد",
      "verse_number": 168
    },
    {
      "id": "5af75b0a-0650-4374-8d30-3b20852c86d0",
      "m1": "فرستاده پر منش برگزید",
      "m2": "که آن خلعت ناسزا را سزید",
      "verse_number": 169
    },
    {
      "id": "89a95c22-fb31-4f9c-b9e9-f25a3e33e735",
      "m1": "بدو گفت کاین پیش بهرام بر",
      "m2": "بگو ای سبک مایه بیهنر",
      "verse_number": 170
    },
    {
      "id": "d260815b-9fbd-4fbe-8e9f-35e35f97e424",
      "m1": "توخاقان چین را ببندی همی",
      "m2": "گزند بزرگان پسندی همی",
      "verse_number": 171
    },
    {
      "id": "054a7455-ea91-4141-a8d0-07358f6255d2",
      "m1": "زتختی که هستی فرود آرمت",
      "m2": "ازین پس بکس نیزنشمارمت",
      "verse_number": 172
    },
    {
      "id": "ce563696-1172-447f-8d63-d51ecb1b46c5",
      "m1": "فرستاده با خلعت آمد چوباد",
      "m2": "شنیده سخنها همه کرد یاد",
      "verse_number": 173
    },
    {
      "id": "4f573e0e-ee5b-49db-a7c4-13eb18d754ee",
      "m1": "چو بهرام با نامه خلعت بدید",
      "m2": "شکیبایی وخامشی برگزید",
      "verse_number": 174
    },
    {
      "id": "88889fe2-2323-4ebc-8d1f-80ded01bc68b",
      "m1": "همیگفت کینست پاداش من",
      "m2": "چنین از پی شاه پرخاش من",
      "verse_number": 175
    },
    {
      "id": "51ded0a0-50cd-4d26-87ea-6a1902e19570",
      "m1": "چنین بد ز اندیشه شاه نیست",
      "m2": "جز ار ناسزا گفت بدخواه نیست",
      "verse_number": 176
    },
    {
      "id": "da8e3f71-a688-47fa-aef9-60aaa2d7cfab",
      "m1": "که خلعت ازینسان فرستد بمن",
      "m2": "بدان تا ببینند هر انجمن",
      "verse_number": 177
    },
    {
      "id": "274d8220-1239-4ae8-a42b-13d28e4887d5",
      "m1": "جهاندار بر بندگان پادشاست",
      "m2": "اگر مر مرا خوار گیرد رواست",
      "verse_number": 178
    },
    {
      "id": "38649e19-5e51-436c-af66-509471351b3d",
      "m1": "گمانی نبردم که نزدیک شاه",
      "m2": "بداندیشگان تیز یابند راه",
      "verse_number": 179
    },
    {
      "id": "1a9f7018-673d-424b-9a75-5b4ea78d39e6",
      "m1": "ولیکن چوهرمز مرا خوار کرد",
      "m2": "به گفتار آهرمنان کارکرد",
      "verse_number": 180
    },
    {
      "id": "5e4db4b8-ad16-49e7-90b1-bcd3275f0f4c",
      "m1": "زشاه جهان اینچنین کارکرد",
      "m2": "نزیبد به پیش خردمند مرد",
      "verse_number": 181
    },
    {
      "id": "d9d51eb0-d1b3-4095-b8b1-eac361eae5a4",
      "m1": "ازان پس که با خار مایه سپاه",
      "m2": "بتندی برفتم زدرگاه شاه",
      "verse_number": 182
    },
    {
      "id": "622436b3-7f4d-40a9-98af-4f6d7caefc67",
      "m1": "همه دیدهاند آنچ من کردهام",
      "m2": "غم و رنج وسختی که من بردهام",
      "verse_number": 183
    },
    {
      "id": "4e97ba31-3a23-4979-93d0-433df41f2001",
      "m1": "چوپاداش آن رنج خواری بود",
      "m2": "گر ازبخت ناسازگاری بود",
      "verse_number": 184
    },
    {
      "id": "29264cd0-4bdf-4e5d-a52c-f400adf9707f",
      "m1": "به یزدان بنالم ز گردان سپهر",
      "m2": "که از من چنین پاک بگسست مهر",
      "verse_number": 185
    },
    {
      "id": "41c8a296-a30b-47af-a334-b50568965dc7",
      "m1": "زدادار نیکی دهش یاد کرد",
      "m2": "بپوشید پس جامهٔ سرخ و زرد",
      "verse_number": 186
    },
    {
      "id": "05b17698-dfcc-4184-b22e-7735cb309304",
      "m1": "به پیش اندرون دوکدان سیاه",
      "m2": "نهاده هرآنچش فرستاد شاه",
      "verse_number": 187
    },
    {
      "id": "cb24dbeb-0b91-4f91-8d31-c670f8b7ba27",
      "m1": "بفرمود تا هرک بود ازمهان",
      "m2": "ازان نامداران شاه جهان",
      "verse_number": 188
    },
    {
      "id": "f51217a4-f624-4c0d-a862-35b9d03c9154",
      "m1": "زلشکر برفتند نزدیک اوی",
      "m2": "پراندیشه بد جان تاریک اوی",
      "verse_number": 189
    },
    {
      "id": "53d09dc4-c732-4538-a00f-c81011c494c2",
      "m1": "چورفتند و دیدند پیر وجوان",
      "m2": "بران گونه آن پوشش پهلوان",
      "verse_number": 190
    },
    {
      "id": "0afb7c5b-75a1-4bbe-a87b-f21592f843a1",
      "m1": "بماندند زان کار یکسر شگفت",
      "m2": "دل هرکس اندیشهای برگرفت",
      "verse_number": 191
    },
    {
      "id": "217ee12c-0670-455a-b46c-38cbb324e373",
      "m1": "چنین گفت پس پهلوان با سپاه",
      "m2": "که خلعت بدین سان فرستاد شاه",
      "verse_number": 192
    },
    {
      "id": "6a59ffbd-7c24-49c1-bd0e-38032604ca3d",
      "m1": "جهاندار شاهست وما بندهایم",
      "m2": "دل و جان به مهر وی آگندهایم",
      "verse_number": 193
    },
    {
      "id": "f64cc33a-cfe8-46d8-a74f-d4bc9d287344",
      "m1": "چه بینید بینندگان اندرین",
      "m2": "چه گوییم با شهریار زمین",
      "verse_number": 194
    },
    {
      "id": "19a97bcf-66b6-4d12-9beb-c46d4b3b4341",
      "m1": "بپاسخ گشادند یکسر زبان",
      "m2": "کهای نامور پرهنر پهلوان",
      "verse_number": 195
    },
    {
      "id": "9d3e2231-0867-427a-a407-39d13742222b",
      "m1": "چو ارج تو اینست نزدیک شاه",
      "m2": "سگانند بر بارگاهش سپاه",
      "verse_number": 196
    },
    {
      "id": "574c41f8-b599-4aa7-b85a-25ced172c0f7",
      "m1": "نگر تا چه گفت آن خردمند پیر",
      "m2": "به ری چون دلش تنگ شد ز اردشیر",
      "verse_number": 197
    },
    {
      "id": "f0a442cb-2e39-40f8-b9b1-5b827a332bae",
      "m1": "سری پر زکینه دلی پر زدرد",
      "m2": "زبان و روان پر زگفتار سرد",
      "verse_number": 198
    },
    {
      "id": "6dd89f70-ae45-4e05-b925-63d34a6ab171",
      "m1": "بیامد دمان تا باصطخر پارس",
      "m2": "که اصطخر بد بر زمین فخر پارس",
      "verse_number": 199
    },
    {
      "id": "4719e922-93a0-4197-b75b-7020d57562d8",
      "m1": "که بیزارم از تخت وز تاج شاه",
      "m2": "چونیک وبد من ندارد نگاه",
      "verse_number": 200
    },
    {
      "id": "b7b5e098-3ac8-4ea9-8a4a-2b580047bbd6",
      "m1": "بدو گفت بهرام کین خود مگوی",
      "m2": "که از شاه گیرد سپاه آبروی",
      "verse_number": 201
    },
    {
      "id": "a34576e6-96ec-4c9b-a5f9-7c82f51db007",
      "m1": "همه سر به سر بندگان وییم",
      "m2": "دهندهست وخواهندگان وییم",
      "verse_number": 202
    },
    {
      "id": "1652e3b8-af46-4deb-87a2-a0f1067cbf51",
      "m1": "چنین یافت پاسخ ز ایرانیان",
      "m2": "که ماخود نبندیم زین پس میان",
      "verse_number": 203
    },
    {
      "id": "8af3f4cc-cc61-4633-adb4-9fb3c35555bd",
      "m1": "به ایران کس اورا نخوانیم شاه",
      "m2": "نه بهرام را پهلوان سپاه",
      "verse_number": 204
    },
    {
      "id": "ae9c90f8-6311-41fe-b080-23ed3519fd41",
      "m1": "بگفتند وز پیش بیرون شدند",
      "m2": "ز کاخ همایون به هامون شدند",
      "verse_number": 205
    },
    {
      "id": "c4766f1e-3eb4-4fea-bb07-1bdf808a3214",
      "m1": "سپهبد سپه را همیداد پند",
      "m2": "همیداشت با پند لب را ببند",
      "verse_number": 206
    },
    {
      "id": "1904bbaf-a5e8-4aff-922c-b27c119d354d",
      "m1": "چنین تا دوهفته برین برگذشت",
      "m2": "سپهبد ز ایوان بیامد به دشت",
      "verse_number": 207
    },
    {
      "id": "afbd77dd-dacc-4ad9-a6d0-b68b3f7870da",
      "m1": "یکی بیشه پیش آمدش پر درخت",
      "m2": "سزاوار میخوارهٔ نیکبخت",
      "verse_number": 208
    },
    {
      "id": "445f27c6-5f2d-46fc-a845-6c6bc31d27ce",
      "m1": "یکی گور دید اندر آن مرغزار",
      "m2": "کزان خوبتر کس نبیند نگار",
      "verse_number": 209
    },
    {
      "id": "fb39cf52-9fbd-47cb-8f56-9c18503e6c1b",
      "m1": "پس اندر همیراند بهرام نرم",
      "m2": "برو بارگی را نکرد ایچ گرم",
      "verse_number": 210
    },
    {
      "id": "24f3010d-8f51-4330-8eba-4dfd77c20176",
      "m1": "بدان بیشه در جای نخچیرگاه",
      "m2": "به پیش اندر آمد یکی تنگ راه",
      "verse_number": 211
    },
    {
      "id": "4f2998c4-82bd-4220-a9e8-0224d8fdfd27",
      "m1": "ز تنگی چو گور ژیان برگذشت",
      "m2": "بیابان پدید آمد و راغ ودشت",
      "verse_number": 212
    },
    {
      "id": "94a5c5b7-5ebb-46ec-a0da-779d8e07e030",
      "m1": "گرازنده بهرام و تازنده گور",
      "m2": "ز گرمای آن دشت تفسیده هور",
      "verse_number": 213
    }
  ],
