      "m1": "دو هفته بدین بارگاه مهی",
      "m2": "نیامد ز بهرام هیچ آگهی",
      "verse_number": 154
    },
    {
      "id": "1c467295-29b9-4404-b993-c7fc95c68911",
      "m1": "چه گویید ازین پس چه شاید بدن",
      "m2": "بباید بدین داستانها زدن",
      "verse_number": 155
    },
    {
      "id": "4cbf2d64-5bb9-4672-9dc7-80d4d940a053",
      "m1": "همانگه که گفت این سخن شهریار",
      "m2": "بیامد ز درگاه سالار بار",
      "verse_number": 156
    },
    {
      "id": "57bfcf20-a002-41b6-bcbb-00a172d957b9",
      "m1": "شهنشاه را زان سخن مژده داد",
      "m2": "که جاوید بادا جهاندار شاد",
      "verse_number": 157
    },
    {
      "id": "ec947e96-5ce2-4a27-aff2-e7f1a8adddd6",
      "m1": "که بهرام بر ساوه پیروز گشت",
      "m2": "به رزم اندرون گیتی افروز گشت",
      "verse_number": 158
    },
    {
      "id": "d7872f77-0ecc-430e-8833-25b8e2e6e2b3",
      "m1": "سبک مرد بهرام را پیش خواند",
      "m2": "وزان نامدارانش برتر نشاند",
      "verse_number": 159
    },
    {
      "id": "a0e64292-e4d9-412e-a48a-1b1ee6940cad",
      "m1": "فرستاده گفت ای سر افراز شاه",
      "m2": "به کام تو شد کام آن رزمگاه",
      "verse_number": 160
    },
    {
      "id": "51292c3d-8912-4032-bc70-6d05a3d4b5a3",
      "m1": "انوشه بدی شاد و رامشپذیر",
      "m2": "که بخت بد اندیش توگشت پیر",
      "verse_number": 161
    },
    {
      "id": "6440eeb4-e0f7-475b-8c55-1fdec1612344",
      "m1": "سر ساوه شاهست و کهتر پسر",
      "m2": "که فغفور خواندیش ویرا پدر",
      "verse_number": 162
    },
    {
      "id": "11e09c52-b48b-4dbb-9fdc-a832e6d581eb",
      "m1": "زده بر سرنیزهها بر درست",
      "m2": "همه شهر نظاره آن سرست",
      "verse_number": 163
    },
    {
      "id": "bd209661-ef37-47cd-b8c3-6e52a22afcc4",
      "m1": "شهنشاه بشنید بر پای خاست",
      "m2": "بزودی خم آورد بالای راست",
      "verse_number": 164
    },
    {
      "id": "4e833a6d-0714-428b-8aa8-8c48a2f94394",
      "m1": "همیبود بر پیش یزدان بهپای",
      "m2": "همیگفت کای داور رهنمای",
      "verse_number": 165
    },
    {
      "id": "766ace4d-d182-4a20-b2eb-79df3fd79141",
      "m1": "بد اندیش ما را تو کردی تباه",
      "m2": "تویی آفریننده هور و ماه",
      "verse_number": 166
    },
    {
      "id": "d7fd6c00-d4cb-49b8-8b68-ad804657bd82",
      "m1": "چنان زار و نومید بودم ز بخت",
      "m2": "که دشمن نگون اندر آمد ز تخت",
      "verse_number": 167
    },
    {
      "id": "c19772c8-3704-42c1-8816-0d4413554c0b",
      "m1": "سپهبد نکرد این نه جنگی سپاه",
      "m2": "که یزدان بد این جنگ را نیک خواه",
      "verse_number": 168
    },
    {
      "id": "c46b6b24-5846-4198-b7a4-1d7d1ff9a5b3",
      "m1": "بیاورد زان پس صد و سی هزار",
      "m2": "ز گنجی که بود از پدر یادگار",
      "verse_number": 169
    },
    {
      "id": "27725bfb-d24f-4bff-9f9a-451e166f5828",
      "m1": "سه یک زان نخستین بدرویش داد",
      "m2": "پرستندگان را درم بیش داد",
      "verse_number": 170
    },
    {
      "id": "059df5ea-6048-408d-8574-3993c6b06cee",
      "m1": "سه یک دیگر از بهر آتشکده",
      "m2": "همان بهر نوروز و جشن سده",
      "verse_number": 171
    },
    {
      "id": "522d8131-0840-4148-b858-1da19769e1ee",
      "m1": "فرستاد تا هیربد را دهند",
      "m2": "که در پیش آتشکده برنهند",
      "verse_number": 172
    },
    {
      "id": "1fe3f22e-8a77-4358-8692-6333a5e65ad4",
      "m1": "سیم بهره جایی که ویران بود",
      "m2": "رباطی که اندر بیابان بود",
      "verse_number": 173
    },
    {
      "id": "25280b75-1e24-425b-ba76-462d761fb541",
      "m1": "کند یکسر آباد جوینده مرد",
      "m2": "نباشد به راه اندرون بیم و درد",
      "verse_number": 174
    },
    {
      "id": "bd286a0b-e6a5-463e-9945-8a00f3009e2c",
      "m1": "ببخشید پس چار ساله خراج",
      "m2": "به درویش و آن را که بد تخت عاج",
      "verse_number": 175
    },
    {
      "id": "5c032e97-254b-4eeb-9ed3-e37d43802aaf",
      "m1": "نبشتند پس نامه از شهریار",
      "m2": "به هرکشوری سوی هرنامدار",
      "verse_number": 176
    },
    {
      "id": "b36ab5e9-816d-4272-874e-56a031cff1d7",
      "m1": "که بهرام پیروز شد بر سپاه",
      "m2": "بریدند بیبر سر ساوه شاه",
      "verse_number": 177
    },
    {
      "id": "e99edcbd-ea02-4346-8f7f-4e47e653591b",
      "m1": "پرستنده بد شاه در هفت روز",
      "m2": "به هشتم چو بفروخت گیتی فروز",
      "verse_number": 178
    },
    {
      "id": "84d75f8c-0844-404a-83da-90bfd0ee1707",
      "m1": "فرستادهٔ پهلوان رابخواند",
      "m2": "به مهر از بر نامداران نشاند",
      "verse_number": 179
    },
    {
      "id": "5e507a32-2107-4028-8918-a76afe4b6894",
      "m1": "مر آن نامه را خوب پاسخ نبشت",
      "m2": "درختی به باغ بزرگی بکشت",
      "verse_number": 180
    },
    {
      "id": "9ba9b567-d625-4a7c-b1c4-8c74cb9e972d",
      "m1": "یکی تخت سیمین فرستاد نیز",
      "m2": "دو نعلین زرین و هر گونه چیز",
      "verse_number": 181
    },
    {
      "id": "79a6395a-da01-41a6-bb8c-2534e2ce7de9",
      "m1": "ز هیتال تا پیش رود برک",
      "m2": "به بهرام بخشید و بنوشت چک",
      "verse_number": 182
    },
    {
      "id": "ff2c8538-bd7f-4e8e-979f-b64a513b374d",
      "m1": "بفرمود کان خواسته بر سپاه",
      "m2": "ببخش آنچ آوردی از رزمگاه",
      "verse_number": 183
    },
    {
      "id": "3ff1e60a-68cc-4d74-a9b0-f588f953a249",
      "m1": "مگرگنج ویژه تن ساوه شاه",
      "m2": "که آورد باید بدین بارگاه",
      "verse_number": 184
    },
    {
      "id": "2bfa55a7-8d3c-450f-806c-3300bede4094",
      "m1": "وزان پس تو خود جنگ پرموده ساز",
      "m2": "ممان تا شود خصم گردن فراز",
      "verse_number": 185
    },
    {
      "id": "2c7a030b-76e4-47cc-b47a-61b433f9bc36",
      "m1": "هم ایرانیان را فرستاد چیز",
      "m2": "نبشته به هر شهر منشور نیز",
      "verse_number": 186
    },
    {
      "id": "f041af2e-01c0-415f-98d5-9642fe7a5e83",
      "m1": "فرستاده را خلعت آراستند",
      "m2": "پس اسب جهان پهلوان خواستند",
      "verse_number": 187
    },
    {
      "id": "c35d9f99-55d2-4db2-a6e9-c41fef726736",
      "m1": "فرستاده چون پیش بهرام شد",
      "m2": "سپهدار از و شاد و پدرام شد",
      "verse_number": 188
    },
    {
      "id": "3bb01468-c88d-4445-8fd6-6036f2070ec0",
      "m1": "غنیمت ببخشید پس بر سپاه",
      "m2": "جز از گنج ناپاک دل ساوه شاه",
      "verse_number": 189
    },
    {
      "id": "d07d9031-1527-475a-b6d6-ea34f7d102fd",
      "m1": "فرستاد تا استواران خویش",
      "m2": "جهاندیده ونامداران خویش",
      "verse_number": 190
    },
    {
      "id": "a71edbd2-82bd-42bc-a1d5-e112df721b10",
      "m1": "ببردند یکسر به درگاه شاه",
      "m2": "سپهبد سوی جنگ شد با سپاه",
      "verse_number": 191
    },
    {
      "id": "8d0faa19-63dc-421a-9ef2-9bebcccc690b",
      "m1": "ازو چون بپرموده شد آگهی",
      "m2": "که جوید همی تخت شاهنشهی",
      "verse_number": 192
    },
    {
      "id": "a9466e80-4a60-4e22-85f7-f92bd71efe91",
      "m1": "دزی داشت پرموده افراز نام",
      "m2": "کزان دز بدی ایمن و شادکام",
      "verse_number": 193
    },
    {
      "id": "36c16bb9-dc3c-4b8e-8bfd-098f33bd3940",
      "m1": "نهاد آنچ بودش بدز در درم",
      "m2": "ز دینار وز گوهر و بیش و کم",
      "verse_number": 194
    },
    {
      "id": "70b21c29-e00c-4aaa-8e6a-071f1c01cf35",
      "m1": "ز جیحون گذر کرد خود با سپاه",
      "m2": "بیامد گرازان سوی زرمگاه",
      "verse_number": 195
    },
    {
      "id": "5897d409-70ed-44e9-bef7-d7c89ce44b14",
      "m1": "دو لشکر به تنگ اندر آمد به جنگ",
      "m2": "بهره بر نکردند جایی درنگ",
      "verse_number": 196
    },
    {
      "id": "042c834c-434c-4264-ae55-97c3e4daa1d2",
      "m1": "بدو منزل بلخ هر دو سپاه",
      "m2": "گزیدند شایسته دو رزمگاه",
      "verse_number": 197
    },
    {
      "id": "1acb8465-45e6-4d49-bffa-2efdde2e7416",
      "m1": "میان دو لشکر دو فرسنگ بود",
      "m2": "که پهنای دشت از در جنگ بود",
      "verse_number": 198
    },
    {
      "id": "763a9718-55e0-4862-b303-9d6be001b98d",
      "m1": "دگر روز بهرام جنگی برفت",
      "m2": "به دیدار گردان پرموده تفت",
      "verse_number": 199
    },
    {
      "id": "c5b643e3-ef0d-4699-984f-aa03a147ae02",
      "m1": "نگه کرد پرموده را بدید",
      "m2": "ز هامون یکی تند بالا گزید",
      "verse_number": 200
    },
    {
      "id": "5fae9a64-521f-4073-b57b-41ecd40b13a0",
      "m1": "سپه را سراسر همه برنشاند",
      "m2": "چنان شد که در دشت جایی نماند",
      "verse_number": 201
    },
    {
      "id": "31d1ac0d-fd11-46c7-96ac-dd419e631344",
      "m1": "سپه دید پرموده چندانک دشت",
      "m2": "ز دیدار ایشان همی خیره گشت",
      "verse_number": 202
    },
    {
      "id": "18a7290e-803f-4a99-859c-71512036affa",
      "m1": "و را دید در پیش آن لشکرش",
      "m2": "به گردون برآورده جنگی سرش",
      "verse_number": 203
    },
    {
      "id": "91c1dbf1-e264-4397-8168-bfc4a85c0b28",
      "m1": "غمی گشت و با لشکر خویش گفت",
      "m2": "که این پیشرو را هزبرست جفت",
      "verse_number": 204
    },
    {
      "id": "a2e0b12a-e0e5-4c24-8bc3-02cffa7014d9",
      "m1": "شمار سپاهش پدیدار نیست",
      "m2": "هم این رزم را کس خریدار نیست",
      "verse_number": 205
    },
    {
      "id": "de7c8b90-715d-4fb5-9e93-edbe19184209",
      "m1": "سپهدار گردنکش و خشمناک",
      "m2": "همی خون شود زیر او تیره خاک",
      "verse_number": 206
    },
    {
      "id": "a23853cf-59f3-498f-8c8a-8d1be380d1a2",
      "m1": "چو شب تیره گردد شبیخون کنیم",
      "m2": "ز دل بیم و اندیشه بیرون کنیم",
      "verse_number": 207
    },
    {
      "id": "4ecbc377-92ed-48c9-be62-394da7abd0d6",
      "m1": "چو پرموده آمد به پرده سرای",
      "m2": "همیزد ز هر گونه از جنگ رای",
      "verse_number": 208
    },
    {
      "id": "68494f06-c06e-406e-b160-4e77432c2cff",
      "m1": "همیگفت کین از هنرها یکیست",
      "m2": "اگر چه سپهشان کنون اندکیست",
      "verse_number": 209
    },
    {
      "id": "594e6954-0c17-4c29-a851-141beddf11ae",
      "m1": "سواران و گردان پر مایهاند",
      "m2": "ز گردنکشان برترین پایهاند",
      "verse_number": 210
    },
    {
      "id": "116b2130-673e-44af-bc2e-7767198013bc",
      "m1": "سلیحست وبهرامشان پیشرو",
      "m2": "که گردد سنان پیش او خار و خو",
      "verse_number": 211
    },
    {
      "id": "e78806e7-31d3-47b3-983c-6010738d4220",
      "m1": "به پیروزی ساوه شاه اندرون",
      "m2": "گرفته دل و مست گشته به خون",
      "verse_number": 212
    },
    {
      "id": "84a68083-2785-4d14-9b16-7dae26c79c48",
      "m1": "اگر یار باشد جهان آفرین",
      "m2": "به خون پدر خواهم از کوه کین",
      "verse_number": 213
    },
    {
      "id": "5a40d775-e78e-43e5-8395-f8cf2d1621ae",
      "m1": "بدانگه که بهرام شد جنگجوی",
      "m2": "از ایران سوی ترک بنهاد روی",
      "verse_number": 214
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 214
  }
}