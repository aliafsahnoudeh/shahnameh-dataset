{
  "id": "63844e75-c78c-4101-b746-81428f4ef00e",
  "title": "بخش ۶",
  "verses": [
    {
      "id": "4be733b1-88e7-4937-bb81-7a6150921fd5",
      "m1": "چنین گفت پس با سپه ساوه شاه",
      "m2": "که از جادوی اندر آرید راه",
      "verse_number": 1
    },
    {
      "id": "50dd214e-a28f-440c-8804-1aecdaeade41",
      "m1": "بدان تا دل و چشم ایرانیان",
      "m2": "بپیچد نیاید شما را زیان",
      "verse_number": 2
    },
    {
      "id": "b47a7dc1-47ba-4a90-a3d4-86d9157be247",
      "m1": "همه جاودان جادوی ساختند",
      "m2": "همی در هوا آتش انداختند",
      "verse_number": 3
    },
    {
      "id": "b097afaf-9ff0-4112-8124-1db44283deaf",
      "m1": "برآمد یکی باد و ابری سیاه",
      "m2": "همی تیر بارید ازو بر سپاه",
      "verse_number": 4
    },
    {
      "id": "f0ef8912-7c73-42ad-95c5-b3d86b7cde4b",
      "m1": "خروشید بهرام کای مهتران",
      "m2": "بزرگان ایران و کنداوران",
      "verse_number": 5
    },
    {
      "id": "50fd502a-51e0-421c-9aef-a61ad5bcdce2",
      "m1": "بدین جادویها مدارید چشم",
      "m2": "به جنگ اندر آیید یکسر بخشم",
      "verse_number": 6
    },
    {
      "id": "2a87fa7c-3a36-4eb9-9ab5-88c91df7766b",
      "m1": "که آن سر به سر تنبل وجادویست",
      "m2": "ز چاره برایشان بباید گریست",
      "verse_number": 7
    },
    {
      "id": "9ae01ca7-ee39-41fa-90d7-c4c6f2aa7cb4",
      "m1": "خروشی برآمد ز ایرانیان",
      "m2": "ببستند خون ریختن را میان",
      "verse_number": 8
    },
    {
      "id": "d163ee5f-0742-4bca-96dc-0b75a4e3739c",
      "m1": "نگه کرد زان رزمگه ساوه شاه",
      "m2": "که آن جادویی را ندادند راه",
      "verse_number": 9
    },
    {
      "id": "ae800fd8-4a20-4eb1-bea2-7cde0616173e",
      "m1": "بیاورد لشکر سوی میسره",
      "m2": "چو گرگ اندر آمد بهپیش بره",
      "verse_number": 10
    },
    {
      "id": "fd97f4c7-ee76-4bba-b36f-bf46be2a2955",
      "m1": "چویک روی لشکر بههم برشکست",
      "m2": "سوی قلب بهرام یازید دست",
      "verse_number": 11
    },
    {
      "id": "080d456d-0f21-4201-ab62-8ab91be91574",
      "m1": "نگه کرد بهرام زان قلبگاه",
      "m2": "گریزان سپه دید پیش سپاه",
      "verse_number": 12
    },
    {
      "id": "fc608a06-764e-439b-8668-43b514368fe2",
      "m1": "بیامد بهنیزه سه تن را ز زین",
      "m2": "نگونسار کرد و بزد بر زمین",
      "verse_number": 13
    },
    {
      "id": "23b3409d-7207-441e-8675-8f5bffad1096",
      "m1": "همیگفت زین سان بود کارزار",
      "m2": "همین بود رسم و همین بود کار",
      "verse_number": 14
    },
    {
      "id": "5acc597c-2ddc-4529-a1b4-5543b6df775c",
      "m1": "ندارید شرم از خدای جهان",
      "m2": "نه از نامداران فرخ مهان",
      "verse_number": 15
    },
    {
      "id": "b73903df-1551-423e-b561-976eef06b637",
      "m1": "و زان پس بیامد سوی میمنه",
      "m2": "چو شیر ژیان کو شود گرسنه",
      "verse_number": 16
    },
    {
      "id": "2edf2f70-ab79-4296-9dcf-86d74b103d12",
      "m1": "چنان لشکری رابههم بردرید",
      "m2": "درفش سپهدار شد ناپدید",
      "verse_number": 17
    },
    {
      "id": "a88e4cd2-02fb-49cb-bf1b-5495ba6a2271",
      "m1": "و زان جایگه شد سوی قلبگاه",
      "m2": "بران سو که سالار بد با سپاه",
      "verse_number": 18
    },
    {
      "id": "e4556331-950f-427c-99e5-e93e6a15f0bf",
      "m1": "بدو گفت برگشت باد این سخن",
      "m2": "گر ای دون که این رزم گردد کهن",
      "verse_number": 19
    },
    {
      "id": "48d79b12-c7da-454d-bbec-5468d4a8cf7b",
      "m1": "پراکنده گردد به جنگ این سپاه",
      "m2": "نگه کن کنون تا کدامست راه",
      "verse_number": 20
    },
    {
      "id": "8ad068c0-a7bc-411b-ac81-4da2b1100253",
      "m1": "برفتند وجستند راهی نبود",
      "m2": "کزان راه شایست بالا نمود",
      "verse_number": 21
    },
    {
      "id": "2530d28a-95ee-4fcb-aeca-ef47a7d993e3",
      "m1": "چنین گفت با لشکر آرای خویش",
      "m2": "که دیوار ما آهنینست پیش",
      "verse_number": 22
    },
    {
      "id": "fa03a508-21e5-45a1-a258-330c8c36c8df",
      "m1": "هر آنکس که او رخنه داند زدن",
      "m2": "ز دیوار بیرون تواند شدن",
      "verse_number": 23
    },
    {
      "id": "0fdbd96a-c1f1-42fd-84c9-6abbff66adfb",
      "m1": "شود ایمن و جان به ایران برد",
      "m2": "به نزدیک شاه دلیران برد",
      "verse_number": 24
    },
    {
      "id": "cf8e7fe0-9142-4e18-ab5b-9a82a4f0212d",
      "m1": "همه دل به خون ریختن برنهید",
      "m2": "سپر بر سر آرید و خنجر دهید",
      "verse_number": 25
    },
    {
      "id": "f0ea5467-1fc7-4034-80e0-6738c6561a39",
      "m1": "ز یزدان نباشد کسی ناامید",
      "m2": "و گر تیره بینند روز سپید",
      "verse_number": 26
    },
    {
      "id": "8a9dd54b-8c97-4a35-8a0d-fc4008fa84f9",
      "m1": "چنین گفت با مهتران ساوه شاه",
      "m2": "که پیلان بیارید پیش سپاه",
      "verse_number": 27
    },
    {
      "id": "05b17d31-58b3-490f-914e-bac95bbcefb0",
      "m1": "به انبوه لشکر به جنگ آورید",
      "m2": "بدیشان جهان تار و تنگ آورید",
      "verse_number": 28
    },
    {
      "id": "63ca2ac0-007c-434a-9c07-1277ff51c039",
      "m1": "چو از دور بهرام پیلان بدید",
      "m2": "غمی گشت و تیغ از میان برکشید",
      "verse_number": 29
    },
    {
      "id": "05d13127-e3e1-4bb2-937f-302c1b6079d1",
      "m1": "از آن پس چنین گفت با مهتران",
      "m2": "که ای نامداران و جنگ آوران",
      "verse_number": 30
    },
    {
      "id": "2deefd62-c396-4b48-bdc3-cffb65d30824",
      "m1": "کمانهای چاچی بزه برنهید",
      "m2": "همه یکسره ترگ برسرنهید",
      "verse_number": 31
    },
    {
      "id": "1c50f5e0-cf83-4ccc-9abc-33e0a49f9781",
      "m1": "بهجان و سر شهریار جهان",
      "m2": "گزین بزرگان و تاج مهان",
      "verse_number": 32
    },
    {
      "id": "853102c0-c5ca-4a71-b289-70d6c77c62db",
      "m1": "که هرکس که بااو کمانست و تیر",
      "m2": "کمان را بزه برنهد ناگزیر",
      "verse_number": 33
    },
    {
      "id": "e70ce779-20e1-41ba-b18a-a2a650dbe6bd",
      "m1": "خدنگی که پیکانش یازد بهخون",
      "m2": "سه چوبه بهخرطوم پیل اندرون",
      "verse_number": 34
    },
    {
      "id": "b57303fa-e44f-4db5-a9df-341cb644c23f",
      "m1": "نشانید و پس گرزها برکشید",
      "m2": "به جنگ اندر آیید و دشمن کشید",
      "verse_number": 35
    },
    {
      "id": "43f8f125-831a-4cb1-88e4-ddb5d1de32a1",
      "m1": "سپهبد کمان را بزه برنهاد",
      "m2": "یکی خود پولاد بر سر نهاد",
      "verse_number": 36
    },
    {
      "id": "2a6635ea-975c-4fef-b65d-88a5241c00a6",
      "m1": "بهپیل اندرون تیر باران گرفت",
      "m2": "کمان را چو ابر بهاران گرفت",
      "verse_number": 37
    },
    {
      "id": "41304ad2-77f5-425a-a3b9-7d738d22afcf",
      "m1": "پس پشت او اندر آمد سپاه",
      "m2": "ستاره شد از پر و پیکان سیاه",
      "verse_number": 38
    },
    {
      "id": "e728e766-15b4-4fab-9e4d-6bf45ff8675b",
      "m1": "بخستند خرطوم پیلان بهتیر",
      "m2": "ز خون شد در و دشت چون آبگیر",
      "verse_number": 39
    },
    {
      "id": "ca4dc653-5e5e-41f7-8389-86d2f0089085",
      "m1": "از آن خستگی پشت برگاشتند",
      "m2": "بدو دشت پیکار بگذاشتند",
      "verse_number": 40
    },
    {
      "id": "fc7c7061-f863-48e2-aae9-dffc14b41363",
      "m1": "چو پیل آنچنان زخم پیکان بدید",
      "m2": "همه لشکر خویش را بسپرید",
      "verse_number": 41
    },
    {
      "id": "828174cf-d873-4f66-8929-977ef22911f2",
      "m1": "سپه بر هم افتاد و چندی بمرد",
      "m2": "همان بخت بد کامکاری ببرد",
      "verse_number": 42
    },
    {
      "id": "3f402b75-8631-4671-a054-3c9e381a341b",
      "m1": "سپاه اندر آمد پس پشت پیل",
      "m2": "زمین شد بکردار دریای نیل",
      "verse_number": 43
    },
    {
      "id": "cb9b5039-5e4d-412b-8a8f-50acde027eee",
      "m1": "تلی بود خرم بدان جایگاه",
      "m2": "پس پشت آن رنج دیده سپاه",
      "verse_number": 44
    },
    {
      "id": "9a3a38aa-1835-4f32-9d85-f4cc1ca038ce",
      "m1": "یکی تخت زرین نهاده بروی",
      "m2": "نشسته برو ساوهٔ رزمجوی",
      "verse_number": 45
    },
    {
      "id": "84defd33-9b60-477e-b428-3c5addb5719d",
      "m1": "سپه دید چون کوه آهن روان",
      "m2": "همه سر پر از گرد و تیره روان",
      "verse_number": 46
    },
    {
      "id": "4e052303-887f-4265-bd06-6230599b5c33",
      "m1": "پس پشت آن زنده پیلان مست",
      "m2": "همیکوفتند آن سپه را بدست",
      "verse_number": 47
    },
    {
      "id": "2172cffc-5da7-4244-9aa0-89ea6e295e3f",
      "m1": "پر از آب شد دیدهٔ ساوه شاه",
      "m2": "بدان تا چرا شد هزیمت سپاه",
      "verse_number": 48
    },
    {
      "id": "89d5eafa-6bbb-4fd4-8a01-e3e00b07a933",
      "m1": "نشست از بر تازی اسب سمند",
      "m2": "همیتاخت ترسان ز بیم گزند",
      "verse_number": 49
    },
    {
      "id": "6c66dec1-44c8-474c-a698-a91306eebf65",
      "m1": "بر ساوه بهرام چون پیل مست",
      "m2": "کمندی به بازو کمانی بدست",
      "verse_number": 50
    },
    {
      "id": "baede14a-dde5-4eb9-a355-d0ee9037b504",
      "m1": "به لشکر چنین گفت کای سرکشان",
      "m2": "زبخت بد آمد بر ایشان نشان",
      "verse_number": 51
    },
    {
      "id": "50a700d9-abd7-44a8-8a3d-7615a2768a66",
      "m1": "نه هنگام رازست و روز سخن",
      "m2": "بتازید با تیغهای کهن",
      "verse_number": 52
    },
    {
      "id": "006b26bf-b282-4102-8518-e4d7f1fe2bdc",
      "m1": "بر ایشان یکی تیر باران کنید",
      "m2": "بکوشید وکار سواران کنید",
      "verse_number": 53
    },
    {
      "id": "2b847407-9a6a-4727-a635-0b704ff33de3",
      "m1": "بران تل بر آمد کجا ساوه شاه",
      "m2": "همیبود بر تخت زر با کلاه",
      "verse_number": 54
    },
    {
      "id": "a368d483-381f-44c1-8401-703c8ee012ae",
      "m1": "و را دید برتازیی چون هزبر",
      "m2": "همیتاخت در دشت برسان ابر",
      "verse_number": 55
    },
    {
      "id": "82671256-abc8-406f-a56e-10b76870e3be",
      "m1": "خدنگی گزین کرد پیکان چو آب",
      "m2": "نهاده برو چار پر عقاب",
      "verse_number": 56
    },
    {
      "id": "d03c672e-8a12-44e2-ab58-9c6713a96fd4",
      "m1": "بمالید چاچی کمان را بدست",
      "m2": "به چرم گوزن اندر آورد شست",
      "verse_number": 57
    },
    {
      "id": "c87c3264-1dd6-4e5f-b97a-b6eb9f2bbab0",
      "m1": "چو چپ راست کرد و خم آورد راست",
      "m2": "خروش از خم چرخ چاچی بخاست",
      "verse_number": 58
    },
    {
      "id": "a741faef-2f79-421f-8025-0be728da2169",
      "m1": "چو آورد یال یلی رابهگوش",
      "m2": "ز شاخ گوزنان برآمد خروش",
      "verse_number": 59
    },
    {
      "id": "19dc7433-55d5-46e4-a1ce-0f4fc711e1fd",
      "m1": "چو بگذشت پیکان از انگشت اوی",
      "m2": "گذر کرد از مهرهٔ پشت اوی",
      "verse_number": 60
    },
    {
      "id": "78b2c260-b8a5-4559-bf13-5ba468a4da30",
      "m1": "سر ساوه آمد بخاک اندرون",
      "m2": "بزیر اندرش خاک شد جوی خون",
      "verse_number": 61
    },
    {
      "id": "6e420b84-f646-4005-bcca-c15293e79a9b",
      "m1": "شد آن نامور شاه و چندان سپاه",
      "m2": "همان تخت زرین و زرین کلاه",
      "verse_number": 62
    },
    {
      "id": "ea66f8f8-e442-42c2-92f7-c8950d205151",
      "m1": "چنینست کردار گردان سپهر",
      "m2": "نه نامهربانیش پیدا نه مهر",
      "verse_number": 63
    },
    {
      "id": "75ffa86d-cef8-4146-9beb-8dee80781354",
      "m1": "نگر تا ننازی بهتخت بلند",
      "m2": "چو ایمن شوی دورباش از گزند",
      "verse_number": 64
    },
    {
      "id": "22ca320a-6050-4e8c-9467-0b042ab26730",
      "m1": "چو بهرام جنگی رسید اندروی",
      "m2": "کشیدش بر آن خاک تفته بروی",
      "verse_number": 65
    },
    {
      "id": "f1cdf75e-52b7-4ad6-80e9-81993a183946",
      "m1": "برید آن سر شاهوارش ز تن",
      "m2": "نیامد کسی پیشش از انجمن",
      "verse_number": 66
    },
    {
      "id": "46486508-4d32-4a75-bb43-d65b880f208e",
      "m1": "چوترکان رسیدند نزدیک شاه",
      "m2": "فگنده تنی بود بیسر به راه",
      "verse_number": 67
    },
    {
      "id": "305b5a77-22a4-4cf6-97a9-13ff609179c9",
      "m1": "همه برگرفتند یکسر خروش",
      "m2": "زمین پر خروش و هوا پر ز جوش",
      "verse_number": 68
    },
    {
      "id": "23fcb526-9119-4057-abff-e8a35819fa4a",
      "m1": "پسر گفت کاین ایزدی کار بود",
      "m2": "که بهرام را بخت بیدار بود",
      "verse_number": 69
    },
    {
      "id": "85f5b9c4-1dd1-45ae-8202-7a2e469d797f",
      "m1": "ز تنگی کجا راه بد بر سپاه",
      "m2": "فراوان بمردند زان تنگ راه",
      "verse_number": 70
    },
    {
      "id": "02afbeb8-4047-4299-bba6-64ba88b7e426",
      "m1": "بسی پیل بسپرد مردم بهپای",
      "m2": "نشد زان سپه ده یکی باز جای",
      "verse_number": 71
    },
    {
      "id": "0399602e-f66e-4790-9b9f-72a475a0fedc",
      "m1": "چه زیر پی پیل گشته تباه",
      "m2": "چه سرها بریده بهآوردگاه",
      "verse_number": 72
    },
    {
      "id": "5ce9b3c2-bc9b-4961-bc0e-f58738e59dc0",
      "m1": "چو بگذشت زان روز بد به زمان",
      "m2": "ندیدند زنده یکی بد گمان",
      "verse_number": 73
    },
    {
      "id": "63be6fe3-e46d-4f2c-956e-c24dd3688fef",
      "m1": "مگرآنک بودند گشته اسیر",
      "m2": "روانها به غم خسته و تن به تیر",
      "verse_number": 74
    },
    {
      "id": "cb430c94-b241-457e-8f81-fcc5ccaa4798",
      "m1": "همه راه برگستوان بود و ترگ",
      "m2": "سران را ز ترگ آمده روز مرگ",
      "verse_number": 75
    },
    {
      "id": "3894f2f1-e43d-47ca-9bca-04246c04bfdf",
      "m1": "همان تیغ هندی و تیر و کمان",
      "m2": "به هرسوی افگنده بد بدگمان",
      "verse_number": 76
    },
