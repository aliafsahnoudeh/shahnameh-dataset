      "verse_number": 154
    },
    {
      "id": "363bf8fd-a0c6-4cfd-b98a-d64bb2d3a722",
      "m1": "کنون بیمنش زینهاری شدم",
      "m2": "ز ارج بلندی بخواری شدم",
      "verse_number": 155
    },
    {
      "id": "2e806bb5-13ca-40c1-8fb2-67e8caeb5a7e",
      "m1": "بدین روز خود نیستی خوش منش",
      "m2": "که پیش آمدم ای بد بد کنش",
      "verse_number": 156
    },
    {
      "id": "a7dc9451-50ac-46ab-8678-1f9256a221b2",
      "m1": "کنون یافتم نامهٔ زینهار",
      "m2": "همیرفت خواهم بر شهریار",
      "verse_number": 157
    },
    {
      "id": "6178b023-c4cf-40fa-ba66-2414b7f5b26c",
      "m1": "مگر با من او چون برادر شود",
      "m2": "ازو رنج بر من سبکتر شود",
      "verse_number": 158
    },
    {
      "id": "12ba1075-9a15-49d1-a12e-d2a19968c4d3",
      "m1": "تو را با من اکنون چه کارست نیز",
      "m2": "سپردم تو را تخت شاهی و چیز",
      "verse_number": 159
    }
  ],
