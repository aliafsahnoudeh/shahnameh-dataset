{
  "id": "62c6dc66-26fc-48bf-b10e-2b9df8ec11be",
  "title": "بخش ۷",
  "verses": [
    {
      "id": "08405e8f-b2e8-458b-8803-4a504229fe58",
      "m1": "ستاره شمر گفت بهرام را",
      "m2": "که در چارشنبه مزن گام را",
      "verse_number": 1
    },
    {
      "id": "1ffa16a8-367a-49d6-8a5b-399c624aeda6",
      "m1": "اگر زین به پیچی گزند آیدت",
      "m2": "همه کار ناسودمند آیدت",
      "verse_number": 2
    },
    {
      "id": "090a2795-fa82-4902-8c25-f606c8ae87a8",
      "m1": "یکی باغ بد در میان سپاه",
      "m2": "ازین روی و زان روی بد رزمگاه",
      "verse_number": 3
    },
    {
      "id": "611f43f4-56a4-45a1-b815-1cfa4eba7345",
      "m1": "بشد چارشنبه هم از بامداد",
      "m2": "بدان باغ کامروز باشیم شاد",
      "verse_number": 4
    },
    {
      "id": "6a01cd9b-dfff-4040-a766-ed8a2c34a9a8",
      "m1": "ببردند پرمایه گستردنی",
      "m2": "می و رود و رامشگر و خوردنی",
      "verse_number": 5
    },
    {
      "id": "c398da89-b90d-4783-a9e9-d058d07042d1",
      "m1": "بیامد بدان باغ و می درکشید",
      "m2": "چوپاسی ز تیره شب اندر کشید",
      "verse_number": 6
    },
    {
      "id": "47e88195-16da-4ce0-b45f-3396a70c867d",
      "m1": "طلایه بیامد بپرموده گفت",
      "m2": "که بهرام را جام و باغست جفت",
      "verse_number": 7
    },
    {
      "id": "37057bee-a3ca-4f2c-a9e9-13d3d16636ee",
      "m1": "سپهدار ازان جنگیان شش هزار",
      "m2": "زلشکر گزین کرد گرد و سوار",
      "verse_number": 8
    },
    {
      "id": "7796c43b-f7df-44e1-94d2-630780aeb0d7",
      "m1": "فرستاد تا گرد بر گرد باغ",
      "m2": "بگیرند گردنکشان بیچراغ",
      "verse_number": 9
    },
    {
      "id": "8e22aa06-ecef-4bfd-9795-abc26018d47a",
      "m1": "چو بهرام آگه شد از کارشان",
      "m2": "ز رای جهانجوی و بازارشان",
      "verse_number": 10
    },
    {
      "id": "af4ec845-a23a-4018-aa80-f1d8d712c133",
      "m1": "یلان سینه را گفت کای سرافراز",
      "m2": "بدیوار باغ اندرون رخنه ساز",
      "verse_number": 11
    },
    {
      "id": "173d8bb4-3a95-486b-92a7-cd51232b1390",
      "m1": "پس آنگاه بهرام و ایزد گشسب",
      "m2": "نشستند با جنگجویان بر اسب",
      "verse_number": 12
    },
    {
      "id": "c1a10b13-b716-437c-8bfc-8f617ed1b993",
      "m1": "ازان رخنه باغ بیرون شدند",
      "m2": "که دانست کان سرکشان چون شدند",
      "verse_number": 13
    },
    {
      "id": "25fa4c40-aafa-4be8-ab94-1e493d83794b",
      "m1": "برآمد ز در نالهٔ کرنای",
      "m2": "سپهبد باسب اندر آورد پای",
      "verse_number": 14
    },
    {
      "id": "ac35dc47-32b9-4e57-bc25-0b0fb62b6070",
      "m1": "سبک رخنهٔ دیگر اندر زدند",
      "m2": "سپه را یکایک بهم بر زدند",
      "verse_number": 15
    },
    {
      "id": "9ecb12f3-d3a6-42ca-9476-77da28300e97",
      "m1": "هم تاخت بهرام خشتی بدست",
      "m2": "چناچون بود مردم نیم مست",
      "verse_number": 16
    },
    {
      "id": "4a2f4ec4-f901-4bd6-8794-c4cc080bed2c",
      "m1": "نجستند گردان کس از دست اوی",
      "m2": "به خون گشت یازان سر شست اوی",
      "verse_number": 17
    },
    {
      "id": "83cd8d32-2113-4b0e-a9df-8b919846e6a5",
      "m1": "برآمد چکاچاک و بانگ سران",
      "m2": "چو پولاد را پتک آهنگران",
      "verse_number": 18
    },
    {
      "id": "f57236b6-8e38-4ccd-ad36-989494be0860",
      "m1": "ازان باغ تا جای پرموده شاه",
      "m2": "تن بیسران بد فگنده به راه",
      "verse_number": 19
    },
    {
      "id": "58f42147-8d7e-4675-a089-091f497b6ffb",
      "m1": "چوآمد بلشکر گه خویش باز",
      "m2": "شبیخون سگالید گردن فراز",
      "verse_number": 20
    },
    {
      "id": "452a18d7-c43a-451b-9cf8-9188cf7ae4e2",
      "m1": "چو نیمی زتیره شب اندر گذشت",
      "m2": "سپهدار جنگی برون شد به دشت",
      "verse_number": 21
    },
    {
      "id": "1ed8c800-436f-41d3-bc78-1951923239fa",
      "m1": "سپهبد بران سوی لشکر کشید",
      "m2": "زترکان طلایه کس او را ندید",
      "verse_number": 22
    },
    {
      "id": "0247aa6f-967a-4838-b86f-802125e9ee31",
      "m1": "چو آمد به نزدیکی رزمگاه",
      "m2": "دم نای رویین برآمد ز راه",
      "verse_number": 23
    },
    {
      "id": "adb22db2-585e-439a-8c77-77c35aafc105",
      "m1": "چو آواز کوس آمد و کرنای",
      "m2": "بجستند ترکان جنگی ز جای",
      "verse_number": 24
    },
    {
      "id": "38c60fe2-ff33-4c67-a0b0-eebeb9fc54b9",
      "m1": "زلشکر بران سان برآمد خروش",
      "m2": "که شیر ژیان را بدرید گوش",
      "verse_number": 25
    },
    {
      "id": "b7542ffe-b233-42e9-b4f6-241ef1c11bcf",
      "m1": "به تاریکی اندر دهاده بخاست",
      "m2": "ز دست چپ لشکر و دست راست",
      "verse_number": 26
    },
    {
      "id": "7f3916fe-a3ce-4ddc-a752-1a1cc74d115b",
      "m1": "یکی مر دگر را ندانست باز",
      "m2": "شب تیره و نیزههای دراز",
      "verse_number": 27
    },
    {
      "id": "3ad819db-8720-4673-9191-4fb269df3565",
      "m1": "بخنجر همی آتش افروختند",
      "m2": "زمین و هوا را همیسوختند",
      "verse_number": 28
    },
    {
      "id": "3374848d-ab38-4ae7-9dc2-1d2b4d660b62",
      "m1": "ز ترکان جنگی فراوان نماند",
      "m2": "ز خون سنگها جز به مرجان نماند",
      "verse_number": 29
    },
    {
      "id": "50ec6b2c-3200-4298-88ef-ca52adfa117e",
      "m1": "گریزان همیرفت مهتر چو گرد",
      "m2": "دهن خشک و لبها شده لاجورد",
      "verse_number": 30
    },
    {
      "id": "9beab03a-eb2e-40f2-9bd4-0c7ddf685c15",
      "m1": "چنین تا سپیدهدمان بردمید",
      "m2": "شب تیره گون دامن اندر کشید",
      "verse_number": 31
    },
    {
      "id": "465543e2-e067-4579-b8e9-c35dbd31861d",
      "m1": "سپهدار ایران بترکان رسید",
      "m2": "خروشی چوشیر ژیان برکشید",
      "verse_number": 32
    },
    {
      "id": "20db8674-d893-4385-b662-0b6e8cc49caf",
      "m1": "بپرموده گفت ای گریزنده مرد",
      "m2": "تو گرد دلیران جنگی مگرد",
      "verse_number": 33
    },
    {
      "id": "8934e9a3-2855-46d9-adf8-8e7d7e1c0a44",
      "m1": "نه مردی هنوز ای پسر کودکی",
      "m2": "روا باشد ار شیرمادر مکی",
      "verse_number": 34
    },
    {
      "id": "11869ecc-c5c8-46b2-a349-0566a6a26a50",
      "m1": "بدو گفت شاه ای گراینده شیر",
      "m2": "به خون ریختن چند باشی دلیر",
      "verse_number": 35
    },
    {
      "id": "3c548525-0e6f-4737-a662-30a61e50e855",
      "m1": "زخون سران سیر شد روز جنگ",
      "m2": "بخشکی پلنگ و بدریا نهنگ",
      "verse_number": 36
    },
    {
      "id": "88f1f0b8-cde6-476a-9ca0-7064ed2b0859",
      "m1": "نخواهی شد از خون مردم تو سیر",
      "m2": "برآنم که هستی تو درنده شیر",
      "verse_number": 37
    },
    {
      "id": "a48eb783-2565-4a8a-b3f8-2f1ca325b2f0",
      "m1": "بریده سر ساوه شاه آنک مهر",
      "m2": "برو داشت تا بود گردان سپهر",
      "verse_number": 38
    },
    {
      "id": "fe5f7834-275e-496b-a158-9d459d5794f8",
      "m1": "سپاهی بران گونه کردی تباه",
      "m2": "که بخشایش آورد خورشید و ماه",
      "verse_number": 39
    },
    {
      "id": "5639269a-025f-4f26-ba7e-6c91b48f766c",
      "m1": "ازان شاه جنگی منم یادگار",
      "m2": "مراهم چنان دان که کشتی بزار",
      "verse_number": 40
    },
    {
      "id": "35a0e482-2213-4532-afcc-63eaa136fd52",
      "m1": "ز مادر همه مرگ را زادهایم",
      "m2": "ار ای دون که ترکیم ار آزادهایم",
      "verse_number": 41
    },
    {
      "id": "2fa2c78e-fcd2-4b52-820d-19b3327a520b",
      "m1": "گریزانم و تو پس اندر دمان",
      "m2": "نیابی مرا تا نیاید زمان",
      "verse_number": 42
    },
    {
      "id": "5ff077f2-1323-49e8-bb8d-f40cbd0a631e",
      "m1": "اگر باز گردم سلیحی بچنگ",
      "m2": "مگر من شوم کشته گر تو به جنگ",
      "verse_number": 43
    },
    {
      "id": "3c8718a8-0fb6-4e51-8c65-48b845d4299c",
      "m1": "مکن تیز مغزی و آتش سری",
      "m2": "نه زین سان بود مهتر لشکری",
      "verse_number": 44
    },
    {
      "id": "544c48e2-768a-4e19-97f5-ed57f2b9e93b",
      "m1": "من ایدون شوم سوی خرگاه خویش",
      "m2": "یکی بازجویم سر راه خویش",
      "verse_number": 45
    },
    {
      "id": "f7c08a89-e2e0-4e07-980c-f68d606861d1",
      "m1": "نویسم یکی نامه زی شهریار",
      "m2": "مگر زو شوم ایمن از روزگار",
      "verse_number": 46
    },
    {
      "id": "9e8d61fb-af3b-4b5d-8353-28808b129bf9",
      "m1": "گر ای دون که اندر پذیرد مرا",
      "m2": "ازین ساختن پس گزیرد مرا",
      "verse_number": 47
    },
    {
      "id": "f1f4c10b-bf21-47ba-ac5a-fac93d3c225c",
      "m1": "من آن بارگه رایکی بندهام",
      "m2": "دل از مهتری پاک برکندهام",
      "verse_number": 48
    },
    {
      "id": "d9002156-8393-42ac-8e9f-cf223fba510e",
      "m1": "ز سرکینه وجنگ را دورکن",
      "m2": "بخوبی منش بریکی سورکن",
      "verse_number": 49
    },
    {
      "id": "dc41677e-d928-4ff9-9226-30931471b84d",
      "m1": "چوبشنید بهرام زو بازگشت",
      "m2": "که برساز شاهی خوش آواز گشت",
      "verse_number": 50
    },
    {
      "id": "42e13763-cff2-4ee6-b4c1-6d61bb811d02",
      "m1": "چو از جنگ آن لشکر آسوده شد",
      "m2": "بلشکر گه شاه پرموده شد",
      "verse_number": 51
    },
    {
      "id": "0aa2b58a-adf8-4c39-a58b-51c07c1b59d3",
      "m1": "همیگشت بر گرد دشت نبرد",
      "m2": "سرسرکشان را زتن دورکرد",
      "verse_number": 52
    },
    {
      "id": "8828ad7f-078a-4677-89ce-f7abba58b6b0",
      "m1": "چوبرهم نهاده بد انبوه گشت",
      "m2": "ببالا و پهنا یکی کوه گشت",
      "verse_number": 53
    },
    {
      "id": "3bf96038-fa62-4882-a8e0-4190a28b47c8",
      "m1": "مرآن جای را نامداران یل",
      "m2": "همی هرکسی خواند بهرام تل",
      "verse_number": 54
    },
    {
      "id": "a4ba637b-e043-4609-a902-7891334aea0c",
      "m1": "سلیح سواران وچیزی که دید",
      "m2": "بجایی که بد سوی آن تل کشید",
      "verse_number": 55
    },
    {
      "id": "445d3868-0593-470c-94bf-b9ec45e91932",
      "m1": "یکی نامه بنوشت زی شهریار",
      "m2": "ز پر موده و لشکر بیشمار",
      "verse_number": 56
    },
    {
      "id": "8b82c117-d012-40a1-b791-bc31f9e91f24",
      "m1": "بگفت آنک ما را چه آمد بروی",
      "m2": "ز ترکان و آن شاه پرخاشجوی",
      "verse_number": 57
    },
    {
      "id": "f1c3f7e2-fbaa-46b7-96f6-d8374f9c917b",
      "m1": "که از بیم تیغ او سوی چاره شد",
      "m2": "وزان جایگه شد خوار و آواره شد",
      "verse_number": 58
    },
    {
      "id": "c8fd8981-2566-4655-972e-29c82ce9d955",
      "m1": "وزین روی خاقان در دز ببست",
      "m2": "بانبوه و اندیشه اندر نشست",
      "verse_number": 59
    },
    {
      "id": "aba9d6bf-7028-4308-912b-41853c530461",
      "m1": "بگشتند گرد در دز بسی",
      "m2": "ندانست سامان جنگش کسی",
      "verse_number": 60
    },
    {
      "id": "d663aed2-bfd7-4b26-a41b-e366c2e52404",
      "m1": "چنین گفت زان پس که سامان جنگ",
      "m2": "کنون نیست در کارکردن درنگ",
      "verse_number": 61
    },
    {
      "id": "bf24e481-0ea7-413e-93b8-cc95467889fc",
      "m1": "یلان سینه راگفت تا سه هزار",
      "m2": "ازان جنگیان برگزیند سوار",
      "verse_number": 62
    },
    {
      "id": "d784d7da-d3c4-4a54-8c93-841f6d1378f7",
      "m1": "چهار از یلان نیز آذرگشسب",
      "m2": "ازان جنگیان برنشاند بر اسب",
      "verse_number": 63
    },
    {
      "id": "c9838391-9039-4a00-97ae-83d945234c54",
      "m1": "بفرمود تا هر که را یافتند",
      "m2": "بگردن زدن تیز بشتافتند",
      "verse_number": 64
    },
    {
      "id": "512270f4-657d-4a58-a980-21766dc3a7c7",
      "m1": "مگر نامدار از دز آید برون",
      "m2": "چوبیند همه دشت را رود خون",
      "verse_number": 65
    },
    {
      "id": "0c266f37-e652-4201-8ebf-13650ed90db6",
      "m1": "ببد بر در دز ازین سان سه روز",
      "m2": "چهارم چو بفروخت گیتی فروز",
      "verse_number": 66
    },
    {
      "id": "bb00242b-f71d-4d9a-9a80-6a60a19ed6ac",
      "m1": "پیامی فرستاد پرموده را",
      "m2": "مر آن مهتر کشور و دوده را",
      "verse_number": 67
    },
    {
      "id": "5c6c8227-c637-4bad-8c96-2620a0fc77b4",
      "m1": "کهای مهتر و شاه ترکان چین",
      "m2": "زگیتی چرا کردی این دز گزین",
      "verse_number": 68
    },
    {
      "id": "35150e5f-6ec8-4e0c-b61e-3e0d43757098",
      "m1": "کجا آن جهان جستن ساوه شاه",
      "m2": "کجا آن همه گنج و آن دستگاه",
      "verse_number": 69
    },
    {
      "id": "7e91d935-48bd-436f-9f66-0bf290e15c08",
      "m1": "کجا آن همه پیل و برگستوان",
      "m2": "کجا آن بزرگان روشن روان",
      "verse_number": 70
    },
    {
      "id": "45afc440-de3f-4bdf-ad78-3ec731ba30b3",
      "m1": "کجا آن همه تنبل و جادوی",
      "m2": "که اکنون از ایشان تو بر یکسوی",
      "verse_number": 71
    },
    {
      "id": "0bd5473c-7ecf-44d6-9625-f41afedb5b06",
      "m1": "همی شهر ترکان تو را بس نبود",
      "m2": "چو باب تو اندر جهان کس نبود",
      "verse_number": 72
    },
    {
      "id": "d462a335-ebf3-40ba-b168-999b674cff83",
      "m1": "نشستی برین باره بر چون زنان",
      "m2": "پرازخون دل ودست بر سر زنان",
      "verse_number": 73
    },
    {
      "id": "26ae4e6c-0529-44d1-a915-726a89e2e211",
      "m1": "درباره بگشای و زنهار خواه",
      "m2": "برشاه کشور مرا یارخواه",
      "verse_number": 74
    },
    {
      "id": "93c96bad-7c33-465a-9618-0f846b52e595",
      "m1": "ز دز گنج دینار بیرون فرست",
      "m2": "بگیتی نخورد آنک برپای بست",
      "verse_number": 75
    },
    {
      "id": "b822ae53-5de0-44c5-bea6-d6cee7faa5c4",
      "m1": "اگرگنج داری ز کشور بیار",
      "m2": "که دینار خوارست برشهریار",
      "verse_number": 76
    },
    {
      "id": "d9f17817-0743-494f-88dd-b551503c017e",
      "m1": "به درگاه شاهت میانجی منم",
