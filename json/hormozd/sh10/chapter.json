{
  "id": "81e61f4c-3269-4e60-8e58-abe6f8847a2a",
  "title": "بخش ۱۰",
  "verses": [
    {
      "id": "919509b0-9cc7-4448-b2ef-ed3aee1f8fd3",
      "m1": "چوگودرز وچون رستم پهلوان",
      "m2": "بکردند رنجه برین بر روان",
      "verse_number": 1
    },
    {
      "id": "2fab164f-3373-4c48-9c81-82deb658cfdc",
      "m1": "ازان پس کجا شد بهاماوران",
      "m2": "ببستند پایش ببند گران",
      "verse_number": 2
    },
    {
      "id": "2513b288-2d96-4ee0-b3cc-eb512923d1b6",
      "m1": "کس آهنگ این تخت شاهی نکرد",
      "m2": "جز از گرم و تیمار ایشان نخورد",
      "verse_number": 3
    },
    {
      "id": "bc9faf71-84cc-429b-9e72-72560d0a60bd",
      "m1": "چوگفتند با رستم ایرانیان",
      "m2": "که هستی تو زیبای تخت کیان",
      "verse_number": 4
    },
    {
      "id": "3ed06709-0d54-4076-8add-ca508ae8a355",
      "m1": "یکی بانگ برزد برآنکس که گفت",
      "m2": "که با دخمهٔ تنگ باشید جفت",
      "verse_number": 5
    },
    {
      "id": "122f10c9-bf28-4336-9347-d79e7259fd53",
      "m1": "که باشاه باشد کجا پهلوان",
      "m2": "نشستند بآیین و روشن روان",
      "verse_number": 6
    },
    {
      "id": "13c1f44f-e873-4dc6-9867-5d0c9c62a6f0",
      "m1": "مرا تخت زر باید و بسته شاه",
      "m2": "مباد این گمان ومباد این کلاه",
      "verse_number": 7
    },
    {
      "id": "cbba3861-221c-43c8-b4be-bc50e0acc0d8",
      "m1": "گزین کرد زایران ده ودوهزار",
      "m2": "جهانگیر وبرگستوانور سوار",
      "verse_number": 8
    },
    {
      "id": "1ca9c3f6-f5e6-4fdf-9be6-acca32646543",
      "m1": "رهانید ازبند کاوس را",
      "m2": "همان گیو و گودرز وهم طوس را",
      "verse_number": 9
    },
    {
      "id": "9ec00d79-48a5-4332-a733-7b9c5b846385",
      "m1": "همان شاه پیروز چون کشته شد",
      "m2": "بایرانیان کار برگشته شد",
      "verse_number": 10
    },
    {
      "id": "4af06a7a-e0c8-477c-a580-c9d3cedd462f",
      "m1": "دلاور شد از کار آن خوشنوار",
      "m2": "برام بنشست برتخت ناز",
      "verse_number": 11
    },
    {
      "id": "4e9db18d-82bd-4a4f-9ac7-b150ad6d9164",
      "m1": "چو فرزند قارن بشد سوفزای",
      "m2": "که آورد گاه مهی بازجای",
      "verse_number": 12
    },
    {
      "id": "1cd12712-0890-49a4-bd7c-2003b71ead9c",
      "m1": "ز پیروزی او چو آمد نشان",
      "m2": "ز ایران برفتند گردنکشان",
      "verse_number": 13
    },
    {
      "id": "f29de702-c5f2-41c2-bdeb-58a0e25d4d43",
      "m1": "که بروی بشاهی کنند آفرین",
      "m2": "شود کهتری شهریار زمین",
      "verse_number": 14
    },
    {
      "id": "b5c10ff8-1df8-4214-b253-38ea2f94b148",
      "m1": "بایرانیان گفت کین ناسزاست",
      "m2": "بزرگی وتاج ازپی پادشاست",
      "verse_number": 15
    },
    {
      "id": "574cc16a-8b24-4ff3-a279-45d38d54a5e1",
      "m1": "قباد ارچه خردست گردد بزرگ",
      "m2": "نیاریم دربیشهٔ شیرگرگ",
      "verse_number": 16
    },
    {
      "id": "c26fcd4a-a9eb-4ed0-9480-20da1ac7e942",
      "m1": "چوخواهی که شاهی کنی بینژاد",
      "m2": "همه دوده را داد خواهی بباد",
      "verse_number": 17
    },
    {
      "id": "f4c2a061-b95f-4249-a4f1-693346d08df5",
      "m1": "قباد آن زمان چون بمردی رسید",
      "m2": "سرسوفزای از درتاج دید",
      "verse_number": 18
    },
    {
      "id": "e6da6eae-5304-44e6-b247-060508465fec",
      "m1": "به گفتار بدگوهرانش بکشت",
      "m2": "کجا بود درپادشاهیش پشت",
      "verse_number": 19
    },
    {
      "id": "4d63d064-042d-44b5-85ae-559857aca693",
      "m1": "وزان پس ببستند پای قباد",
      "m2": "دلاور سواری گوی کی نژاد",
      "verse_number": 20
    },
    {
      "id": "8a336a61-a856-465b-815f-2a2b027990dd",
      "m1": "بزرمهر دادش یکی پرهنر",
      "m2": "که کین پدربازخواهد مگر",
      "verse_number": 21
    },
    {
      "id": "0ffead25-7289-4ea7-9a40-f457ee00d080",
      "m1": "نگه کرد زرمهر کس راندید",
      "m2": "که با تاج برتخت شاهی سزید",
      "verse_number": 22
    },
    {
      "id": "bca5328b-55e2-49d3-b938-30f0dd426087",
      "m1": "چوبرشاه افگند زرمهر مهر",
      "m2": "بروآفرین خواند گردان سپهر",
      "verse_number": 23
    },
    {
      "id": "808ca2a9-0fc6-4d2e-8a86-18e62d1edd3b",
      "m1": "ازو بند برداشت تاکار خویش",
      "m2": "بجوید کند تیز بازار خویش",
      "verse_number": 24
    },
    {
      "id": "2d3d42f1-54c5-47cf-acbf-b2b57e54cacb",
      "m1": "کس ازبندگان تاج هرگز نجست",
      "m2": "وگر چند بودی نژادش درست",
      "verse_number": 25
    },
    {
      "id": "24be11cd-4704-4ff8-9b28-1a80a1ddf530",
      "m1": "زترکان یکی نامور ساوهشاه",
      "m2": "بیامد که جوید نگین وکلاه",
      "verse_number": 26
    },
    {
      "id": "cf8d3c11-1de0-4d32-84d9-8e43bfb68441",
      "m1": "چنان خواست روشن جهان آفرین",
      "m2": "که اونیست گردد به ایران زمین",
      "verse_number": 27
    },
    {
      "id": "10d2e57d-b864-48b7-abd2-3178700ff750",
      "m1": "تو را آرزو تخت شاهنشهی",
      "m2": "چراکرد زان پس که بودی رهی",
      "verse_number": 28
    },
    {
      "id": "6dfd5e38-14a0-4333-8c2a-54b5d1cfaed9",
      "m1": "همی بر جهاند یلان سینه اسب",
      "m2": "که تامن زبهرام پورگشسب",
      "verse_number": 29
    },
    {
      "id": "96e85b6b-d8ce-49b9-a6a1-175a2e19383b",
      "m1": "بنودرجهان شهریاری کنم",
      "m2": "تن خویش را یادگاری کنم",
      "verse_number": 30
    },
    {
      "id": "9574ba50-1076-4b85-9acb-623eb13b9c74",
      "m1": "خردمند شاهی چونوشین روان",
      "m2": "بهرمز بدی روز پیری جوان",
      "verse_number": 31
    },
    {
      "id": "8bd77f8f-e8e9-4ab6-ac89-4e0b441074c7",
      "m1": "بزرگان کشور ورا یاورند",
      "m2": "اگر یاورانند گر کهترند",
      "verse_number": 32
    },
    {
      "id": "5557533c-c1b7-40ce-95a0-5b713bdc9b21",
      "m1": "به ایران سوارست سیصدهزار",
      "m2": "همه پهلوان وهمه نامدار",
      "verse_number": 33
    },
    {
      "id": "87374a77-5687-4368-980c-8223a7cafcb7",
      "m1": "همه یک بیک شاه را بندهاند",
      "m2": "بفرمان و رایش سرافگندهاند",
      "verse_number": 34
    },
    {
      "id": "7e8dd9c5-2a8c-44dc-9523-0980f76dd6bd",
      "m1": "شهنشاه گیتی تو را برگزید",
      "m2": "چنان کز ره نامداران سزید",
      "verse_number": 35
    },
    {
      "id": "7c86b8a1-d08d-46d2-b81d-4ff886b2c9e6",
      "m1": "نیاگانت را همچنین نام داد",
      "m2": "بفرجام بر دشمنان کام داد",
      "verse_number": 36
    },
    {
      "id": "d681d8a5-7853-4853-a349-2e75c1467d60",
      "m1": "تو پاداش آن نیکویی بد کنی",
      "m2": "چنان دان که بد با تن خود کنی",
      "verse_number": 37
    },
    {
      "id": "3ee903ec-0251-43a2-9d33-3202de3ea54e",
      "m1": "مکن آز را برخرد پادشا",
      "m2": "که دانا نخواند تو را پارسا",
      "verse_number": 38
    },
    {
      "id": "000a2222-e6df-4e9b-94fe-3a418127b439",
      "m1": "اگر من زنم پند مردان دهم",
      "m2": "ببسیار سال ازبرادر کهم",
      "verse_number": 39
    },
    {
      "id": "317fffcb-177f-4817-b343-10c024551c38",
      "m1": "مده کارکرد نیاکان بباد",
      "m2": "مبادا که پند من آیدت یاد",
      "verse_number": 40
    },
    {
      "id": "0256ca63-fddd-4776-90b0-cc32b2a0e771",
      "m1": "همه انجمن ماند زودرشگفت",
      "m2": "سپهدار لب را بدندان گرفت",
      "verse_number": 41
    },
    {
      "id": "05f22a3b-30d9-4261-a9ab-397b0482db8e",
      "m1": "بدانست کو راست گوید همی",
      "m2": "جز از راه نیکی نجوید همی",
      "verse_number": 42
    },
    {
      "id": "6976fbe0-d164-4d77-9a12-8c124747635b",
      "m1": "یلان سینه گفت ای گرانمایه زن",
      "m2": "تو درانجمن رای شاهان مزن",
      "verse_number": 43
    },
    {
      "id": "1b445b97-02c7-40a9-9b99-fcb01ed7a9e7",
      "m1": "که هرمز بدین چندگه بگذرد",
      "m2": "زتخت مهی پهلوان برخورد",
      "verse_number": 44
    },
    {
      "id": "6c775a05-2eb4-44f8-9a80-f1d16be5e0fb",
      "m1": "زهرمز چنین باشد اندر خبر",
      "m2": "برادرت را شاه ایران شمر",
      "verse_number": 45
    },
    {
      "id": "d6ac5ed4-5e77-4ce4-be8c-cabbae2b68ea",
      "m1": "بتاج کیی گر ننازد همی",
      "m2": "چراخلعت از دوک سازد همی",
      "verse_number": 46
    },
    {
      "id": "af1e3539-9846-4cb8-99f0-0afa67f839a4",
      "m1": "سخن بس کن ازهرمز ترک زاد",
      "m2": "که اندر زمانه مباد آن نژاد",
      "verse_number": 47
    },
    {
      "id": "1a318385-9849-4053-8402-2483a0b2be81",
      "m1": "گر از کیقباد اندرآری شمار",
      "m2": "برین تخمهٔ بر سالیان صدهزار",
      "verse_number": 48
    },
    {
      "id": "6fb9c400-8e67-416c-902c-9a8f954f8641",
      "m1": "که با تاج بودند برتخت زر",
      "m2": "سرآمد کنون نام ایشان مبر",
      "verse_number": 49
    },
    {
      "id": "e3e0a9d9-40e8-4d00-8516-839f119f1260",
      "m1": "ز پرویز خسرو میندیش نیز",
      "m2": "کزویاد کردن نیرزد بچیز",
      "verse_number": 50
    },
    {
      "id": "bfd92a95-00b0-4dc3-b55c-220b29d3e509",
      "m1": "بدرگاه او هرک ویژهترند",
      "m2": "برادرت راکهتر وچاکرند",
      "verse_number": 51
    },
    {
      "id": "df517083-0973-4f77-93ee-acca666dfe84",
      "m1": "چو بهرام گوید بران کهتران",
      "m2": "ببندند پایش ببند گران",
      "verse_number": 52
    },
    {
      "id": "3b59446b-7183-482e-84a7-1297eca6640e",
      "m1": "بدو گردیه گفت کای دیو ساز",
      "m2": "همی دیوتان دام سازد براز",
      "verse_number": 53
    },
    {
      "id": "c1d8cf15-ef1f-4a28-baf3-2eb8f2101bb7",
      "m1": "مکن برتن وجام ما بر ستم",
      "m2": "که از تو ببینم همی باد و دم",
      "verse_number": 54
    },
    {
      "id": "3526f0c9-4603-4886-b391-fe0c2e9495c5",
      "m1": "پدر مرزبان بود مارا بری",
      "m2": "تو افگندی این جستن تخت پی",
      "verse_number": 55
    },
    {
      "id": "264bdfdc-84c9-4677-a6b5-53dd2aab5266",
      "m1": "چو بهرام را دل بجوش آوری",
      "m2": "تبار مرا درخروش آوری",
      "verse_number": 56
    },
    {
      "id": "1796c84a-f90e-4760-85f7-4205bcc41a46",
      "m1": "شود رنج این تخمهٔ ما بباد",
      "m2": "به گفتار تو کهتر بدنژاد",
      "verse_number": 57
    },
    {
      "id": "903792a3-5ae2-4589-9b11-9fe15c0d18bf",
      "m1": "کنون راهبر باش بهرام را",
      "m2": "پرآشوب کن بزم و آرام را",
      "verse_number": 58
    },
    {
      "id": "1f21a944-bbe1-45e7-9253-f123c96038d6",
      "m1": "بگفت این وگریان سوی خانه شد",
      "m2": "به دل با برادر چو بیگانه شد",
      "verse_number": 59
    },
    {
      "id": "43a450c0-a44e-499e-9473-aa16eef730e4",
      "m1": "همیگفت هرکس که این پاک زن",
      "m2": "سخن گوی و روشن دل و رای زن",
      "verse_number": 60
    },
    {
      "id": "a5fa16d5-90be-4baa-a1bc-341d260529f8",
      "m1": "تو گویی که گفتارش از دفترست",
      "m2": "بدانش ز جاماسب نامی ترست",
      "verse_number": 61
    },
    {
      "id": "ccf3c104-304e-4389-a022-6cb34a6c2191",
      "m1": "چو بهرام را آن نیامد پسند",
      "m2": "همیبود ز آواز خواهر نژند",
      "verse_number": 62
    },
    {
      "id": "06ea1c9f-efc6-4d08-bc24-2b7968a77f7a",
      "m1": "دل تیره اندیشهٔ دیریاب",
      "m2": "همی تخت شاهی نمودش بخواب",
      "verse_number": 63
    },
    {
      "id": "82073537-a134-4984-bf03-945bb18daf77",
      "m1": "چنین گفت پس کین سرای سپنج",
      "m2": "نیابند جویندگان جز به رنج",
      "verse_number": 64
    },
    {
      "id": "a27e1e60-80ef-4c9f-92c9-1b67d33c3db9",
      "m1": "بفرمود تا خوان بیاراستند",
      "m2": "می و رود و رامشگران خواستند",
      "verse_number": 65
    },
    {
      "id": "7498e979-d2cb-4d7b-bbc7-a1684952aec6",
      "m1": "برامشگری گفت کامروز رود",
      "m2": "بیارای با پهلوانی سرود",
      "verse_number": 66
    },
    {
      "id": "95e622a3-b62d-4ef1-be58-573589122de9",
      "m1": "نخوانیم جز نامهٔ هفتخوان",
      "m2": "برین میگساریم لختی بخوان",
      "verse_number": 67
    },
    {
      "id": "474aecc9-e573-4d8c-9354-ff556233a5a2",
      "m1": "که چون شد برویین دز اسفندیار",
      "m2": "چه بازی نمود اندران روزگار",
      "verse_number": 68
    },
    {
      "id": "e7d70814-b5d6-4f4f-b565-4dbd23579298",
      "m1": "بخوردند بر یاد او چند می",
      "m2": "که آباد بادا برو بوم ری",
      "verse_number": 69
    },
    {
      "id": "80c90ca8-051c-4e01-9d9f-f47ef6ae3ec2",
      "m1": "کزان بوم خیزد سپهبد چوتو",
      "m2": "فزون آفریناد ایزد چو تو",
      "verse_number": 70
    },
    {
      "id": "3fb06e5d-59bc-4db2-a56f-8bbcb84a30fa",
      "m1": "پراگنده گشتند چون تیره شد",
      "m2": "سرمیگساران ز میخیره شد",
      "verse_number": 71
    },
    {
      "id": "392db015-b828-4f74-a80e-0e188f59a53f",
      "m1": "چو برزد سنان آفتاب بلند",
      "m2": "شب تیره گشت از درفشش نژند",
      "verse_number": 72
    },
    {
      "id": "50968b9f-e33e-455e-8563-10d235353c8d",
      "m1": "سپهدار بهرام گرد سترگ",
      "m2": "بفرمود تا شد دبیر بزرگ",
      "verse_number": 73
    },
    {
      "id": "b206ca64-b1bc-4ee5-a05e-eef7389d769a",
      "m1": "بخاقان یکی نامه ارتنگ وار",
      "m2": "نبشتند پربوی ورنگ ونگار",
      "verse_number": 74
    },
    {
      "id": "8c30eb72-bf36-4ccb-be15-a09a321a518f",
      "m1": "بپوزش کنان گفت هستم بدرد",
      "m2": "دلی پرپشیمانی و باد سرد",
      "verse_number": 75
    },
    {
      "id": "1798f413-7911-46f6-a559-2442581d05ef",
      "m1": "ازین پس من آن بوم و مرز تو را",
      "m2": "نگه دارم از بهر ارز تو را",
      "verse_number": 76
    },
    {
      "id": "34bd4efe-c463-46ea-9af5-f8919bc53723",
      "m1": "اگر بر جهان پاک مهتر شوم",
      "m2": "تو را همچو کهتر برادر شوم",
      "verse_number": 77
    },
    {
