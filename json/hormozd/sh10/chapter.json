    {
      "id": "6b22088a-ede7-4bee-964c-330d9a408b90",
      "m1": "سواران که آیین گشسب سوار",
      "m2": "بیاورده بود از در شهریار",
      "verse_number": 231
    },
    {
      "id": "88baae6d-22f2-497a-ba01-bdf2eb878217",
      "m1": "چوکار سپهبد بفرجام شد",
      "m2": "زلشکر بسی پیش بهرام شد",
      "verse_number": 232
    },
    {
      "id": "19c4d276-73b8-4620-ac8f-620ec151289d",
      "m1": "بسی نیز نزدیک خسرو شدند",
      "m2": "بمردانگی در جهان نو شدند",
      "verse_number": 233
    },
    {
      "id": "5058cea8-d31b-4084-8452-8c9f7b6c930f",
      "m1": "چنان شد که از بی شبانی رمه",
      "m2": "پراگنده گردد به روز دمه",
      "verse_number": 234
    },
    {
      "id": "19632739-cec2-4170-8bc6-ab22a72f9c88",
      "m1": "چوآگاهی آمد بر شهریار",
      "m2": "ز آیین گشسب آنک بد نامدار",
      "verse_number": 235
    },
    {
      "id": "633b49be-28f7-4a7d-b4ab-6e53c32d0916",
      "m1": "ز تنگی دربار دادن ببست",
      "m2": "ندیدش کسی نیز بامی بدست",
      "verse_number": 236
    },
    {
      "id": "94c8653e-7e77-4ec9-ba56-06fffa150f7b",
      "m1": "برآمد ز آرام وز خورد و خواب",
      "m2": "همیبود با دیدگان پر آب",
      "verse_number": 237
    },
    {
      "id": "d8a58778-caac-475a-a8fb-8ebe9b459b29",
      "m1": "بدربر سخن رفت چندی ز شاه",
      "m2": "که پرده فروهشت از بارگاه",
      "verse_number": 238
    },
    {
      "id": "f9781823-d249-4230-a7a3-15192b8e3c56",
      "m1": "یکی گفت بهرام شد جنگجوی",
      "m2": "بتخت بزرگی نهادست روی",
      "verse_number": 239
    },
    {
      "id": "c3400850-c200-46e9-81f6-1c64b8eaa515",
      "m1": "دگر گفت خسرو ز آزار شاه",
      "m2": "همی سوی ایران گذارد سپاه",
      "verse_number": 240
    },
    {
      "id": "874db61c-36a6-4d74-b0a8-a9aa0a29d695",
      "m1": "بماندند زان کار گردان شگفت",
      "m2": "همی هرکسی رای دیگر گرفت",
      "verse_number": 241
    },
    {
      "id": "60498f07-7564-4613-89b0-4984f818149c",
      "m1": "چو در طیسفون برشد این گفتگوی",
      "m2": "ازان پادشاهی بشد رنگ وبوی",
      "verse_number": 242
    },
    {
      "id": "f79c3806-a17b-42ad-aaf0-5b61b0de5b6f",
      "m1": "سربندگان پرشد از درد و کین",
      "m2": "گزیدند نفرینش بر آفرین",
      "verse_number": 243
    },
    {
      "id": "1c55eb2d-3556-48e0-9dd1-84d1cf826e13",
      "m1": "سپاه اندکی بد بدرگاه بر",
      "m2": "جهان تنگ شد بر دل شاه بر",
      "verse_number": 244
    },
    {
      "id": "b66b13d1-2ab8-4e9d-bfbc-966625713a56",
      "m1": "ببندوی و گستهم شد آگهی",
      "m2": "که تیره شد آن فر شاهنشهی",
      "verse_number": 245
    },
    {
      "id": "beda78d2-29c8-4819-9785-a7f44944a8da",
      "m1": "همه بستگان بند برداشتند",
      "m2": "یکی را بران کار بگماشتند",
      "verse_number": 246
    },
    {
      "id": "9ce39dc9-8d6b-44be-9594-dcfc2aebdf98",
      "m1": "کزان آگهی بازجوید که چیست",
      "m2": "ز جنگ آوران بر در شاه کیست",
      "verse_number": 247
    },
    {
      "id": "8dfbb3a2-0b9b-4403-829a-336578b03832",
      "m1": "ز کار زمانه چو آگه شدند",
      "m2": "ز فرمان بگشتند و بیره شدند",
      "verse_number": 248
    },
    {
      "id": "7660016a-439a-482d-90c2-e79108f021dd",
      "m1": "شکستند زندان و برشد خروش",
      "m2": "بران سان که هامون برآید بجوش",
      "verse_number": 249
    },
    {
      "id": "6fd4538b-7416-4667-a0ba-23f6726b1e74",
      "m1": "بشهر اندرون هرک بد لشکری",
      "m2": "بماندند بیچاره زان داوری",
      "verse_number": 250
    },
    {
      "id": "663e67c2-cc55-4f15-ada2-5bfc2e29f8a5",
      "m1": "همیرفت گستهم و بندوی پیش",
      "m2": "زره دار با لشکر و ساز خویش",
      "verse_number": 251
    },
    {
      "id": "df36be02-29b3-4202-8b85-0ba843382050",
      "m1": "یکایک ز دیده بشستند شرم",
      "m2": "سواران بدرگاه رفتند گرم",
      "verse_number": 252
    },
    {
      "id": "14e9f24e-699c-4629-b1de-8dc91899f919",
      "m1": "ز بازار پیش سپاه آمدند",
      "m2": "دلاور بدرگاه شاه آمدند",
      "verse_number": 253
    },
    {
      "id": "b6884faf-0bf4-4d10-9f5f-b5997e7235c5",
      "m1": "که گر گشت خواهید با مایکی",
      "m2": "مجویید آزرم شاه اندکی",
      "verse_number": 254
    },
    {
      "id": "6a731644-93f9-431e-b4fa-1605d38a7d56",
      "m1": "که هرمز بگشتست از رای وراه",
      "m2": "ازین پس مر اورا مخوانید شاه",
      "verse_number": 255
    },
    {
      "id": "b9a52d4b-f8ef-4bba-bd3d-10e1916e60d0",
      "m1": "بباد افره او بیازید دست",
      "m2": "برو بر کنید آب ایران کبست",
      "verse_number": 256
    },
    {
      "id": "ad52afc0-f9f0-453e-8e08-d945a6761d0f",
      "m1": "شما را بویم اندرین پیشرو",
      "m2": "نشانیم برگاه اوشاه نو",
      "verse_number": 257
    },
    {
      "id": "85ef23e5-0c4d-4a4e-b434-9ef60df918dd",
      "m1": "وگر هیچ پستی کنید اندرین",
      "m2": "شما را سپاریم ایران زمین",
      "verse_number": 258
    },
    {
      "id": "fb558803-f342-4c7c-9091-5ea845d0036b",
      "m1": "یکی گوشهای بس کنیم ازجهان",
      "m2": "بیک سو خرامیم باهمرهان",
      "verse_number": 259
    },
    {
      "id": "927e4d1a-d4ac-420f-9070-0fb4ce2be386",
      "m1": "بگفتار گستهم یکسر سپاه",
      "m2": "گرفتند نفرین برام شاه",
      "verse_number": 260
    },
    {
      "id": "a7be227a-753f-4c30-a254-5da976bd7435",
      "m1": "که هرگز مبادا چنین تاجور",
      "m2": "کجا دست یازد به خون پسر",
      "verse_number": 261
    },
    {
      "id": "0b62d09c-9503-4d03-8021-864e4539c4dd",
      "m1": "به گفتار چون شوخ شد لشکرش",
      "m2": "هم آنگه زدند آتش اندر درش",
      "verse_number": 262
    },
    {
      "id": "fe312671-8fde-4a3f-a83f-8de519338356",
      "m1": "شدند اندرایوان شاهنشهی",
      "m2": "به نزدیک آن تخت بافرهی",
      "verse_number": 263
    },
    {
      "id": "455e9604-592b-4a1e-8bfd-311ba9f3df9b",
      "m1": "چوتاج از سرشاه برداشتند",
      "m2": "ز تختش نگونسار برگاشتند",
      "verse_number": 264
    },
    {
      "id": "cef11516-9064-4654-a451-2a748f91b50f",
      "m1": "نهادند پس داغ بر چشم شاه",
      "m2": "شد آنگاه آن شمع رخشان سیاه",
      "verse_number": 265
    },
    {
      "id": "6a107034-a4df-45f2-ba3f-1d0b0b4cd0fc",
      "m1": "ورا همچنان زنده بگذاشتند",
      "m2": "زگنج آنچ بد پاک برداشتند",
      "verse_number": 266
    },
    {
      "id": "5b37f605-8da0-4121-aef6-66e89147f4dd",
      "m1": "چنینست کردار چرخ بلند",
      "m2": "دل اندر سرای سپنجی مبند",
      "verse_number": 267
    },
    {
      "id": "cd8feba5-a843-4869-9533-f434079e5a75",
      "m1": "گهی گنج بینیم ازوگاه رنج",
      "m2": "براید بما بر سرای سپنج",
      "verse_number": 268
    },
    {
      "id": "d86aeb8b-76e3-4fef-8084-0e88804e580c",
      "m1": "اگر صد بود سال اگر صدهزار",
      "m2": "گذشت آن سخن کید اندر شمار",
      "verse_number": 269
    },
    {
      "id": "5d3f94f7-d9c1-4c52-a229-b995db181207",
      "m1": "کسی کو خریدار نیکوشود",
      "m2": "نگوید سخن تا بدی نشنود",
      "verse_number": 270
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 270
  },
