    {
      "id": "5dcf1d40-c437-4c34-b392-04117e483235",
      "m1": "که آن آفرین باز نفرین شود",
      "m2": "وزو چرخ گردنده پرکین شود",
      "verse_number": 154
    },
    {
      "id": "21780f75-c66a-4be8-b6c7-c450f32feb50",
      "m1": "چو یزدان تو را فرهی داد و بخت",
      "m2": "همه لشکر گنج با تاج وتخت",
      "verse_number": 155
    },
    {
      "id": "c06d29ff-9b5e-4e79-8372-742190e19fb9",
      "m1": "ازو گر پذیری بافزون شود",
      "m2": "دل از ناسپاسی پرازخون شود",
      "verse_number": 156
    },
    {
      "id": "cf61ae2c-df8a-4a06-851c-e7d5f47734b1",
      "m1": "ازان پس ببهرام بهرام گفت",
      "m2": "کهای با خردیاروبا رای جفت",
      "verse_number": 157
    },
    {
      "id": "ed7cce07-2a83-48a9-9d55-d5336dada048",
      "m1": "چه گویی کزین جستن تخت وگنج",
      "m2": "بزرگیست فرجام گر درد ورنج",
      "verse_number": 158
    },
    {
      "id": "3fad82a9-5c8c-498a-9466-5d5c1fbd74f6",
      "m1": "بخندید بهرام ازان داوری",
      "m2": "ازان پس برانداخت انگشتری",
      "verse_number": 159
    },
    {
      "id": "2a799530-af4e-4e8d-b670-2bf059562c2f",
      "m1": "بدو گفت چندانک این در هوا",
      "m2": "بماند شود بندهای پادشا",
      "verse_number": 160
    },
    {
      "id": "53f75e57-4a58-4212-ae24-ff1fa7a81a97",
      "m1": "بدو گفت کین را مپندار خرد",
      "m2": "که دیهیم را خرد نتوان شمرد",
      "verse_number": 161
    },
    {
      "id": "ae817c7e-fe6d-4937-9c49-8e8d597e898f",
      "m1": "چنین گفت زان پس بپیداگشسب",
      "m2": "کهای تیغ زن شیر تازنده اسب",
      "verse_number": 162
    },
    {
      "id": "730b72f7-c179-4758-98c2-5d0a01e29f8b",
      "m1": "چه بینی چه گویی بدین کار ما",
      "m2": "بود گاه شاهی سزاوار ما",
      "verse_number": 163
    },
    {
      "id": "f73c2732-fe6c-4e05-8132-7bf7be7f9083",
      "m1": "چنین گفت پیداگشسب سوار",
      "m2": "کهای از یلان جهان یادگار",
      "verse_number": 164
    },
    {
      "id": "825273df-d519-400a-9ed0-098588c8f709",
      "m1": "یکی موبدی داستان زد برین",
      "m2": "که هرکس که دانا بد وپیش بین",
      "verse_number": 165
    },
    {
      "id": "6d0e2a51-3075-4b01-b78e-2c375cdd2fa4",
      "m1": "اگر پادشاهی کند یک زمان",
      "m2": "روانش بپرد سوی آسمان",
      "verse_number": 166
    },
    {
      "id": "0d8298a1-1c45-4426-8755-09a967efff5a",
      "m1": "به ازبنده بودن بسال دراز",
      "m2": "به گنج جهاندار بردن نیاز",
      "verse_number": 167
    },
    {
      "id": "8a7eebb8-e327-42d1-9b36-aae4535d7e55",
      "m1": "چنین گفت پس با دبیر بزرگ",
      "m2": "که بگشای لب را تو ای پیرگرگ",
      "verse_number": 168
    },
    {
      "id": "459a893f-e761-427d-b5ab-bf10da3dd0c3",
      "m1": "دبیر بزرگ آن زمان لب ببست",
      "m2": "بانبوه اندیشه اندر نشست",
      "verse_number": 169
    },
    {
      "id": "4c59279b-57f8-4f9e-b4ad-82784262ca64",
      "m1": "ازان پس چنین گفت بهرام را",
      "m2": "که هرکس که جویا بود کامرا",
      "verse_number": 170
    },
    {
      "id": "ce4946b2-9d8f-417d-be94-7181927cbf7f",
      "m1": "چودرخور بجوید بیابد همان",
      "m2": "درازست ویازنده دست زمان",
      "verse_number": 171
    },
    {
      "id": "4e11595a-1981-44a9-b982-0be19501be20",
      "m1": "زچیزی که بخشش کند دادگر",
      "m2": "چنان دان که کوشش بیاید ببر",
      "verse_number": 172
    },
    {
      "id": "a58882d5-0c8a-4f6a-a0c7-8f98673b5f38",
      "m1": "بهمدان گشسب آن زمان گفت باز",
      "m2": "کهای گشته اندر نشیب وفراز",
      "verse_number": 173
    },
    {
      "id": "cdcda88d-47db-4892-a835-7cec58983380",
      "m1": "سخن هرچگویی بروی کسان",
      "m2": "شود باد وکردار او نارسان",
      "verse_number": 174
    },
    {
      "id": "cad782b9-96ae-4777-9718-2810e17b715e",
      "m1": "بگو آنچ دانی به کار اندورن",
      "m2": "زنیک وبد روزگار اندرون",
      "verse_number": 175
    },
    {
      "id": "688f366f-df90-42c7-bc44-e2654b0e7192",
      "m1": "چنین گفت همدان گشسب بلند",
      "m2": "کهای نزد پرمایگان ارجمند",
      "verse_number": 176
    },
    {
      "id": "0edc81ef-1fcf-4656-8f5a-df6fc0991c83",
      "m1": "زناآمده بد بترسی همی",
      "m2": "زدیهیم شاهان چه پرسی همی",
      "verse_number": 177
    },
    {
      "id": "67b0d5d7-1617-4bad-91a2-bdfd2348c172",
      "m1": "بکن کار وکرده به یزدان سپار",
      "m2": "بخرما چه یازی چوترسی زخار",
      "verse_number": 178
    },
    {
      "id": "9737f468-20fe-4cb9-bda7-486a21600f49",
      "m1": "تن آسان نگردد سرانجمن",
      "m2": "همه بیم جان باشد ورنج تن",
      "verse_number": 179
    },
    {
      "id": "24bcd4c5-81ea-48ec-9d95-54b1ab06e805",
      "m1": "زگفتارشان خواهر پهلوان",
      "m2": "همیبود پیچان وتیره روان",
      "verse_number": 180
    },
    {
      "id": "9e1cf958-100b-4431-9f74-d35bfe0f4a21",
      "m1": "بران داوری هیچ نگشاد لب",
      "m2": "زبرگشتن هور تا نیم شب",
      "verse_number": 181
    },
    {
      "id": "acd5cdbe-065c-48a6-b6b0-38a2f7e864a7",
      "m1": "بدو گفت بهرام کای پاک تن",
      "m2": "چه بینی به گفتار این انجمن",
      "verse_number": 182
    },
    {
      "id": "045c4457-f3dc-4078-b9ce-9326cb515c20",
      "m1": "ورا گردیه هیچ پاسخ نداد",
      "m2": "نه از رای آن مهتران بود شاد",
      "verse_number": 183
    },
    {
      "id": "5cd4fed7-e808-47cd-994c-e01ac2e3510b",
      "m1": "چنین گفت اوبا دبیر بزرگ",
      "m2": "کهای مرد بدساز چون پیرگرگ",
      "verse_number": 184
    },
    {
      "id": "291ef5df-787e-4e40-93c1-b851978679b9",
      "m1": "گمانت چنینست کین تاج وتخت",
      "m2": "سپاه بزرگی و پیروزبخت",
      "verse_number": 185
    },
    {
      "id": "f4ca7acb-e65f-42c0-b596-834c601ad3bf",
      "m1": "ز گیتی کسی را نبد آرزوی",
      "m2": "ازان نامداران آزاده خوی",
      "verse_number": 186
    },
    {
      "id": "0016f7dc-55ef-4bd6-86eb-6233525efaef",
      "m1": "مگر شاهی آسانتر از بندگیست",
      "m2": "بدین دانش تو بباید گریست",
      "verse_number": 187
    },
    {
      "id": "a7e0daa7-3f88-4763-b590-7431f3a7be00",
      "m1": "بر آیین شاهان پیشین رویم",
      "m2": "سخنهای آن برتران بشنویم",
      "verse_number": 188
    },
    {
      "id": "bacf27da-b582-4bc1-b470-27109464c32c",
      "m1": "چنین داد پاسخ مر او را دبیر",
      "m2": "که گر رای من نیستت جایگیر",
      "verse_number": 189
    },
    {
      "id": "e3b2ecbe-a7aa-454d-9b61-0ac1eb6ed286",
      "m1": "هم آن گوی وآن کن که رای آیدت",
      "m2": "بران رو که دل رهنمای آیدت",
      "verse_number": 190
    },
    {
      "id": "15a6eb0d-1855-4c7a-9c0a-6868c76d798f",
      "m1": "همان خواهرش نیز بهرام را",
      "m2": "بگفت آن سواران خودکام را",
      "verse_number": 191
    },
    {
      "id": "e1958d23-e6ff-4251-9c13-818b3052cefb",
      "m1": "نه نیکوست این دانش ورای تو",
      "m2": "بکژی خرامد همی پای تو",
      "verse_number": 192
    },
    {
      "id": "3b00489c-2cc3-4abd-bd2f-fdeed7be329e",
      "m1": "بسی بد که بیکار بدتخت شاه",
      "m2": "نکرد اندرو هیچکهتر نگاه",
      "verse_number": 193
    },
    {
      "id": "54b964ce-41a0-459d-8970-462164258b7a",
      "m1": "جهان را بمردی نگه داشتند",
      "m2": "یکی چشم برتخت نگماشتند",
      "verse_number": 194
    },
    {
      "id": "49cd71fb-b0c1-473f-8bc1-5fe26a20afd7",
      "m1": "هرآنکس که دانا بدو پاک مغز",
      "m2": "زهرگونه اندیشهای راند نغز",
      "verse_number": 195
    },
    {
      "id": "5a4a6976-48ba-4229-93b4-798268264b1a",
      "m1": "بداند که شاهی به ازبندگیست",
      "m2": "همان سرافرازی زافگندگیست",
      "verse_number": 196
    },
    {
      "id": "47bfe5e7-5eb3-4b4b-83c3-1e84b314fc37",
      "m1": "نبودند یازان بتخت کیان",
      "m2": "همه بندگی را کمر برمیان",
      "verse_number": 197
    },
    {
      "id": "2d8fbd3b-f2a6-4fe1-a484-0b9878a7d2b2",
      "m1": "ببستند و زیشان بهی خواستند",
      "m2": "همه دل بفرمانش آراستند",
      "verse_number": 198
    },
    {
      "id": "b5e08318-62dc-4b6a-8fdf-f950c1b5ec40",
      "m1": "نه بیگانه زیبای افسر بود",
      "m2": "سزای بزرگی بگوهر بود",
      "verse_number": 199
    },
    {
      "id": "f03a003c-f59b-4489-9657-cb4e6b7e980d",
      "m1": "زکاوس شاه اندرآیم نخست",
      "m2": "کجا راه یزدان همیبازجست",
      "verse_number": 200
    },
    {
      "id": "d94cfe53-9ba5-4588-923d-22d267502fff",
      "m1": "که برآسمان اختران بشمرد",
      "m2": "خم چرخ گردنده رابشکرد",
      "verse_number": 201
    },
    {
      "id": "7cfb8906-f223-4907-897d-90663331448d",
      "m1": "به خواری و زاری بساری فتاد",
      "m2": "از اندیشهٔ کژ وز بدنهاد",
      "verse_number": 202
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 202
  },
