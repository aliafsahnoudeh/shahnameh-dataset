    },
    {
      "id": "bfc07089-1765-4262-8bee-28d9fb0926c6",
      "m1": "وزان پس یکی شاه خیزد سترگ",
      "m2": "ز ترکان بیارد سپاهی بزرگ",
      "verse_number": 155
    },
    {
      "id": "f88ea67f-a181-47a6-a411-d5d039c4ece7",
      "m1": "بسازد که ایران و شهریمن",
      "m2": "سراسر بگیرد بران انجمن",
      "verse_number": 156
    },
    {
      "id": "a985c1e6-c588-4601-a575-54754ad8eb8f",
      "m1": "ازو شاه ایران شود دردمند",
      "m2": "بترسد ز پیروز بخت بلند",
      "verse_number": 157
    },
    {
      "id": "3df68415-8fc5-4db3-bb86-8e111b40b66c",
      "m1": "یکی کهتری باشدش دوردست",
      "m2": "سواری سرافراز مهترپرست",
      "verse_number": 158
    },
    {
      "id": "31abccea-b50c-418e-9c0c-a8fca20b601b",
      "m1": "ببالا دراز و به اندام خشک",
      "m2": "به گرد سرش جعد مویی چومشک",
      "verse_number": 159
    },
    {
      "id": "b04150d8-0b7d-4b57-b966-19e3da44811c",
      "m1": "سخن آوری جلد و بینی بزرگ",
      "m2": "سیه چرده و تندگوی و سترگ",
      "verse_number": 160
    },
    {
      "id": "33562097-b8a7-4556-b5f0-344b9330d9aa",
      "m1": "جهانجوی چوبینه دارد لقب",
      "m2": "هم از پهلوانانشان باشد نسب",
      "verse_number": 161
    },
    {
      "id": "d35efd02-de7b-4f8e-8c9f-3870aad90291",
      "m1": "چو این مرد چاکر باندک سپاه",
      "m2": "ز جایی بیاید به درگاه شاه",
      "verse_number": 162
    },
    {
      "id": "f5ecce94-2898-4928-acd9-d54535f8bbc1",
      "m1": "مرین ترک را ناگهان بشکند",
      "m2": "همه لشکرش را بهم برزند",
      "verse_number": 163
    },
    {
      "id": "21ca01fb-b77d-40fe-8be1-63cb097f81c2",
      "m1": "چو بشنید گفت ستاره شمر",
      "m2": "ندیدم ز خاقان کسی شادتر",
      "verse_number": 164
    },
    {
      "id": "1573352e-1153-4341-ae6e-4f2813f9f697",
      "m1": "به نوشین روان داد پس دخترش",
      "m2": "که از دختران او بدی افسرش",
      "verse_number": 165
    },
    {
      "id": "f0e003c0-00ef-4d55-b906-4df7f7aeacb5",
      "m1": "پذیرفتم او را من ازبهر شاه",
      "m2": "چو آن کرده بد بازگشتم به راه",
      "verse_number": 166
    },
    {
      "id": "70ec18aa-a8ff-4b0d-92fc-8b5a82eb6dee",
      "m1": "بیاورد چندی گهرها ز گنج",
      "m2": "که ما یافتیم از کشیدنش رنج",
      "verse_number": 167
    },
    {
      "id": "0828c3d5-b76f-423e-88b6-c09bf629edfe",
      "m1": "همان تا لب رود جیحون براند",
      "m2": "جهان بین خود را بکشتی نشاند",
      "verse_number": 168
    },
    {
      "id": "e2db83a7-7c27-4423-993a-b1fb638ff224",
      "m1": "ز جیحون دلی پر ز غم بازگشت",
      "m2": "ز فرزند با درد انباز گشت",
      "verse_number": 169
    },
    {
      "id": "38e25706-ce1d-4f01-8e14-edabe166d18f",
      "m1": "کنون آنچ دیدم بگفتم همه",
      "m2": "به پیش جهاندار شاه رمه",
      "verse_number": 170
    },
    {
      "id": "a026fe54-8517-4ae2-bc5d-ac48703058ce",
      "m1": "ازین کشور این مرد را باز جوی",
      "m2": "بپوینده شاید که گویی بپوی",
      "verse_number": 171
    },
    {
      "id": "4e518936-b3e5-46aa-8ce2-f7929ffdf096",
      "m1": "که پیروزی شاه بر دست اوست",
      "m2": "بدشمن ممان این سخن گر بدوست",
      "verse_number": 172
    },
    {
      "id": "2edf7d53-1106-45ea-b22d-7212d4604155",
      "m1": "بگفت این و جانش برآمد ز تن",
      "m2": "برو زار و گریان شدند انجمن",
      "verse_number": 173
    },
    {
      "id": "c8cf5803-d79e-4013-9d77-ad18250c1e9d",
      "m1": "شهنشاه زو در شگفتی بماند",
      "m2": "به مژگان همی خون دل برفشاند",
      "verse_number": 174
    },
    {
      "id": "2b8798c8-cee4-4e09-bd03-8581706d5e1a",
      "m1": "به ایرانیان گفت مهران ستاد",
      "m2": "همیداشت این راستیها بیاد",
      "verse_number": 175
    },
    {
      "id": "dca208e8-f8da-4935-9b90-170c54cb3cb6",
      "m1": "چو با من یکایک بگفت و بمرد",
      "m2": "پسندیده جانش به یزدان سپرد",
      "verse_number": 176
    },
    {
      "id": "81075636-ba65-45b4-b62a-3ae183c12a4b",
      "m1": "سپاسم ز یزدان کزین مرد پیر",
      "m2": "برآمد چنین گفتن ناگزیر",
      "verse_number": 177
    },
    {
      "id": "57f3a5df-659c-4900-b5fd-5fd9e2b79631",
      "m1": "نشان جست باید ز هر مهتری",
      "m2": "اگر مهتری باشد ار کهتری",
      "verse_number": 178
    },
    {
      "id": "af0f7869-117a-45ce-85b3-c47af715df7b",
      "m1": "بجویید تا این بجای آورید",
      "m2": "همه رنجها را به پای آورید",
      "verse_number": 179
    },
    {
      "id": "b73fd227-b512-4e3d-b78e-9fe4ab4a6276",
      "m1": "یکی مهتری نامبردار بود",
      "m2": "که بر آخر اسب سالار بود",
      "verse_number": 180
    },
    {
      "id": "0e82fc3a-7338-4c12-83a0-52b8cad4b813",
      "m1": "کجا راد فرخ بدی نام اوی",
      "m2": "همه شادی شاه بد کام اوی",
      "verse_number": 181
    },
    {
      "id": "39235ac6-5378-4f88-9341-387f0dc7b8a5",
      "m1": "بیامد بر شاه گفت این نشان",
      "m2": "که داد این ستوده به گردنکشان",
      "verse_number": 182
    },
    {
      "id": "8c46b033-98dd-473a-91d3-6665e6cad985",
      "m1": "ز بهرام بهرام پورگشسب",
      "m2": "سواری سرافراز و پیچنده اسب",
      "verse_number": 183
    },
    {
      "id": "d4269cd7-54c6-4efd-8fb2-fcda954fe924",
      "m1": "ز اندیشهٔ من بخواهد گذشت",
      "m2": "ندیدم چنو مرزبانی به دشت",
      "verse_number": 184
    },
    {
      "id": "3b87200a-0764-48d3-83b0-26796ce9a56f",
      "m1": "که دادی بدو بردع و اردبیل",
      "m2": "یکی نامور گشت باکوس وخیل",
      "verse_number": 185
    },
    {
      "id": "864feb0d-316b-4060-a5ed-cda127f9016b",
      "m1": "فرستاد و بهرام را مژده داد",
      "m2": "سخنهای مهران برو کرد یاد",
      "verse_number": 186
    },
    {
      "id": "359f2a4b-df3a-4a03-bc7f-32ed38ee940c",
      "m1": "جهانجوی پویان ز بردع برفت",
      "m2": "ز گردنکشان لشکری برد تفت",
      "verse_number": 187
    },
    {
      "id": "c1631157-a1d2-4430-946d-53956ef488ff",
      "m1": "چوبهرام تنگ اندر آمد ز راه",
      "m2": "بفرمود تا بار دادند شاه",
      "verse_number": 188
    },
    {
      "id": "4d2e3981-32ca-4f6a-a3ce-da160b88032b",
      "m1": "جهاندیده روی شهنشاه دید",
      "m2": "بران نامدار آفرین گسترید",
      "verse_number": 189
    },
    {
      "id": "bdb15289-2e82-4b4a-9d51-097f7c4f092e",
      "m1": "نگه کرد شاه اندرو یک زمان",
      "m2": "نبودش بدو جز به نیکی گمان",
      "verse_number": 190
    },
    {
      "id": "d650eddf-1f4c-4e1c-89d2-ec4f8fc529f3",
      "m1": "نشاینهای مهران ستاد اندروی",
      "m2": "بدید و بخندید وشد تازه روی",
      "verse_number": 191
    },
    {
      "id": "b91b49f9-4d0d-4a90-8719-85820f38a622",
      "m1": "ازان پس بپرسید و بنواختش",
      "m2": "یکی نامور جایگه ساختش",
      "verse_number": 192
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 192
  },
