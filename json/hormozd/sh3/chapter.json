{
  "id": "15484c0f-ec54-4fb6-adf3-280d49ced2b0",
  "title": "بخش ۳",
  "verses": [
    {
      "id": "015fd2c4-9bdb-47b5-b904-b3ab41d92ab0",
      "m1": "بدانست هرمز که او دست خون",
      "m2": "بیازد همی زنده بیرهنمون",
      "verse_number": 1
    },
    {
      "id": "c8744ef3-5226-428b-b8f2-f1a045e24783",
      "m1": "شنید آن سخنهای بیکام را",
      "m2": "به زندان فرستاد بهرام را",
      "verse_number": 2
    },
    {
      "id": "04ccc711-a998-48a7-a85d-0ba7b42322bb",
      "m1": "دگر شب چو برزد سر از کوه ماه",
      "m2": "به زندان دژ آگاه کردش تباه",
      "verse_number": 3
    },
    {
      "id": "66f5b67b-848f-4f39-a1ad-87edb16c1a13",
      "m1": "نماند آن زمان بر درش بخردی",
      "m2": "همان رهنمائی و هم موبدی",
      "verse_number": 4
    },
    {
      "id": "beddd2ea-93f1-4530-8691-c1c5a09386db",
      "m1": "ز خوی بد آید همه بدتری",
      "m2": "نگر تا سوی خوی بد ننگری",
      "verse_number": 5
    },
    {
      "id": "d762a089-1c27-4bc6-be4e-703ca30c9b97",
      "m1": "وزان پس نبد زندگانیش خوش",
      "m2": "ز تیمار زد بر دل خویش تش",
      "verse_number": 6
    },
    {
      "id": "6926bdaf-bd23-4539-b933-3e17c3c3ada8",
      "m1": "بسالی با صطخر بودی دو ماه",
      "m2": "که کوتاه بودی شبان سیاه",
      "verse_number": 7
    },
    {
      "id": "fc784c66-0d6e-42cd-846b-63a136316643",
      "m1": "که شهری خنک بود و روشن هوا",
      "m2": "از آنجا گذشتن نبودی روا",
      "verse_number": 8
    },
    {
      "id": "5f1262d4-f9c9-4a8d-a7dd-33a638caae5e",
      "m1": "چوپنهان شدی چادر لاژورد",
      "m2": "پدید آمدی کوه یاقوت زرد",
      "verse_number": 9
    },
    {
      "id": "79ace615-a419-47fd-a177-bcbc23487987",
      "m1": "منادیگری برکشیدی خروش",
      "m2": "که این نامداران با فر و هوش",
      "verse_number": 10
    },
    {
      "id": "e62a2736-1cdb-4dde-9454-a1b2edecc1f9",
      "m1": "اگر کشتمندی شود کوفته",
      "m2": "وزان رنج کارنده آشوفته",
      "verse_number": 11
    },
    {
      "id": "2b4836b7-544d-49d9-9e21-aec50435342e",
      "m1": "وگر اسب در کشت زاری رود",
      "m2": "کس نیز بر میوه داری رود",
      "verse_number": 12
    },
    {
      "id": "3a9e8391-7d1e-4e10-a2a6-1a4da54153d7",
      "m1": "دم و گوش اسبش بباید برید",
      "m2": "سر دزد بردار باید کشید",
      "verse_number": 13
    },
    {
      "id": "cbfb8d46-deea-46c4-ae80-39c82be245b7",
      "m1": "بدو ماه گردان بدی درجهان",
      "m2": "بدو نیکویی زو نبودی نهان",
      "verse_number": 14
    },
    {
      "id": "3aec88d7-4083-4cf3-bc77-67c655ba11c8",
      "m1": "بهر کشوری داد کردی چنین",
      "m2": "ز دهقان همییافتی آفرین",
      "verse_number": 15
    },
    {
      "id": "08c8a3a0-655c-43fe-bc89-0860dafbcc6d",
      "m1": "پسر بد مر او را گرامی یکی",
      "m2": "که از ماه پیدا نبود اندکی",
      "verse_number": 16
    },
    {
      "id": "fd003f3d-d2c6-425a-8832-101e991cd9ac",
      "m1": "مر او را پدر کرده پرویز نام",
      "m2": "گهش خواندی خسرو شادکام",
      "verse_number": 17
    },
    {
      "id": "5bbbe063-dfe2-42f5-a609-bd0e4216a433",
      "m1": "نبودی جدا یک زمان از پدر",
      "m2": "پدر نیز نشگیفتی از پسر",
      "verse_number": 18
    },
    {
      "id": "ec94f605-d95c-4a59-8e66-a0336ba3d7d0",
      "m1": "چنان بد که اسبی ز آخر بجست",
      "m2": "که بد شاه پرویز را بر نشست",
      "verse_number": 19
    },
    {
      "id": "c0a557ae-630c-4718-9f55-30c45e9c0c76",
      "m1": "سوی کشتمند آمد اسب جوان",
      "m2": "نگهبان اسب اندر آمد دوان",
      "verse_number": 20
    },
    {
      "id": "ad14cb81-104a-40cb-b058-c2503d8589a4",
      "m1": "بیامد خداوند آن کشت زار",
      "m2": "به پیش موکل بنالید زار",
      "verse_number": 21
    },
    {
      "id": "d8a5c165-1ce6-4754-9e1b-47dd0311030c",
      "m1": "موکل بدو گفت کین اسب کیست",
      "m2": "که بر دم و گوشش بباید گریست",
      "verse_number": 22
    },
    {
      "id": "34464ed0-8914-4ade-a741-9cc48c9e55d4",
      "m1": "خداوند گفت اسب پرویز شاه",
      "m2": "ندارد همی کهترانرا نگاه",
      "verse_number": 23
    },
    {
      "id": "25823857-539d-415a-906d-3f2aefe5b9c2",
      "m1": "بیامد موکل بر شهریار",
      "m2": "بگفت آنچ بشنید از کشت زار",
      "verse_number": 24
    },
    {
      "id": "01381749-b353-452b-ab09-4bf19bebb703",
      "m1": "بدو گفت هرمز برفتن بکوش",
      "m2": "ببر اسب را در زمان دم و گوش",
      "verse_number": 25
    },
    {
      "id": "d224bd4c-70bf-46b7-9c27-ca268202673f",
      "m1": "زیانی که آمد بران کشتمند",
      "m2": "شمارش بباید شمردن که چند",
      "verse_number": 26
    },
    {
      "id": "1b6d95a3-448b-4b54-aaed-d0edebfb461f",
      "m1": "ز خسرو زیان باز باید ستد",
      "m2": "اگر صد زیانست اگر پانصد",
      "verse_number": 27
    },
    {
      "id": "2a87ca66-df2f-4963-a85b-e0ff0d8bd0ec",
      "m1": "درمهای گنجی بران کشت زار",
      "m2": "بریزند پیش خداوند کار",
      "verse_number": 28
    },
    {
      "id": "6a497a80-3d08-4b07-b174-d9e08565c287",
      "m1": "چو بشنید پرویز پوزش کنان",
      "m2": "برانگیخت از هر سویی مهتران",
      "verse_number": 29
    },
    {
      "id": "d7c6c5b4-c873-4dcd-8d54-9bf13df20cd7",
      "m1": "بنزد پدر تا ببخشد گناه",
      "m2": "نبرد دم وگوش اسب سیاه",
      "verse_number": 30
    },
    {
      "id": "a35ed824-4a08-4195-a827-0f37dbfa7e96",
      "m1": "برآشفت ازان پس برو شهریار",
      "m2": "بتندی بزد بانگ بر پیشکار",
      "verse_number": 31
    },
    {
      "id": "677f5133-e684-407c-808e-8307f3336087",
      "m1": "موکل شد از بیم هرمز دوان",
      "m2": "بدان کشت نزدیک اسب جوان",
      "verse_number": 32
    },
    {
      "id": "cd0a1c38-c98c-4fff-a842-f6044c4ea4d5",
      "m1": "بخنجر جداکرد زو گوش و دم",
      "m2": "بران کشت زاری که آزرد سم",
      "verse_number": 33
    },
    {
      "id": "9a44dfb8-6795-4594-a764-5548ea48e671",
      "m1": "همان نیز تاوان بدان دادخواه",
      "m2": "رسانید خسرو بفرمان شاه",
      "verse_number": 34
    },
    {
      "id": "e4da0262-8752-4c08-b3c7-4a90593bb55b",
      "m1": "وزان پس بنخچیر شد شهریار",
      "m2": "بیاورد هر کس فراوان شکار",
      "verse_number": 35
    },
    {
      "id": "1356309e-6920-470a-8d39-96ac4dfd840f",
      "m1": "سواری ردی مرد کنداوری",
      "m2": "سپهبدنژادی بلند اختری",
      "verse_number": 36
    },
    {
      "id": "780c06e0-b92c-4f4d-a45f-d4df0ffb9f0f",
      "m1": "بره بر یکی رز پراز غوره دید",
      "m2": "بفرمود تاکهتر اندر دوید",
      "verse_number": 37
    },
    {
      "id": "275fc951-4625-4b9d-b242-1b96db4c9e6e",
      "m1": "ازان خوشهٔ چند ببردی و برد",
      "m2": "بایوان و خوالیگرش را سپرد",
      "verse_number": 38
    },
    {
      "id": "0ca70b6d-01b4-4f61-aa55-0ce83a9c1be5",
      "m1": "بیامد خداوندش اندر زمان",
      "m2": "بدان مرد گفت ای بد بدگمان",
      "verse_number": 39
    },
    {
      "id": "9a7d1638-abcb-48a3-9f1d-cdb3538d3ca5",
      "m1": "نگهبان این رز نبودی به رنج",
      "m2": "نه دینار دادی بها را نه گنج",
      "verse_number": 40
    },
    {
      "id": "91bac370-9524-428f-a809-be33462d3fb1",
      "m1": "چرا رنج نابرده کردی تباه",
      "m2": "بنالم کنون از تو در پیش شاه",
      "verse_number": 41
    },
    {
      "id": "fba73898-1cae-4ee3-ad6d-7f623af42284",
      "m1": "سوار دلاور ز بیم زیان",
      "m2": "بزودی کمر بازکرد از میان",
      "verse_number": 42
    },
    {
      "id": "868ed91b-f746-4ee6-9136-892a0cc2354d",
      "m1": "بدو داد پرمایه زرین کمر",
      "m2": "بهر مهرهای در نشانده گهر",
      "verse_number": 43
    },
    {
      "id": "580ee3cf-92d4-4924-9506-c219bde367ca",
      "m1": "خداوند رز چون کمر دید گفت",
      "m2": "که کردار بد چند باید نهفت",
      "verse_number": 44
    },
    {
      "id": "0ece3ca0-7a95-4ded-9ab3-8a39ed668192",
      "m1": "تو با شهریار آشنایی مکن",
      "m2": "خریده نداری بهایی مکن",
      "verse_number": 45
    },
    {
      "id": "06b8e4fe-0a25-4d9d-a23e-2c58cca73dea",
      "m1": "سپاسی نهم بر تو بر زین کمر",
      "m2": "بپیچی اگر بشنود دادگر",
      "verse_number": 46
    },
    {
      "id": "6ab9af6c-1656-468f-a023-58bc5c673466",
      "m1": "یکی مرد بد هرمز شهریار",
      "m2": "به پیروزی اندر شده نامدار",
      "verse_number": 47
    },
    {
      "id": "e4461eb3-340e-4dd9-8a72-c5641e18b34a",
      "m1": "بمردی ستوده بهرانجمن",
      "m2": "که از رزم هرگز ندیدی شکن",
      "verse_number": 48
    },
    {
      "id": "67289d02-8d8c-48f6-9257-16a786d053a0",
      "m1": "که هم دادده بود و هم دادخواه",
      "m2": "کلاه کیی برنهاده بماه",
      "verse_number": 49
    },
    {
      "id": "a5fb9a5c-084c-421a-8669-616c367ee98f",
      "m1": "نکردی بشهر مداین درنگ",
      "m2": "دلاور سری بود با نام وننگ",
      "verse_number": 50
    },
    {
      "id": "66a5f7b5-6e7a-4d9d-87e4-51057e2b7147",
      "m1": "بهار و تموز و زمستان وتیر",
      "m2": "نیاسود هرمز یل شیرگیر",
      "verse_number": 51
    },
    {
      "id": "6c069486-cbde-4534-b738-301720fc0c03",
      "m1": "همیگشت گرد جهان سر به سر",
      "m2": "همیجست در پادشاهی هنر",
      "verse_number": 52
    },
    {
      "id": "6cd00dad-7cfb-43c2-bd6c-1601becbad32",
      "m1": "چو ده سال شد پادشاهیش راست",
      "m2": "ز هرکشور آواز بدخواه خاست",
      "verse_number": 53
    },
    {
      "id": "22612926-57ca-490a-b28a-03ac3a56d75b",
      "m1": "بیامد ز راه هری ساوه شاه",
      "m2": "ابا پیل و با کوس و گنج و سپاه",
      "verse_number": 54
    },
    {
      "id": "9cd31197-bdb2-45ac-a83f-bc571b7237b9",
      "m1": "گر از لشکر ساوه گیری شمار",
      "m2": "برو چارصد بار بشمر هزار",
      "verse_number": 55
    },
    {
      "id": "cba84c8e-61e7-457b-9aaa-57d746b1f4e2",
      "m1": "ز پیلان جنگی هزار و دویست",
      "m2": "توگفتی مگر برزمین راه نیست",
      "verse_number": 56
    },
    {
      "id": "d3017372-fb63-45f3-89ce-c35b822b4815",
      "m1": "ز دشت هری تا در مرورود",
      "m2": "سپه بود آگنده چون تار و پود",
      "verse_number": 57
    },
    {
      "id": "f630091a-200e-4ef7-9e37-1ea925520e4b",
      "m1": "وزین روی تا مرو لشکر کشید",
      "m2": "شد از گرد لشکر زمین ناپدید",
      "verse_number": 58
    },
    {
      "id": "94228c07-86f3-4938-83a8-4b9c5dae019f",
      "m1": "بهرمز یکی نامه بنوشت شاه",
      "m2": "که نزدیک خود خوان ز هر سو سپاه",
      "verse_number": 59
    },
    {
      "id": "4a23b8fe-cf02-4830-a9b7-9e6f3ed95a81",
      "m1": "برو راه این لشکر آباد کن",
      "m2": "علف سازو از تیغ ما یادکن",
      "verse_number": 60
    },
    {
      "id": "3b6fe6fc-2c5a-4d8b-a3d7-3f488fe8c671",
      "m1": "برین پادشاهی بخواهم گذشت",
      "m2": "بدریا سپاهست و بر کوه و دشت",
      "verse_number": 61
    },
    {
      "id": "3e96cc74-96a9-4247-bbd6-337bef302478",
      "m1": "چو برخواند آن نامه را شهریار",
      "m2": "بپژمرد زان لشکر بیشمار",
      "verse_number": 62
    },
    {
      "id": "126e4727-c98d-4d83-8b23-72bd6e48a7d2",
      "m1": "وزان روی قیصر بیامد ز روم",
      "m2": "به لشکر بزیر اندر آورد بوم",
      "verse_number": 63
    },
    {
      "id": "b8237f3d-44c9-4156-921e-d36da316746c",
      "m1": "سپه بود رومی عدد صد هزار",
      "m2": "سواران جنگ آور و نامدار",
      "verse_number": 64
    },
    {
      "id": "8611bc29-c534-4b2b-9623-e023494ac0ed",
      "m1": "ز شهری که بگرفت نوشین روان",
      "m2": "که از نام او بود قیصر نوان",
      "verse_number": 65
    },
    {
      "id": "9ad187d2-a315-459b-a314-378e8e233454",
      "m1": "بیامد ز هر کشوری لشکری",
      "m2": "به پیش اندرون نامور مهتری",
      "verse_number": 66
    },
    {
      "id": "4ca1f88c-b599-429f-8ac9-ece28b67d78f",
      "m1": "سپاهی بیامد ز راه خزر",
      "m2": "کز ایشان سیه شد همه بوم و بر",
      "verse_number": 67
    },
    {
      "id": "36b2477c-afeb-4ffe-a54e-41c84600a696",
      "m1": "جهاندیده بدال درپیش بود",
      "m2": "که با گنج و با لشکر خویش بود",
      "verse_number": 68
    },
    {
      "id": "23edec45-eb49-43af-8491-2256ba41bf10",
      "m1": "ز ارمینیه تا در اردبیل",
      "m2": "پراگنده شد لشکرش خیل خیل",
      "verse_number": 69
    },
    {
      "id": "729c3ae9-7284-4fc0-bcf8-f3edc56c07dd",
      "m1": "ز دشت سواران نیزه گزار",
      "m2": "سپاهی بیامد فزون از شمار",
      "verse_number": 70
    },
    {
      "id": "bcb4c2ca-ac1c-46ea-a662-f37496bb88eb",
      "m1": "چوعباس و چو حمزه شان پیشرو",
      "m2": "سواران و گردن فرازان نو",
      "verse_number": 71
    },
    {
      "id": "8afc16e3-d12d-47b8-82dc-246f7468c7d1",
      "m1": "ز تاراج ویران شد آن بوم ورست",
      "m2": "که هرمز همی باژ ایشان بجست",
      "verse_number": 72
    },
    {
      "id": "d4dedf34-49a7-449a-9281-205f18128db6",
      "m1": "بیامد سپه تابه آب فرات",
      "m2": "نماند اندر آن بوم جای نبات",
      "verse_number": 73
    },
    {
      "id": "845a92c6-be10-410f-b132-d5f9112faeff",
      "m1": "چو تاریک شد روزگار بهی",
      "m2": "ز لشکر بهرمز رسید آگهی",
      "verse_number": 74
    },
    {
      "id": "957698b6-3fd3-4151-acb1-1548f3264f04",
      "m1": "چو بشنید گفتار کارآگهان",
      "m2": "به پژمرد شاداب شاه جهان",
      "verse_number": 75
    },
    {
      "id": "d11a129a-7086-475b-abc4-f36d83be1a26",
      "m1": "فرستاد و ایرانیان را بخواند",
      "m2": "سراسر همه کاخ مردم نشاند",
      "verse_number": 76
    },
    {
      "id": "faa6d096-8d6c-4adc-8a2f-a323eb0772bd",
      "m1": "برآورد رازی که بود از نهفت",
      "m2": "بدان نامداران ایران بگفت",
      "verse_number": 77
    },
    {
      "id": "ad6b60ef-d7d1-48ae-96bf-4b09c69ad1c7",
