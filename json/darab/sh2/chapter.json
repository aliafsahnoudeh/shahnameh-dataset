{
  "id": "68066867-37ec-40d2-824f-c06cda68dadb",
  "title": "بخش ۲",
  "verses": [
    {
      "id": "631c7fd6-81c0-4476-8a0d-5d9bda778925",
      "m1": "چنان بد که از تازیان صدهزار",
      "m2": "نبرده سواران نیزه گزار",
      "verse_number": 1
    },
    {
      "id": "b048c362-b8bd-49d2-9761-883590d8cf90",
      "m1": "برفتند و سالار ایشان شعیب",
      "m2": "یکی نامدار از نژاد قتیب",
      "verse_number": 2
    },
    {
      "id": "eaadc96e-f029-47e3-89f0-c30de2c8d2b8",
      "m1": "جهاندار ایران سپاهی ببرد",
      "m2": "بگفتند کان را نشاید شمرد",
      "verse_number": 3
    },
    {
      "id": "9265b31a-3130-4a59-b5ac-cf1bcc04aff5",
      "m1": "فراز آمدند آن دو لشکر بهم",
      "m2": "جهان شد ز پرخاشجویان دژم",
      "verse_number": 4
    },
    {
      "id": "a4b29807-5831-406c-ad91-427c687b191f",
      "m1": "زمین آن سپه را همی برنتافت",
      "m2": "بران بوم کس جای رفتن نیافت",
      "verse_number": 5
    },
    {
      "id": "a15310fc-a244-459f-8133-214355619d01",
      "m1": "ز باران ژوپین و باران تیر",
      "m2": "زمین شد ز خون چون یکی آبگیر",
      "verse_number": 6
    },
    {
      "id": "ea7658da-cd28-4665-b59f-eb3f379035de",
      "m1": "خروشی برآمد ز هر پهلوی",
      "m2": "تلی کشته دیدند بر هر سوی",
      "verse_number": 7
    },
    {
      "id": "6fe91ec1-7b0f-4d27-8e9b-0a74e6aa918a",
      "m1": "سه روز و سه شب زین نشان جنگ بود",
      "m2": "تو گفتی بریشان جهان تنگ بود",
      "verse_number": 8
    },
    {
      "id": "37daee09-f199-4b63-9ccf-0ec7ac89801a",
      "m1": "چهارم عرب روی برگاشتند",
      "m2": "به شب دشت پیکار بگذاشتند",
      "verse_number": 9
    },
    {
      "id": "c9c267c8-a007-467e-ab74-8df63720c376",
      "m1": "شعیب اندران رزمگه کشته شد",
      "m2": "عرب را همه روز برگشته شد",
      "verse_number": 10
    },
    {
      "id": "b3a88c01-1356-49b2-8461-4584c320e58b",
      "m1": "بسی اسپ تازی به زین خدنگ",
      "m2": "هم از نیزه و تیغ و خفتان جنگ",
      "verse_number": 11
    },
    {
      "id": "a3d44243-d561-4557-b7bd-f9fc610a98b4",
      "m1": "ازان رفتگان ماند آنجا به جای",
      "m2": "به نزد جهاندار پور همای",
      "verse_number": 12
    },
    {
      "id": "04973a90-ab17-4856-bf0f-5ce4dd8fb9e1",
      "m1": "ببخشید چیزی که بد بر سپاه",
      "m2": "ز اسپ و ز رمح و ز تیغ و کلاه",
      "verse_number": 13
    },
    {
      "id": "22f2dcf7-d3a8-40db-b2b0-6630a81585b7",
      "m1": "ز لشکر یکی مرزبان برگزید",
      "m2": "که گفتار ایشان بداند شنید",
      "verse_number": 14
    },
    {
      "id": "799f323a-09ce-44cd-b073-7983d0a26141",
      "m1": "فرستاد تا باژ خواهد ز دشت",
      "m2": "ازان سال و آن سال کاندر گذشت",
      "verse_number": 15
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 15
  }
}