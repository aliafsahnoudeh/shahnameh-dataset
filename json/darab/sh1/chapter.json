{
  "id": "bf5bbd93-6a7b-44ba-b94f-a7a6a284ff3d",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "3c94c026-d42e-4e9f-894e-7ffc3b1bb85c",
      "m1": "کنون آفرین جهانآفرین",
      "m2": "بخوانیم بر شهریار زمین",
      "verse_number": 1
    },
    {
      "id": "24e595d3-8644-432c-b219-38e1e9710d7b",
      "m1": "ابوالقاسم آن شاه خورشید چهر",
      "m2": "بیاراست گیتی به داد و به مهر",
      "verse_number": 2
    },
    {
      "id": "94eb475e-6896-423d-aa30-90cd412ce6db",
      "m1": "نجوید جز از خوبی و راستی",
      "m2": "نیارد بداد اندرون کاستی",
      "verse_number": 3
    },
    {
      "id": "30f12f7f-619d-431e-9a5e-8292853e990c",
      "m1": "جهان روشن از تاج محمود باد",
      "m2": "همه روزگارانش مسعود باد",
      "verse_number": 4
    },
    {
      "id": "442831cd-b0fa-49bc-810b-aa6f08d0506f",
      "m1": "همیشه جوان تا جوانی بود",
      "m2": "همان زنده تا زندگانی بود",
      "verse_number": 5
    },
    {
      "id": "6d9fb736-9c7b-47c2-97b4-9858b979fb04",
      "m1": "چه گفت آن سراینده دهقان پیر",
      "m2": "ز گشتاسپ وز نامدار اردشیر",
      "verse_number": 6
    },
    {
      "id": "7b55245b-17ec-4f5a-9496-d504437e97c8",
      "m1": "وزان نامداران پاکیزهرای",
      "m2": "ز داراب وز رسم و رای همای",
      "verse_number": 7
    },
    {
      "id": "c7464304-d32d-4962-be10-ee91b4fd6381",
      "m1": "چو دارا به تخت مهی برنشست",
      "m2": "کمر بر میان بست و بگشاد دست",
      "verse_number": 8
    },
    {
      "id": "07a4a2e7-d6c0-4892-8d6a-f4e4b29b1820",
      "m1": "چنین گفت با موبدان و ردان",
      "m2": "بزرگان و بیداردل بخردان",
      "verse_number": 9
    },
    {
      "id": "0834639e-9729-4c90-953d-bb6f19150c25",
      "m1": "که گیتی نجستم به رنج و به داد",
      "m2": "مرا تاج یزدان به سر بر نهاد",
      "verse_number": 10
    },
    {
      "id": "1fb447ee-724c-46cc-ad02-db56aaa4eb8d",
      "m1": "شگفتیتر از کار من در جهان",
      "m2": "نبیند کسی آشکار و نهان",
      "verse_number": 11
    },
    {
      "id": "c9cb983e-0b24-4794-a5d7-2d62ffac3e01",
      "m1": "ندانیم جز داد پاداش این",
      "m2": "که بر ما پس از ما کنند آفرین",
      "verse_number": 12
    },
    {
      "id": "6b19b569-8f1e-48a8-bdcb-071b18feca4e",
      "m1": "نباید که پیچد کس از رنج ما",
      "m2": "ز بیشی و آگندن گنج ما",
      "verse_number": 13
    },
    {
      "id": "bc5ac593-8e70-440b-a27d-6e046b2259cd",
      "m1": "زمانه ز داد من آباد باد",
      "m2": "دل زیر دستان ما شاد باد",
      "verse_number": 14
    },
    {
      "id": "f1c120b6-1627-42f5-a1fe-e6004fcb8e1d",
      "m1": "ازان پس ز هندوستان و ز روم",
      "m2": "ز هر مرز باارز و آباد بوم",
      "verse_number": 15
    },
    {
      "id": "a3334390-9a2a-467e-a75f-c8ccd5fd7655",
      "m1": "برفتند با هدیه و با نثار",
      "m2": "بجستند خشنودی شهریار",
      "verse_number": 16
    },
    {
      "id": "650af94b-dd18-4af4-952d-448bfc25853f",
      "m1": "چنان بد که روزی ز بهر گله",
      "m2": "بیامد که اسپان ببیند یله",
      "verse_number": 17
    },
    {
      "id": "e82b57e5-9af3-4e7e-a72d-c55b29177f6b",
      "m1": "ز پستی برآمد به کوهی رسید",
      "m2": "یکی بیکران ژرف دریا بدید",
      "verse_number": 18
    },
    {
      "id": "7e796e68-7ce1-43a0-b388-d006b817d619",
      "m1": "بفرمود کز روم و وز هندوان",
      "m2": "بیارند کارآزموده گوان",
      "verse_number": 19
    },
    {
      "id": "0460828c-8b12-4a04-b31b-b78d5777abe1",
      "m1": "بجویند زان آب دریا دری",
      "m2": "رسانند رودی به هر کشوری",
      "verse_number": 20
    },
    {
      "id": "f22a0b4e-9a34-4237-8de6-a77cdeb948b4",
      "m1": "چو بگشاد داننده از آب بند",
      "m2": "یکی شهر فرمود بس سودمند",
      "verse_number": 21
    },
    {
      "id": "2fdff4b2-0e52-4782-beae-591178feb8e6",
      "m1": "چو دیوار شهر اندرآورد گرد",
      "m2": "ورا نام کردند داراب گرد",
      "verse_number": 22
    },
    {
      "id": "e181f865-5b3e-46a0-8ec4-6f1a08ad0ae6",
      "m1": "یکی آتش افروخت از تیغ کوه",
      "m2": "پرستندهٔ آذر آمد گروه",
      "verse_number": 23
    },
    {
      "id": "e1c6bc01-eb55-4ebc-81da-7a44b509ae46",
      "m1": "ز هر پیشهای کارگر خواستند",
      "m2": "همی شهر ایران بیاراستند",
      "verse_number": 24
    },
    {
      "id": "0d9e39d5-c21f-4cc8-9a31-8ef6020e96cc",
      "m1": "به هر سو فرستاد بیمر سپاه",
      "m2": "ز دشمن همی داشت گیتی نگاه",
      "verse_number": 25
    },
    {
      "id": "99d0697b-abfb-47ab-bccd-bdf37475a480",
      "m1": "جهان از بداندیش بیبیم کرد",
      "m2": "دل بدسگالان بدو نیم کرد",
      "verse_number": 26
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 26
  },
