{
  "id": "5ed2fe69-316e-49ad-a7e5-1ec426f40121",
  "title": "بخش ۱۲",
  "verses": [
    {
      "id": "eb56b5fe-5ecc-4367-ada7-ac0ed85db845",
      "m1": "برفتند با رستم آن هفت گرد",
      "m2": "بنه اشکش تیزهش را سپرد",
      "verse_number": 1
    },
    {
      "id": "5f539e15-5053-4ce1-a22b-388682c0a23f",
      "m1": "عنانها فگندند بر پیش زین",
      "m2": "کشیدند یکسر همه تیغ کین",
      "verse_number": 2
    },
    {
      "id": "2d5913e6-f7c1-48c1-ba97-afdbf99bf5c2",
      "m1": "بشد تا بدرگاه افراسیاب",
      "m2": "بهنگام سستی و آرام و خواب",
      "verse_number": 3
    },
    {
      "id": "7716cba1-9eec-413e-b272-9f105007846b",
      "m1": "برآمد ز ناگه ده و دار و گیر",
      "m2": "درخشیدن تیغ و باران تیر",
      "verse_number": 4
    },
    {
      "id": "8ab8d317-ad7c-45cd-8a52-24c6d974015c",
      "m1": "سران را بسی سر جدا شد ز تن",
      "m2": "پر از خاک ریش و پر از خون دهن",
      "verse_number": 5
    },
    {
      "id": "360309a4-37f3-4b81-aed8-712ed402c850",
      "m1": "ز دهلیز در رستم آواز داد",
      "m2": "که خواب تو خوش باد و گردانت شاد",
      "verse_number": 6
    },
    {
      "id": "748fe226-60ee-4b27-90dc-42b48889818b",
      "m1": "بخفتی تو بر گاه و بیژن بچاه",
      "m2": "مگر باره دیدی ز آهن براه",
      "verse_number": 7
    },
    {
      "id": "01c0ea1b-fbba-42c1-9e69-0ee85358f547",
      "m1": "منم رستم زابلی پور زال",
      "m2": "نه هنگام خوابست و آرام و هال",
      "verse_number": 8
    },
    {
      "id": "e38564a7-3701-4dc0-b172-eec9fabcf4ff",
      "m1": "شکستم در بند زندان تو",
      "m2": "که سنگ گران بد نگهبان تو",
      "verse_number": 9
    },
    {
      "id": "198247dd-7f1e-4498-aa34-e32b89025126",
      "m1": "رها شد سر و پای بیژن ز بند",
      "m2": "بداماد بر کس نسازد گزند",
      "verse_number": 10
    },
    {
      "id": "fc789e3f-38e8-462d-91dd-f84637e85819",
      "m1": "ترا رزم و کین سیاوخش بس",
      "m2": "بدین دشت گردیدن رخش بس",
      "verse_number": 11
    },
    {
      "id": "6ae4b62c-a36f-4f76-afab-1dbc999207ea",
      "m1": "همیدون برآورد بیژن خروش",
      "m2": "که ای ترک بدگوهر تیره هوش",
      "verse_number": 12
    },
    {
      "id": "3b777545-ae12-4b5d-b775-edce98a09ac1",
      "m1": "براندیش زان تخت فرخندهجای",
      "m2": "مرا بسته در پیش کرده بپای",
      "verse_number": 13
    },
    {
      "id": "b6f5fd8b-8a32-4767-986a-dc86dd55a108",
      "m1": "همی رزم جستی بسان پلنگ",
      "m2": "مرا دست بسته بکردار سنگ",
      "verse_number": 14
    },
    {
      "id": "00679ccb-27e6-4298-a5f5-7078719deaef",
      "m1": "کنونم گشاده بهامون ببین",
      "m2": "که با من نجوید ژیان شیر کین",
      "verse_number": 15
    },
    {
      "id": "b9e39ebc-baba-4046-956f-e7cfa9caec13",
      "m1": "بزد دست بر جامه افراسیاب",
      "m2": "که جنگآوران را ببستست خواب",
      "verse_number": 16
    },
    {
      "id": "47d5aecf-0a43-46f9-8ee2-9bda54f51b84",
      "m1": "بفرمود زان پس که گیرند راه",
      "m2": "بدان نامداران جوینده گاه",
      "verse_number": 17
    },
    {
      "id": "0fd2feb2-a4a6-43cf-8277-7e816ead2ca6",
      "m1": "ز هر سو خروش تکاپوی خاست",
      "m2": "ز خون ریختن بر درش جوی خاست",
      "verse_number": 18
    },
    {
      "id": "82061cfa-182f-4b74-a2d8-84d53b0b506c",
      "m1": "هرآنکس که آمد ز توران سپاه",
      "m2": "زمانه تهی ماند زو جایگاه",
      "verse_number": 19
    },
    {
      "id": "e41f1805-93da-4863-9ddb-943401592782",
      "m1": "گرفتند بر کینه جستن شتاب",
      "m2": "ازان خانه بگریخت افراسیاب",
      "verse_number": 20
    },
    {
      "id": "703336c8-d8d9-47e5-b6dc-f3ac2b75f519",
      "m1": "بکاخ اندر آمد خداوند رخش",
      "m2": "همه فرش و دیبای او کرد بخش",
      "verse_number": 21
    },
    {
      "id": "abc02047-48d2-47e8-b435-53f72043e293",
      "m1": "پریچهرگان سپهبدپرست",
      "m2": "گرفته همه دست گردان بدست",
      "verse_number": 22
    },
    {
      "id": "57f1e0d5-9d90-40b8-9c3a-478ab33e6860",
      "m1": "گرانمایه اسبان و زین پلنگ",
      "m2": "نشانده گهر در جناغ خدنگ",
      "verse_number": 23
    },
    {
      "id": "4e00b70e-92ed-4d82-9e71-01a5c18f5c60",
      "m1": "ازان پس ز ایوان ببستند بار",
      "m2": "بتوران نکردند بس روزگار",
      "verse_number": 24
    },
    {
      "id": "7215604e-6cdb-44c4-8c82-5b0ce304e495",
      "m1": "ز بهر بنه تاخت اسبان بزور",
      "m2": "بدان تا نخیزد ازان کار شور",
      "verse_number": 25
    },
    {
      "id": "3e6c7936-130d-4d84-a091-0ecd1d6fb68e",
      "m1": "چنان رنجه بد رستم از رنج راه",
      "m2": "که بر سرش بر درد بود از کلاه",
      "verse_number": 26
    },
    {
      "id": "9fa94a71-d874-4767-afb5-77a4306691ce",
      "m1": "سواران ز بس رنج و اسبان ز تگ",
      "m2": "یکی را بتن بر نجنبید رگ",
      "verse_number": 27
    },
    {
      "id": "04162e44-6ec4-4f88-a16e-92719d57a479",
      "m1": "بلشکر فرستاد رستم پیام",
      "m2": "که شمشیر کین بر کشید از نیام",
      "verse_number": 28
    },
    {
      "id": "67393463-b984-4286-bef6-3ed666a595a9",
      "m1": "که من بیگمانم کزین پس بکین",
      "m2": "سیه گردد از سم اسبان زمین",
      "verse_number": 29
    },
    {
      "id": "452822f9-c53e-4ac9-b9ae-f99b572b693b",
      "m1": "گشن لشکری سازد افراسیاب",
      "m2": "بنیزه بپوشد رخ آفتاب",
      "verse_number": 30
    },
    {
      "id": "9842118f-d05b-4da2-93c8-77c2fdb4413a",
      "m1": "برفتند یکسر سواران جنگ",
      "m2": "همه رزم را تیز کردند چنگ",
      "verse_number": 31
    },
    {
      "id": "eb1b17a7-1a77-4b34-ba58-dd48c3da6018",
      "m1": "همه نیزهداران زدوده سنان",
      "m2": "همه جنگ را گرد کرده عنان",
      "verse_number": 32
    },
    {
      "id": "435b9e5a-6868-4e08-9ad5-31ca43a5e783",
      "m1": "منیژه نشسته بخیمه درون",
      "m2": "پرستنده بر پیش او رهنمون",
      "verse_number": 33
    },
    {
      "id": "d8b16255-dea5-44a2-83e4-d771e6037326",
      "m1": "یکی داستان زد تهمتن بروی",
      "m2": "که گر می بریزد نریزدش بوی",
      "verse_number": 34
    },
    {
      "id": "287c61cd-5611-48ad-a24c-73eeafb4f6fc",
      "m1": "چنینست رسم سرای سپنج",
      "m2": "گهی ناز و نوش و گهی درد و رنج",
      "verse_number": 35
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 35
  }
}