{
  "id": "da474c1c-dca6-4679-a716-677abbc2ad6e",
  "title": "بخش ۶",
  "verses": [
    {
      "id": "8545a3b7-a854-4e06-a633-0e230dea8952",
      "m1": "نویسندهٔ نامه را پیش خواند",
      "m2": "وزین داستان چند با او براند",
      "verse_number": 1
    },
    {
      "id": "b2f10c12-25b3-45a4-8b78-5bfcd12880cb",
      "m1": "برستم یکی نامه فرمود شاه",
      "m2": "نوشتن ز مهتر سوی نیکخواه",
      "verse_number": 2
    },
    {
      "id": "a949c575-87ac-47f8-88f5-6b12dbb8be97",
      "m1": "که ای پهلوان زادهٔ پر هنر",
      "m2": "ز گردان لشکر برآورده سر",
      "verse_number": 3
    },
    {
      "id": "80ce42bb-3c09-4fdb-9090-f826a1d8e232",
      "m1": "دل شهریاران و پشت کیان",
      "m2": "بفرمان هر کس کمر بر میان",
      "verse_number": 4
    },
    {
      "id": "8f0d9e0b-f719-45f1-a948-21cd27d0c3dd",
      "m1": "توی از نیاکان مرا یادگار",
      "m2": "همیشه کمربستهٔ کارزار",
      "verse_number": 5
    },
    {
      "id": "1a8bf692-a87a-40a7-b4a5-cb8dff94a684",
      "m1": "ترا داد گردون بمردی پلنگ",
      "m2": "بدریا ز بیمت خروشان نهنگ",
      "verse_number": 6
    },
    {
      "id": "5fcb620c-971b-4d01-9e17-b7719e32adfd",
      "m1": "جهان را ز دیوان مازندران",
      "m2": "بشستی و کندی بدان را سران",
      "verse_number": 7
    },
    {
      "id": "033860d2-3d78-4c49-a0f9-2bf8385c7568",
      "m1": "چه مایه سر تاجداران ز گاه",
      "m2": "ربودی و برکندی از پیشگاه",
      "verse_number": 8
    },
    {
      "id": "a77139be-7036-4d31-854b-a264999e397e",
      "m1": "بسا دشمنان کز تو بیجان شدست",
      "m2": "بسا بوم و بر کز تو ویران شدست",
      "verse_number": 9
    },
    {
      "id": "840ba0d3-508c-4517-bc53-f2269ac24afa",
      "m1": "سر پهلوانی و لشکر پناه",
      "m2": "بنزدیک شاهان ترا دستگاه",
      "verse_number": 10
    },
    {
      "id": "23c03b72-7ca7-4b89-96db-a5c3886654ab",
      "m1": "همه جادوان را ببستی بگرز",
      "m2": "بیفروختی تاج شاهان ببرز",
      "verse_number": 11
    },
    {
      "id": "335aac17-5526-4e2b-a897-c4d24688be5f",
      "m1": "چه افراسیاب و چه شاهان چین",
      "m2": "نوشته همه نام تو بر نگین",
      "verse_number": 12
    },
    {
      "id": "c68d9434-fd7a-4780-a9d0-fd8c82f790f7",
      "m1": "هران بند کز دست تو بسته شد",
      "m2": "گشایندگان را جگر خسته شد",
      "verse_number": 13
    },
    {
      "id": "5b0c5a86-060b-4de0-abaa-ef8aa5b42e90",
      "m1": "گشایندهٔ بند بسته توی",
      "m2": "کیان را سپهر خجسته توی",
      "verse_number": 14
    },
    {
      "id": "0a2b1f37-8301-4b58-a354-317d38628013",
      "m1": "ترا ایزد این زور پیلان که داد",
      "m2": "دل و هوش و فرهنگ فرخنژاد",
      "verse_number": 15
    },
    {
      "id": "f48ac574-29b0-4dc1-bbb0-f7c13b467135",
      "m1": "بدان داد تا دست فریاد خواه",
      "m2": "بگیری برآری ز تاریک چاه",
      "verse_number": 16
    },
    {
      "id": "6bfece57-ba0c-4995-b19d-2115ed9d7aac",
      "m1": "کنون این یکی کار بایسته پیش",
      "m2": "فراز آمد و اینت شایسته خویش",
      "verse_number": 17
    },
    {
      "id": "a4b15fd2-f6a8-40ee-9a05-91f0ec1047da",
      "m1": "بتو دارد امید گودرز و گیو",
      "m2": "که هستی بهر کشور امروز نیو",
      "verse_number": 18
    },
    {
      "id": "9b6334f3-e3f9-4589-8826-8e90c97d27f5",
      "m1": "شناسی بنزدیک من جاهشان",
      "m2": "زبان و دل و رای یکتاهشان",
      "verse_number": 19
    },
    {
      "id": "1c924b25-8a5d-481e-b512-d2752b653410",
      "m1": "سزدگر تو اینرا نداری برنج",
      "m2": "بخواه آنچ باید ز مردان و گنج",
      "verse_number": 20
    },
    {
      "id": "b8f02fd2-431c-44ec-927e-b6cc14494e70",
      "m1": "که هرگز بدین دودمان غم نبود",
      "m2": "فروزندهتر زین چنانکم شنود",
      "verse_number": 21
    },
    {
      "id": "ac0e6e31-660d-45ed-83f3-0da3c2b9f662",
      "m1": "نبد گیو را خود جز این پور کس",
      "m2": "چه فرزند بود و چه فریادرس",
      "verse_number": 22
    },
    {
      "id": "6081afc9-b405-4a74-af7d-fa1dea7e4bf5",
      "m1": "فراوان بنزد منش دستگاه",
      "m2": "مرا و نیای مرا نیکخواه",
      "verse_number": 23
    },
    {
      "id": "2f4134c6-fb03-4776-aa79-d76d8be292d5",
      "m1": "بهر سو که جویمش یابم بجای",
      "m2": "بهر نیک و بد پیش من بربپای",
      "verse_number": 24
    },
    {
      "id": "52a11187-d288-4f4d-a1af-584904895996",
      "m1": "چو این نامهٔ من بخوانی مپای",
      "m2": "بزودی تو با گیو خیز اندرآی",
      "verse_number": 25
    },
    {
      "id": "95bed03d-0592-4741-a757-6c623b726365",
      "m1": "بدان تا بدین کار با ما بهم",
      "m2": "زنی رای فرخ بهر بیش و کم",
      "verse_number": 26
    },
    {
      "id": "39a79d03-90d1-4f7f-82cf-74ce216c5df1",
      "m1": "ز مردان وز گنج وز خواسته",
      "m2": "بیارم بپیش تو آراسته",
      "verse_number": 27
    },
    {
      "id": "23285e71-d9be-47fa-97f3-9ed09ca7b74b",
      "m1": "بفرخ پی و بر شده نام تو",
      "m2": "ز توران برآید همه کام تو",
      "verse_number": 28
    },
    {
      "id": "9ab8cbbe-95a6-4444-b659-e8763707ac63",
      "m1": "چنانچون بباید بسازی نوا",
      "m2": "مگر بیژن از بند یابد رها",
      "verse_number": 29
    },
    {
      "id": "6ff47560-769d-42fe-904f-a9dcea884230",
      "m1": "چو برنامه بنهاد خسرو نگین",
      "m2": "بشد گیو و بر شاه کرد آفرین",
      "verse_number": 30
    },
    {
      "id": "36d46f34-5c00-4aea-b1aa-ad988f29cf9e",
      "m1": "سواران دوده همه برنشاند",
      "m2": "بیزدان پناهید و لشکر براند",
      "verse_number": 31
    },
    {
      "id": "7a45a10e-b157-4a46-8cbc-0233289d9854",
      "m1": "چو نخجیر از آنجا که برداشتی",
      "m2": "دو روزه بیک روزه بگذاشتی",
      "verse_number": 32
    },
    {
      "id": "6e03aab6-938c-4e73-8b72-78f40cacfb39",
      "m1": "بیابان گرفت و ره هیرمند",
      "m2": "همی رفت پویان بسان نوند",
      "verse_number": 33
    },
    {
      "id": "a02a2b79-ccb6-45be-9ac4-5c7d1c944ad4",
      "m1": "بکوه و بصحرا نهادند روی",
      "m2": "همی شد خلیده دل و راهجوی",
      "verse_number": 34
    },
    {
      "id": "047785c3-d0de-4d98-ba5a-e5bc77dcf3ca",
      "m1": "چو از دیدهگه دیدهبانش بدید",
      "m2": "سوی زابلستان فغان برکشید",
      "verse_number": 35
    },
    {
      "id": "0668f472-c92e-4e70-bf46-7f05deb86910",
      "m1": "که آمد سواری سوی هیرمند",
      "m2": "سواران بگرد اندرش نیز چند",
      "verse_number": 36
    },
    {
      "id": "4845dc92-6728-436c-a9b2-7b133a82aa11",
      "m1": "درفشی درفشان پس پشت اوی",
      "m2": "یکی زابلی تیغ در مشت اوی",
      "verse_number": 37
    },
    {
      "id": "7a63bc81-492d-4081-bfcb-f85c6fc79a12",
      "m1": "غو دیده بشنید دستان سام",
      "m2": "بفرمود بر چرمه کردن لگام",
      "verse_number": 38
    },
    {
      "id": "79a7a143-f015-4e95-a713-d6834e80d22c",
      "m1": "پراندیشه آمد پذیره براه",
      "m2": "بدان تا نباشد یکی کینه خواه",
      "verse_number": 39
    },
    {
      "id": "fa50f0c0-6b55-43d3-8f36-40a7344be3ea",
      "m1": "ز ره گیو را دید پژمرده روی",
      "m2": "همی آمد آسیمه و پوی پوی",
      "verse_number": 40
    },
    {
      "id": "e9d9fc74-4ab2-4bea-95f0-8c6664f4972e",
      "m1": "بدل گفت کاری نو آمد بشاه",
      "m2": "فرستاده گیوست کامد براه",
      "verse_number": 41
    },
    {
      "id": "b9abeceb-dd25-4dbb-8fbf-acb012f4dd25",
      "m1": "چو نزدیک شد پهلوان سپاه",
      "m2": "نیایش کنان برگرفتند راه",
      "verse_number": 42
    },
    {
      "id": "f1ece4ce-7d01-48c5-8e3b-c822eca0e1c4",
      "m1": "بپرسید دستان ز ایرانیان",
      "m2": "ز شاه و ز پیکار تورانیان",
      "verse_number": 43
    },
    {
      "id": "53d52c5d-e350-4122-8e60-01f2534633ea",
      "m1": "درود بزرگان بدستان بداد",
      "m2": "ز شاه و ز گردان فرخ نژاد",
      "verse_number": 44
    },
    {
      "id": "9d8224b7-74de-4161-a3f9-8dd19727ec4a",
      "m1": "همه درد دل پیش دستان بخواند",
      "m2": "غم پور گم بوده با او براند",
      "verse_number": 45
    },
    {
      "id": "a66af0d1-f564-40cf-b4f8-9b22979e98bd",
      "m1": "همی گفت رویم نبینی برنگ",
      "m2": "ز خون مژه پشت پایم بلنگ",
      "verse_number": 46
    },
    {
      "id": "ee62bcd9-5d7e-4ea3-b3e5-5f35984e0a70",
      "m1": "ازان پس نشان تهمتن بخواست",
      "m2": "بپرسید و گفتش که رستم کجاست",
      "verse_number": 47
    },
    {
      "id": "0e6b7776-8d57-4f95-b3d2-bb47b5b21536",
      "m1": "بدو گفت رستم بنخچیر گور",
      "m2": "بیاید همانا که برگشت هور",
      "verse_number": 48
    },
    {
      "id": "66d2894d-ae16-45b6-9f2c-d42df50af558",
      "m1": "شوم گفت تا من ببینمش روی",
      "m2": "ز خسرو یکی نامه دارم بدوی",
      "verse_number": 49
    },
    {
      "id": "fbef4411-b04b-40a5-9aef-868cc8a4e05e",
      "m1": "بدو گفت دستان کز ایدر مرو",
      "m2": "که زود آید از دشت نخچیرگو",
      "verse_number": 50
    },
    {
      "id": "648e609b-e921-47b1-bbdd-d36f4acfbde4",
      "m1": "تو تا رستم آید بخانه بپای",
      "m2": "یک امروز با ما بشادی گرای",
      "verse_number": 51
    },
    {
      "id": "358aebf7-bc60-4bb2-983b-7077a45a13de",
      "m1": "چو گیو اندر آمد بایوان ز راه",
      "m2": "تهمتن بیامد ز نخچیرگاه",
      "verse_number": 52
    },
    {
      "id": "25e8fabc-06ee-4aff-9444-d7050ff2aed9",
      "m1": "پذیره شدش گیو کامد فراز",
      "m2": "پیاده شد از اسب و بردش نماز",
      "verse_number": 53
    },
    {
      "id": "f3ff7035-840a-46de-a557-111925c460cf",
      "m1": "پر از آرزو دل پر از رنگ روی",
      "m2": "به رُخ برنهاد از دو دیده دو جوی",
      "verse_number": 54
    },
    {
      "id": "ff5734d8-5e8b-4466-b51d-d685270a33d4",
      "m1": "چو رستم دل گیو را خسته دید",
      "m2": "بآب مژه روی او شسته دید",
      "verse_number": 55
    },
    {
      "id": "285b7b95-006a-40a6-a91a-4c6926f28863",
      "m1": "بدو گفت باری تباهست کار",
      "m2": "بایوان و بر شاه بد روزگار",
      "verse_number": 56
    },
    {
      "id": "6d717524-c36e-46f7-932e-0b52a1f482b2",
      "m1": "ز اسب اندر آمد گرفتش ببر",
      "m2": "بپرسیدش از خسرو تاجور",
      "verse_number": 57
    },
    {
      "id": "ba3e1497-8a9c-4b70-b88d-ffe926405e88",
      "m1": "ز گودرز وز طوس وز گستهم",
      "m2": "ز گردان لشکر همه بیش و کم",
      "verse_number": 58
    },
    {
      "id": "2a40226a-655b-48f4-8017-901e15efbf29",
      "m1": "ز شاپور و فرهاد وز بیژنا",
      "m2": "ز رهام و گرگین وز هرتنا",
      "verse_number": 59
    },
    {
      "id": "48135dd8-6cf2-4824-9da9-aead59b1c85e",
      "m1": "چو آواز بیژن رسیدش بگوش",
      "m2": "برآمد بناکام ازو یک خروش",
      "verse_number": 60
    },
    {
      "id": "25152bc4-2201-47fd-a24f-12701a77a69f",
      "m1": "برستم چنین گفت کای بآفرین",
      "m2": "گزین همه خسروان زمین",
      "verse_number": 61
    },
    {
      "id": "8884d0ce-057e-4112-bef6-91f750957360",
      "m1": "چنان شاد گشتم بدیدار تو",
      "m2": "بدین پرسش خوب و گفتار تو",
      "verse_number": 62
    },
    {
      "id": "0ed90652-1807-4714-a8ac-5fb97d655475",
      "m1": "درستند ازین هرک بردی تو نام",
      "m2": "ازیشان فراوان درود و پیام",
      "verse_number": 63
    },
    {
      "id": "2ef29ca3-603f-4321-9800-75a0d2b47064",
      "m1": "نبینی که بر من بپیران سرم",
      "m2": "چه آمد ز بخت بد اندر خورم",
      "verse_number": 64
    },
    {
      "id": "ed40786c-3747-41af-80c6-ac5bef2ff6b5",
      "m1": "چه چشم بد آمد بگودرزیان",
      "m2": "کزان سود ما را سر آمد زیان",
      "verse_number": 65
    },
    {
      "id": "f76892ab-485c-4d14-a7ab-09fb398d91ad",
      "m1": "ز گیتی مرا خود یکی پور بود",
      "m2": "همم پور و هم پاک دستور بود",
      "verse_number": 66
    },
    {
      "id": "0b339c04-4095-4e77-834e-9f9c0fc96b7f",
      "m1": "شد از چشم من در جهان ناپدید",
      "m2": "بدین دودمان کس چنین غم ندید",
      "verse_number": 67
    },
    {
      "id": "00fc3ecf-bf0b-420c-9ff6-8a99c5808323",
      "m1": "چنینم که بینی بپشت ستور",
      "m2": "شب و روز تازان بتاریک هور",
      "verse_number": 68
    },
    {
      "id": "9657dcbb-d96b-4349-82b0-d463840e6ba3",
      "m1": "ز بیژن شب و روز چون بیهشان",
      "m2": "بجستم بهر سو ز هر کس نشان",
      "verse_number": 69
    },
    {
      "id": "1b624e08-9201-433c-bd90-25e42efab582",
      "m1": "کنون شاه با جام گیتی نمای",
      "m2": "بپیش جهان آفرین شد بپای",
      "verse_number": 70
    },
    {
      "id": "b5613a29-a6a5-4c36-805a-2a064c4d9227",
      "m1": "چه مایه خروشید و کرد آفرین",
      "m2": "بجشن کیان هرمز فرودین",
      "verse_number": 71
    },
    {
      "id": "62c815a2-ce9f-4dfc-8a9f-8a9876ec0a76",
      "m1": "پس آمد ز آتشکده تا بگاه",
      "m2": "کمربست و بنهاد بر سر کلاه",
      "verse_number": 72
    },
    {
      "id": "6dbcedc1-a79a-4ce5-893f-c8c1bdea1a48",
      "m1": "همان جام رخشنده بنهاد پیش",
      "m2": "بهر سو نگه کرد ز اندازه بیش",
      "verse_number": 73
    },
    {
      "id": "958002f8-d250-446e-91ee-8dd2145983d7",
      "m1": "بتوران نشان داد زو شهریار",
      "m2": "ببند گران و ببد روزگار",
      "verse_number": 74
    },
    {
      "id": "83a94700-cf16-4c85-aa5a-89a493561d0f",
      "m1": "چو در جام کیخسرو ایدون نمود",
      "m2": "سوی پهلوانم دوانید زود",
      "verse_number": 75
    },
    {
      "id": "4cd28cdc-f029-403a-9a4a-22f66d31b9d2",
      "m1": "کنون آمدم با دلی پر امید",
      "m2": "دو رخساره زرد و دو دیده سپید",
      "verse_number": 76
    },
    {
      "id": "a0fc4392-d73c-4063-8bce-b2dabe577436",
      "m1": "ترا دیدم اندر جهان چارهگر",
      "m2": "تو بندی بفریاد هر کس کمر",
