{
  "id": "a403654d-95f0-4133-a207-bba9c82a8af3",
  "title": "بخش ۹",
  "verses": [
    {
      "id": "558bf042-bf9d-4ea3-bba8-7c4f50aafe7a",
      "m1": "چو پالیزبان گفت و موبد شنید",
      "m2": "به روشن روان مرد دانا بدید",
      "verse_number": 1
    },
    {
      "id": "775b9758-6655-4f72-9704-edeb0d748b52",
      "m1": "که آن شیردل مرد جز شاه نیست",
      "m2": "همان چهر او جز در گاه نیست",
      "verse_number": 2
    },
    {
      "id": "b607bf65-2160-411e-955a-35c7d307c7b5",
      "m1": "فرستادهای جست روشنروان",
      "m2": "فرستاد موبد بر پهلوان",
      "verse_number": 3
    },
    {
      "id": "bafeee14-6a19-4cd9-a5d7-97514358753d",
      "m1": "که پیدا شد آن فر شاپور شاه",
      "m2": "تو از هر سوی انجمن کن سپاه",
      "verse_number": 4
    },
    {
      "id": "db107a81-acff-486c-820a-47f446728da5",
      "m1": "فرستادهٔ موبد آمد دوان",
      "m2": "ز جایی که بد تا در پهلوان",
      "verse_number": 5
    },
    {
      "id": "347f992a-92bf-49b0-85c0-859446b8f1c3",
      "m1": "بگفت آنک در باغ شادی و بخت",
      "m2": "شکفته شد آن خسروانی درخت",
      "verse_number": 6
    },
    {
      "id": "feedf736-0440-4c5e-9e49-f5de0f7deaab",
      "m1": "سپهبد ز گفتار او گشت شاد",
      "m2": "دلش پر ز کین گشت و لب پر ز باد",
      "verse_number": 7
    },
    {
      "id": "20497da1-2566-43f5-94e3-4813c6031723",
      "m1": "به دادار گفت ای جهاندار راست",
      "m2": "پرستش کنی جز ترا ناسزاست",
      "verse_number": 8
    },
    {
      "id": "1970c029-380c-4938-b76a-9483f37361ac",
      "m1": "که دانست هرگز که شاپور شاه",
      "m2": "ببیند سپه نیز و او را سپاه",
      "verse_number": 9
    },
    {
      "id": "ea954c0a-8759-44d4-8799-e54c1d80e95c",
      "m1": "سپاس از تو ای دادگر یک خدای",
      "m2": "جهاندار و بر نیکویی رهنمای",
      "verse_number": 10
    },
    {
      "id": "b814c6eb-8726-4967-be58-cb3a6f7d296a",
      "m1": "چو شب برکشید آن درفش سیاه",
      "m2": "ستاره پدید آمد از گرد ماه",
      "verse_number": 11
    },
    {
      "id": "858f5daa-4126-46a7-9dde-2f7475f46e5c",
      "m1": "فراز آمد از هر سوی لشکری",
      "m2": "به جایی که بد در جهان مهتری",
      "verse_number": 12
    },
    {
      "id": "77ceb4c0-c9a5-479d-828f-ab1c2c41f5b5",
      "m1": "سوی سورستان سربرافراختند",
      "m2": "یگان و دوگانه همی تاختند",
      "verse_number": 13
    },
    {
      "id": "27a55add-1b0d-4a3b-9f01-32076a508ec0",
      "m1": "به درگاه پالیزبان آمدند",
      "m2": "به شادی بر میزبان آمدند",
      "verse_number": 14
    },
    {
      "id": "08e1af01-96e4-41d8-b523-2346efa8f989",
      "m1": "چو لشکر شد آسوده بر درسرای",
      "m2": "به نزدیک شاه آمد آن پاکرای",
      "verse_number": 15
    },
    {
      "id": "9b575a49-1cc6-48e6-8d7f-21c97d62f65d",
      "m1": "به شاه جهان گفت پس میزبان",
      "m2": "خجستست بر ماه پالیزبان",
      "verse_number": 16
    },
    {
      "id": "5f99397b-9c3f-4927-a0d5-e585623db9c2",
      "m1": "سپاه انجمن شد بدین درسرای",
      "m2": "نگه کن کنون تا چه آیدت رای",
      "verse_number": 17
    },
    {
      "id": "57f22aeb-594e-41ff-8bc9-ec4e1c32d150",
      "m1": "بفرمود تا برگشادند راه",
      "m2": "اگر چه فرومایه بد جایگاه",
      "verse_number": 18
    },
    {
      "id": "b1cef3f7-6572-45d2-b87f-3fc5a6f52871",
      "m1": "چو رفتند نزدیک آن نامجوی",
      "m2": "یکایک نهادند بر خاک روی",
      "verse_number": 19
    },
    {
      "id": "13e1be59-1a93-44ba-9136-7c8a27160209",
      "m1": "مهان را همه شاه در بر گرفت",
      "m2": "ز بدها خروشیدن اندر گرفت",
      "verse_number": 20
    },
    {
      "id": "279a01ae-131d-4c23-8988-ccf8fd07c50d",
      "m1": "بگفت آنک از چرم خر دیده بود",
      "m2": "سخنهای قیصر که بشنیده بود",
      "verse_number": 21
    },
    {
      "id": "d8712075-fb91-4d13-9876-ea932ce193ee",
      "m1": "هم آزادی آن بت خوبچهر",
      "m2": "بگفت آنچ او کرد پیدا ز مهر",
      "verse_number": 22
    },
    {
      "id": "4f403aa3-3cef-4129-b43d-79d1dd28b9eb",
      "m1": "کزو یافتم جان و از کردگار",
      "m2": "که فرخنده بادا برو روزگار",
      "verse_number": 23
    },
    {
      "id": "14965847-c997-4bed-b8e2-f92e7c0ace44",
      "m1": "وگر شهریاری و فرخندهای",
      "m2": "بود بندهٔ پرهنر بندهای",
      "verse_number": 24
    },
    {
      "id": "4e4003c1-c6c2-416d-9e88-c8062de9c921",
      "m1": "منم بنده این مهربان بنده را",
      "m2": "گشادهدل و نازپرورده را",
      "verse_number": 25
    },
    {
      "id": "285cb57f-7f22-42d9-90a2-ce411ba57028",
      "m1": "ز هر سو که اکنون سپاه منست",
      "m2": "وگر پادشاهی و راه منست",
      "verse_number": 26
    },
    {
      "id": "5c140612-9570-4417-b6c8-fd568a5606af",
      "m1": "همه کس فرستید و آگه کنید",
      "m2": "طلایه پراگنده بر ره کنید",
      "verse_number": 27
    },
    {
      "id": "6e24e433-adb1-46dc-b43d-0347ab12bd98",
      "m1": "ببندید ویژه ره طیسفون",
      "m2": "نباید که آگاهی آید برون",
      "verse_number": 28
    },
    {
      "id": "f3c6786f-f919-4a7e-b3d6-64cb38ebaf26",
      "m1": "چو قیصر بیابد ز ما آگهی",
      "m2": "که بیدار شد فر شاهنشهی",
      "verse_number": 29
    },
    {
      "id": "9f2041ee-0373-46fe-80ff-eb1d82ab3e57",
      "m1": "بیاید سپاه مرا برکند",
      "m2": "دل و پشت ایرانیان بشکند",
      "verse_number": 30
    },
    {
      "id": "afaabea6-4af9-4891-91a9-ee94d752582a",
      "m1": "کنون ما نداریم پایاب اوی",
      "m2": "نه پیچیم با بخت شاداب اوی",
      "verse_number": 31
    },
    {
      "id": "7f0adf06-282c-4467-bd96-d37c8e5e0170",
      "m1": "چو موبد بیاید بیارد سپاه",
      "m2": "ز لشکر ببندیم بر پشه راه",
      "verse_number": 32
    },
    {
      "id": "d5eca151-1603-443e-9c39-bc7d82b6d520",
      "m1": "بسازیم و آرایشی نو کنیم",
      "m2": "نهانی مگر باغ بیخو کنیم",
      "verse_number": 33
    },
    {
      "id": "90949e6a-4767-47e9-a341-822490849fd7",
      "m1": "بباید به هر گوشهای دیدهبان",
      "m2": "طلایه به روز و به شب پاسبان",
      "verse_number": 34
    },
    {
      "id": "1bfcf0a2-4b39-4f3b-a522-a1671bbf88ad",
      "m1": "ازان پس نمانیم از رومیان",
      "m2": "کسی خسپد ایمن گشادهمیان",
      "verse_number": 35
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 35
  },
  "order_in_parent": 9
}