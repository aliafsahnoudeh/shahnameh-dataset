{
  "id": "719be74b-f4f2-4edb-a3a1-ae8ece1096b6",
  "title": "بخش ۱۳",
  "verses": [
    {
      "id": "a1a34a70-cd54-44c0-8b77-1445e917dbe3",
      "m1": "یکی مرد بود از نژاد سران",
      "m2": "هم از تخمهٔ نامور قیصران",
      "verse_number": 1
    },
    {
      "id": "b1376c6f-4f1b-435e-a7db-29981187d7da",
      "m1": "برانوش نام و خردمند بود",
      "m2": "زبان و روانش پر از بند بود",
      "verse_number": 2
    },
    {
      "id": "79fa1e08-40db-408d-af9f-194066236ac5",
      "m1": "بدو گفت لشکر که قیصر تو باش",
      "m2": "برین لشکر و بوم مهتر تو باش",
      "verse_number": 3
    },
    {
      "id": "3466d8d2-bfc5-4abf-887f-2397cc5bbf23",
      "m1": "به گفتار تو گوش دارد سپاه",
      "m2": "بیفروز تاج و بیارای گاه",
      "verse_number": 4
    },
    {
      "id": "034efa70-9068-4e19-85cf-3d5bc7a19334",
      "m1": "بیاراستند از برش تخت عاج",
      "m2": "برانوش بنشست بر سرش تاج",
      "verse_number": 5
    },
    {
      "id": "81d65787-57b4-44a7-ad66-e8a3e61ce48d",
      "m1": "به جای بزرگیش بنشاندند",
      "m2": "همه رومیان آفرین خواندند",
      "verse_number": 6
    },
    {
      "id": "cba7219c-2b9e-4de6-b197-b1cd0785e637",
      "m1": "برانوش بنشست و اندیشه کرد",
      "m2": "ز روم و ز آوردگاه نبرد",
      "verse_number": 7
    },
    {
      "id": "a01282b0-4606-4866-8c80-cd41e8183e7b",
      "m1": "بدانست کو را ز شاه بلند",
      "m2": "ز روم و ز آویزش آید گزند",
      "verse_number": 8
    },
    {
      "id": "d3e61b19-5fb9-4f23-a07d-def2f1c4c8e9",
      "m1": "فرستادهای جست بارای و شرم",
      "m2": "که دانش سراید به آواز نرم",
      "verse_number": 9
    },
    {
      "id": "ec184ee1-aa9b-4945-95ff-2ab2e54ab968",
      "m1": "دبیری بزرگ و جهاندیدهای",
      "m2": "خردمند و دانا پسندیدهای",
      "verse_number": 10
    },
    {
      "id": "3b7cb12b-1773-4c8c-bbc0-14cc1bdda86b",
      "m1": "بیاورد و بنشاند نزدیک خویش",
      "m2": "بگفت آن سخنهای باریک خویش",
      "verse_number": 11
    },
    {
      "id": "df72a4ee-136b-448e-9d99-28946d880121",
      "m1": "یکی نامه بنوشت پرآفرین",
      "m2": "ز دادار بر شهریار زمین",
      "verse_number": 12
    },
    {
      "id": "e50158ea-d924-4f76-b206-dd9c41facfeb",
      "m1": "که جاوید تاج تو پاینده باد",
      "m2": "همه مهتران پیش تو بنده باد",
      "verse_number": 13
    },
    {
      "id": "0fb38d50-0138-41df-a13f-c2162c6f0b5d",
      "m1": "تو دانی که تاراج و خون ریختن",
      "m2": "چه با بیگنه مردم آویختن",
      "verse_number": 14
    },
    {
      "id": "1f432792-89cb-42d0-8fa5-97ee7478afcf",
      "m1": "مهان سرافراز دارند شوم",
      "m2": "چه با شهر ایران چه با مرز روم",
      "verse_number": 15
    },
    {
      "id": "a2fce64d-7175-42c2-ab16-6b26a4dbf17b",
      "m1": "گر این کین ایرج به دست از نخست",
      "m2": "منوچهر کرد آن به مردی درست",
      "verse_number": 16
    },
    {
      "id": "d4cd63d0-e113-4c91-bc56-43174db628f8",
      "m1": "تن سلم زان کین کنون خاک شد",
      "m2": "هم از تور روی زمین پاک شد",
      "verse_number": 17
    },
    {
      "id": "822a5d10-5095-4fb2-99e0-56c91c4a766c",
      "m1": "وگر کین داراست و اسکندری",
      "m2": "که نو شد بر وی زمین داوری",
      "verse_number": 18
    },
    {
      "id": "f4889b5a-9050-4086-a047-ea27d98074ce",
      "m1": "مر او را دو دستور بد کشته بود",
      "m2": "و دیگر کزو بخت برگشته بود",
      "verse_number": 19
    },
    {
      "id": "8aad0e15-4181-4af2-b047-162c656c4d8e",
      "m1": "گرت کین قیصر فزاید همی",
      "m2": "به زندان تو بند ساید همی",
      "verse_number": 20
    },
    {
      "id": "fab14799-b7dd-4268-a070-8c7f8e3db5f2",
      "m1": "نباید که ویران شود بوم روم",
      "m2": "که چون روم دیگر نبودست بوم",
      "verse_number": 21
    },
    {
      "id": "b0e7df51-290e-49ad-84da-b294ac1ac78a",
      "m1": "وگر غارت و کشتنت بود رای",
      "m2": "همه روم گشتند بیدست و پای",
      "verse_number": 22
    },
    {
      "id": "9bc3c068-1e26-44fd-9b76-3b7a3465b00c",
      "m1": "زن و کودکانش اسیر تواند",
      "m2": "جگر خسته از تیغ و تیر تواند",
      "verse_number": 23
    },
    {
      "id": "e1d9068a-ff8f-4276-bd65-2fcdf006b616",
      "m1": "گه آمد که کمتر کنی کین و خشم",
      "m2": "فرو خوابنی از گذشته دو چشم",
      "verse_number": 24
    },
    {
      "id": "0f34ca3b-55cb-488f-bae6-442b09412a13",
      "m1": "فدای تو بادا همه خواسته",
      "m2": "کزین کین همی جان شود کاسته",
      "verse_number": 25
    },
    {
      "id": "ecb6d587-574f-420c-8f2a-d7a19fb1f35e",
      "m1": "تو دل خوش کن و شهر چندین مسوز",
      "m2": "نباید که روز اندر آید به روز",
      "verse_number": 26
    },
    {
      "id": "97accb10-647f-4c23-856f-bedc94ced26a",
      "m1": "نباشد پسند جهانآفرین",
      "m2": "که بیداد جوید جهاندار کین",
      "verse_number": 27
    },
    {
      "id": "be5f3bad-53fa-403b-9411-738ee0840632",
      "m1": "درود جهاندار بر شاه باد",
      "m2": "بلند اخترش افسر ماه باد",
      "verse_number": 28
    },
    {
      "id": "4b0db71a-80b5-4a65-9b03-11289502f67d",
      "m1": "نویسنده بنهاد پس خامه را",
      "m2": "چو اندر نوشت آن کیی نامه را",
      "verse_number": 29
    },
    {
      "id": "05f31532-f9e2-4b17-a1b1-521f54b3bdf9",
      "m1": "نهادند پس مهر قیصر بروی",
      "m2": "فرستاده بنهاد زی شاه روی",
      "verse_number": 30
    },
    {
      "id": "bfabe02a-1c23-4530-8e27-cbf72f05488e",
      "m1": "بیامد خردمند و نامه بداد",
      "m2": "ز قیصر به شاپور فرخ نژاد",
      "verse_number": 31
    },
    {
      "id": "0d19b3fc-b570-4668-8eea-d637e5324807",
      "m1": "چو آن نامور نامه برخواندند",
      "m2": "سخنهای نغزش برافشاندند",
      "verse_number": 32
    },
    {
      "id": "c7bb9f60-87cb-45ce-84b4-59068708a364",
      "m1": "ببخشود و دیده پر از آب کرد",
      "m2": "بروهای جنگی پر از تاب کرد",
      "verse_number": 33
    },
    {
      "id": "61f245fb-c135-446f-8188-3eea94005dc4",
      "m1": "هماندر زمان نامه پاسخ نوشت",
      "m2": "بگفت آنکجا رفته بد خوب و زشت",
      "verse_number": 34
    },
    {
      "id": "aa32936c-9f68-4b04-b3fa-a311ce990809",
      "m1": "که مهمان به چرم خر اندر که دوخت",
      "m2": "که بازار کین کهن برفروخت",
      "verse_number": 35
    },
    {
      "id": "0713eb69-6645-4cf2-962f-6fb9acc3b737",
      "m1": "تو گرد بخردی خیز پیش من آی",
      "m2": "خود و فیلسوفان پاکیزه رای",
      "verse_number": 36
    },
    {
      "id": "cdb9b460-969d-424d-b2f2-4f17f8737803",
      "m1": "چو زنهار دادم نسازمت جنگ",
      "m2": "گشاده کنم بر تو این راه تنگ",
      "verse_number": 37
    },
    {
      "id": "2ccfb567-98bb-47cc-ad2b-56d8184f30cd",
      "m1": "فرستاده برگشت و پاسخ ببرد",
      "m2": "سخنها یکایک همه برشمرد",
      "verse_number": 38
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 38
  },
  "order_in_parent": 13
}