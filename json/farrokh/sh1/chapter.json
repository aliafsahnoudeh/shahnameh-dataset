{
  "id": "4c973386-1cb6-4429-bf68-46b5ed169fcf",
  "title": "پادشاهی فرخ زاد",
  "verses": [
    {
      "id": "9524e3e5-1a7a-472a-a845-1df9e3c9e3d3",
      "m1": "ز جهرم فرخ زاد راخواندند",
      "m2": "بران تخت شاهیش بنشاندند",
      "verse_number": 1
    },
    {
      "id": "0fb6f410-d880-488b-b73b-5339078db47e",
      "m1": "چو برتخت بنشست و کرد آفرین",
      "m2": "ز نیکی دهش بر جهان آفرین",
      "verse_number": 2
    },
    {
      "id": "26beef50-ddbc-4ccb-8990-1792700ecfa2",
      "m1": "منم گفت فرزند شاهنشهان",
      "m2": "نخواهم جز از ایمنی در جهان",
      "verse_number": 3
    },
    {
      "id": "2ba63ff5-962a-4f4e-9082-7ff43e739f3b",
      "m1": "ز گیتی هرآنکس که جوید گزند",
      "m2": "چو من شاه باشم نگردد بلند",
      "verse_number": 4
    },
    {
      "id": "abf77cc4-5e78-442e-b764-a7245f6ccd17",
      "m1": "هر آنکس که جوید به دل راستی",
      "m2": "نیارد به کار اندرون کاستی",
      "verse_number": 5
    },
    {
      "id": "868af2d2-a4ea-401e-a344-ee2cb9935672",
      "m1": "بدارمش چون جان پاک ارجمند",
      "m2": "نجویم ابر بیگزندان گزند",
      "verse_number": 6
    },
    {
      "id": "82d0e63d-9192-489c-99ca-24a723419070",
      "m1": "چو یک ماه بگذشت بر تخت اوی",
      "m2": "بخاک اندر آمد سر و بخت اوی",
      "verse_number": 7
    },
    {
      "id": "3f929f93-a4c3-4d0f-afbd-8a91872844e5",
      "m1": "همین بودش از روز و آرام بهر",
      "m2": "یکی بنده در می برآمیخت زهر",
      "verse_number": 8
    },
    {
      "id": "fb950cf0-7388-4182-9031-f40d053248fb",
      "m1": "بخورد و یکی هفته زان پس بزیست",
      "m2": "هرآنکس که بشنید بروی گریست",
      "verse_number": 9
    },
    {
      "id": "b8e2127e-4d7d-4c3e-b236-c83264504433",
      "m1": "همی پادشاهی به پایان رسید",
      "m2": "ز هر سو همی دشمن آمد پدید",
      "verse_number": 10
    },
    {
      "id": "235aa2c2-baad-4fd6-85fe-6a8096e21d46",
      "m1": "چنین است کردار گردنده دهر",
      "m2": "نگه کن کزو چند یابی تو بهر",
      "verse_number": 11
    },
    {
      "id": "b0be465d-65ae-44f4-8316-13f4d937f64c",
      "m1": "بخور هرچ داری به فردا مپای",
      "m2": "که فردا مگر دیگر آیدش رای",
      "verse_number": 12
    },
    {
      "id": "21f057e2-fc19-4894-a08f-b7f55e145923",
      "m1": "ستاند ز تو دیگری را دهد",
      "m2": "جهان خوانیش بیگمان بر جهد",
      "verse_number": 13
    },
    {
      "id": "983a3cdf-74e4-4649-bf57-249f47daf65a",
      "m1": "بخور هرچ داری فزونی بده",
      "m2": "تو رنجیدهای بهر دشمن منه",
      "verse_number": 14
    },
    {
      "id": "0549e016-3c51-4829-a980-bcffb41450d2",
      "m1": "هرآنگه که روز تو اندر گذشت",
      "m2": "نهاده همه باد گردد به دشت",
      "verse_number": 15
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 15
  }
}