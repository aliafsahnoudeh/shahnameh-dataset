{
  "id": "f5c57876-b5ef-4f87-a483-54f6c3c9a343",
  "title": "بخش ۱۷",
  "verses": [
    {
      "id": "77604692-68e5-4d91-a883-6e2506f827f0",
      "m1": "چو بگذشت سال از برم شست و پنج",
      "m2": "فزون کردم اندیشه و درد و رنج",
      "verse_number": 1
    },
    {
      "id": "6932ec6c-7933-4de0-8553-fdbc0f91a704",
      "m1": "به تاریخ شاهان نیاز آمدم",
      "m2": "به پیش اختر دیرساز آمدم",
      "verse_number": 2
    },
    {
      "id": "fed6fe20-0282-4a0b-93a8-745d9fe37fe8",
      "m1": "بزرگان و بادانش آزادگان",
      "m2": "نبشتند یکسر همه رایگان",
      "verse_number": 3
    },
    {
      "id": "6ab2d677-6fe5-4444-927f-1c398345d5ff",
      "m1": "نشسته نظاره من از دورشان",
      "m2": "تو گفتی بدم پیش مزدورشان",
      "verse_number": 4
    },
    {
      "id": "534276d7-153c-4aaf-8459-027a9decf69c",
      "m1": "جز احسنت از ایشان نبُد بهرهام",
      "m2": "بِکَفت اندر احسنتشان زهرهام",
      "verse_number": 5
    },
    {
      "id": "4463046e-62c6-4aa9-8a9d-fb7d74e121ab",
      "m1": "سر بدرههای کهن بسته شد",
      "m2": "وزان بند روشن دلم خسته شد",
      "verse_number": 6
    },
    {
      "id": "6486015c-8e9b-47a3-aaa3-9dca4afab342",
      "m1": "ازین نامور نامداران شهر",
      "m2": "علی دیلمی بود کو راست بهر",
      "verse_number": 7
    },
    {
      "id": "2658af4f-878b-4224-9585-2b741c18deab",
      "m1": "که همواره کارش بخوبی روان",
      "m2": "به نزد بزرگان روشنروان",
      "verse_number": 8
    },
    {
      "id": "34042a9c-8a31-4db5-8819-8b3b51556cc7",
      "m1": "حسین قتیب است از آزادگان",
      "m2": "که از من نخواهد سخن رایگان",
      "verse_number": 9
    },
    {
      "id": "9fc1454b-a9be-4c19-adf0-a076cf43643f",
      "m1": "ازویم خور و پوشش و سیم و زر",
      "m2": "وزو یافتم جنبش و پای و پر",
      "verse_number": 10
    },
    {
      "id": "ee453281-4d0b-4a5e-9b17-ad35e4b93605",
      "m1": "نیَم آگه از اصل و فرع خراج",
      "m2": "همیغلتم اندر میان دواج",
      "verse_number": 11
    },
    {
      "id": "a47da1a8-72eb-4b7e-9b30-d267870903e5",
      "m1": "جهاندار اگر نیستی تنگدست",
      "m2": "مرا بر سر گاه بودی نشست",
      "verse_number": 12
    },
    {
      "id": "59a44ce1-9a0b-4af2-afab-fd6ec7996e1a",
      "m1": "چو سال اندر آمد به هفتاد و یک",
      "m2": "همی زیر بیت اندر آرم فلک",
      "verse_number": 13
    },
    {
      "id": "a35367e5-57e1-4877-814e-695d0bc86afc",
      "m1": "همی گاه محمود آباد باد",
      "m2": "سرش سبز باد و دلش شاد باد",
      "verse_number": 14
    },
    {
      "id": "f914e4b6-ace3-42ae-9666-b8b977ae2701",
      "m1": "چنانش ستایم که اندر جهان",
      "m2": "سخن باشد از آشکار و نهان",
      "verse_number": 15
    },
    {
      "id": "85b5b2fa-7e31-4732-95ef-fcbafe237770",
      "m1": "مرا از بزرگان ستایش بود",
      "m2": "ستایش ورا در فزایش بود",
      "verse_number": 16
    },
    {
      "id": "fe76b173-dd81-4f31-ba25-55ccb94b1f56",
      "m1": "که جاوید باد آن خردمند مرد",
      "m2": "همیشه به کام دلش کار کرد",
      "verse_number": 17
    },
    {
      "id": "03bb2b46-63a5-433a-be6a-f55fbd1aedb9",
      "m1": "همش رای و هم دانش و هم نسب",
      "m2": "چراغ عجم آفتاب عرب",
      "verse_number": 18
    },
    {
      "id": "e658db37-4ac8-4cd2-a6b3-68d38951d719",
      "m1": "سرآمد کنون قصهٔ یزدگرد",
      "m2": "به ماه سفندارمذ روزِ اَرد",
      "verse_number": 19
    },
    {
      "id": "f1a7658b-922c-4402-9a87-65837bc3cee3",
      "m1": "ز هجرت شده پنج هشتاد بار",
      "m2": "به نام جهانداور کردگار",
      "verse_number": 20
    },
    {
      "id": "bb9b14a1-8522-45be-b551-0cc2b0942c2e",
      "m1": "چو این نامورنامه آمد به بُن",
      "m2": "ز من روی کشور شود پر سخُن",
      "verse_number": 21
    },
    {
      "id": "f4e14dba-7c81-4b9a-9551-84ce681d718f",
      "m1": "از آن پس نمیرم که من زندهام",
      "m2": "که تخم سخن من پراگندهام",
      "verse_number": 22
    },
    {
      "id": "b4e664eb-8de4-425f-a563-f9e10f652126",
      "m1": "هر آنکس که دارد هُش و رای و دین",
      "m2": "پس از مرگ بر من کند آفرین",
      "verse_number": 23
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 23
  }
}