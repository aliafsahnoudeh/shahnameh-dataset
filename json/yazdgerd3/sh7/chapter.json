{
  "id": "f802cc97-ffd3-46df-ba3a-3407f93a03f6",
  "title": "بخش ۷",
  "verses": [
    {
      "id": "6f2c5bd8-8428-4500-8252-173cc55577cf",
      "m1": "دبیر جهاندیده را پیش خواند",
      "m2": "دل آگنده بودش همه برفشاند",
      "verse_number": 1
    },
    {
      "id": "007aeea8-3745-4274-9ae0-cf1a8859b02f",
      "m1": "جهاندار چون کرد آهنگ مرو",
      "m2": "به ماهوی سوری کنارنگ مرو",
      "verse_number": 2
    },
    {
      "id": "add50c12-cdbe-434e-967a-2b3e950b81d2",
      "m1": "یکی نامه بنوشت با درد و خشم",
      "m2": "پر از آرزو دل پر از آب چشم",
      "verse_number": 3
    },
    {
      "id": "9433af01-d407-4101-8dad-9ead2a529cda",
      "m1": "نخست آفرین کرد بر کردگار",
      "m2": "خداوند دانا و پروردگار",
      "verse_number": 4
    },
    {
      "id": "a498d200-4a3f-4e18-89d4-f0e4856e5732",
      "m1": "خداوند گردنده بهرام و هور",
      "m2": "خداوند پیل و خداوند مور",
      "verse_number": 5
    },
    {
      "id": "cf1a5a43-1056-4859-86e1-7e441559de58",
      "m1": "کند چون بخواهد ز ناچیز چیز",
      "m2": "که آموزگارش نباید به نیز",
      "verse_number": 6
    },
    {
      "id": "4df98bf2-fd1f-42f4-9833-0947074c825f",
      "m1": "بگفت آنک ما را چه آمد بروی",
      "m2": "وزین پادشاهی بشد رنگ و بوی",
      "verse_number": 7
    },
    {
      "id": "90eca276-4295-4c87-aa78-b2093a04e24c",
      "m1": "ز رستم کجا کشته شد روز جنگ",
      "m2": "ز تیمار بر ما جهان گشت تنگ",
      "verse_number": 8
    },
    {
      "id": "b62fdefb-577f-4272-bce5-9e5a5413c3cb",
      "m1": "بدست یکی سعد وقاص نام",
      "m2": "نه بوم و نژاد و نه دانش نه کام",
      "verse_number": 9
    },
    {
      "id": "2f5db7e7-f416-4381-93e2-7355af3e500e",
      "m1": "کنون تا در طیسفون لشکرست",
      "m2": "همین زاغ پیسه به پیش اندرست",
      "verse_number": 10
    },
    {
      "id": "ba569432-b638-4911-841c-761e9f501bdf",
      "m1": "تو با لشکرت رزم را ساز کن",
      "m2": "سپه را برین بر هم آواز کن",
      "verse_number": 11
    },
    {
      "id": "5b398184-0288-4cfe-92e2-9a0d2a503bad",
      "m1": "من اینک پس نامه بر سانِ باد",
      "m2": "بیایم به نزد تو ای پاک و راد",
      "verse_number": 12
    },
    {
      "id": "5e0d5cb3-3a17-459f-8383-65b3a419321e",
      "m1": "فرستادهٔ دیگر از انجمن",
      "m2": "گزین کرد بینادل و رایزن",
      "verse_number": 13
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 13
  }
}