{
  "id": "7d49cd9b-38e8-4943-b99b-010415f1c4f2",
  "title": "بخش ۱۳",
  "verses": [
    {
      "id": "a44d437c-6d38-4ffe-bbd8-bc0c2bf97642",
      "m1": "چنین داد خوانیم بر یزدگرد",
      "m2": "وگر کینه خوانیم ازین هفت گرد",
      "verse_number": 1
    },
    {
      "id": "5700c2c2-7c32-4a86-82e7-37a411782115",
      "m1": "اگر خود نداند همی کین و داد",
      "m2": "مرا فیلسوف ایچ پاسخ نداد",
      "verse_number": 2
    },
    {
      "id": "ef9ef14e-484f-4346-94bb-8add72e34279",
      "m1": "وگر گفت دینی همه بسته گفت",
      "m2": "بماند همی پاسخ اندر نهفت",
      "verse_number": 3
    },
    {
      "id": "bbd41693-afab-492f-8f70-d1c000820368",
      "m1": "اگر هیچ گنجست ای نیک رای",
      "m2": "بیارای و دل را به فردا مپای",
      "verse_number": 4
    },
    {
      "id": "a6a9ed7d-258f-469e-b00e-88339f166679",
      "m1": "که گیتی همی بر تو بر بگذرد",
      "m2": "زمانه دم ما همیبشمرد",
      "verse_number": 5
    },
    {
      "id": "4dbeda80-3e36-43f5-a982-e82172b19b4e",
      "m1": "در خوردنت چیره کن برنهاد",
      "m2": "اگر خود بمانی دهد آنک داد",
      "verse_number": 6
    },
    {
      "id": "e02e6cca-0acb-478a-9c29-8eecba6ac688",
      "m1": "مرا دخل و خرج ار برابر بدی",
      "m2": "زمانه مرا چون برادر بدی",
      "verse_number": 7
    },
    {
      "id": "d31b1993-da0f-487b-b494-47cea70986f5",
      "m1": "تگرگ آمد امسال بر سان مرگ",
      "m2": "مرا مرگ بهتر بُدی از تگرگ",
      "verse_number": 8
    },
    {
      "id": "f5dc807e-c871-4013-8b07-5dd3d4fe44d8",
      "m1": "در هیزم و گندم و گوسفند",
      "m2": "ببست این برآورده چرخ بلند",
      "verse_number": 9
    },
    {
      "id": "5eca353d-e04d-4340-9edb-1fec05f04f76",
      "m1": "می آور که از روزمان بس نماند",
      "m2": "چنین بود تا بود و بر کس نماند",
      "verse_number": 10
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 10
  },
  "order_in_parent": 13
}