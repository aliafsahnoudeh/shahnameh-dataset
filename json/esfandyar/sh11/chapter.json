{
  "id": "33aa8355-14b0-4959-afab-b53751720746",
  "title": "بخش ۱۱",
  "verses": [
    {
      "id": "51f3e5d3-c708-498b-901f-cb0654227edd",
      "m1": "ز رستم چو بشنید بهمن سخن",
      "m2": "روان گشت با موبد پاکتن",
      "verse_number": 1
    },
    {
      "id": "4881f3d2-371a-4dba-aba6-4b38dc12358a",
      "m1": "تهمتن زمانی به ره در بماند",
      "m2": "زواره فرامرز را پیش خواند",
      "verse_number": 2
    },
    {
      "id": "5e4b6c1a-e3a4-436e-b541-990d663f90ee",
      "m1": "کز ایدر به نزدیک دستان شوید",
      "m2": "به نزد مه کابلستان شوید",
      "verse_number": 3
    },
    {
      "id": "048d3e84-60a8-4423-aab3-0adc280470da",
      "m1": "بگویید کاسفندیار آمدست",
      "m2": "جهان را یکی خواستار آمدست",
      "verse_number": 4
    },
    {
      "id": "b66ab20b-59dc-40de-a07e-86f043547563",
      "m1": "به ایوانها تخت زرین نهید",
      "m2": "برو جامهٔ خسرو آیین نهید",
      "verse_number": 5
    },
    {
      "id": "1f872acc-159c-4eee-bbe3-78f3e87a2f31",
      "m1": "چنان هم که هنگام کاوس شاه",
      "m2": "ازان نیز پرمایهتر پایگاه",
      "verse_number": 6
    },
    {
      "id": "e148f49b-412d-4582-80df-a25ac83380c5",
      "m1": "بسازید چیزی که باید خورش",
      "m2": "خورشهای خوب از پی پرورش",
      "verse_number": 7
    },
    {
      "id": "51285fe1-f6b4-41e7-a04b-c586e5a0e802",
      "m1": "که نزدیک ما پور شاه آمدست",
      "m2": "پر از کینه و رزمخواه آمدست",
      "verse_number": 8
    },
    {
      "id": "dcf1c4fa-be84-4f2e-908a-04a41862d165",
      "m1": "گوی نامدارست و شاهی دلیر",
      "m2": "نیندیشد از جنگ یک دشت شیر",
      "verse_number": 9
    },
    {
      "id": "f184ec97-31ef-4767-a253-13cb5fec3602",
      "m1": "شوم پیش او گر پذیرد نوید",
      "m2": "به نیکی بود هرکسی را امید",
      "verse_number": 10
    },
    {
      "id": "d9d6e987-b352-400c-ab19-85d0b69137df",
      "m1": "اگر نیکویی بینم اندر سرش",
      "m2": "ز یاقوت و زر آورم افسرش",
      "verse_number": 11
    },
    {
      "id": "822370b2-8cbc-4301-a5fb-f10845df3075",
      "m1": "ندارم ازو گنج و گوهر دریغ",
      "m2": "نه برگستوان و نه گوپال و تیغ",
      "verse_number": 12
    },
    {
      "id": "d64ba95b-b50d-4d00-846d-1ff88f1663c0",
      "m1": "وگر بازگرداندم ناامید",
      "m2": "نباشد مرا روز با او سپید",
      "verse_number": 13
    },
    {
      "id": "b1f2aae1-33af-4eb3-8a93-933161d57d1c",
      "m1": "تو دانی که آن تابداده کمند",
      "m2": "سر ژنده پیل اندر آرد به بند",
      "verse_number": 14
    },
    {
      "id": "3abcfce0-ef1d-4444-8825-25eb6e1127aa",
      "m1": "زواره بدو گفت مندیش ازین",
      "m2": "نجوید کسی رزم کش نیست کین",
      "verse_number": 15
    },
    {
      "id": "ab2ea5fa-54b4-4ab8-a48a-35bd0f54da6f",
      "m1": "ندانم به گیتی چو اسفندیار",
      "m2": "برای و به مردی یکی نامدار",
      "verse_number": 16
    },
    {
      "id": "c8dd7d64-c0ad-4a12-b389-04f7f20f51b9",
      "m1": "نیاید ز مرد خرد کار بد",
      "m2": "ندید او ز ما هیچ کردار بد",
      "verse_number": 17
    },
    {
      "id": "a5f3a67a-1ce1-4572-a456-34fafdc2ba36",
      "m1": "زواره بیامد به نزدیک زال",
      "m2": "وزان روی رستم برافراخت یال",
      "verse_number": 18
    },
    {
      "id": "cfedc1c2-da36-4978-b6e5-e3d10ae46403",
      "m1": "بیامد دمان تا لب هیرمند",
      "m2": "سرش تیز گشته ز بیم گزند",
      "verse_number": 19
    },
    {
      "id": "e30ab085-6c4d-4522-ac03-b5c2d74fa0f1",
      "m1": "عنان را گران کرد بر پیش رود",
      "m2": "همی بود تا بهمن آرد درود",
      "verse_number": 20
    },
    {
      "id": "2dfb4abb-3331-485e-aa3f-be8f7b1fc62b",
      "m1": "چو بهمن بیامد به پردهسرای",
      "m2": "همی بود پیش پدر بر به پای",
      "verse_number": 21
    },
    {
      "id": "c75d08f0-0d44-4ab0-9791-769e36fdceb5",
      "m1": "بپرسید ازو فرخ اسفندیار",
      "m2": "که پاسخ چه کرد آن یل نامدار",
      "verse_number": 22
    },
    {
      "id": "cbfee021-46f6-4009-bd90-cbe0aae10710",
      "m1": "چو بشنید بنشست پیش پدر",
      "m2": "بگفت آنچ بشنیده بد در بدر",
      "verse_number": 23
    },
    {
      "id": "d770faea-2927-49c6-bfc1-6c2c7094fa27",
      "m1": "نخستین درودش ز رستم بداد",
      "m2": "پسانگاه گفتار او کرد یاد",
      "verse_number": 24
    },
    {
      "id": "83f38d05-c664-44e0-9aae-e691f437ea77",
      "m1": "همه دیده پیش پدر بازگفت",
      "m2": "همان نیز نادیده اندر نهفت",
      "verse_number": 25
    },
    {
      "id": "c2807f72-b08c-47ce-9492-e061bb9bb756",
      "m1": "بدو گفت چون رستم پیلتن",
      "m2": "ندیده بود کس بهر انجمن",
      "verse_number": 26
    },
    {
      "id": "40b0fbd6-ae8a-4a32-b2d7-f643b7b58281",
      "m1": "دل شیر دارد تن ژنده پیل",
      "m2": "نهنگان برآرد ز دریای نیل",
      "verse_number": 27
    },
    {
      "id": "314daa02-2aef-4671-b417-aa4d60afcb5d",
      "m1": "بیامد کنون تا لب هیرمند",
      "m2": "ابی جوشن و خود و گرز و کمند",
      "verse_number": 28
    },
    {
      "id": "b0ada126-1117-46ae-a0ac-944bad2848ec",
      "m1": "به دیدار شاه آمدستش نیاز",
      "m2": "ندانم چه دارد همی با تو راز",
      "verse_number": 29
    },
    {
      "id": "9d51d3ad-7067-433d-87ea-ddb304dfb9ab",
      "m1": "ز بهمن برآشفت اسفندیار",
      "m2": "ورا بر سر انجمن کرد خوار",
      "verse_number": 30
    },
    {
      "id": "a54cb5e4-9650-4958-814d-74ef26e9beea",
      "m1": "بدو گفت کز مردم سرفراز",
      "m2": "نزیبد که با زن نشیند به راز",
      "verse_number": 31
    },
    {
      "id": "54cbd907-15ce-4f84-b757-ceb7a510b28a",
      "m1": "وگر کودکان را بکاری بزرگ",
      "m2": "فرستی نباشد دلیر و سترگ",
      "verse_number": 32
    },
    {
      "id": "b0632a9f-930a-4db7-a0dd-e97e410e063e",
      "m1": "تو گردنکشان را کجا دیدهای",
      "m2": "که آواز روباه بشنیدهای",
      "verse_number": 33
    },
    {
      "id": "695aa605-1855-45db-a901-48c900ffcd28",
      "m1": "که رستم همی پیل جنگی کنی",
      "m2": "دل نامور انجمن بشکنی",
      "verse_number": 34
    },
    {
      "id": "8f61ab8b-bed5-4b86-b2e0-eeef1934fd86",
      "m1": "چنین گفت پس با پشوتن به راز",
      "m2": "که این شیر رزمآور جنگ ساز",
      "verse_number": 35
    },
    {
      "id": "3d2a4fc0-9c68-47cc-8bfd-0f9f8715cbee",
      "m1": "جوانی همی سازد از خویشتن",
      "m2": "ز سالش همانا نیامد شکن",
      "verse_number": 36
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 36
  },
  "order_in_parent": 11
}