{
  "id": "12076ded-e991-4c74-a5bf-bfc6d4fe07e6",
  "title": "بخش ۱ - آغاز داستان",
  "verses": [
    {
      "id": "8876e5b7-2374-44a4-b78d-29d72000f9b9",
      "m1": "کنون خورد باید می خوشگُوار",
      "m2": "که می بویِ مشک آید از جویبار",
      "verse_number": 1
    },
    {
      "id": "bae12989-17de-44b5-ac41-229e53ecafe9",
      "m1": "هوا پر خروش و زمین پر ز جوش",
      "m2": "خنُک آنک دل شاد دارد به نوش",
      "verse_number": 2
    },
    {
      "id": "baa4efb6-239f-4b4e-a516-c45ce642abe5",
      "m1": "دِرَم دارد و نُقل و جامِ نبید",
      "m2": "سر گوسفندی تواند برید",
      "verse_number": 3
    },
    {
      "id": "7483cc31-4be8-4852-ab02-5bd887ec464b",
      "m1": "مرا نیست فرخ مر آن را که هست",
      "m2": "ببخشایْ بر مردم تنگدست",
      "verse_number": 4
    },
    {
      "id": "dd1874f4-239c-44ea-9ffc-a4eed2695f5d",
      "m1": "همه بوستان زیر برگ گلست",
      "m2": "همه کوه پرلاله و سنبلست",
      "verse_number": 5
    },
    {
      "id": "56f414d6-63de-4bab-8894-663916127e2f",
      "m1": "به پالیز بلبل بنالد همی",
      "m2": "گل از نالهٔ او ببالد همی",
      "verse_number": 6
    },
    {
      "id": "14a4d039-69b6-4250-aee2-6450cce3bf44",
      "m1": "چو از ابر بینم همی باد و نم",
      "m2": "ندانم که نرگس چرا شد دُژَم",
      "verse_number": 7
    },
    {
      "id": "ea2f43f3-6d2a-4649-bbe8-357be2223537",
      "m1": "شب تیره بلبل نخسپد همی",
      "m2": "گل از باد و باران بجنبد همی",
      "verse_number": 8
    },
    {
      "id": "ff0d91bf-9113-4e44-ab8b-aaba55461233",
      "m1": "بخندد همی بلبل از هر دوان",
      "m2": "چو بر گل نشیند گشاید زبان",
      "verse_number": 9
    },
    {
      "id": "c89a90f9-8267-4a1b-8bb1-804bf04f30aa",
      "m1": "ندانم که عاشق گل آمد گر ابر",
      "m2": "چو از ابر بینم خروش هژبر",
      "verse_number": 10
    },
    {
      "id": "3e172055-1f68-4ac4-9443-de803ea9e116",
      "m1": "بَدَرَّد همی باد پیراهنش",
      "m2": "دُرفشان شود آتش اندر تنش",
      "verse_number": 11
    },
    {
      "id": "a57c789a-d131-4409-9905-5787310f4af5",
      "m1": "به عشق هوا بر زمین شد گوا",
      "m2": "به نزدیک خورشید فرمانروا",
      "verse_number": 12
    },
    {
      "id": "ade9ebbe-a4d5-4303-8624-ef4de5aeba0d",
      "m1": "که داند که بلبل چه گوید همی",
      "m2": "به زیر گلاندر چه موید همی",
      "verse_number": 13
    },
    {
      "id": "d2d278d9-bba1-4da5-bbf9-fa8a8ea8f85a",
      "m1": "نگه کن سحرگاه تا بشنوی",
      "m2": "ز بلبل سخن گفتنی پهلوی",
      "verse_number": 14
    },
    {
      "id": "743e6140-d6cd-43f7-85af-8828b7d4e31d",
      "m1": "همی نالد از مرگِ اسفندیار",
      "m2": "ندارد به جز ناله زو یادگار",
      "verse_number": 15
    },
    {
      "id": "088a8a1d-720b-42d8-8e7e-f5e8fe01db4b",
      "m1": "چو آوازِ رستم شبِ تیره ابر",
      "m2": "بِدَرَّد دل و گوشِ غرانهژبر",
      "verse_number": 16
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 16
  },
  "order_in_parent": 1
}