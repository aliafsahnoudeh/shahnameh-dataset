{
  "id": "773c6d4f-f705-47c4-8889-823f5a7eaa8a",
  "title": "بخش ۱۵",
  "verses": [
    {
      "id": "3eefa830-e1bf-4f0b-a729-a093a21b1300",
      "m1": "چنین گفت با رستم اسفندیار",
      "m2": "که ای نیک دل مهتر نامدار",
      "verse_number": 1
    },
    {
      "id": "c6784b54-2c9a-406f-b9d8-2f39c8bdc533",
      "m1": "من ایدون شنیدستم از بخردان",
      "m2": "بزرگان و بیداردل موبدان",
      "verse_number": 2
    },
    {
      "id": "b2a37f6a-712e-4634-8680-1a347962aaf4",
      "m1": "ازان برگذشته نیاکان تو",
      "m2": "سرافراز و دیندار و پاکان تو",
      "verse_number": 3
    },
    {
      "id": "f84fd8a9-1d65-4843-a43e-2799e468e888",
      "m1": "که دستان بدگوهر دیوزاد",
      "m2": "به گیتی فزونی ندارد نژاد",
      "verse_number": 4
    },
    {
      "id": "68ff37ae-45b9-48c4-b1d8-f52c4fb60c13",
      "m1": "فراوان ز سامش نهان داشتند",
      "m2": "همی رستخیز جهان داشتند",
      "verse_number": 5
    },
    {
      "id": "1a1e8f6b-d9d8-426c-8c36-198e34d11b1d",
      "m1": "تنش تیره بد موی و رویش سپید",
      "m2": "چو دیدش دل سام شد ناامید",
      "verse_number": 6
    },
    {
      "id": "74ecd7f5-413d-49b5-97b8-2903503a50f0",
      "m1": "بفرمود تا پیش دریا برند",
      "m2": "مگر مرغ و ماهی ورا بشکرند",
      "verse_number": 7
    },
    {
      "id": "15d7f065-430b-468b-9da1-915345a52b72",
      "m1": "بیامد بگسترد سیمرغ پر",
      "m2": "ندید اندرو هیچ آیین و فر",
      "verse_number": 8
    },
    {
      "id": "6d947b8e-863a-49a2-9f50-bc39aac9893e",
      "m1": "ببردش به جایی که بودش کنام",
      "m2": "ز دستان مر او را خورش بود کام",
      "verse_number": 9
    },
    {
      "id": "daa2e62e-f557-465e-9455-b83963a535a4",
      "m1": "اگر چند سیمرغ ناهار بود",
      "m2": "تن زال پیش اندرش خوار بود",
      "verse_number": 10
    },
    {
      "id": "62e93246-5721-415d-a5ba-ca34231d3286",
      "m1": "بینداختش پس به پیش کنام",
      "m2": "به دیدار او کس نبد شادکام",
      "verse_number": 11
    },
    {
      "id": "22dc989f-2587-48b8-9cbc-dfaf054dcbf7",
      "m1": "همی خورد افگنده مردار اوی",
      "m2": "ز جامه برهنه تن خوار اوی",
      "verse_number": 12
    },
    {
      "id": "47e0631f-a9b1-4ade-a710-2a8f33e2b8f4",
      "m1": "چو افگند سیمرغ بر زال مهر",
      "m2": "برو گشت زین گونه چندی سپهر",
      "verse_number": 13
    },
    {
      "id": "99347706-ac93-4cf7-9afa-c584a970392f",
      "m1": "ازان پس که مردار چندی چشید",
      "m2": "برهنه سوی سیستانش کشید",
      "verse_number": 14
    },
    {
      "id": "bd815ce5-6177-4798-9e6b-3d74fbfb9ec7",
      "m1": "پذیرفت سامش ز بیبچگی",
      "m2": "ز نادانی و دیوی و غرچگی",
      "verse_number": 15
    },
    {
      "id": "a9aef119-bfa8-4102-b853-e696aef399e7",
      "m1": "خجسته بزرگان و شاهان من",
      "m2": "نیای من و نیکخواهان من",
      "verse_number": 16
    },
    {
      "id": "af10d23c-7bc0-441d-a6a7-c9d8b020d3ad",
      "m1": "ورا برکشیدند و دادند چیز",
      "m2": "فراوان برین سال بگذشت نیز",
      "verse_number": 17
    },
    {
      "id": "bf5cdbb2-8aee-4e1a-8ad8-ab74367dd511",
      "m1": "یکی سرو بد نابسوده سرش",
      "m2": "چو با شاخ شد رستم آمد برش",
      "verse_number": 18
    },
    {
      "id": "4ce951d9-b727-4045-bfb6-2c6a6799bff5",
      "m1": "ز مردی و بالا و دیدار اوی",
      "m2": "به گردون برآمد چنین کار اوی",
      "verse_number": 19
    },
    {
      "id": "438c4ece-bd5d-4307-af13-f77187ee4cb7",
      "m1": "برین گونه پادشاهی گرفت",
      "m2": "ببالید و ناپارسایی گرفت",
      "verse_number": 20
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 20
  },
  "order_in_parent": 15
}