{
  "id": "5c0b8e0d-d4be-46f8-b8cf-979e61fd241d",
  "title": "بخش ۲۳",
  "verses": [
    {
      "id": "5fb6839d-339a-45e9-b9bb-b279518e0eb6",
      "m1": "بدانگه که رزم یلان شد دراز",
      "m2": "همی دیر شد رستم سرفراز",
      "verse_number": 1
    },
    {
      "id": "e8551594-bcfc-4a2f-87b1-8a71d9b61d9c",
      "m1": "زواره بیاورد زان سو سپاه",
      "m2": "یکی لشکری داغدل کینهخواه",
      "verse_number": 2
    },
    {
      "id": "d84e1144-5cbc-4497-93bc-b1570fccd911",
      "m1": "به ایرانیان گفت رستم کجاست",
      "m2": "برین روز بیهوده خامش چراست",
      "verse_number": 3
    },
    {
      "id": "dd683bf2-8b56-4dfb-b4a1-d26915b4e81b",
      "m1": "شما سوی رستم به جنگ آمدید",
      "m2": "خرامان به چنگ نهنگ آمدید",
      "verse_number": 4
    },
    {
      "id": "a596353e-d450-4cae-bdeb-3728f22d6dc3",
      "m1": "همی دست رستم نخواهید بست",
      "m2": "برین رزمگه بر نشاید نشست",
      "verse_number": 5
    },
    {
      "id": "a343b965-46b5-4114-8445-8e3531e314e8",
      "m1": "زواره به دشنام لب برگشاد",
      "m2": "همی کرد گفتار ناخوب یاد",
      "verse_number": 6
    },
    {
      "id": "4bc0168b-52bc-49c9-8ba9-34bed848b074",
      "m1": "برآشفت ازان پور اسفندیار",
      "m2": "سواری بد اسپافگن و نامدار",
      "verse_number": 7
    },
    {
      "id": "28055e35-fcf3-4a38-bf9e-e97043a44f8c",
      "m1": "جوانی که نوش آذرش بود نام",
      "m2": "سرافراز و جنگاور و شادکام",
      "verse_number": 8
    },
    {
      "id": "2f33c1c1-9eeb-4b9c-87df-87eb71faab07",
      "m1": "برآشفت با سگزی آن نامدار",
      "m2": "زبان را به دشنام بگشاد خوار",
      "verse_number": 9
    },
    {
      "id": "359560e0-c41b-4799-895a-4d2ee5a4ec3f",
      "m1": "چنین گفت کآری گو برمنش",
      "m2": "به فرمان شاهان کند بدکنش",
      "verse_number": 10
    },
    {
      "id": "ec9a5595-3dca-4a44-821c-0851a8e6b27f",
      "m1": "نفرمود ما را یل اسفندیار",
      "m2": "چنین با سگان ساختن کارزار",
      "verse_number": 11
    },
    {
      "id": "0058eb11-8d76-499c-88f6-a929811a3f6b",
      "m1": "که پیچد سر از رای و فرمان او",
      "m2": "که یارد گذشتن ز پیمان او",
      "verse_number": 12
    },
    {
      "id": "81ae3c16-2601-4275-92a7-7678e2159fba",
      "m1": "اگر جنگ بر نادرستی کنید",
      "m2": "به کار اندرون پیش دستی کنید",
      "verse_number": 13
    },
    {
      "id": "b1780dbb-03c3-4804-8a7c-7b29c67f7535",
      "m1": "ببینید پیکار جنگاوران",
      "m2": "به تیغ و سنان و به گرز گران",
      "verse_number": 14
    },
    {
      "id": "14edffe4-baaf-41c6-84b9-89ac701377bb",
      "m1": "زواره بفرمود کاندر نهید",
      "m2": "سران را ز خون بر سر افسر نهید",
      "verse_number": 15
    },
    {
      "id": "53c154bb-1008-4c71-bb42-d033a7dcbcfb",
      "m1": "زواره بیامد به پیش سپاه",
      "m2": "دهاده برآمد ز آوردگاه",
      "verse_number": 16
    },
    {
      "id": "3e2509ef-7980-4911-a0a6-4275d543d4a3",
      "m1": "بکشتند ز ایرانیان بیشمار",
      "m2": "چو نوشآذر آن دید بر ساخت کار",
      "verse_number": 17
    },
    {
      "id": "b9774bd6-b370-43ad-a220-9530aa18cd9f",
      "m1": "سمند سرافراز را بر نشست",
      "m2": "بیامد یکی تیغ هندی به دست",
      "verse_number": 18
    },
    {
      "id": "81cc2e8b-78db-4519-9cfb-a397f04e2b27",
      "m1": "یکی نامور بود الوای نام",
      "m2": "سرافراز و اسپافگن و شادکام",
      "verse_number": 19
    },
    {
      "id": "60d3134f-ade9-4726-b3d0-1986cbe05f56",
      "m1": "کجا نیزهٔ رستم او داشتی",
      "m2": "پس پشت او هیچ نگذاشتی",
      "verse_number": 20
    },
    {
      "id": "f7e9b327-e931-44ee-96ff-a6657aa3aebb",
      "m1": "چو از دور نوشآذر او را بدید",
      "m2": "بزد دست و تیغ از میان برکشید",
      "verse_number": 21
    },
    {
      "id": "5952d59b-6c16-4c93-adbb-bb1c65215ba8",
      "m1": "یکی تیغ زد بر سر و گردنش",
      "m2": "بدو نیمه شد پیلپیکر تنش",
      "verse_number": 22
    },
    {
      "id": "075d7f94-7ad3-40cd-88d8-78389237989f",
      "m1": "زواره برانگیخت اسپ نبرد",
      "m2": "به تندی به نوشآذر آواز کرد",
      "verse_number": 23
    },
    {
      "id": "9b479e85-309d-4c76-a518-e2843538e4ab",
      "m1": "که او را فگندی کنون پای دار",
      "m2": "چو الوای را من نخوانم سوار",
      "verse_number": 24
    },
    {
      "id": "5068f66d-bb63-4ece-905f-0a873eab2dfa",
      "m1": "زواره یکی نیزه زد بر برش",
      "m2": "به خاک اندر آمد همانگه سرش",
      "verse_number": 25
    },
    {
      "id": "f157c79b-7c91-4f82-9bdc-1daf795d9f5d",
      "m1": "چو نوشآذر نامور کشته شد",
      "m2": "سپه را همه روز برگشته شد",
      "verse_number": 26
    },
    {
      "id": "3e0fea49-cb13-4dda-8aba-7ef40bf372db",
      "m1": "برادرش گریان و دل پر ز جوش",
      "m2": "جوانی که بد نام او مهرنوش",
      "verse_number": 27
    },
    {
      "id": "0509d032-af37-4327-b656-1efcf387a0ad",
      "m1": "غمی شد دل مرد شمشیرزن",
      "m2": "برانگیخت آن بارهٔ پیلتن",
      "verse_number": 28
    },
    {
      "id": "ff616ae2-0713-4756-890e-23821d37c06f",
      "m1": "برفت از میان سپه پیش صف",
      "m2": "ز درد جگر بر لب آورده کف",
      "verse_number": 29
    },
    {
      "id": "a0932b17-dcd4-4801-b0ac-0407582b50e4",
      "m1": "وزان سو فرامرز چون پیل مست",
      "m2": "بیامد یکی تیغ هندی به دست",
      "verse_number": 30
    },
    {
      "id": "fa2d7dd5-af23-4a92-9d85-251d11aed4cb",
      "m1": "برآویخت با او همی مهرنوش",
      "m2": "دو رویه ز لشکر برآمد خروش",
      "verse_number": 31
    },
    {
      "id": "731637e5-0ffd-41c2-ab94-19066e4672ca",
      "m1": "گرامی دو پرخاشجوی جوان",
      "m2": "یکی شاهزاده دگر پهلوان",
      "verse_number": 32
    },
    {
      "id": "0f2bac88-3734-4713-b39c-dd08343683cc",
      "m1": "چو شیران جنگی برآشوفتند",
      "m2": "همی بر سر یکدگر کوفتند",
      "verse_number": 33
    },
    {
      "id": "5030d931-79aa-41a5-850c-b62050d7987d",
      "m1": "در آوردگه تیز شد مهرنوش",
      "m2": "نبودش همی با فرامرز توش",
      "verse_number": 34
    },
    {
      "id": "339a9614-1e2e-42c1-8d87-b33364bcbae5",
      "m1": "بزد تیغ بر گردن اسپ خویش",
      "m2": "سر بادپای اندرافگند پیش",
      "verse_number": 35
    },
    {
      "id": "38da6f61-465f-47f5-8877-c637705d4065",
      "m1": "فرامرز کردش پیاده تباه",
      "m2": "ز خون لعل شد خاک آوردگاه",
      "verse_number": 36
    },
    {
      "id": "fc3bf6b2-0631-467f-98cb-c3fea771609a",
      "m1": "چو بهمن برادرش را کشته دید",
      "m2": "زمین زیر او چون گل آغشته دید",
      "verse_number": 37
    },
    {
      "id": "97a5699d-8c79-44af-b5b9-bbc42dc369ad",
      "m1": "بیامد دوان نزد اسفندیار",
      "m2": "به جایی که بود آتش کارزار",
      "verse_number": 38
    },
    {
      "id": "71922a98-b5a5-4730-89d3-9d5ff2b5b9bd",
      "m1": "بدو گفت کای نره شیر ژیان",
      "m2": "سپاهی به جنگ آمد از سگزیان",
      "verse_number": 39
    },
    {
      "id": "d28e23ba-a4f1-45bb-bc1d-8c99600883c3",
      "m1": "دو پور تو نوشآذر و مهرنوش",
      "m2": "به خواری به سگزی سپردند هوش",
      "verse_number": 40
    },
    {
      "id": "5b312a10-2d7d-4825-bdbf-afd8dff68456",
      "m1": "تو اندر نبردی و ما پر ز درد",
      "m2": "جوانان و کیزادگان زیر گرد",
      "verse_number": 41
    },
    {
      "id": "f49217fc-c0f5-44b2-afc6-c923df1b8e3f",
      "m1": "برین تخمه این ننگ تا جاودان",
      "m2": "بماند ز کردار نابخردان",
      "verse_number": 42
    },
    {
      "id": "e706b0c8-86de-43f1-871f-9b678b54f948",
      "m1": "دل مرد بیدارتر شد ز خشم",
      "m2": "پر از تاب مغز و پر از آب چشم",
      "verse_number": 43
    },
    {
      "id": "77c9213b-54eb-4300-a2cd-335ce10abba7",
      "m1": "به رستم چنین گفت کای بدنشان",
      "m2": "چنین بود پیمان گردنکشان",
      "verse_number": 44
    },
    {
      "id": "7e87b89f-8bdc-4edc-9118-e6dcddd148d7",
      "m1": "تو گفتی که لشکر نیارم به جنگ",
      "m2": "ترا نیست آرایش نام و ننگ",
      "verse_number": 45
    },
    {
      "id": "60fad736-74b3-44f9-b125-6439f51ae611",
      "m1": "نداری ز من شرم وز کردگار",
      "m2": "نترسی که پرسند روز شمار",
      "verse_number": 46
    },
    {
      "id": "c61e2779-0efb-4374-9c40-7c2ff04dbe1e",
      "m1": "ندانی که مردان پیمانشکن",
      "m2": "ستوده نباشد بر انجمن",
      "verse_number": 47
    },
    {
      "id": "4bc21372-879d-4f08-96c1-6423bf8cf723",
      "m1": "دو سگزی دو پور مرا کشتهاند",
      "m2": "بران خیرگی باز برگشتهاند",
      "verse_number": 48
    },
    {
      "id": "8ab610a4-d652-408e-aff9-4265bf8d12dd",
      "m1": "چو بشنید رستم غمی گشت سخت",
      "m2": "بلرزید برسان شاخ درخت",
      "verse_number": 49
    },
    {
      "id": "a7e14321-5541-4251-ad96-59569a5a21b0",
      "m1": "به جان و سر شاه سوگند خورد",
      "m2": "به خورشید و شمشیر و دشت نبرد",
      "verse_number": 50
    },
    {
      "id": "fbe50b4d-fa4c-4fc9-92b7-91a5f0db58f4",
      "m1": "که من جنگ هرگز نفرمودهام",
      "m2": "کسی کین چنین کرد نستودهام",
      "verse_number": 51
    },
    {
      "id": "c344709c-bb2b-405f-918d-4df6483b21d1",
      "m1": "ببندم دو دست برادر کنون",
      "m2": "گر او بود اندر بدی رهنمون",
      "verse_number": 52
    },
    {
      "id": "ed4bb869-4c79-4e29-aeef-378ab9a6f20b",
      "m1": "فرامرز را نیز بسته دو دست",
      "m2": "بیارم بر شاه یزدانپرست",
      "verse_number": 53
    },
    {
      "id": "3868a292-b649-48e3-9ff0-a20771628390",
      "m1": "به خون گرانمایگانشان بکش",
      "m2": "مشوران ازین رای بیهوده هش",
      "verse_number": 54
    },
    {
      "id": "5003096b-06d2-42d3-bc62-4ab9ffef3a71",
      "m1": "چنین گفت با رستم اسفندیار",
      "m2": "که بر کین طاوس نر خون مار",
      "verse_number": 55
    },
    {
      "id": "7877d525-5d95-4026-bf76-f7db2938039a",
      "m1": "بریزیم ناخوب و ناخوش بود",
      "m2": "نه آیین شاهان سرکش بود",
      "verse_number": 56
    },
    {
      "id": "27843b54-0090-401a-aaa9-b1544a2c0448",
      "m1": "تو ای بدنشان چارهٔ خویش ساز",
      "m2": "که آمد زمانت به تنگی فراز",
      "verse_number": 57
    },
    {
      "id": "a68512d1-a953-4663-a776-37f02e78a962",
      "m1": "بر رخش با هردو رانت به تیر",
      "m2": "برآمیزم اکنون چو با آب شیر",
      "verse_number": 58
    },
    {
      "id": "f0b4327d-70a0-4e04-910e-2c7a0dc947d4",
      "m1": "بدان تا کس از بندگان زین سپس",
      "m2": "نجویند کین خداوند کس",
      "verse_number": 59
    },
    {
      "id": "266968a3-b535-4e42-ac52-5ca04d5ec7f3",
      "m1": "وگر زنده مانی ببندمت چنگ",
      "m2": "به نزدیک شاهت برم بیدرنگ",
      "verse_number": 60
    },
    {
      "id": "d9d914f5-4984-4e7b-a088-a6c0a683d909",
      "m1": "بدو گفت رستم کزین گفت و گوی",
      "m2": "چه باشد مگر کم شود آبروی",
      "verse_number": 61
    },
    {
      "id": "2a16e02e-d555-462b-8242-18f05e87c084",
      "m1": "به یزدان پناه و به یزدان گرای",
      "m2": "که اویست بر نیک و بد رهنمای",
      "verse_number": 62
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 62
  },
  "order_in_parent": 23
}