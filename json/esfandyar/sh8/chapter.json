{
  "id": "25940c78-2307-43ea-a58e-f82e6fefad78",
  "title": "بخش ۸",
  "verses": [
    {
      "id": "00420a03-ed57-4ebc-be91-eaee910d77e5",
      "m1": "سخنهای آن نامور پیشگاه",
      "m2": "چو بشنید بهمن بیامد به راه",
      "verse_number": 1
    },
    {
      "id": "a7104687-4916-4d98-bcd7-188ea11be8bf",
      "m1": "بپوشید زربفت شاهنشهی",
      "m2": "بسر بر نهاد آن کلاه مهی",
      "verse_number": 2
    },
    {
      "id": "fc7d4e57-322e-4f87-b39c-d543edf89ab4",
      "m1": "خرامان بیامد ز پردهسرای",
      "m2": "درفشی درفشان پس او به پای",
      "verse_number": 3
    },
    {
      "id": "04200091-ff55-4e66-8403-22031811f7f7",
      "m1": "جهانجوی بگذشت بر هیرمند",
      "m2": "جوانی سرافراز و اسپی بلند",
      "verse_number": 4
    },
    {
      "id": "69564794-4843-4542-9496-593faac1a95d",
      "m1": "هماندر زمان دیدهبانش بدید",
      "m2": "سوی زاولستان فغان برکشید",
      "verse_number": 5
    },
    {
      "id": "1391c580-7dd2-4155-aaec-825d4114612e",
      "m1": "که آمد نبرده سواری دلیر",
      "m2": "به هرّای زرین سیاهی به زیر",
      "verse_number": 6
    },
    {
      "id": "70af766e-2d1d-4437-b8e1-a4bd873065ad",
      "m1": "پس پشت او خوار مایه سوار",
      "m2": "تنآسان گذشت از لب جویبار",
      "verse_number": 7
    },
    {
      "id": "64ee4b83-db58-4f8e-9e88-9a5e73da2b40",
      "m1": "هماندر زمان زال زر برنشست",
      "m2": "کمندی به فتراک و گرزی به دست",
      "verse_number": 8
    },
    {
      "id": "a5dc738d-376e-4f4d-bf88-2f9a8b374933",
      "m1": "بیامد ز دیده مر او را بدید",
      "m2": "یکی باد سرد از جگر برکشید",
      "verse_number": 9
    },
    {
      "id": "bd90b024-5a28-45ac-9df8-b985f23de244",
      "m1": "چنین گفت کین نامور پهلوست",
      "m2": "سرافراز با جامهٔ خسروست",
      "verse_number": 10
    },
    {
      "id": "ae03626d-c628-4766-ab4d-b9f3871a6492",
      "m1": "ز لهراسپ دارد همانا نژاد",
      "m2": "پی او برین بوم فرخنده باد",
      "verse_number": 11
    },
    {
      "id": "1ea1091d-8458-4d5a-a916-5fb592328ae1",
      "m1": "ز دیده بیامد به درگاه رفت",
      "m2": "زمانی به اندیشه بر زین بخفت",
      "verse_number": 12
    },
    {
      "id": "2570d66a-9633-48fe-a33f-18918f905ee6",
      "m1": "هماندر زمان بهمن آمد پدید",
      "m2": "ازو رایت خسروی گسترید",
      "verse_number": 13
    },
    {
      "id": "281ee082-a8d8-4901-ab20-279eb826a6d0",
      "m1": "ندانست مرد جوان زال را",
      "m2": "بیفراخت آن خسروی یال را",
      "verse_number": 14
    },
    {
      "id": "f90ccd23-a25d-43b2-9be2-6a21f0c5a46a",
      "m1": "چو نزدیکتر گشت آواز داد",
      "m2": "بدو گفت کای مرد دهقاننژاد",
      "verse_number": 15
    },
    {
      "id": "7e2f4bef-3362-413f-88cb-0077e27d3e4f",
      "m1": "سرانجمن پور دستان کجاست",
      "m2": "که دارد زمانه بدو پشت راست",
      "verse_number": 16
    },
    {
      "id": "5e2bb1ff-0a21-48fd-8a1b-e50a358a9e37",
      "m1": "که آمد به زاول گو اسفندیار",
      "m2": "سراپرده زد بر لب رودبار",
      "verse_number": 17
    },
    {
      "id": "560d1b4b-8375-4009-8423-f3e1734ef827",
      "m1": "بدو گفت زال ای پسر کام جوی",
      "m2": "فرود آی و می خواه و آرام جوی",
      "verse_number": 18
    },
    {
      "id": "557b5a91-9bb2-4b5f-a6d8-852868da391e",
      "m1": "کنون رستم آید ز نخچیرگاه",
      "m2": "زواره فرامرز و چندی سپاه",
      "verse_number": 19
    },
    {
      "id": "f0f4d902-8942-4118-9263-24f438c9d9c4",
      "m1": "تو با این سواران بباش ارجمند",
      "m2": "بیارای دل را به بگماز چند",
      "verse_number": 20
    },
    {
      "id": "f10f2af5-61a3-4548-9ed3-fe39e53034c0",
      "m1": "چنین داد پاسخ که اسفندیار",
      "m2": "نفرمودمان رامش و میگسار",
      "verse_number": 21
    },
    {
      "id": "025e5360-4a63-4f62-b2bf-ac7ec36dffd7",
      "m1": "گزین کن یکی مرد جوینده راه",
      "m2": "که با من بیاید به نخچیرگاه",
      "verse_number": 22
    },
    {
      "id": "7c0a2ebb-28ab-4867-88a4-da84a974adce",
      "m1": "بدو گفت دستان که نام تو چیست",
      "m2": "همی بگذری تیز کام تو چیست",
      "verse_number": 23
    },
    {
      "id": "a04c6e13-5652-4238-9db1-f9afd1855d51",
      "m1": "برآنم که تو خویش لهراسپی",
      "m2": "گر از تخمهٔ شاه گشتاسپی",
      "verse_number": 24
    },
    {
      "id": "326bcf0f-97d0-44cd-bc2b-e30c9657f3a0",
      "m1": "چنین داد پاسخ که من بهمنم",
      "m2": "نبیرهٔ جهاندار رویین تنم",
      "verse_number": 25
    },
    {
      "id": "56a9a8e5-4bf8-4807-a6ac-4bc3cd7b7b72",
      "m1": "چو بشنید گفتار آن سرفراز",
      "m2": "فرود آمد از باره بردش نماز",
      "verse_number": 26
    },
    {
      "id": "b782f86d-9085-4da2-afbe-622d443e27ea",
      "m1": "بخندید بهمن پیاده ببود",
      "m2": "بپرسیدش و گفت بهمن شنود",
      "verse_number": 27
    },
    {
      "id": "405d4270-6413-46b7-bf56-85e516b30127",
      "m1": "بسی خواهشش کرد کایدر بایست",
      "m2": "چنین تیز رفتن ترا روی نیست",
      "verse_number": 28
    },
    {
      "id": "e71bb2b3-cbbf-43cd-9106-c627677b3c38",
      "m1": "بدو گفت فرمان اسفندیار",
      "m2": "نشاید گرفتن چنین سست و خوار",
      "verse_number": 29
    },
    {
      "id": "5e10a518-4973-4248-ad78-deb31880b93d",
      "m1": "گزین کرد مردی که دانست راه",
      "m2": "فرستاده با او به نخچیرگاه",
      "verse_number": 30
    },
    {
      "id": "53c6a3e3-051a-41cd-9fda-6336087fd9c5",
      "m1": "همی رفت پیش اندرون رهنمون",
      "m2": "جهاندیدهای نام او شیرخون",
      "verse_number": 31
    },
    {
      "id": "8796d71c-8dec-4795-a8d3-daa2507233e4",
      "m1": "به انگشت بنمود نخچیرگاه",
      "m2": "هماندر زمان بازگشت او ز راه",
      "verse_number": 32
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 32
  }
}