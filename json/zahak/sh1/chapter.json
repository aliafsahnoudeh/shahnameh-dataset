{
  "id": "cf96d371-9666-476c-a064-8209a72b1ba9",
  "title": "بخش ۱ - پادشاهی ضحاک تازی هزار سال بود",
  "verses": [
    {
      "id": "004ee354-afdb-41c7-9018-2bd60d7dd278",
      "m1": "چو ضحاک شد بر جهان شهریار",
      "m2": "بر او سالیان انجمن شد هزار",
      "verse_number": 1
    },
    {
      "id": "f2fccd30-71d0-4c06-9cc0-1d0fbfa7fc5e",
      "m1": "سراسر زمانه بدو گشت باز",
      "m2": "برآمد بر این روزگار دراز",
      "verse_number": 2
    },
    {
      "id": "4fb02842-574e-469d-aa69-2e7edd7775c8",
      "m1": "نهان گشت کردار فرزانگان",
      "m2": "پراگنده شد کام دیوانگان",
      "verse_number": 3
    },
    {
      "id": "86d27a67-70a4-48aa-95db-adba0abced9b",
      "m1": "هنر خوار شد جادویی ارجمند",
      "m2": "نهان راستی آشکارا گزند",
      "verse_number": 4
    },
    {
      "id": "281b96db-dc98-4f46-af05-dc4c344265e3",
      "m1": "شده بر بدی دست دیوان دراز",
      "m2": "به نیکی نرفتی سخن جز به راز",
      "verse_number": 5
    },
    {
      "id": "22f3e065-5a02-4313-9712-e2eb5f74dcb6",
      "m1": "دو پاکیزه از خانهٔ جمّشید",
      "m2": "برون آوریدند لرزان چو بید",
      "verse_number": 6
    },
    {
      "id": "3a26e294-2a4a-488b-ab15-657c0d78758f",
      "m1": "که جمشید را هر دو دختر بدند",
      "m2": "سر بانوان را چو افسر بدند",
      "verse_number": 7
    },
    {
      "id": "8097a577-2b6e-4bf0-8d3e-67a9c24bc7bd",
      "m1": "ز پوشیدهرویان یکی شهرناز",
      "m2": "دگر پاکدامن به نام ارنواز",
      "verse_number": 8
    },
    {
      "id": "111964f6-0c21-4eaa-b896-8e0e88844d3a",
      "m1": "به ایوان ضحاک بردندشان",
      "m2": "بر آن اژدهافش سپردندشان",
      "verse_number": 9
    },
    {
      "id": "e794258c-4b48-4696-9fbf-4865081b24ea",
      "m1": "بپروردشان از ره جادویی",
      "m2": "بیاموختشان کژی و بدخویی",
      "verse_number": 10
    },
    {
      "id": "c93a3a39-9870-4722-acd5-5eb53cbd8742",
      "m1": "ندانست جز کژی آموختن",
      "m2": "جز از کشتن و غارت و سوختن",
      "verse_number": 11
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 11
  },
  "order_in_parent": 1
}