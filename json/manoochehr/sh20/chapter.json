{
  "id": "5442ad41-094a-4323-9fa0-592cee5dd39c",
  "title": "بخش ۲۰",
  "verses": [
    {
      "id": "87036d2c-c64d-4684-a963-f77ab0c1b5b0",
      "m1": "چنین گفت پس شاه گردن فراز",
      "m2": "کزین هرچه گفتید دارید راز",
      "verse_number": 1
    },
    {
      "id": "f1f9bb99-837a-4804-820f-77a50b15756c",
      "m1": "بخواند آن زمان زال را شهریار",
      "m2": "کزو خواست کردن سخن خواستار",
      "verse_number": 2
    },
    {
      "id": "491faa89-39a1-4541-9cc3-9b104b5fbe52",
      "m1": "بدان تا بپرسند ازو چند چیز",
      "m2": "نهفته سخنهای دیرینه نیز",
      "verse_number": 3
    },
    {
      "id": "c3a66403-cd05-4a2f-aae1-f4522573ca97",
      "m1": "نشستند بیدار دل بخردان",
      "m2": "همان زال با نامور موبدان",
      "verse_number": 4
    },
    {
      "id": "5a9da324-b982-4393-a4cf-fd03608760b2",
      "m1": "بپرسید مر زال را موبدی",
      "m2": "ازین تیزهش راهبین بخردی",
      "verse_number": 5
    },
    {
      "id": "6eb7d8ac-514e-4bb6-80af-badd47d807e1",
      "m1": "که از ده و دو تای سرو سهی",
      "m2": "که رُستهست شاداب با فرهی",
      "verse_number": 6
    },
    {
      "id": "786ced19-ed47-4c6b-a6ab-a3a72e0d64b6",
      "m1": "ازآن برزده هر یکی شاخ سی",
      "m2": "نگردد کم و بیش در پارسی",
      "verse_number": 7
    },
    {
      "id": "932db118-8b89-402f-a1d1-9c51b2a4d5dc",
      "m1": "دگر موبدی گفت کای سرفراز",
      "m2": "دو اسپ گرانمایه و تیزتاز",
      "verse_number": 8
    },
    {
      "id": "ea522fb7-ff22-40df-a775-22e867072662",
      "m1": "یکی زان به کردار دریای قار",
      "m2": "یکی چون بلور سپید آبدار",
      "verse_number": 9
    },
    {
      "id": "9decd4bc-633b-45ff-b7d9-222fb83424ab",
      "m1": "بجنبید و هر دو شتابندهاند",
      "m2": "همان یکدیگر را نیابندهاند",
      "verse_number": 10
    },
    {
      "id": "4edbff14-9c8d-473d-ae27-3675e5a6ee7a",
      "m1": "سهدیگر چنین گفت کان سی سوار",
      "m2": "کجا بگذرانند بر شهریار",
      "verse_number": 11
    },
    {
      "id": "e49d226c-5150-4090-9d14-49652e1252ab",
      "m1": "یکی کم شود باز چون بشمری",
      "m2": "همان سی بود باز چون بنگری",
      "verse_number": 12
    },
    {
      "id": "47c77b6c-fbcf-43ad-9f75-e1d222d80881",
      "m1": "چهارم چنین گفت کان مرغزار",
      "m2": "که بینی پر از سبزه و جویبار",
      "verse_number": 13
    },
    {
      "id": "1e5311f5-31a4-4d8b-aa01-a22196fbe916",
      "m1": "یکی مرد با تیز داسی بزرگ",
      "m2": "سوی مرغزار اندر آید سترگ",
      "verse_number": 14
    },
    {
      "id": "009e4bb9-2a71-49d2-86c6-61a9a6e07573",
      "m1": "همی بدرود آن گیا خشک و تر",
      "m2": "نه بردارد او هیچ ازآن کار سر",
      "verse_number": 15
    },
    {
      "id": "fd830000-28f8-4de7-be9d-406b48862da7",
      "m1": "دگر گفت کان برکشیده دو سرو",
      "m2": "ز دریای با موج برسان غرو",
      "verse_number": 16
    },
    {
      "id": "23814bac-1030-4b03-a4d8-4ca5defa54c3",
      "m1": "یکی مرغ دارد بریشان کنام",
      "m2": "نشیمش به شام آن بود این به بام",
      "verse_number": 17
    },
    {
      "id": "9cdf0b0f-d8ab-42c6-a5d5-0e905dbc1be1",
      "m1": "ازین چون بپرد شود برگ خشک",
      "m2": "برآن بر نشیند دهد بوی مشک",
      "verse_number": 18
    },
    {
      "id": "affee344-d470-4eb8-aa70-390dc97a781e",
      "m1": "ازآن دو همیشه یکی آبدار",
      "m2": "یکی پژمریده شده سوگوار",
      "verse_number": 19
    },
    {
      "id": "fc3e0e05-fed8-4a3d-8165-2a5f837a5586",
      "m1": "بپرسید دیگر که بر کوهسار",
      "m2": "یکی شارستان یافتم استوار",
      "verse_number": 20
    },
    {
      "id": "540e73e3-3648-41b1-9726-18c9596aa568",
      "m1": "خرامند مردم ازان شارستان",
      "m2": "گرفته به هامون یکی خارستان",
      "verse_number": 21
    },
    {
      "id": "998e4b3a-37dc-44e1-bfc3-52a953dc922c",
      "m1": "بناها کشیدند سر تا به ماه",
      "m2": "پرستنده گشتند و هم پیشگاه",
      "verse_number": 22
    },
    {
      "id": "782bb0d1-d161-481b-b25f-9bbe9ef85035",
      "m1": "وزآن شارستانشان به دل نگذرد",
      "m2": "کس از یاد کردن سخن نشمرد",
      "verse_number": 23
    },
    {
      "id": "57d3a96c-03fa-4505-a6a7-c2d3c6afe5d4",
      "m1": "یکی بومهین خیزد از ناگهان",
      "m2": "بر و بومشان پاک گردد نهان",
      "verse_number": 24
    },
    {
      "id": "db52a56b-6d1e-449a-81e3-d54ae63387c2",
      "m1": "بدان شارستانشان نیاز آورد",
      "m2": "هم اندیشگان دراز آورد",
      "verse_number": 25
    },
    {
      "id": "74f24d31-7f33-4abb-8af1-aac89a80acd7",
      "m1": "به پرده درست این سخنها بجوی",
      "m2": "به پیش ردان آشکارا بگوی",
      "verse_number": 26
    },
    {
      "id": "934359a4-b6f8-482c-89c8-4d0c23bde140",
      "m1": "گر این رازها آشکارا کنی",
      "m2": "ز خاک سیه مشک سارا کنی",
      "verse_number": 27
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 27
  },
  "order_in_parent": 20
}