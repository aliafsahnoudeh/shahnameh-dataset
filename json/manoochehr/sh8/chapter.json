{
  "id": "ba1987ba-b853-4e41-9558-ff27e6c0a9ce",
  "title": "بخش ۸",
  "verses": [
    {
      "id": "f2cf600f-48aa-43a2-87a5-ec2d355211d6",
      "m1": "رسیدند خوبان به درگاه کاخ",
      "m2": "به دست اندرون هر یک از گل دو شاخ",
      "verse_number": 1
    },
    {
      "id": "fc0cdc81-623f-44eb-b2fe-8979e6cb0473",
      "m1": "نگه کرد دربان برآراست جنگ",
      "m2": "زبان کرد گستاخ و دل کرد تنگ",
      "verse_number": 2
    },
    {
      "id": "94730df6-e60c-4c6c-9de7-a59e91155818",
      "m1": "که بیگه ز درگاه بیرون شوید",
      "m2": "شگفت آیدم تا شما چون شوید",
      "verse_number": 3
    },
    {
      "id": "5fca2d87-f930-41a6-95c1-3e9e21779dba",
      "m1": "بتان پاسخش را بیاراستند",
      "m2": "به تنگی دل از جای برخاستند",
      "verse_number": 4
    },
    {
      "id": "bf4be34a-be73-43fa-8173-af856f11548a",
      "m1": "که امروز روزی دگر گونه نیست",
      "m2": "به راه گلان دیو واژونه نیست",
      "verse_number": 5
    },
    {
      "id": "3a313a3d-0cae-4c96-8aec-f95f68c46672",
      "m1": "بهار آمد ازگلستان گل چنیم",
      "m2": "ز روی زمین شاخ سنبل چنیم",
      "verse_number": 6
    },
    {
      "id": "e543ebe4-e018-49b8-a7f0-245323dadf69",
      "m1": "نگهبان در گفت کامروز کار",
      "m2": "نباید گرفتن بدان هم شمار",
      "verse_number": 7
    },
    {
      "id": "1173d06f-27df-40c3-846f-52bd4a4566af",
      "m1": "که زال سپهبد بکابل نبود",
      "m2": "سراپردهٔ شاه زابل نبود",
      "verse_number": 8
    },
    {
      "id": "59f27e5a-c0a2-409d-8b46-170bd7e57876",
      "m1": "نبینید کز کاخ کابل خدای",
      "m2": "به زین اندر آرد بشبگیر پای",
      "verse_number": 9
    },
    {
      "id": "72ec1fb8-7eaa-4637-9024-e5e775ed0444",
      "m1": "اگرتان ببیند چنین گل بدست",
      "m2": "کند بر زمینتان هم آنگاه پست",
      "verse_number": 10
    },
    {
      "id": "eee37948-c983-4338-aaad-ac31b6838b62",
      "m1": "شدند اندر ایوان بتان طراز",
      "m2": "نشستند و با ماه گفتند راز",
      "verse_number": 11
    },
    {
      "id": "b7bd221b-7567-43ee-adf3-f2d3636a0dee",
      "m1": "نهادند دینار و گوهر به پیش",
      "m2": "بپرسید رودابه از کم و بیش",
      "verse_number": 12
    },
    {
      "id": "b60817e0-a14c-4e1b-aa73-e7aece6c4a37",
      "m1": "که چون بودتان کار با پور سام",
      "m2": "بدیدن بهست ار به آواز و نام",
      "verse_number": 13
    },
    {
      "id": "9939782a-266b-453e-bdb9-0a03fdfd7895",
      "m1": "پری چهره هر پنج بشتافتند",
      "m2": "چو با ماه جای سخن یافتند",
      "verse_number": 14
    },
    {
      "id": "de1e4d6a-42c7-417c-8924-5f1ab3b1a31c",
      "m1": "که مردیست برسان سرو سهی",
      "m2": "همش زیب و هم فر شاهنشهی",
      "verse_number": 15
    },
    {
      "id": "919c9b10-66e3-438e-9548-e21a3b8d2fb4",
      "m1": "همش رنگ و بوی و همش قد و شاخ",
      "m2": "سواری میان لاغر و بر فراخ",
      "verse_number": 16
    },
    {
      "id": "4c8255bb-49e7-4469-a85d-1d6c660fb233",
      "m1": "دو چشمش چو دو نرگس قیرگون",
      "m2": "لبانش چو بسد رخانش چو خون",
      "verse_number": 17
    },
    {
      "id": "272b46ce-990c-4bba-b214-ae6bd7679caf",
      "m1": "کف و ساعدش چو کف شیر نر",
      "m2": "هیون ران و موبد دل و شاه فر",
      "verse_number": 18
    },
    {
      "id": "f469d44e-f479-4a84-86fd-99c75d94da49",
      "m1": "سراسر سپیدست مویش برنگ",
      "m2": "از آهو همین است و این نیست ننگ",
      "verse_number": 19
    },
    {
      "id": "dc1c76b5-d2e5-4032-9633-162a87385ddc",
      "m1": "سر جعد آن پهلوان جهان",
      "m2": "چو سیمین زره بر گل ارغوان",
      "verse_number": 20
    },
    {
      "id": "28ee521e-d4f6-4b3d-b68a-f149c57fde5a",
      "m1": "که گویی همی خود چنان بایدی",
      "m2": "وگر نیستی مهر نفزایدی",
      "verse_number": 21
    },
    {
      "id": "d48814f5-39cd-4a79-bdd2-01665157b475",
      "m1": "به دیار تو دادهایمش نوید",
      "m2": "ز ما بازگشتست دل پرامید",
      "verse_number": 22
    },
    {
      "id": "72ea9aab-b6a1-44e7-be3c-9aac7afb7389",
      "m1": "کنون چارهٔ کار مهمان بساز",
      "m2": "بفرمای تا بر چه گردیم باز",
      "verse_number": 23
    },
    {
      "id": "eb586b24-7f5f-47f2-806a-ce03a67599d4",
      "m1": "چنین گفت با بندگان سرو بن",
      "m2": "که دیگر شدستی به رای و سخن",
      "verse_number": 24
    },
    {
      "id": "0696f59f-a4a8-4e74-870c-8feb7e0252dc",
      "m1": "همان زال کو مرغ پرورده بود",
      "m2": "چنان پیر سر بود و پژمرده بود",
      "verse_number": 25
    },
    {
      "id": "92a759d7-2d97-490b-b00e-4a273f8087d0",
      "m1": "به دیدار شد چون گل ارغوان",
      "m2": "سهی قد و زیبا رخ و پهلوان",
      "verse_number": 26
    },
    {
      "id": "e6ae3eb9-b444-4ff5-b1d7-42287b046e9f",
      "m1": "رخ من به پیشش بیاراستی",
      "m2": "به گفتار و زان پس بهاخواستی",
      "verse_number": 27
    },
    {
      "id": "6b430513-6029-4209-a2f5-363c415503c7",
      "m1": "همی گفت و لب را پر از خنده داشت",
      "m2": "رخان هم چو گلنار آگنده داشت",
      "verse_number": 28
    },
    {
      "id": "9867681b-9f5a-4794-9f9d-2a1de371e7ac",
      "m1": "پرستنده با بانوی ماهروی",
      "m2": "چنین گفت کاکنون ره چاره جوی",
      "verse_number": 29
    },
    {
      "id": "df833cb5-c2d6-4a13-a619-563ef95a0f23",
      "m1": "که یزدان هر آنچت هوا بود داد",
      "m2": "سرانجام این کار فرخنده باد",
      "verse_number": 30
    },
    {
      "id": "0b49020c-4d57-41d0-9f79-36458fc2ace0",
      "m1": "یکی خانه بودش چو خرم بهار",
      "m2": "ز چهر بزرگان برو بر نگار",
      "verse_number": 31
    },
    {
      "id": "94a4a004-9536-49f0-9f72-323399268937",
      "m1": "به دیبای چینی بیاراستند",
      "m2": "طبقهای زرین بپیراستند",
      "verse_number": 32
    },
    {
      "id": "c3aeb190-298c-41b0-833f-fdde0f01a416",
      "m1": "عقیق و زبرجد برو ریختند",
      "m2": "می و مشک و عنبر برآمیختند",
      "verse_number": 33
    },
    {
      "id": "a8763d69-1c26-4b33-a3aa-fd08fde6ef67",
      "m1": "همه زر و پیروزه بد جامشان",
      "m2": "به روشن گلاب اندر آشامشان",
      "verse_number": 34
    },
    {
      "id": "98896361-501b-455d-9b34-4c8a899ed162",
      "m1": "بنفشه گل و نرگس و ارغوان",
      "m2": "سمن شاخ و سنبل به دیگر کران",
      "verse_number": 35
    },
    {
      "id": "b803d090-4510-49a8-85b4-ee548aacc105",
      "m1": "از آن خانهٔ دخت خورشید روی",
      "m2": "برآمد همی تا به خورشید بوی",
      "verse_number": 36
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 36
  },
  "order_in_parent": 8
}