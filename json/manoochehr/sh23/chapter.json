{
  "id": "1b2d4f2f-c8c1-4faf-befa-fd598024a483",
  "title": "بخش ۲۳",
  "verses": [
    {
      "id": "f1abb355-d715-4e34-a1db-e01c9eef2fe1",
      "m1": "همی راند دستان گرفته شتاب",
      "m2": "چو پرنده مرغ و چو کشتی برآب",
      "verse_number": 1
    },
    {
      "id": "62baed2a-b297-4184-9b38-06a4a4b97a3e",
      "m1": "کسی را نبد ز آمدنش آگهی",
      "m2": "پذیره نرفتند با فرهی",
      "verse_number": 2
    },
    {
      "id": "64c7cd93-cf31-4cbf-ba63-fbb3b386d581",
      "m1": "خروشی برآمد ز پرده سرای",
      "m2": "که آمد ز ره زال فرخندهرای",
      "verse_number": 3
    },
    {
      "id": "69a13169-ce62-4755-bed5-57f5f6b89992",
      "m1": "پذیره شدش سام یل شادمان",
      "m2": "همی داشت اندر برش یک زمان",
      "verse_number": 4
    },
    {
      "id": "68252ad9-0347-43fc-8c0c-4550452939ec",
      "m1": "فرود آمد از باره بوسید خاک",
      "m2": "بگفت آن کجا دید و بشنید پاک",
      "verse_number": 5
    },
    {
      "id": "ecd5a021-f1c7-41cb-847f-1473b650876e",
      "m1": "نشست از بر تخت پرمایه سام",
      "m2": "ابا زال خرم دل و شادکام",
      "verse_number": 6
    },
    {
      "id": "a653ce4f-d76c-414c-90ad-b0d3f80eeac9",
      "m1": "سخنهای سیندخت گفتن گرفت",
      "m2": "لبش گشت خندان نهفتن گرفت",
      "verse_number": 7
    },
    {
      "id": "caaab6d5-99a2-44fa-9518-923031b0e238",
      "m1": "چنین گفت کامد ز کابل پیام",
      "m2": "پیمبر زنی بود سیندخت نام",
      "verse_number": 8
    },
    {
      "id": "07e25695-ff61-47f1-9f7a-eb7c2c53fb23",
      "m1": "ز من خواست پیمان و دادم زمان",
      "m2": "که هرگز نباشم بدو بدگمان",
      "verse_number": 9
    },
    {
      "id": "4d325c49-e685-46fa-a2ae-81bf4bef376b",
      "m1": "ز هر چیز کز من به خوبی بخواست",
      "m2": "سخنها بران برنهادیم راست",
      "verse_number": 10
    },
    {
      "id": "3887c0fd-fbf4-42e3-b11d-add60f22f035",
      "m1": "نخست آنکه با ماه کابلستان",
      "m2": "شود جفت خورشید زابلستان",
      "verse_number": 11
    },
    {
      "id": "6f0a9292-eb10-434d-991d-a085d9a2a455",
      "m1": "دگر آنکه زی او به مهمان شویم",
      "m2": "بران دردها پاک درمان شویم",
      "verse_number": 12
    },
    {
      "id": "4fd746d7-6907-4eaf-bdf7-2bcc45a31755",
      "m1": "فرستادهای آمد از نزد اوی",
      "m2": "که پردخته شد کار بنمای روی",
      "verse_number": 13
    },
    {
      "id": "b308e569-ff40-42b6-8e76-98e22a528622",
      "m1": "کنون چیست پاسخ فرستاده را",
      "m2": "چه گوییم مهراب آزاده را",
      "verse_number": 14
    },
    {
      "id": "a0240f09-352d-47e5-9fa1-871e1c376f8d",
      "m1": "ز شادی چنان شد دل زال سام",
      "m2": "که رنگش سراپای شد لعل فام",
      "verse_number": 15
    },
    {
      "id": "bbfa604a-db95-4ce5-bc5d-e29d235d1645",
      "m1": "چنین داد پاسخ که ای پهلوان",
      "m2": "گر ایدون که بینی به روشن روان",
      "verse_number": 16
    },
    {
      "id": "58d06df4-574c-4a51-b7bf-92261a6070cc",
      "m1": "سپه رانی و ما به کابل شویم",
      "m2": "بگوییم زین در سخن بشنویم",
      "verse_number": 17
    },
    {
      "id": "9d1b0c8b-210e-49e7-bec9-02fa260a219f",
      "m1": "به دستان نگه کرد فرخنده سام",
      "m2": "بدانست کورا ازین چیست کام",
      "verse_number": 18
    },
    {
      "id": "2c52a32e-4c6f-4256-8697-232b630006ed",
      "m1": "سخن هر چه از دخت مهراب نیست",
      "m2": "به نزدیک زال آن جز از خواب نیست",
      "verse_number": 19
    },
    {
      "id": "7733778f-fc2f-4e15-aeb3-be55566c713e",
      "m1": "بفرمود تا زنگ و هندی درای",
      "m2": "زدند و گشادند پرده سرای",
      "verse_number": 20
    },
    {
      "id": "92c08125-e873-439e-8991-0c87689608cf",
      "m1": "هیونی برافگند مرد دلیر",
      "m2": "بدان تا شود نزد مهراب شیر",
      "verse_number": 21
    },
    {
      "id": "710b7384-7f3b-454a-a082-ca75edb201ee",
      "m1": "بگوید که آمد سپهبد ز راه",
      "m2": "ابا زال با پیل و چندی سپاه",
      "verse_number": 22
    },
    {
      "id": "b8f7f5cc-70ab-4356-9795-8f2f67bbe943",
      "m1": "فرستاده تازان به کابل رسید",
      "m2": "خروشی برآمد چنان چون سزید",
      "verse_number": 23
    },
    {
      "id": "6b250712-1945-435a-b4d7-bdf587f47a1d",
      "m1": "چنان شاد شد شاه کابلستان",
      "m2": "ز پیوند خورشید زابلستان",
      "verse_number": 24
    },
    {
      "id": "58241cf1-c208-49d5-9426-b22f6697c1a9",
      "m1": "که گفتی همی جان برافشاندند",
      "m2": "ز هر جای رامشگران خواندند",
      "verse_number": 25
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 25
  },
