{
  "id": "8bb94da9-8fe7-4e82-9a6a-96fed7bd0b5d",
  "title": "بخش ۱۹",
  "verses": [
    {
      "id": "790da2c6-1622-4a63-abb6-ea4107e250bf",
      "m1": "بفرمود تا موبدان و ردان",
      "m2": "ستارهشناسان و هم بخردان",
      "verse_number": 1
    },
    {
      "id": "b37d373e-b388-47aa-8d55-d26594284227",
      "m1": "کنند انجمن پیش تخت بلند",
      "m2": "به کار سپهری پژوهش کنند",
      "verse_number": 2
    },
    {
      "id": "02285b97-1ef8-4239-91aa-ce6d85b2f147",
      "m1": "برفتند و بردند رنج دراز",
      "m2": "که تا با ستاره چه دارند راز",
      "verse_number": 3
    },
    {
      "id": "7480ec6d-885b-4cab-919c-fcb7d04ffc1d",
      "m1": "سه روز اندران کارشان شد درنگ",
      "m2": "برفتند با زیج رومی به چنگ",
      "verse_number": 4
    },
    {
      "id": "bc37c2be-c65b-4ba5-a902-859a7a07d3b3",
      "m1": "زبان بر گشادند بر شهریار",
      "m2": "که کردیم با چرخ گردان شمار",
      "verse_number": 5
    },
    {
      "id": "0b01a87a-6892-410a-aae9-82efa781a180",
      "m1": "چنین آمد از داد اختر پدید",
      "m2": "که این آب روشن بخواهد دوید",
      "verse_number": 6
    },
    {
      "id": "7cbb6fc5-ef01-4cd1-a270-a88afc5535f6",
      "m1": "ازین دخت مهراب و از پور سام",
      "m2": "گوی پر منش زاید و نیک نام",
      "verse_number": 7
    },
    {
      "id": "01e9beed-3e06-464d-97cf-dffc63909d10",
      "m1": "بود زندگانیش بسیار مر",
      "m2": "همش زور باشد هم آیین و فر",
      "verse_number": 8
    },
    {
      "id": "ed5b3b1e-91a0-4262-9d47-0659533a47dd",
      "m1": "همش برز باشد همش شاخ و یال",
      "m2": "به رزم و به بزمش نباشد همال",
      "verse_number": 9
    },
    {
      "id": "bfd67fb9-052f-45ac-a06f-a51c61ac1cdb",
      "m1": "کجا بارهٔ او کند موی تر",
      "m2": "شود خشک همرزم او را جگر",
      "verse_number": 10
    },
    {
      "id": "0b249039-84d3-4d17-8ee2-83157d65418f",
      "m1": "عقاب از بر ترگ او نگذرد",
      "m2": "سران جهان را بکس نشمرد",
      "verse_number": 11
    },
    {
      "id": "e967a78b-1449-447d-a5f2-2ed1b363d40d",
      "m1": "یکی برز بالا بود فرمند",
      "m2": "همه شیر گیرد به خم کمند",
      "verse_number": 12
    },
    {
      "id": "4c5397bd-cea6-464f-96ce-11e991599b4a",
      "m1": "هوا را به شمشیر گریان کند",
      "m2": "بر آتش یکی گور بریان کند",
      "verse_number": 13
    },
    {
      "id": "67d510d2-c314-40d0-8b02-5b9109aac360",
      "m1": "کمر بستهٔ شهریاران بود",
      "m2": "به ایران پناه سواران بود",
      "verse_number": 14
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 14
  }
}