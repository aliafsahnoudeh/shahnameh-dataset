{
  "id": "0572b199-38e8-4cb2-9aaa-465ed5c83fb8",
  "title": "بخش ۹",
  "verses": [
    {
      "id": "ec5b3f24-38e5-43be-acb7-7fa1757578dc",
      "m1": "و دیگر که از شهر ارمان شدند",
      "m2": "به کینه سوی زابلستان شدند",
      "verse_number": 1
    },
    {
      "id": "0da46042-bb56-41aa-a7fe-3605c2e5cab7",
      "m1": "شماساس کز پیش جیحون برفت",
      "m2": "سوی سیستان روی بنهاد و تفت",
      "verse_number": 2
    },
    {
      "id": "b480ee98-2a11-4e5c-8cdf-3467dcda4d59",
      "m1": "خزروان ابا تیغزن سی هزار",
      "m2": "ز ترکان بزرگان خنجرگزار",
      "verse_number": 3
    },
    {
      "id": "0d3de9b9-2778-494a-9f7d-1883b9b0f2b9",
      "m1": "برفتند بیدار تا هیرمند",
      "m2": "ابا تیغ و با گرز و بخت بلند",
      "verse_number": 4
    },
    {
      "id": "fc3dfc51-affd-4f2d-b4be-d272cf6296c5",
      "m1": "ز بهر پدر زال با سوگ و درد",
      "m2": "به گوراب اندر همی دخمه کرد",
      "verse_number": 5
    },
    {
      "id": "7fded3f6-9a8c-41a4-b770-79e0dfce56d5",
      "m1": "به شهر اندرون گرد مهراب بود",
      "m2": "که روشن روان بود و بیخواب بود",
      "verse_number": 6
    },
    {
      "id": "4f06c161-e76c-42ae-baa8-57a1cd1fa785",
      "m1": "فرستادهای آمد از نزد اوی",
      "m2": "به سوی شماساس بنهاد روی",
      "verse_number": 7
    },
    {
      "id": "74fad094-5cfa-4bd1-9090-e15cb528f03a",
      "m1": "به پیش سراپرده آمد فرود",
      "m2": "ز مهراب دادش فراوان درود",
      "verse_number": 8
    },
    {
      "id": "23a7ae2b-599e-4c8a-81d1-cef903714af4",
      "m1": "که بیداردل شاه توران سپاه",
      "m2": "بماناد تا جاودان با کلاه",
      "verse_number": 9
    },
    {
      "id": "f93ec8c1-4c2e-48f8-8cbd-70c6cbbdee50",
      "m1": "ز ضحاک تازیست ما را نژاد",
      "m2": "بدین پادشاهی نیم سخت شاد",
      "verse_number": 10
    },
    {
      "id": "8586496c-ffd3-48ed-89d8-4fec871d60ec",
      "m1": "به پیوستگی جان خریدم همی",
      "m2": "جز این نیز چاره ندیدم همی",
      "verse_number": 11
    },
    {
      "id": "39d1eb96-7a3e-4f62-bb85-7b32992c0a59",
      "m1": "کنون این سرای و نشست منست",
      "m2": "همان زاولستان به دست منست",
      "verse_number": 12
    },
    {
      "id": "fa3ecede-06dd-4d73-a526-9c9a6664b5e7",
      "m1": "ازایدر چو دستان بشد سوگوار",
      "m2": "ز بهر ستودان سام سوار",
      "verse_number": 13
    },
    {
      "id": "d508549e-ff05-4bba-9d31-b24afd438af8",
      "m1": "دلم شادمان شد به تیمار اوی",
      "m2": "برآنم که هرگز نبینمش روی",
      "verse_number": 14
    },
    {
      "id": "e0c9f1e7-91e5-47ba-9515-82d45cbe9079",
      "m1": "زمان خواهم از نامور پهلوان",
      "m2": "بدان تا فرستم هیونی دوان",
      "verse_number": 15
    },
    {
      "id": "7fb15d14-bd39-4b2a-a8b4-740cbb79b876",
      "m1": "یکی مرد بینادل و پرشتاب",
      "m2": "فرستم به نزدیک افراسیاب",
      "verse_number": 16
    },
    {
      "id": "2a6f9e39-3948-4581-acc7-a809a83c55bd",
      "m1": "مگر کز نهان من آگه شود",
      "m2": "سخنهای گوینده کوته شود",
      "verse_number": 17
    },
    {
      "id": "d5b1ac3d-81f4-4f47-86f7-d73272752f1a",
      "m1": "نثاری فرستم چنان چون سزاست",
      "m2": "جز این نیز هرچ از در پادشاست",
      "verse_number": 18
    },
    {
      "id": "ab5967eb-b7f4-48b5-a591-3010348c994d",
      "m1": "گر ایدونک گوید به نزد من آی",
      "m2": "جز از پیش تختش نباشم به پای",
      "verse_number": 19
    },
    {
      "id": "2519cc60-b4f9-4e1a-9d20-0d9f1efdc68a",
      "m1": "همه پادشاهی سپارم بدوی",
      "m2": "همیشه دلی شاد دارم بدوی",
      "verse_number": 20
    },
    {
      "id": "9574bc91-29c7-4e82-a1c8-d60073360194",
      "m1": "تن پهلوان را نیارم به رنج",
      "m2": "فرستمش هرگونه آگنده گنج",
      "verse_number": 21
    },
    {
      "id": "b05cc5f4-03ee-4621-8024-4571d469a732",
      "m1": "ازین سو دل پهلوان را ببست",
      "m2": "وزان در سوی چاره یازید دست",
      "verse_number": 22
    },
    {
      "id": "95167b2a-2261-44df-a136-d8dcaf134326",
      "m1": "نوندی برافگند نزدیک زال",
      "m2": "که پرنده شو باز کن پر و بال",
      "verse_number": 23
    },
    {
      "id": "9e7b5a66-c1ab-47fb-9c8a-28c6eaa7e0fa",
      "m1": "به دستان بگو آنچ دیدی ز کار",
      "m2": "بگویش که از آمدن سر مخار",
      "verse_number": 24
    },
    {
      "id": "715aac56-d0f6-41cd-bb3d-7e72828b6336",
      "m1": "که دو پهلوان آمد ایدر بجنگ",
      "m2": "ز ترکان سپاهی چو دشتی پلنگ",
      "verse_number": 25
    },
    {
      "id": "f200f9c1-373b-43aa-b646-1f1dd2ceef57",
      "m1": "دو لشکر کشیدند بر هیرمند",
      "m2": "به دینارشان پای کردم به بند",
      "verse_number": 26
    },
    {
      "id": "94c1686a-a126-4443-ba6d-b5216e474999",
      "m1": "گر از آمدن دم زنی یک زمان",
      "m2": "برآید همی کامهٔ بدگمان",
      "verse_number": 27
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 27
  },
  "order_in_parent": 9
}