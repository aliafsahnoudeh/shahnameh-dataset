{
  "id": "38fbe644-9c9d-4e5f-894c-720999299028",
  "title": "بخش ۱۱",
  "verses": [
    {
      "id": "8b6056d8-f973-4761-9ef8-a044359e3670",
      "m1": "سوی شاه ترکان رسید آگهی",
      "m2": "کزان نامداران جهان شد تهی",
      "verse_number": 1
    },
    {
      "id": "6aa925f0-1996-47b3-aafe-46e4452f28dc",
      "m1": "دلش گشت پر آتش از درد و غم",
      "m2": "دو رخ را به خون جگر داد نم",
      "verse_number": 2
    },
    {
      "id": "f5302a8a-a838-45df-8389-fe71dce3a90f",
      "m1": "برآشفت و گفتا که نوذر کجاست",
      "m2": "کزو ویسه خواهد همی کینه خواست",
      "verse_number": 3
    },
    {
      "id": "3ba3e1dc-13b3-4c8c-a5e4-ad1ee2cd2a79",
      "m1": "چه چاره است جز خون او ریختن",
      "m2": "یکی کینهٔ نو برانگیختن",
      "verse_number": 4
    },
    {
      "id": "0ad12988-5a27-42d0-b8a8-4dc90cf93d9f",
      "m1": "به دژخیم فرمود کو را کشان",
      "m2": "ببر تا بیاموزد او سرفشان",
      "verse_number": 5
    },
    {
      "id": "5c2d2f44-96dd-4ef4-abfd-c566e2005c75",
      "m1": "سپهدار نوذر چو آگاه شد",
      "m2": "بدانست کش روز کوتاه شد",
      "verse_number": 6
    },
    {
      "id": "07a01a4d-f04b-4b98-8573-d4ab033496f7",
      "m1": "سپاهی پر از غلغل و گفت و گوی",
      "m2": "سوی شاه نوذر نهادند روی",
      "verse_number": 7
    },
    {
      "id": "3dd5057d-0607-42dd-b9de-22f47e7087a0",
      "m1": "ببستند بازوش با بند تنگ",
      "m2": "کشیدندش از جای پیش نهنگ",
      "verse_number": 8
    },
    {
      "id": "61c05256-b73d-47c3-a748-5b6a7f068c42",
      "m1": "به دشت آوریدندش از خیمه خوار",
      "m2": "برهنه سر و پای و برگشته کار",
      "verse_number": 9
    },
    {
      "id": "bbd55bd0-c0c9-412f-8129-021a83a4aa02",
      "m1": "چو از دور دیدش زبان برگشاد",
      "m2": "ز کین نیاگان همی کرد یاد",
      "verse_number": 10
    },
    {
      "id": "2263828a-ec57-4d28-89a3-b46f2251125e",
      "m1": "ز تور و ز سلم اندر آمد نخست",
      "m2": "دل و دیده از شرم شاهان بشست",
      "verse_number": 11
    },
    {
      "id": "982e1dd1-13e9-47d5-b7ee-72c3f5ee432c",
      "m1": "بدو گفت هر بد که آید سزاست",
      "m2": "بگفت و برآشفت و شمشیر خواست",
      "verse_number": 12
    },
    {
      "id": "86192b3a-4f02-4bc3-a431-c59b8d3f4032",
      "m1": "بزد گردن خسرو تاجدار",
      "m2": "تنش را بخاک اندر افگند خوار",
      "verse_number": 13
    },
    {
      "id": "af857975-dc68-479b-b213-cd75ebeed9a8",
      "m1": "شد آن یادگار منوچهر شاه",
      "m2": "تهی ماند ایران ز تخت و کلاه",
      "verse_number": 14
    },
    {
      "id": "20e5adfa-f678-4f68-a26a-b154a666976e",
      "m1": "ایا دانشی مرد بسیار هوش",
      "m2": "همه چادر آزمندی مپوش",
      "verse_number": 15
    },
    {
      "id": "8944bcf4-2c24-41ff-9a52-4a20106a4186",
      "m1": "که تخت و کله چون تو بسیار دید",
      "m2": "چنین داستان چند خواهی شنید",
      "verse_number": 16
    },
    {
      "id": "81223734-35a2-420b-9dd9-0eb1fa64ede8",
      "m1": "رسیدی به جایی که بشتافتی",
      "m2": "سرآمد کزو آرزو یافتی",
      "verse_number": 17
    },
    {
      "id": "a78d6844-85c6-46aa-9adc-58a6ec4f0a6e",
      "m1": "چه جویی از این تیره خاک نژند",
      "m2": "که هم بازگرداندت مستمند",
      "verse_number": 18
    },
    {
      "id": "e1c95a3b-4c2f-402f-a642-864ee7748618",
      "m1": "که گر چرخ گردان کشد زین تو",
      "m2": "سرانجام خاکست بالین تو",
      "verse_number": 19
    },
    {
      "id": "4ef7de0a-a78c-45aa-8029-fe2e0128291f",
      "m1": "پس آن بستگان را کشیدند خوار",
      "m2": "به جان خواستند آنگهی زینهار",
      "verse_number": 20
    },
    {
      "id": "06a397eb-753e-4036-8494-d41f08361e62",
      "m1": "چو اغریرث پرهنر آن بدید",
      "m2": "دل او ببر در چو آتش دمید",
      "verse_number": 21
    },
    {
      "id": "6461cd7d-b9c4-4c2b-a9ba-917ec36a20ef",
      "m1": "همی گفت چندین سر بیگناه",
      "m2": "ز تن دور ماند به فرمان شاه",
      "verse_number": 22
    },
    {
      "id": "34773235-e054-4a14-8fa9-b5a8434aa9b7",
      "m1": "بیامد خروشان به خواهشگری",
      "m2": "بیاراست با نامور داوری",
      "verse_number": 23
    },
    {
      "id": "c97c7d12-6717-4a78-a223-24eb2638be43",
      "m1": "که چندین سرافراز گرد و سوار",
      "m2": "نه با ترگ و جوشن نه در کارزار",
      "verse_number": 24
    },
    {
      "id": "9eefb825-c939-40a7-ae05-b073d0bf0652",
      "m1": "گرفتار کشتن نه والا بود",
      "m2": "نشیبست جایی که بالا بود",
      "verse_number": 25
    },
    {
      "id": "20641c65-6e8f-414c-9dab-edfd5c83740c",
      "m1": "سزد گر نیاید به جانشان گزند",
      "m2": "سپاری همیدون به من شان ببند",
      "verse_number": 26
    },
    {
      "id": "23e15fce-eddd-4c24-801f-65a610de9fea",
      "m1": "بریشان یکی غار زندان کنم",
      "m2": "نگهدارشان هوشمندان کنم",
      "verse_number": 27
    },
    {
      "id": "b1a532d9-080d-4004-9702-da4555c1239d",
      "m1": "به ساری به زاری برآرند هوش",
      "m2": "تو از خون به کش دست و چندین مکوش",
      "verse_number": 28
    },
    {
      "id": "a32c481b-b89f-4963-bce9-4e92453ac29d",
      "m1": "ببخشید جانشان به گفتار اوی",
      "m2": "چو بشنید با درد پیکار اوی",
      "verse_number": 29
    },
    {
      "id": "472512b7-725f-4cf7-a170-2ef9607f3a03",
      "m1": "بفرمودشان تا به ساری برند",
      "m2": "به غل و به مسمار و خواری برند",
      "verse_number": 30
    },
    {
      "id": "cc5ecf9b-04fa-4a4d-9f72-a3477263f310",
      "m1": "چو این کرده شد ساز رفتن گرفت",
      "m2": "زمین زیر اسپان نهفتن گرفت",
      "verse_number": 31
    },
    {
      "id": "955dc17b-b1a9-4cca-a843-f0e6d65e7721",
      "m1": "ز پیش دهستان سوی ری کشید",
      "m2": "از اسپان به رنج و به تک خوی کشید",
      "verse_number": 32
    },
    {
      "id": "c0d91934-f1c1-4556-b204-7b3a8120f347",
      "m1": "کلاه کیانی به سر بر نهاد",
      "m2": "به دینار دادن در اندرگشاد",
      "verse_number": 33
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 33
  },
  "order_in_parent": 11
}