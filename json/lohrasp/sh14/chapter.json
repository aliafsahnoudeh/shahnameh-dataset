{
  "id": "23959acf-8966-4fb1-9c32-49eba8137e5e",
  "title": "بخش ۱۴",
  "verses": [
    {
      "id": "5619f1e0-fef8-42df-a658-c3a7af046cb7",
      "m1": "چو خورشید شد بر سر کوه زرد",
      "m2": "نماند آن زمان روزگار نبرد",
      "verse_number": 1
    },
    {
      "id": "39f80239-685a-402a-afa6-21f36e0a6026",
      "m1": "شب آمد یکی پردهٔ آبنوس",
      "m2": "بپوشید بر چهرهٔ سندروس",
      "verse_number": 2
    },
    {
      "id": "36bc64b4-04f8-4868-8bf5-4a55279b2590",
      "m1": "چو خورشید ازان کوشش آگاه شد",
      "m2": "ز برج کمان بر سر گاه شد",
      "verse_number": 3
    },
    {
      "id": "cc14a647-399b-40ad-80de-0153cc533b42",
      "m1": "ببد چشمهٔ روز چون سندروس",
      "m2": "ز هر سو برآمد دم نای و کوس",
      "verse_number": 4
    },
    {
      "id": "f800958c-ee9a-4c56-8512-1b52110c0599",
      "m1": "چکاچاک برخاست از هر دو روی",
      "m2": "ز خون شد همه رزمگه جوی جوی",
      "verse_number": 5
    },
    {
      "id": "298232d2-12ad-4cfb-8f02-001b54966879",
      "m1": "بیامد سبک قیصر از میمنه",
      "m2": "دو داماد را کرد پیش بنه",
      "verse_number": 6
    },
    {
      "id": "8915e395-450d-44b6-9693-3b81b7dcbc1b",
      "m1": "ابر میمنه پور قیصر سقیل",
      "m2": "ابر میسره قیصر و کوس و پیل",
      "verse_number": 7
    },
    {
      "id": "c15b9b6f-27be-4ccf-a54f-7e55acac67b1",
      "m1": "دهاده برآمد ز هر دو سپاه",
      "m2": "تو گفتی برآویخت با شید ماه",
      "verse_number": 8
    },
    {
      "id": "05faa75b-b065-4fc9-8efd-c6fbf33e728f",
      "m1": "بجنبید گشتاسپ از پیش صف",
      "m2": "یکی باره زیر اژدهایی به کف",
      "verse_number": 9
    },
    {
      "id": "9059a514-81d0-4fc5-97ab-21a43efff225",
      "m1": "چنین گفت الیاس با انجمن",
      "m2": "که قیصر همی باژ خواهد ز من",
      "verse_number": 10
    },
    {
      "id": "122db643-9ff2-480a-9c9c-865bc5b2d488",
      "m1": "چو بر در چنین اژدها باشدش",
      "m2": "ازیرا منش بابها باشدش",
      "verse_number": 11
    },
    {
      "id": "45a8a416-df2c-4292-9573-7dcc5a5b04ce",
      "m1": "چو گشتاسپ الیاس را دید گفت",
      "m2": "که اکنون هنرها نباید نهفت",
      "verse_number": 12
    },
    {
      "id": "cceac5b5-3fe6-4667-a257-27a255b232bb",
      "m1": "برانگیختند اسپ هر دو سوار",
      "m2": "ابا نیزه و تیر جوشن گذار",
      "verse_number": 13
    },
    {
      "id": "c5a50654-6937-459c-9ded-39a4e39d080f",
      "m1": "ازان لشکر الیاس بگشاد شست",
      "m2": "که گشتاسپ را برکند کار پست",
      "verse_number": 14
    },
    {
      "id": "a280f6b4-3cac-482d-bdfd-5678399a2f0a",
      "m1": "بزد نیزه گشتاسپ بر جوشنش",
      "m2": "بخست آن زمان کارزاری تنش",
      "verse_number": 15
    },
    {
      "id": "b620ad1b-1403-4ec5-b64f-fcfb22934e8e",
      "m1": "بیفگندش از باره برسان مست",
      "m2": "بیازید و بگرفت دستش به دست",
      "verse_number": 16
    },
    {
      "id": "1993bb28-386a-4ab7-a3f9-74093b080c80",
      "m1": "ز پیش سواران کشانش ببرد",
      "m2": "بیاورد و نزدیک قیصر سپرد",
      "verse_number": 17
    },
    {
      "id": "51b67613-b8e7-4b76-b45a-4c49ef86ab6b",
      "m1": "بیاورد لشکر به پیش سپاه",
      "m2": "به کردار باد اندر آمد ز راه",
      "verse_number": 18
    },
    {
      "id": "9e0fa1f1-5997-490a-95ad-5a26fac2c677",
      "m1": "ازیشان چه مایه گرفت و بکشت",
      "m2": "بکشتند مر هرک آمد به مشت",
      "verse_number": 19
    },
    {
      "id": "6bb755aa-2fca-428f-89df-b59777ea98e5",
      "m1": "چو رومی پساندر همآواز شد",
      "m2": "چو گشتاسپ زان جایگه باز شد",
      "verse_number": 20
    },
    {
      "id": "ed4c1482-a8bc-4a72-8026-3ebe19699775",
      "m1": "بر قیصر آمد سپه تاخته",
      "m2": "به پیروزی و گردن افراخته",
      "verse_number": 21
    },
    {
      "id": "95856598-7a44-43e3-8f84-fd2200a38ff4",
      "m1": "ز لشکر چو قیصر بدیدش به راه",
      "m2": "ز شادی پذیره شدش با سپاه",
      "verse_number": 22
    },
    {
      "id": "a3abf9c7-9d06-4c66-860c-113120f7c141",
      "m1": "سر و چشم آن نامور بوس داد",
      "m2": "جهانآفرین را همی کرد یاد",
      "verse_number": 23
    },
    {
      "id": "251ccfbc-3f12-4151-8b37-ed8684107434",
      "m1": "وزان جایگه بازگشتند شاد",
      "m2": "سپهبد کلاه کیان برنهاد",
      "verse_number": 24
    },
    {
      "id": "b501968c-d437-4673-b333-4f9dd2f71dec",
      "m1": "همه روم با هدیه و با نثار",
      "m2": "برفتند شادان بر نامدار",
      "verse_number": 25
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 25
  }
}