{
  "id": "b0306cd3-43bd-4029-892f-0a4a92b92f8d",
  "title": "بخش ۶",
  "verses": [
    {
      "id": "471ccba0-4f7b-426b-9b3a-416cfca29dde",
      "m1": "همی بود گشتاسپ دل مستمند",
      "m2": "خروشان و جوشان ز چرخ بلند",
      "verse_number": 1
    },
    {
      "id": "1428dd49-e69b-48ef-823c-3b7c7c33a8cc",
      "m1": "نیامد ز گیتیش جز زهر بهر",
      "m2": "یکی روستا دید نزدیک شهر",
      "verse_number": 2
    },
    {
      "id": "b003e4b1-0c71-4311-abf2-74ac6e0a2659",
      "m1": "درخت و گل و آبهای روان",
      "m2": "نشستنگه شاد مرد جوان",
      "verse_number": 3
    },
    {
      "id": "66f08e51-057f-4ed3-8072-384ecee1ada6",
      "m1": "درختی گشن سایه بر پیش آب",
      "m2": "نهان گشته زو چشمهٔ آفتاب",
      "verse_number": 4
    },
    {
      "id": "c62a5db7-b048-4aee-b6ca-39812dbf26f3",
      "m1": "بران سایه بنشست مرد جوان",
      "m2": "پر از درد پیچان و تیرهروان",
      "verse_number": 5
    },
    {
      "id": "94e91896-6924-4420-9a96-ad45ecd533fc",
      "m1": "همی گفت کای داور کردگار",
      "m2": "غم آمد مرا بهره زین روزگار",
      "verse_number": 6
    },
    {
      "id": "c19c8342-b7ef-4e10-b58a-c98ec1f67f86",
      "m1": "نبینم همی اختر خویش بد",
      "m2": "ندانم چرا بر سرم بد رسد",
      "verse_number": 7
    },
    {
      "id": "2518f157-67a6-44a2-b19d-0284fc839226",
      "m1": "یکی نامور زان پسندیده ده",
      "m2": "گذر کرد بر وی که او بود مه",
      "verse_number": 8
    },
    {
      "id": "066a7cfa-2155-4b71-8015-0dc7f8cf7f7c",
      "m1": "ورا دید با دیدگان پر ز خون",
      "m2": "به زیر زنخ دست کرده ستون",
      "verse_number": 9
    },
    {
      "id": "868d13e6-03aa-4b36-9311-d54fede5e310",
      "m1": "بدو گفت کای پاک مرد جوان",
      "m2": "چرایی پر از درد و تیرهروان",
      "verse_number": 10
    },
    {
      "id": "4a027aaf-b3eb-4792-b354-540e77ab9dee",
      "m1": "اگر آیدت رای ایوان من",
      "m2": "بوی شاد یکچند مهمان من",
      "verse_number": 11
    },
    {
      "id": "34798956-2789-41da-9a29-510934cc4f50",
      "m1": "مگر کین غمان بر دلت کم شود",
      "m2": "سر تیر مژگانت بی نم شود",
      "verse_number": 12
    },
    {
      "id": "48cc7b54-3832-484f-8eb8-2abb925cc213",
      "m1": "بدو گفت گشتاسپ کای نامجوی",
      "m2": "نژاد تو از کیست با من بگوی",
      "verse_number": 13
    },
    {
      "id": "c4d3ada4-af7c-4447-b47c-e8eac67b8c0c",
      "m1": "چنین داد پاسخ ورا کدخدای",
      "m2": "کزین پرسش اکنون ترا چیست رای",
      "verse_number": 14
    },
    {
      "id": "992ab780-a2c3-46c6-84bf-4b630fab9c74",
      "m1": "من از تخم شاه آفریدون گرد",
      "m2": "کزان تخمه کس در جهان نیست خرد",
      "verse_number": 15
    },
    {
      "id": "99c3eea7-ea6e-4787-9fc8-6509e40a1630",
      "m1": "چو بشنید گشتاسپ برداشت پای",
      "m2": "همی رفت با نامور کدخدای",
      "verse_number": 16
    },
    {
      "id": "13806aa4-6c4d-49bf-a56f-ffe287f6263f",
      "m1": "چو آن مهتر آمد سوی خان خویش",
      "m2": "به مهمان بیاراست ایوان خویش",
      "verse_number": 17
    },
    {
      "id": "d1b36bb2-6035-4743-8b59-35b2058823fd",
      "m1": "بسان برادر همی داشتش",
      "m2": "زمانی به ناکام نگذاشتش",
      "verse_number": 18
    },
    {
      "id": "25c6482e-592f-400a-bdfb-df797350fff8",
      "m1": "زمانه برین نیز چندی بگشت",
      "m2": "برین کار بر ماهیان برگذشت",
      "verse_number": 19
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 19
  }
}