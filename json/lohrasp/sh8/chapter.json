{
  "id": "7f35bede-2e9e-452b-9b83-4423d71fd26f",
  "title": "بخش ۸",
  "verses": [
    {
      "id": "99d25f95-db00-4da4-9920-c10ef9af39ae",
      "m1": "چو بشنید قیصر بر آن برنهاد",
      "m2": "که دخت گرامی به گشتاسپ داد",
      "verse_number": 1
    },
    {
      "id": "66088755-0dfc-43ad-8e51-cb796741f513",
      "m1": "بدو گفت با او برو همچنین",
      "m2": "نیابی ز من گنج و تاج و نگین",
      "verse_number": 2
    },
    {
      "id": "84eb6f86-1c42-4143-a54e-740463b6166f",
      "m1": "چو گشتاسپ آن دید خیره بماند",
      "m2": "جهانآفرین را فراوان بخواند",
      "verse_number": 3
    },
    {
      "id": "6a97623d-bcd2-4e66-8965-21f63a38ca0a",
      "m1": "چنین گفت با دختر سرفراز",
      "m2": "که ای پروریده بنام و بناز",
      "verse_number": 4
    },
    {
      "id": "4604ff2d-c111-4f24-88d5-954892b18122",
      "m1": "ز چندین سر و افسر نامدار",
      "m2": "چرا کرد رایت مرا خواستار",
      "verse_number": 5
    },
    {
      "id": "c9fa8916-13e7-43db-8de2-54ad2b49c158",
      "m1": "غریبی همی برگزینی که گنج",
      "m2": "نیابی و با او بمانی به رنج",
      "verse_number": 6
    },
    {
      "id": "81ee72b0-bec6-4bb1-b8d2-2c14f6afc3a0",
      "m1": "ازین سرفرازان همالی بجوی",
      "m2": "که باشد به نزد پدرت آبروی",
      "verse_number": 7
    },
    {
      "id": "84e38603-4bb6-48ee-9f8a-14444364d14b",
      "m1": "کتایون بدو گفت کای بدگمان",
      "m2": "مشو تیز با گردش آسمان",
      "verse_number": 8
    },
    {
      "id": "7503a799-7e9c-4b32-a5b7-97e5e7dd29bf",
      "m1": "چو من با تو خرسند باشم به بخت",
      "m2": "تو افسر چرا جویی و تاج و تخت",
      "verse_number": 9
    },
    {
      "id": "43496b5d-2953-4e0d-99d1-36ce2da3b853",
      "m1": "برفتند ز ایوان قیصر به درد",
      "m2": "کتایون و گشتاسپ با باد سرد",
      "verse_number": 10
    },
    {
      "id": "9cce4e4d-65c5-4750-8126-d7f9535d631a",
      "m1": "چنین گفت با شوی و زن کدخدای",
      "m2": "که خرسند باشید و فرخندهرای",
      "verse_number": 11
    },
    {
      "id": "e66b3bbc-119c-4271-8432-dc5500c1ffc9",
      "m1": "سرایی به پردخت مهتر بده",
      "m2": "خورشها و گستردنی هرچ به",
      "verse_number": 12
    },
    {
      "id": "909335eb-4ae6-4ae9-8893-a4f91f72195b",
      "m1": "چو آن دید گشتاسپ کرد آفرین",
      "m2": "بران نامور مهتر پاکدین",
      "verse_number": 13
    },
    {
      "id": "532c4494-0fb4-4494-b48b-43199e530bcb",
      "m1": "کتایون بیاندازه پیرایه داشت",
      "m2": "ز یاقوت و هر گوهری مایه داشت",
      "verse_number": 14
    },
    {
      "id": "f2646a58-f48d-43bd-b9a6-dbfbd5a9cfa5",
      "m1": "یکی گوهری از میان برگزید",
      "m2": "که چشم خردمند زان سان ندید",
      "verse_number": 15
    },
    {
      "id": "c132f76f-0ba2-427e-bce2-ddabc2243bd8",
      "m1": "ببردند نزدیک گوهرشناس",
      "m2": "پذیرفت ز اندازه بیرون سپاس",
      "verse_number": 16
    },
    {
      "id": "5a73ce99-fdd6-4e5d-9327-d88c6016ce46",
      "m1": "بها داد یاقوت را ششهزار",
      "m2": "ز دینار و گنج از در شهریار",
      "verse_number": 17
    },
    {
      "id": "f8be3232-62a2-43d9-8822-543b61b45169",
      "m1": "خریدند چیزی که بایسته بود",
      "m2": "بدان روز بد نیز شایسته بود",
      "verse_number": 18
    },
    {
      "id": "db2bb2a6-4f06-4dfc-a3f9-d46f4ed2733f",
      "m1": "ازان سان که آمد همی زیستند",
      "m2": "گهی شادمان گاه بگریستند",
      "verse_number": 19
    },
    {
      "id": "42e32bc4-36ab-40ea-aade-28b4d9037fa6",
      "m1": "همه کار گشتاسپ نخچیر بود",
      "m2": "همه ساله با ترکش و تیر بود",
      "verse_number": 20
    },
    {
      "id": "d5c7f9ae-28b0-443d-935b-d5b121a8ff3f",
      "m1": "چنان بد که روزی ز نخچیرگاه",
      "m2": "مر او را به هیشوی بر بود راه",
      "verse_number": 21
    },
    {
      "id": "34a9af13-7a80-4744-8b3e-a05d6f14ae4e",
      "m1": "ز هرگونهای چند نخچیر داشت",
      "m2": "همی رفت و ترکش پر از تیر داشت",
      "verse_number": 22
    },
    {
      "id": "a388453b-9972-4376-ae08-cc1872ef5f56",
      "m1": "همه هرچ بود از بزرگان و خرد",
      "m2": "هم از راه نزدیک هیشوی برد",
      "verse_number": 23
    },
    {
      "id": "5940894f-ba5a-45d9-bc85-50b1c547289e",
      "m1": "چو هیشو بدیدش بیامد دوان",
      "m2": "پذیره شدش شاد و روشنروان",
      "verse_number": 24
    },
    {
      "id": "1eaa6077-3089-4bbf-bd78-026db061cb76",
      "m1": "به زیرش بگسترد گستردنی",
      "m2": "بیاورد چیزی که بد خوردنی",
      "verse_number": 25
    },
    {
      "id": "f1bdefea-adb1-4b99-be92-b325f63d43bb",
      "m1": "برآسود گشتاسپ و چیزی بخورد",
      "m2": "بیامد به نزد کتایون چو گرد",
      "verse_number": 26
    },
    {
      "id": "e43737f2-65bb-4292-943b-b380d3b147dd",
      "m1": "چو گشتاسپ هیشوی را دوست کرد",
      "m2": "به دانش ورا چون تن و پوست کرد",
      "verse_number": 27
    },
    {
      "id": "519b2160-784a-4436-9091-cbec3ccf29b9",
      "m1": "چو رفتی به نخچیر آهو ز شهر",
      "m2": "به ره بر به هیشوی دادی دو بهر",
      "verse_number": 28
    },
    {
      "id": "51844299-9dcf-4c65-bbfb-35544fccc0e6",
      "m1": "دگر بهرهٔ مهتر ده بدی",
      "m2": "هرانکس کزان روستا مه بدی",
      "verse_number": 29
    },
    {
      "id": "2bab6570-1bbd-4f31-9366-a2b42135a9ab",
      "m1": "چنان شد که گشتاسپ با کدخدای",
      "m2": "یکی شد به خورد و به آرام و رای",
      "verse_number": 30
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 30
  }
}