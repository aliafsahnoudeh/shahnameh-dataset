{
  "id": "cc3122c1-4e5a-4e74-ac4f-95b85fd57adf",
  "title": "بخش ۱۲",
  "verses": [
    {
      "id": "f2f45134-b4fe-4369-8d0c-90e7437d24dc",
      "m1": "چو شد روی گیتی چو دریای قیر",
      "m2": "نه ناهید پیدا نه بهرام و تیر",
      "verse_number": 1
    },
    {
      "id": "a78ada3a-832d-48a5-8a1c-71a1c8719834",
      "m1": "بیامد دمان دیدهبان پیش طوس",
      "m2": "دوان و شده روی چون سندروس",
      "verse_number": 2
    },
    {
      "id": "ff583bdd-3a50-4268-b261-31ff0c27984e",
      "m1": "چنین گفت کای پهلوان سپاه",
      "m2": "از ایران سپاه آمد از نزد شاه",
      "verse_number": 3
    },
    {
      "id": "d71aee9f-3b32-4ce1-9752-3def01d0ae1e",
      "m1": "سپهبد بخندید با مهتران",
      "m2": "که ای نامداران و کنداوران",
      "verse_number": 4
    },
    {
      "id": "a5fddd37-26fb-4095-8aa9-86fa7809173c",
      "m1": "چو یار آمد اکنون نسازیم جنگ",
      "m2": "گهی با شتابیم و گه با درنگ",
      "verse_number": 5
    },
    {
      "id": "590f3200-5c56-4948-a57c-4229ec920bb6",
      "m1": "بنیروی یزدان گو پیلتن",
      "m2": "بیاری بیاید بدین انجمن",
      "verse_number": 6
    },
    {
      "id": "39dbff00-3a17-4370-886b-2789b6e3a5c7",
      "m1": "ازان دیدهبان گشت روشنروان",
      "m2": "همه مژده دادند پیر و جوان",
      "verse_number": 7
    },
    {
      "id": "57d6ebc2-5d43-43a6-9b87-a3cd04aeaab4",
      "m1": "طلایه فرستاد بر دشت جنگ",
      "m2": "خروش آمد از کوه و آوای زنگ",
      "verse_number": 8
    },
    {
      "id": "f5d5ccf3-b6bb-455c-a280-fcd59911f141",
      "m1": "چو خورشید بر چرخ گنبد کشید",
      "m2": "شب تار شد از جهان ناپدید",
      "verse_number": 9
    },
    {
      "id": "df38f77e-c2aa-4c52-ae65-4e4ab04efac8",
      "m1": "یکی انجمن کرد خاقان چین",
      "m2": "بدیبا بیاراست روی زمین",
      "verse_number": 10
    },
    {
      "id": "c2c0fb22-ff59-4e7d-aeb5-cd2a24d3e666",
      "m1": "بپیران چنین گفت کامروز جنگ",
      "m2": "بسازیم و روزی نباید درنگ",
      "verse_number": 11
    },
    {
      "id": "eb7fb7db-fa05-4222-857e-002e9cd99e3a",
      "m1": "یکی با سرافراز گردنکشان",
      "m2": "خنیده سواران دشمن کشان",
      "verse_number": 12
    },
    {
      "id": "f27ff90f-93df-43f9-9a0e-5b4dfbca7d5c",
      "m1": "ببینیم کایرانیان برچیند",
      "m2": "بدین رزمگه اندرون با کیند",
      "verse_number": 13
    },
    {
      "id": "bd5e65ae-147e-44c5-bf76-d978aa41274b",
      "m1": "چنین گفت پیران که خاقان چین",
      "m2": "خردمند شاهیست با آفرین",
      "verse_number": 14
    },
    {
      "id": "b87d8479-5787-4537-b1ad-76b13fb3f9f0",
      "m1": "بران رفت باید که او را هواست",
      "m2": "که رای تو بر ما همه پادشاست",
      "verse_number": 15
    },
    {
      "id": "e8e19258-bace-4c43-9099-9933b799985b",
      "m1": "وزان پس برآمد ز پردهسرای",
      "m2": "خروشیدن کوس با کرنای",
      "verse_number": 16
    },
    {
      "id": "6cb3771b-716b-42f0-964a-8d1e82542f24",
      "m1": "سنانهای رخشان و جوشان سپاه",
      "m2": "شده روی کشور ز لشکر سیاه",
      "verse_number": 17
    },
    {
      "id": "953ce046-a375-4d86-a83b-94567b9d5fb5",
      "m1": "ز پیلان نهادند بر پنج زین",
      "m2": "بیاراست دیگر بدیبای چین",
      "verse_number": 18
    },
    {
      "id": "2cda857e-d4a3-4d31-a636-85e54f750e8b",
      "m1": "زبرجد نشانده بزین اندرون",
      "m2": "ز دیبای زربفت پیروزهگون",
      "verse_number": 19
    },
    {
      "id": "f33be71e-a17d-410e-8b2f-36b7dbe26e85",
      "m1": "بزرین رکیب و جناغ پلنگ",
      "m2": "بزرین و سیمین جرسها و زنگ",
      "verse_number": 20
    },
    {
      "id": "355ae7a9-25a5-4a20-b7de-709b694e26b0",
      "m1": "ز افسر سر پیلبان پرنگار",
      "m2": "همه پاک با طوق و با گوشوار",
      "verse_number": 21
    },
    {
      "id": "834ef906-2686-459c-8fed-1907a24505bb",
      "m1": "هوا شد ز بس پرنیانی درفش",
      "m2": "چو بازار چین سرخ و زرد و بنفش",
      "verse_number": 22
    },
    {
      "id": "8d9bc43e-447f-47a7-9cf8-e1df60bc05cc",
      "m1": "سپاهی برفت اندران دشت رزم",
      "m2": "کزیشان همی آرزو خواست بزم",
      "verse_number": 23
    },
    {
      "id": "348f5510-265e-4053-9178-7ea40dc384c7",
      "m1": "زمین شد بکردار چشم خروس",
      "m2": "ز بس رنگ و آرایش و پیل و کوس",
      "verse_number": 24
    },
    {
      "id": "761546ca-3845-4c1e-bd36-b83d58dc826d",
      "m1": "برفتند شاهان لشکر ز جای",
      "m2": "هوا پر شد از نالهٔ کرنای",
      "verse_number": 25
    },
    {
      "id": "d20b2712-43fe-4476-9ccb-94e77ea1c3fc",
      "m1": "چو از دور طوس سپهبد بدید",
      "m2": "سپاه آنچ بودش رده برکشید",
      "verse_number": 26
    },
    {
      "id": "41073358-8ec8-4254-bc8f-a341714c43bb",
      "m1": "ببستند گردان ایران میان",
      "m2": "بیاورد گیو اختر کاویان",
      "verse_number": 27
    },
    {
      "id": "d2387491-42c1-44b6-a14e-5dbed597f2b1",
      "m1": "از آوردگه تا سر تیغ کوه",
      "m2": "سپه بود از ایران گروها گروه",
      "verse_number": 28
    },
    {
      "id": "d3f922e4-47d6-4c8a-9836-e5539dae78b1",
      "m1": "چو کاموس و منشور و خاقان چین",
      "m2": "چو بیورد و چون شنگل بافرین",
      "verse_number": 29
    },
    {
      "id": "59148151-6cc6-461c-89bb-442717118afc",
      "m1": "نظاره بکوه هماون شدند",
      "m2": "نه بر آرزو پیش دشمن شدند",
      "verse_number": 30
    },
    {
      "id": "f89382e3-a8fa-4bf6-a177-f98cba1a0f01",
      "m1": "چو از دور خاقان چین بنگرید",
      "m2": "خروش سواران ایران شنید",
      "verse_number": 31
    },
    {
      "id": "ab48fea7-654b-4da2-8e0d-11d1a2382ffd",
      "m1": "پسند آمدش گفت کاینت سپاه",
      "m2": "سوران رزم آور و کینهخواه",
      "verse_number": 32
    },
    {
      "id": "f87a8777-6a45-4d08-99aa-1a0dd4946ead",
      "m1": "سپهدار پیران دگرگونه گفت",
      "m2": "هنرهای مردان نشاید نهفت",
      "verse_number": 33
    },
    {
      "id": "64ad3b43-34f2-47e8-b139-30d022e661db",
      "m1": "سپهدار کو چاه پوشد بخار",
      "m2": "برو اسپ تازد بروز شکار",
      "verse_number": 34
    },
    {
      "id": "d1cdd1bc-7ba1-4dee-88e2-af37c80ee162",
      "m1": "ازان به که بر خیره روز نبرد",
      "m2": "هنرهای دشمن کند زیر گرد",
      "verse_number": 35
    },
    {
      "id": "55dc0902-296f-4f93-a322-747326bec9e0",
      "m1": "ندیدم سواران و گردنکشان",
      "m2": "بگردی و مردانگی زین نشان",
      "verse_number": 36
    },
    {
      "id": "1001d372-f699-4fcb-badf-914b0d22f57c",
      "m1": "بپیران چنین گفت خاقان چین",
      "m2": "که اکنون چه سازیم بر دشت کین",
      "verse_number": 37
    },
    {
      "id": "9ec4bda9-e4a5-4760-9449-5fd9425e9a18",
      "m1": "ورا گفت پیران کز اندک سپاه",
      "m2": "نگیرند یاد اندرین رزمگاه",
      "verse_number": 38
    },
    {
      "id": "7459f6ac-53d7-4134-bfd3-3f587020cb5b",
      "m1": "کشیدی چنین رنج و راه دراز",
      "m2": "سپردی و دیدی نشیب و فراز",
      "verse_number": 39
    },
    {
      "id": "467d0e85-c177-4e41-8164-431af7ec0325",
      "m1": "بمان تا سه روز اندرین رزمگاه",
      "m2": "بباشیم و آسوده گردد سپاه",
      "verse_number": 40
    },
    {
      "id": "3739caf2-eef8-436a-963b-83abaaabfb92",
      "m1": "سپه را کنم زان سپس به دو نیم",
      "m2": "سرآمد کنون روز پیکار و بیم",
      "verse_number": 41
    },
    {
      "id": "88294892-1170-49f1-91cb-34ab0763d447",
      "m1": "بتازند شبگیر تا نیمروز",
      "m2": "نبرده سواران گیتیفروز",
      "verse_number": 42
    },
    {
      "id": "70ee3b64-01c3-4a9d-b86d-cdcb9677353a",
      "m1": "بژوپین و خنجر بتیر و کمان",
      "m2": "همی رزم جویند با بدگمان",
      "verse_number": 43
    },
    {
      "id": "8425dd09-374b-4755-81e6-408f8ace4530",
      "m1": "دگر نیمهٔ روز دیگر گروه",
      "m2": "بکوشند تا شب برآید ز کوه",
      "verse_number": 44
    },
    {
      "id": "fa3b05aa-da8b-446c-8bc7-797e5a86ea23",
      "m1": "شب تیره آسودگان را بجنگ",
      "m2": "برم تا بریشان شود کار تنگ",
      "verse_number": 45
    },
    {
      "id": "03997286-3ba9-4b27-a849-20e762b83504",
      "m1": "نمانم که آرام گیرند هیچ",
      "m2": "سواران من با سپاه و بسیچ",
      "verse_number": 46
    },
    {
      "id": "cd60c8f9-a3d1-41b9-a9e2-029b55628a8f",
      "m1": "بدو گفت کاموس کین رای نیست",
      "m2": "بدین مولش اندر مرا جای نیست",
      "verse_number": 47
    },
    {
      "id": "8913435d-2c0f-41bf-809c-e4a1eabb84e3",
      "m1": "بدین مایه مردم بدین گونه جنگ",
      "m2": "چه باید بدین گونه چندین درنگ",
      "verse_number": 48
    },
    {
      "id": "afd87679-5f3f-4574-89dc-91a46f530081",
      "m1": "بسازیم یکبار و جنگآوریم",
      "m2": "بریشان در و کوه تنگ آوریم",
      "verse_number": 49
    },
    {
      "id": "d85f366b-dd13-4168-b30a-053e64036207",
      "m1": "بایران گذاریم ز ایدر سپاه",
      "m2": "نمانیم تخت و نه تاج و نه شاه",
      "verse_number": 50
    },
    {
      "id": "b9470a79-8118-408c-ad08-cea59662b9d4",
      "m1": "بر و بومشان پاک و یران کنیم",
      "m2": "نه جنگ یلان جنگ شیران کنیم",
      "verse_number": 51
    },
    {
      "id": "a50a9c4d-ea17-44bc-afb8-517884001604",
      "m1": "زن و کودک خرد و پیر و جوان",
      "m2": "نه شاه و کنارنگ و نه پهلوان",
      "verse_number": 52
    },
    {
      "id": "b9d71b4d-69f2-47ac-8e84-6b3cd8d51a2b",
      "m1": "بایران نمانم بر و بوم و جای",
      "m2": "نه کاخ و نه ایوان و نه چارپای",
      "verse_number": 53
    },
    {
      "id": "eb0a9409-895e-4b7e-a0fc-e404221ec14c",
      "m1": "ببد روز چندین چه باید گذاشت",
      "m2": "غم و درد و تیمار بیهوده داشت",
      "verse_number": 54
    },
    {
      "id": "406acae3-276d-4cc1-80e4-a7b6cc46695a",
      "m1": "یک امشب گشاده مدارید راه",
      "m2": "که ایشان برانند زین رزمگاه",
      "verse_number": 55
    },
    {
      "id": "1933a475-6375-42be-80c3-bff9f6d2a47f",
      "m1": "چو باد سپیده دمان بردمد",
      "m2": "سپه جمله باید که اندر چمد",
      "verse_number": 56
    },
    {
      "id": "47fc96a9-ec10-4ffc-9b33-3449fefbdf46",
      "m1": "تلی کشته بینی ببالای کوه",
      "m2": "تو فردا ز گردان ایران گروه",
      "verse_number": 57
    },
    {
      "id": "2b589dc2-e208-4bbf-b67d-63c3e70d6174",
      "m1": "بدانسان که ایرانیان سربسر",
      "m2": "ازین پی نبینند جز مویه گر",
      "verse_number": 58
    },
    {
      "id": "44ff0bcf-d210-45e0-a23e-e8ce71344f8a",
      "m1": "بدو گفت خاقان جزین رای نیست",
      "m2": "بگیتی چو تو لشکر آرای نیست",
      "verse_number": 59
    },
    {
      "id": "d6f16b2a-2721-423b-8652-fef4f8ca042d",
      "m1": "همه نامدارن بدین هم سخن",
      "m2": "که کاموس شیراوژن افگند بن",
      "verse_number": 60
    },
    {
      "id": "3d50135d-19a9-4639-86a6-dfeffd15e4f4",
      "m1": "برفتند وز جای برخاستند",
      "m2": "همه شب همی لشکر آراستند",
      "verse_number": 61
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 61
  },
  "order_in_parent": 12
}