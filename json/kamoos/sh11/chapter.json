{
  "id": "a933d518-7301-4eb8-b10d-6d9afb5c95d0",
  "title": "بخش ۱۱",
  "verses": [
    {
      "id": "f0b02878-7249-46ea-8901-3bf6e781b523",
      "m1": "شبی داغ دل پر ز تیمار طوس",
      "m2": "بخواب اندر آمد گه زخم کوس",
      "verse_number": 1
    },
    {
      "id": "e3d45421-5fa4-46a8-aced-481c6f07aeaf",
      "m1": "چنان دید روشن روانش بخواب",
      "m2": "که رخشنده شمعی برآمد ز آب",
      "verse_number": 2
    },
    {
      "id": "618b1bfa-0940-4f5e-9793-6466324a2c31",
      "m1": "بر شمع رخشان یکی تخت عاج",
      "m2": "سیاوش بران تخت با فر و تاج",
      "verse_number": 3
    },
    {
      "id": "f2238451-f2e4-42f9-b341-43cb3ddd66d5",
      "m1": "لبان پر ز خنده زبان چربگوی",
      "m2": "سوی طوس کردی چو خورشید روی",
      "verse_number": 4
    },
    {
      "id": "4eec7042-6c91-4091-a8d8-6b90868964a4",
      "m1": "که ایرانیان را هم ایدر بدار",
      "m2": "که پیروزگر باشی از کارزار",
      "verse_number": 5
    },
    {
      "id": "f7ea0e76-b8f7-4bbb-999a-779b0a38a01f",
      "m1": "بگو در زیان هیچ غمگین مشو",
      "m2": "که ایدر یکی گلستانست نو",
      "verse_number": 6
    },
    {
      "id": "5da64b62-68e8-409f-bd3a-cbd8d749bd86",
      "m1": "بزیر گل اندر همی میخوریم",
      "m2": "چه دانیم کین باده تا کی خوریم",
      "verse_number": 7
    },
    {
      "id": "ea8f38ad-f642-4085-a9f1-077d4d63a0e6",
      "m1": "ز خواب اندر آمد شده شاد دل",
      "m2": "ز درد و غمان گشته آزاد دل",
      "verse_number": 8
    },
    {
      "id": "2c2bca81-ca1e-481e-b2ca-2f981d1690c6",
      "m1": "بگودرز گفت ای جهان پهلوان",
      "m2": "یکی خواب دیدم بروشن روان",
      "verse_number": 9
    },
    {
      "id": "510bf3cd-fa02-4bed-a71f-388c17b3b4b7",
      "m1": "نگه کن که رستم چو باد دمان",
      "m2": "بیاید بر ما زمان تا زمان",
      "verse_number": 10
    },
    {
      "id": "71c00f2f-3e5d-4f69-902f-5b3a42d6da08",
      "m1": "بفرمود تا برکشیدند نای",
      "m2": "بجنبید بر کوه لشکر ز جای",
      "verse_number": 11
    },
    {
      "id": "a9581c8e-d7f2-4b65-955a-e851fcacec33",
      "m1": "ببستند گردان ایران میان",
      "m2": "برافراختند اختر کاویان",
      "verse_number": 12
    },
    {
      "id": "2602e068-4695-41b7-b14f-04136c681729",
      "m1": "بیاورد زان روی پیران سپاه",
      "m2": "شد از گرد خورشید تابان سیاه",
      "verse_number": 13
    },
    {
      "id": "e3963fa8-af3d-48db-b5f2-369bb500e407",
      "m1": "از آواز گردان و باران تیر",
      "m2": "همی چشم خورشید شد خیره خیر",
      "verse_number": 14
    },
    {
      "id": "cffca41a-c13e-4164-b53d-7fb241ea4c9d",
      "m1": "دو لشکر بروی اندر آورده روی",
      "m2": "ز گردان نشد هیچ کس جنگجوی",
      "verse_number": 15
    },
    {
      "id": "977ad915-aa99-4f92-bfd0-598071ccef6f",
      "m1": "چنین گفت هومان بپیران که جنگ",
      "m2": "همی جست باید چه جویی درنگ",
      "verse_number": 16
    },
    {
      "id": "96f07ece-a858-4e8e-bca2-1e902973182e",
      "m1": "نه لشکر بدشت شکار اندرند",
      "m2": "که اسپان ما زیر بار اندرند",
      "verse_number": 17
    },
    {
      "id": "f9a1f8fa-c7a9-43c2-ba49-41b79aa0a162",
      "m1": "بدو گفت پیران که تندی مکن",
      "m2": "نه روز شتابست و گاه سخن",
      "verse_number": 18
    },
    {
      "id": "57109f92-4988-49db-a6bb-c43ae6f7abb2",
      "m1": "سه تن دوش با خوار مایه سپاه",
      "m2": "برفتند بیگاه زین رزمگاه",
      "verse_number": 19
    },
    {
      "id": "3bac240d-3987-4878-abd8-2d82888c5c64",
      "m1": "چو شیران جنگی و ما چون رمه",
      "m2": "که از کوهسار اندر آید دمه",
      "verse_number": 20
    },
    {
      "id": "038feca5-a78e-48c4-9dde-673c3896afd7",
      "m1": "همه دشت پر جوی خون یافتیم",
      "m2": "سر نامداران نگون یافتیم",
      "verse_number": 21
    },
    {
      "id": "976e3dd5-e40c-4779-962a-4408a6500ee4",
      "m1": "یکی کوه دارند خارا و خشک",
      "m2": "همی خار بویند اسپان چو مشک",
      "verse_number": 22
    },
    {
      "id": "b0f8c890-72e4-4afd-abd4-fb84e3208817",
      "m1": "بمان تا بران سنگ پیچان شوند",
      "m2": "چو بیچاره گردند بیجان شوند",
      "verse_number": 23
    },
    {
      "id": "c7874865-fba7-4207-8510-9d220176e3ff",
      "m1": "گشاده نباید که دارید راه",
      "m2": "دو رویه پس و پیش این رزمگاه",
      "verse_number": 24
    },
    {
      "id": "b7baf97a-fff6-4ee9-99c3-71e0b74e1c33",
      "m1": "چو بیرنج دشمن بچنگ آیدت",
      "m2": "چو بشتابیش کار تنگ آیدت",
      "verse_number": 25
    },
    {
      "id": "d40ec319-acd5-4350-8b71-9640e1afbafb",
      "m1": "چرا جست باید همی کارزار",
      "m2": "طلایه برین دشت بس صد سوار",
      "verse_number": 26
    },
    {
      "id": "49032ecf-f103-404a-9695-5a550486b454",
      "m1": "بباشیم تا دشمن از آب و نان",
      "m2": "شود تنگ و زنهار خواهد بجان",
      "verse_number": 27
    },
    {
      "id": "9dcc2a5e-1069-4243-88ac-99453e0c5890",
      "m1": "مگر خاکگر سنگ خارا خورند",
      "m2": "چو روزی سرآید خورند و مرند",
      "verse_number": 28
    },
    {
      "id": "374513ab-a896-4128-8c80-8b13c94467c6",
      "m1": "سوی خیمه رفتند زان رزمگاه",
      "m2": "طلایه بیامد به پیش سپاه",
      "verse_number": 29
    },
    {
      "id": "499d2eb8-b98c-4610-81a4-efa6fca8f0a3",
      "m1": "گشادند گردان سراسر کمر",
      "m2": "بخوان و بخوردن نهادند سر",
      "verse_number": 30
    },
    {
      "id": "abf22b54-1cee-45be-8258-4087f6ccc1e7",
      "m1": "بلشکر گه آمد سپهدار طوس",
      "m2": "پر از خون دل و روی چون سندروس",
      "verse_number": 31
    },
    {
      "id": "4e0b2067-489a-44c3-9144-2bb77036a63e",
      "m1": "بگودرز گفت این سخن تیره گشت",
      "m2": "سر بخت ایرانیان خیره گشت",
      "verse_number": 32
    },
    {
      "id": "1f97b259-1e83-4eb1-81b4-f316db87a997",
      "m1": "همه گرد بر گرد ما لشکرست",
      "m2": "خور بارگی خارگر خاورست",
      "verse_number": 33
    },
    {
      "id": "f1e1f702-ca93-4f94-a98a-45b18d0828f3",
      "m1": "سپه را خورش بس فراوان نماند",
      "m2": "جز از گرز و شمشیر درمان نماند",
      "verse_number": 34
    },
    {
      "id": "73004de6-4daf-4cb0-a6b3-dba9ebcaf9d5",
      "m1": "بشبگیر شمشیرها برکشیم",
      "m2": "همه دامن کوه لشکر کشیم",
      "verse_number": 35
    },
    {
      "id": "18d4ba16-0a35-41f3-912c-8ce0494048d8",
      "m1": "اگر اختر نیک یاری دهد",
      "m2": "بریشان مرا کامگاری دهد",
      "verse_number": 36
    },
    {
      "id": "705a52eb-09d6-4f94-8751-0f9cfbf25b0a",
      "m1": "ور ایدون کجا داور آسمان",
      "m2": "بشمشیر بر ما سرآرد زمان",
      "verse_number": 37
    },
    {
      "id": "5f82cbd5-4b77-4dbf-983e-afa466c9f7c4",
      "m1": "ز بخش جهانآفرین بیش و کم",
      "m2": "نباشد مپیمای بر خیره دم",
      "verse_number": 38
    },
    {
      "id": "fa52a607-a3cf-4aef-b031-6dc94f93b375",
      "m1": "مرا مرگ خوشتر بنام بلند",
      "m2": "ازین زیستن با هراس و گزند",
      "verse_number": 39
    },
    {
      "id": "bd211b20-3f6e-4850-b0c2-5ce5193bbe47",
      "m1": "برین برنهادند یکسر سخن",
      "m2": "که سالار نیک اختر افگند بن",
      "verse_number": 40
    },
    {
      "id": "3c83be2d-cd66-4c9a-aaba-be56bf1381c8",
      "m1": "چو خورشید برزد ز خرچنگ چنگ",
      "m2": "بدرید پیراهن مشک رنگ",
      "verse_number": 41
    },
    {
      "id": "156de426-fcdd-4c7d-926a-21071b8c9833",
      "m1": "به پیران فرستاده آمد ز شاه",
      "m2": "که آمد ز هر جای بیمر سپاه",
      "verse_number": 42
    },
    {
      "id": "665d206f-08c8-42a2-8df9-a7efc530b020",
      "m1": "سپاهی که دریای چین را ز گرد",
      "m2": "کند چون بیابان بروز نبرد",
      "verse_number": 43
    },
    {
      "id": "207a1842-71d7-478f-a866-21ac5d1f1aa3",
      "m1": "نخستین سپهدار خاقان چین",
      "m2": "که تختش همی برنتابد زمین",
      "verse_number": 44
    },
    {
      "id": "916be108-f1fb-4195-ab44-e5c8b59721e8",
      "m1": "تنش زور دارد چو صد نره شیر",
      "m2": "سر ژنده پیل اندر آرد بزیر",
      "verse_number": 45
    },
    {
      "id": "41a85776-6a5e-4aed-928a-989476e5acf8",
      "m1": "یکی مهتر از ماورالنهر بر",
      "m2": "که بگذارد از چرخ گردنده سر",
      "verse_number": 46
    },
    {
      "id": "0170d759-b2b3-400f-bc8a-d580f0d2ad06",
      "m1": "ببالا چو سرو و بدیدار ماه",
      "m2": "جهانگیر و نازان بدو تاج و گاه",
      "verse_number": 47
    },
    {
      "id": "29293c70-b1a8-430c-930a-9c4f230ad7e4",
      "m1": "سر سرافرازان و کاموس نام",
      "m2": "برآرد ز گودرز و از طوس نام",
      "verse_number": 48
    },
    {
      "id": "507bf26a-e3bb-4076-bb0b-d89a977932ef",
      "m1": "ز مرز سپیجاب تا دشت روم",
      "m2": "سپاهی که بود اندر آباد بوم",
      "verse_number": 49
    },
    {
      "id": "3da17d48-fcce-453e-b9ab-85fd5facda46",
      "m1": "فرستادم اینک سوی کارزار",
      "m2": "برآرند از طوس و خسرو دمار",
      "verse_number": 50
    },
    {
      "id": "4c8b2eff-cb9e-4896-8056-34dd52ce9422",
      "m1": "چو بشنید پیران بتوران سپاه",
      "m2": "چنین گفت کای سرفرازان شاه",
      "verse_number": 51
    },
    {
      "id": "b0a5d0ff-3fe3-4ef5-b379-4d2ca998b176",
      "m1": "بدین مژدهٔ شاه پیر و جوان",
      "m2": "همه شاد باشید و روشنروان",
      "verse_number": 52
    },
    {
      "id": "1993228d-11fb-429c-8efc-eee99fc5d3a9",
      "m1": "بباید کنون دل ز تیمار شست",
      "m2": "بایران نمانم بر و بوم و رست",
      "verse_number": 53
    },
    {
      "id": "3de9954c-7016-46f0-80db-d277cdc151a1",
      "m1": "سر از رزم و از رنج و کین خواستن",
      "m2": "برآسود وز لشکر آراستن",
      "verse_number": 54
    },
    {
      "id": "b76a225e-65e7-48a3-ad17-a286b285ac19",
      "m1": "بایران و توران و بر خشک و آب",
      "m2": "نبینند جز کام افراسیاب",
      "verse_number": 55
    },
    {
      "id": "31935bd3-6a90-41a3-919f-976de0627ca7",
      "m1": "ز لشکر بر پهلوان پیش رو",
      "m2": "بمژده بیامد همی نو بنو",
      "verse_number": 56
    },
    {
      "id": "a795799c-9170-4f3c-ba15-ebe2082ca935",
      "m1": "بگفتند کای نامور پهلوان",
      "m2": "همیشه بزی شاد و روشنروان",
      "verse_number": 57
    },
    {
      "id": "79c0a791-1f5a-482c-9bb0-4be4409289c1",
      "m1": "بدیدار شاهان دلت شاددار",
      "m2": "روانت ز اندیشه آزاد دار",
      "verse_number": 58
    },
    {
      "id": "c317c336-ee56-49ab-b179-c7d2c372333f",
      "m1": "ز کشمیر تا برتر از رود شهد",
      "m2": "درفش و سپاهست و پیلان و مهد",
      "verse_number": 59
    },
    {
      "id": "cbe6f922-899e-403c-af7a-6f0029028fc7",
      "m1": "نخست اندر آیم ز خاقان چین",
      "m2": "که تاجش سپهرست و تختش زمین",
      "verse_number": 60
    },
    {
      "id": "190e5653-dbbd-4086-ae1f-b39d8b88d1da",
      "m1": "چو منشور جنگی که با تیغ اوی",
      "m2": "بخاک اندر آید سر جنگجوی",
      "verse_number": 61
    },
    {
      "id": "260b7a2c-5dd5-41f4-a80b-bb9a8b643900",
      "m1": "دلاور چو کاموس شمشیرزن",
      "m2": "که چشمش ندیدست هرگز شکن",
      "verse_number": 62
    },
    {
      "id": "c1fe6de6-cc95-4601-ad87-9a7c472c5523",
      "m1": "همه کارهای شگرف آورد",
      "m2": "چو خشم آورد باد و برف آورد",
      "verse_number": 63
    },
    {
      "id": "9e5db733-a8af-4e49-9bc0-4469a1a5ad31",
      "m1": "چو خشنود باشد بهار آردت",
      "m2": "گل و سنبل جویبار آردت",
      "verse_number": 64
    },
    {
      "id": "72e72b8e-0743-4d44-a5ea-a28b5682ac2a",
      "m1": "ز سقلاب چون کندر شیر مرد",
      "m2": "چو پیروز کانی سپهر نبرد",
      "verse_number": 65
    },
    {
      "id": "52c927c2-13e5-405c-858c-0cdc1945c6c0",
      "m1": "چو سگسار غرچه چو شنگل ز هند",
      "m2": "هوا پردرفش و زمین پر پرند",
      "verse_number": 66
    },
    {
      "id": "3751e2bf-86d6-4b5d-85b9-ec6fdb4c21b9",
      "m1": "چغانی چو فرطوس لشکر فروز",
      "m2": "گهار گهانی گو گردسوز",
      "verse_number": 67
    },
    {
      "id": "fd920ee5-6621-4001-ae4b-7a473bc5b4db",
      "m1": "شمیران شگنی و گردوی وهر",
      "m2": "پراگنده بر نیزه و تیغ زهر",
      "verse_number": 68
    },
    {
      "id": "9c68d64a-9032-47d3-bb8f-3aa49829494f",
      "m1": "تو اکنون سرافراز و رامش پذیر",
      "m2": "کزین مژده بر نا شود مرد پیر",
      "verse_number": 69
    },
    {
      "id": "3e1b6388-6844-42e3-87b1-3ffd61650407",
      "m1": "ز لشکر توی پهلو و پیش رو",
      "m2": "همیشه بزی شاد و فرمانت نو",
      "verse_number": 70
    },
    {
      "id": "d38a961d-a4db-41a5-a178-ec70f3151114",
      "m1": "دل و جان پیران پر از خنده گشت",
      "m2": "تو گفتی مگر مرده بد زنده گشت",
      "verse_number": 71
    },
    {
      "id": "f9cce871-ccc3-4f6e-8566-da09e1d3491b",
      "m1": "بهومان چنین گفت پیران که من",
      "m2": "پذیره شوم پیش این انجمن",
      "verse_number": 72
    },
    {
      "id": "738e98b7-dce5-4899-8b59-9a6a4da980c4",
      "m1": "که ایشان ز راه دراز آمدند",
      "m2": "پراندیشه و رزمساز آمدند",
      "verse_number": 73
    },
    {
      "id": "75ae9afd-77c1-4b2c-9b0c-c1318d139b68",
      "m1": "ازین آمدن بینیازند سخت",
      "m2": "خداوند تاجاند و زیبای تخت",
      "verse_number": 74
    },
    {
      "id": "cdc0f6e4-4a1e-4b43-b19b-b69105c83040",
      "m1": "ندارند سر کم ز افراسیاب",
      "m2": "که با تخت و گنجاند و با جاه و آب",
      "verse_number": 75
    },
    {
      "id": "ebc05721-83c5-4847-8ce0-aa9e50eae1de",
      "m1": "شوم تا ببینم که چند و چیند",
      "m2": "سپهبد کدامند و گردان کیند",
      "verse_number": 76
    },
    {
