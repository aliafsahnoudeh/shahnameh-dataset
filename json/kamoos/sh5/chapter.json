{
  "id": "879e3fff-4a02-429a-92e9-95429f90947b",
  "title": "بخش ۵",
  "verses": [
    {
      "id": "05d7d5c5-08ab-427d-b454-ca02295948df",
      "m1": "بیاراست لشکر سپهدار طوس",
      "m2": "بپیلان جنگی و مردان و کوس",
      "verse_number": 1
    },
    {
      "id": "f52812b6-5d6e-4158-a9b5-6ef6f4c1559f",
      "m1": "پیاده سوی کوه شد با بنه",
      "m2": "سپهدار گودرز بر میمنه",
      "verse_number": 2
    },
    {
      "id": "6c950cc7-0dcc-43b0-9cf4-625958c07415",
      "m1": "رده برکشیده همه یکسره",
      "m2": "چو رهام گودرز بر میسره",
      "verse_number": 3
    },
    {
      "id": "2b64e819-29eb-43d6-963a-68b34298b2c4",
      "m1": "ز نالیدن کوس با کرنای",
      "m2": "همی آسمان اندر آمد ز جای",
      "verse_number": 4
    },
    {
      "id": "f6dfa9ef-3ad3-499a-a422-7416af12a1bd",
      "m1": "دل چرخ گردان بدو چاک شد",
      "m2": "همه کام خورشید پرخاک شد",
      "verse_number": 5
    },
    {
      "id": "4612b729-8eff-4507-981e-9d805085058f",
      "m1": "چنان شد که کس روی هامون ندید",
      "m2": "ز بس گرد کز رزمگه بردمید",
      "verse_number": 6
    },
    {
      "id": "4fbc99df-9c24-4beb-a37b-13c909ac4ff7",
      "m1": "ببارید الماس از تیره میغ",
      "m2": "همی آتش افروخت از گرز و تیغ",
      "verse_number": 7
    },
    {
      "id": "b6e69a25-0bd9-4926-8fe8-e522431d1684",
      "m1": "سنانهای رخشان و تیغ سران",
      "m2": "درفش از بر و زیر گرز گران",
      "verse_number": 8
    },
    {
      "id": "766f3c85-96de-4b77-8f7a-a538035545a7",
      "m1": "هوا گفتی از گرز و از آهنست",
      "m2": "زمین یکسر از نعل در جوشنست",
      "verse_number": 9
    },
    {
      "id": "19df62fb-5b3f-4ca3-81b6-fa6f768956a8",
      "m1": "چو دریای خون شد همه دشت و راغ",
      "m2": "جهان چون شب و تیغها چون چراغ",
      "verse_number": 10
    },
    {
      "id": "b1391320-da7e-4d79-b48c-73efa34f32c1",
      "m1": "ز بس نالهٔ کوس با کرنای",
      "m2": "همی کس ندانست سر را ز پای",
      "verse_number": 11
    },
    {
      "id": "b4af8b1c-06e2-4fd9-932e-2f3944b39a42",
      "m1": "سپهبد به گودرز گفت آن زمان",
      "m2": "که تاریک شد گردش آسمان",
      "verse_number": 12
    },
    {
      "id": "f60c7800-5eaa-4bc2-bc1e-be4ca2e6237b",
      "m1": "مرا گفته بود این ستارهشناس",
      "m2": "که امروز تا شب گذشته سه پاس",
      "verse_number": 13
    },
    {
      "id": "717b848c-3932-4686-821d-3712608f73e6",
      "m1": "ز شمشیر گردان چو ابر سیاه",
      "m2": "همی خون فشاند به آوردگاه",
      "verse_number": 14
    },
    {
      "id": "06e47e55-038a-41ec-91dd-0cd5bce25e1e",
      "m1": "سرانجام ترسم که پیروزگر",
      "m2": "نباشد مگر دشمن کینه ور",
      "verse_number": 15
    },
    {
      "id": "eb7da225-f8aa-447c-ab1b-e7d25f087f9e",
      "m1": "چو شیدوش و رهام و گستهم و گیو",
      "m2": "زرهدار خراد و برزین نیو",
      "verse_number": 16
    },
    {
      "id": "019da0c3-c0fa-4c60-912f-5ad283c04d8e",
      "m1": "ز صف در میان سپاه آمدند",
      "m2": "جگر خسته و کینهخواه آمدند",
      "verse_number": 17
    },
    {
      "id": "22f186d4-f25f-4690-9daf-2861419e9b9d",
      "m1": "بابر اندر آمد ز هر سو غریو",
      "m2": "بسان شب تار و انبوه دیو",
      "verse_number": 18
    },
    {
      "id": "8c5a1ce1-b102-469a-a229-e93bd7a39f0b",
      "m1": "وزان روی هومان بکردار کوه",
      "m2": "بیاورد لشکر همه همگروه",
      "verse_number": 19
    },
    {
      "id": "61790ffe-5728-4d1e-b702-9f7bbe63cf90",
      "m1": "وزان پس گزیدند مردان مرد",
      "m2": "که بردشت سازند جای نبرد",
      "verse_number": 20
    },
    {
      "id": "4a68f97d-3211-4015-9f19-283e4ff05846",
      "m1": "گرازه سر گیوگان با نهل",
      "m2": "دو گرد گرانمایهٔ شیردل",
      "verse_number": 21
    },
    {
      "id": "4a150348-9481-41cb-a7e4-6786122bb8df",
      "m1": "چو رهام گودرز و فرشیدورد",
      "m2": "چو شیدوش و لهاک شد هم نبرد",
      "verse_number": 22
    },
    {
      "id": "be8e9e7c-0879-4932-9096-0e8f25b32f93",
      "m1": "ابا بیژن گیو کلباد را",
      "m2": "که بر هم زدی آتش و باد را",
      "verse_number": 23
    },
    {
      "id": "c20a93cf-0f3d-45fe-afa5-d571a4713fc6",
      "m1": "ابا شطرخ نامور گیو را",
      "m2": "دو گرد گرانمایهٔ نیو را",
      "verse_number": 24
    },
    {
      "id": "91cdd982-d028-4904-a492-36f6241afed4",
      "m1": "چو گودرز و پیران و هومان و طوس",
      "m2": "نبد هیچ پیدا درنگ و فسوس",
      "verse_number": 25
    },
    {
      "id": "105eb959-4247-44e0-a2b8-47004b76c312",
      "m1": "چنین گفت هومان که امروز کار",
      "m2": "نباید که چون دی بود کارزار",
      "verse_number": 26
    },
    {
      "id": "a2fd38e9-ff28-4f69-b91f-34c95d91e54c",
      "m1": "همه جان شیرین بکف برنهید",
      "m2": "چو من برخروشم دمید و دهید",
      "verse_number": 27
    },
    {
      "id": "5c314bc7-a02e-41c6-ab2d-e9bd06724620",
      "m1": "تهی کرد باید ازیشان زمین",
      "m2": "نباید که آیند زین پس بکین",
      "verse_number": 28
    },
    {
      "id": "2a1dfb52-ddad-4408-ba6c-683234c94f16",
      "m1": "بپیش اندر آمد سپهدار طوس",
      "m2": "پیاده بیاورد و پیلان و کوس",
      "verse_number": 29
    },
    {
      "id": "9b282301-6cad-43ac-9991-eb0ed18ca256",
      "m1": "صفی برکشیدند پیش سوار",
      "m2": "سپردار و ژوپینور و نیزهدار",
      "verse_number": 30
    },
    {
      "id": "f7391f8f-3722-4fc3-926f-2019f41ddc08",
      "m1": "مجنبید گفت ایچ از جای خویش",
      "m2": "سپر با سنان اندرارید پیش",
      "verse_number": 31
    },
    {
      "id": "694f8a78-f155-49df-a252-f7bcdcc74429",
      "m1": "ببینیم تا این نبرده سران",
      "m2": "چگونه گزارند گرز گران",
      "verse_number": 32
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 32
  }
}