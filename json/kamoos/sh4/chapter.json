{
  "id": "9eac7a6b-9eb2-43ca-b605-96f7f6785774",
  "title": "بخش ۴",
  "verses": [
    {
      "id": "0d5b03b2-a012-4d3b-92e3-1d09a3698982",
      "m1": "بهاسپ عقاب اندر آورد پای",
      "m2": "برانگیخت آن بارگی را ز جای",
      "verse_number": 1
    },
    {
      "id": "4c3e69a0-9c95-4326-b4bd-6a64bcc7529a",
      "m1": "تو گفتی یکی بارهٔ آهنست",
      "m2": "وگر کوه البرز در جوشنست",
      "verse_number": 2
    },
    {
      "id": "810317ad-872a-494f-a736-02d7aae42706",
      "m1": "به پیش سپاه اندر آمد بجنگ",
      "m2": "یکی خشت رخشان گرفته بچنگ",
      "verse_number": 3
    },
    {
      "id": "b90236b8-19f3-4b74-b494-deb69973ec77",
      "m1": "بجنبید طوس سپهبد ز جای",
      "m2": "جهان پر شد از نالهٔ کر نای",
      "verse_number": 4
    },
    {
      "id": "e8778ddf-545c-4104-98e7-4414eab63a55",
      "m1": "بهومان چنین گفت کای شوربخت",
      "m2": "ز پالیز کین برنیامد درخت",
      "verse_number": 5
    },
    {
      "id": "21282aea-af4f-4257-9181-4b3ceb1d4014",
      "m1": "نمودم بارژنگ یک دست برد",
      "m2": "که بود از شما نامبردار و گرد",
      "verse_number": 6
    },
    {
      "id": "ebad899f-44fb-43f2-8c93-78422405dd21",
      "m1": "تو اکنون همانا بکین آمدی",
      "m2": "که با خشت بر پشت زین آمدی",
      "verse_number": 7
    },
    {
      "id": "ed061165-4314-49f3-93e0-beacbcf51cd5",
      "m1": "بجان و سر شاه ایران سپاه",
      "m2": "که بیجوشن و گرز و رومی کلاه",
      "verse_number": 8
    },
    {
      "id": "3f128c65-1cd3-4eca-b96b-037423aaee0b",
      "m1": "بجنگ تو آیم بسان پلنگ",
      "m2": "که از کوه یازد بنخچیر چنگ",
      "verse_number": 9
    },
    {
      "id": "3229f1a7-5579-465d-8d8c-534426be3c3a",
      "m1": "ببینی تو پیکار مردان مرد",
      "m2": "چو آورد گیرم بدشت نبرد",
      "verse_number": 10
    },
    {
      "id": "007eeff9-fc7d-42f4-97c8-3f2ee7c011f0",
      "m1": "چنین پاسخ آورد هومان بدوی",
      "m2": "که بیشی نه خوبست بیشی مجوی",
      "verse_number": 11
    },
    {
      "id": "ec02415d-a807-4daf-adfc-916bc5e5ae1c",
      "m1": "گر ایدونک بیچارهای را زمان",
      "m2": "بدست تو آمد مشو در گمان",
      "verse_number": 12
    },
    {
      "id": "48e2dc54-da77-4073-9216-0a8980c58fec",
      "m1": "بجنگ من ارژنگ روز نبرد",
      "m2": "کجا داشتی خویشتن را بمرد",
      "verse_number": 13
    },
    {
      "id": "c07aeb1c-0808-4826-94e4-a4dfb70ba8de",
      "m1": "دلیران لشکر ندارند شرم",
      "m2": "نجوشد یکی را برگ خون گرم",
      "verse_number": 14
    },
    {
      "id": "99eb2d1d-0410-4d85-8e3b-10c762ddd002",
      "m1": "که پیکار ایشان سپهبد کند",
      "m2": "برزم اندرون دستشان بد کند",
      "verse_number": 15
    },
    {
      "id": "f3686785-f1db-4ea6-8e7e-d3d0d4929bd1",
      "m1": "کجا بیژن و گیو آزادگان",
      "m2": "جهانگیر گودرز کشوادگان",
      "verse_number": 16
    },
    {
      "id": "11743302-1ef9-4d13-86e3-8045b11895e9",
      "m1": "تو گر پهلوانی ز قلب سپاه",
      "m2": "چرا آمدستی بدین رزمگاه",
      "verse_number": 17
    },
    {
      "id": "73bd1a0e-f384-4dad-af62-ee2461c67715",
      "m1": "خردمند بیگانه خواند ترا",
      "m2": "هشیوار دیوانه خواند ترا",
      "verse_number": 18
    },
    {
      "id": "773563f1-9d87-4648-8ea3-f75c2123be13",
      "m1": "تو شو اختر کاویانی بدار",
      "m2": "سپهبد نیاید سوی کارزار",
      "verse_number": 19
    },
    {
      "id": "ab3950e6-0c83-4406-884e-aeb9b5755ff3",
      "m1": "نگه کن که خلعت کرا داد شاه",
      "m2": "ز گردان که جوید نگین و کلاه",
      "verse_number": 20
    },
    {
      "id": "2d69bd6a-d084-4133-9235-46177cc0b2f3",
      "m1": "بفرمای تا جنگ شیر آورند",
      "m2": "زبردست را دست زیر آورند",
      "verse_number": 21
    },
    {
      "id": "0b532ee3-288f-4e43-b245-0159b677897d",
      "m1": "اگر تو شوی کشته بر دست من",
      "m2": "بد آید بدان نامدار انجمن",
      "verse_number": 22
    },
    {
      "id": "b6ecb179-924d-4a5d-bf7a-1dbcfae1f528",
      "m1": "سپاه تو بییار و بیجان شوند",
      "m2": "اگر زنده مانند پیچان شوند",
      "verse_number": 23
    },
    {
      "id": "ed48c5a8-2399-4946-83f5-14e57b446c90",
      "m1": "و دیگر که گر بشنوی گفت راست",
      "m2": "روان و دلم بر زبانم گواست",
      "verse_number": 24
    },
    {
      "id": "6d4a6e78-0c08-411e-8bd8-751be576e7ad",
      "m1": "که پر درد باشم ز مردان مرد",
      "m2": "که پیش من آیند روز نبرد",
      "verse_number": 25
    },
    {
      "id": "f600022a-8fbf-4eea-838c-c6a746c7261b",
      "m1": "پس از رستم زال سام سوار",
      "m2": "ندیدم چو تو نیز یک نامدار",
      "verse_number": 26
    },
    {
      "id": "0cbf5fa6-e389-4bd7-a9b0-217857f0e107",
      "m1": "پدر بر پدر نامبردار و شاه",
      "m2": "چو تو جنگجویی نیاید سپاه",
      "verse_number": 27
    },
    {
      "id": "03a79041-a490-4b03-b3bb-925bf0143538",
      "m1": "تو شو تا ز لشکر یکی نامجوی",
      "m2": "بیاید بروی اندر آریم روی",
      "verse_number": 28
    },
    {
      "id": "d57b2f5f-03d3-4f01-81aa-37d698f2860f",
      "m1": "بدو گفت طوس ای سرافراز مرد",
      "m2": "سپهبد منم هم سوار نبرد",
      "verse_number": 29
    },
    {
      "id": "5fe9c96d-75b6-43b5-abdc-8d6906ae0eea",
      "m1": "تو هم نامداری ز توران سپاه",
      "m2": "چرا رای کردی به آوردگاه",
      "verse_number": 30
    },
    {
      "id": "578409a3-14f8-480a-b548-1032d4ba132b",
      "m1": "دلت گر پذیرد یکی پند من",
      "m2": "بجویی بدین کار پیوند من",
      "verse_number": 31
    },
    {
      "id": "cad6b91b-fc8c-4eeb-8502-5d357a1a51c4",
      "m1": "کزین کینه تا زنده ماند یکی",
      "m2": "نیاسود خواهد سپاه اندکی",
      "verse_number": 32
    },
    {
      "id": "16908e0c-f11b-4556-863b-bf742e0cbbb0",
      "m1": "تو با خویش وپیوند و چندین سوار",
      "m2": "همه پهلوان و همه نامدار",
      "verse_number": 33
    },
    {
      "id": "54099b6e-8a9b-4965-b299-4a6f974ca945",
      "m1": "بخیره مده خویشتن را بباد",
      "m2": "بباید که پند من آیدت یاد",
      "verse_number": 34
    },
    {
      "id": "f3c88e44-59b9-4a24-aaa5-b7eeece3267a",
      "m1": "سزاوار کشتن هرآنکس که هست",
      "m2": "بمان تا بیازند بر کینه دست",
      "verse_number": 35
    },
    {
      "id": "01ac022f-4024-4be2-9384-c65a58a9d9ce",
      "m1": "کزین کینه مرد گنهکار هیچ",
      "m2": "رهایی نیابد خرد را مپیچ",
      "verse_number": 36
    },
    {
      "id": "7674210f-4427-45a9-8daa-db14c0b65c0f",
      "m1": "مرا شاه ایران چنین داد پند",
      "m2": "که پیران نباید که یابد گزند",
      "verse_number": 37
    },
    {
      "id": "3ed1b58d-6d81-443d-8e71-398f0b1867a0",
      "m1": "که او ویژه پروردگار منست",
      "m2": "جهاندیده و دوستدار منست",
      "verse_number": 38
    },
    {
      "id": "8f2a4f77-2ea8-4be8-bd62-55fd41055013",
      "m1": "به بیداد بر خیره با او مکوش",
      "m2": "نگه کن که دارد بپند تو گوش",
      "verse_number": 39
    },
    {
      "id": "3bec8b94-ad40-46aa-957a-c0fbcc929cb2",
      "m1": "چنین گفت هومان به بیداد و داد",
      "m2": "چو فرمان دهد شاه فرخ نژاد",
      "verse_number": 40
    },
    {
      "id": "bcf6d54f-ab3a-45f7-a41b-6dc33122c530",
      "m1": "بران رفت باید ببیچارگی",
      "m2": "سپردن بدو دل بیکبارگی",
      "verse_number": 41
    },
    {
      "id": "16689a5c-0823-4ae9-9191-dee1b0ee7109",
      "m1": "همان رزم پیران نه بر آرزوست",
      "m2": "که او راد و آزاده و نیک خوست",
      "verse_number": 42
    },
    {
      "id": "6bc0c44a-dae6-4f1e-9d6c-e7003c8b25e9",
      "m1": "بدین گفت و گوی اندرون بود طوس",
      "m2": "که شد گیو را روی چون سندروس",
      "verse_number": 43
    },
    {
      "id": "59c61f71-8af8-4d4f-b39d-9bf36dbcb832",
      "m1": "ز لشکر بیامد بکردار باد",
      "m2": "چنین گفت کای طوس فرخ نژاد",
      "verse_number": 44
    },
    {
      "id": "000c6666-02bb-4986-ad0d-9a88c444a6fa",
      "m1": "فریبنده هومان میان دو صف",
      "m2": "بیامد دمان بر لب آورده کف",
      "verse_number": 45
    },
    {
      "id": "85ad9396-92cd-4144-85a1-b2f0ee5225c7",
      "m1": "کنون با تو چندین چه گوید براز",
      "m2": "میان دو صف گفت و گوی دراز",
      "verse_number": 46
    },
    {
      "id": "02f03c3b-a41e-45c1-9db5-6cfad03a77e2",
      "m1": "سخن جز بشمشیر با او مگوی",
      "m2": "مجوی از در آشتی هیچ روی",
      "verse_number": 47
    },
    {
      "id": "6e94e885-6ace-41bc-89ea-c8c6775769e4",
      "m1": "چو بشنید هومان برآشفت سخت",
      "m2": "چنین گفت با گیو بیدار بخت",
      "verse_number": 48
    },
    {
      "id": "cefe5df0-0510-4de4-aa7d-d8c5db6070a2",
      "m1": "ایا گم شده بخت آزادگان",
      "m2": "که گم باد گودرز کشوادگان",
      "verse_number": 49
    },
    {
      "id": "d5f585f2-5344-4753-9775-d8fff695172d",
      "m1": "فراوان مرا دیدهای روز جنگ",
      "m2": "به آوردگه تیغ هندی بچنگ",
      "verse_number": 50
    },
    {
      "id": "4cb8f1f1-a17a-4cab-9bb5-92e857766704",
      "m1": "کس از تخم کشواد جنگی نماند",
      "m2": "که منشور تیغ مرا برنخواند",
      "verse_number": 51
    },
    {
      "id": "2d4cfa4a-b188-4452-aabe-34392ace60c8",
      "m1": "ترا بخت چون روی آهرمنست",
      "m2": "بخان تو تا جاودان شیونست",
      "verse_number": 52
    },
    {
      "id": "ab44bd30-9e55-4ec9-be05-815d2a7ba1b6",
      "m1": "اگر من شوم کشته بر دست طوس",
      "m2": "نه برخیزد آیین گوپال و کوس",
      "verse_number": 53
    },
    {
      "id": "a97eac3b-aad0-4923-b895-211d18d495fc",
      "m1": "بجایست پیران و افراسیاب",
      "m2": "بخواهد شدن خون من رود آب",
      "verse_number": 54
    },
    {
      "id": "efa55b52-b7ef-4a56-85ba-c61075880dad",
      "m1": "نه گیتی شود پاک ویران ز من",
      "m2": "سخن راند باید بدین انجمن",
      "verse_number": 55
    },
    {
      "id": "3b5afbca-735a-42f3-9176-f9b35467daaa",
      "m1": "وگر طوس گردد بدستم تباه",
      "m2": "یکی ره نیابند ز ایران سپاه",
      "verse_number": 56
    },
    {
      "id": "24682dc4-40dd-4a85-bbca-f068ebdd7eeb",
      "m1": "تو اکنون بمرگ برادر گری",
      "m2": "چه با طوس نوذر کنی داوری",
      "verse_number": 57
    },
    {
      "id": "d8aac707-1ed3-46ab-b62c-ea5b909bcaa4",
      "m1": "بدو گفت طوس این چه آشفتنست",
      "m2": "بدین دشت پیکار تو با منست",
      "verse_number": 58
    },
    {
      "id": "58c26d89-5379-49bf-ac5f-10db8f381c25",
      "m1": "بیا تا بگردیم و کین آوریم",
      "m2": "بجنگ ابروان پر ز چین آوریم",
      "verse_number": 59
    },
    {
      "id": "e3d57116-d6dc-41c7-8aa9-6824b257e350",
      "m1": "بدو گفت هومان که دادست مرگ",
      "m2": "سری زیر تاج و سری زیر ترگ",
      "verse_number": 60
    },
    {
      "id": "7e1b3aff-940b-4769-86c5-a067bb56561d",
      "m1": "اگر مرگ باشد مرا بیگمان",
      "m2": "به آوردگه به که آید زمان",
      "verse_number": 61
    },
    {
      "id": "87f0dbe0-0d56-4588-884a-0e0a70b91156",
      "m1": "بدست سواری که دارد هنر",
      "m2": "سپهبدسر و گرد و پرخاشخر",
      "verse_number": 62
    },
    {
      "id": "ea26102a-bbdb-4803-be02-090339d7458c",
      "m1": "گرفتند هر دو عمود گران",
      "m2": "همی حمله برد آن برین این بران",
      "verse_number": 63
    },
    {
      "id": "ed9e8e75-2862-4389-88c5-9ad6bc6ea59a",
      "m1": "ز می گشت گردان و شد روز تار",
      "m2": "یکی ابر بست از بر کارزار",
      "verse_number": 64
    },
    {
      "id": "06f8b701-218c-4dc1-b9c5-0e117c3ff6a0",
      "m1": "تو گفتی شب آمد بریشان بروز",
      "m2": "نهان گشت خورشید گیتی فروز",
      "verse_number": 65
    },
    {
      "id": "070bfd45-c2e8-4086-a3d9-fed061c9e81a",
      "m1": "ازان چاک چاک عمود گران",
      "m2": "سرانشان چو سندان آهنگران",
      "verse_number": 66
    },
    {
      "id": "6f78bada-de57-43cb-9cbe-bdd70bf3803d",
      "m1": "بابر اندرون بانگ پولاد خاست",
      "m2": "بدریای شهد اندرون باد خاست",
      "verse_number": 67
    },
    {
      "id": "d173f059-cf7a-4b09-8a09-ec235579051a",
      "m1": "ز خون بر کف شیر کفشیر بود",
      "m2": "همه دشت پر بانگ شمشیر بود",
      "verse_number": 68
    },
    {
      "id": "1962f043-087c-49e1-bb03-4e17cd66d912",
      "m1": "خم آورد رویین عمود گران",
      "m2": "شد آهن به کردار چاچی کمان",
      "verse_number": 69
    },
    {
      "id": "15fb3840-71cb-4aec-921d-b7968acbe71e",
      "m1": "تو گفتی که سنگ است سر زیر ترگ",
      "m2": "سیه شد ز زخم یلان روی مرگ",
      "verse_number": 70
    },
    {
      "id": "66f54535-bea3-44ba-8128-55f0e135e89a",
      "m1": "گرفتند شمشیر هندی بچنگ",
      "m2": "فرو ریخت آتش ز پولاد و سنگ",
      "verse_number": 71
    },
    {
      "id": "cc80bfd3-369c-49fc-832e-5facf58e2929",
      "m1": "ز نیروی گردنکشان تیغ تیز",
      "m2": "خم آورد و در زخم شد ریز ریز",
      "verse_number": 72
    },
    {
      "id": "858295ed-ac48-463c-b25d-571175752d80",
      "m1": "همه کام پرخاک و پر خاک سر",
      "m2": "گرفتند هر دو دوال کمر",
      "verse_number": 73
    },
    {
      "id": "0c7c840d-d1fc-4aec-af3e-52a652cce244",
      "m1": "ز نیروی گردان گران شد رکیب",
      "m2": "یکی را نیامد سر اندر نشیب",
      "verse_number": 74
    },
    {
      "id": "f7bbf095-ce09-48dc-ac88-8f35331fe967",
      "m1": "کمربند بگسست و هومان بجست",
      "m2": "یکی اسپ آسوده را برنشست",
      "verse_number": 75
    },
    {
      "id": "84967219-6a63-4330-a211-45ff672c9683",
      "m1": "سپهبد سوی ترکش آورد چنگ",
      "m2": "کمان را بزه کرد و تیر خدنگ",
      "verse_number": 76
    },
    {
      "id": "64823301-fff7-465a-a88a-a44ba4fd5a3b",
      "m1": "بران نامور تیرباران گرفت",
      "m2": "چپ و راست جنگ سواران گرفت",
      "verse_number": 77
    },
    {
      "id": "555f9c23-d13f-40d7-85d5-513e529dc268",
      "m1": "ز پولاد پیکان و پر عقاب",
      "m2": "سپر کرد بر پیش روی آفتاب",
      "verse_number": 78
    },
    {
      "id": "f4a72bd0-4f80-42b5-9638-9b43d587c6f1",
      "m1": "جهان چون ز شب رفته دو پاس گشت",
      "m2": "همه روی کشور پر الماس گشت",
      "verse_number": 79
    },
    {
      "id": "fa2aac64-60bf-446d-9dea-6bdbed429290",
      "m1": "ز تیر خدنگ اسپ هومان بخست",
      "m2": "تن بارگی گشت با خاک پست",
      "verse_number": 80
    },
    {
      "id": "954f738d-fdff-49fe-940b-599f6ed98280",
      "m1": "سپر بر سر آورد و ننمود روی",
      "m2": "نگه داشت هومان سر از تیر اوی",
      "verse_number": 81
    },
    {
      "id": "ac76c513-e1a0-4cab-863b-336ae5d3c984",
      "m1": "چو او را پیاده بران رزمگاه",
      "m2": "بدیدند گفتند توران سپاه",
      "verse_number": 82
    },
    {
      "id": "81f8a59f-4d73-4aad-b3a5-bc13b5fd2eab",
      "m1": "که پردخت ماند کنون جای اوی",
      "m2": "ببردند پرمایه بالای اوی",
      "verse_number": 83
    },
    {
      "id": "32f61495-7bfa-43a6-b30b-37c6707d2a68",
      "m1": "چو هومان بران زین توزی نشست",
      "m2": "یکی تیغ بگرفت هندی بدست",
      "verse_number": 84
    },
    {
      "id": "60aeefb9-7a7c-4be8-90d9-f55e5aeac9e6",
      "m1": "که آید دگر باره بر جنگ طوس",
      "m2": "شد از شب جهان تیره چون آبنوس",
      "verse_number": 85
    },
    {
      "id": "e02d99ae-36ca-43f8-ae8b-793712fbe53f",
      "m1": "همه نامداران پرخاشجوی",
      "m2": "یکایک بدو در نهادند روی",
      "verse_number": 86
    },
    {
      "id": "7fa471bd-1214-4da5-b0c3-7c55934f4699",
      "m1": "چو شد روز تاریک و بیگاه گشت",
      "m2": "ز جنگ یلان دست کوتاه گشت",
      "verse_number": 87
    },
    {
      "id": "5bbdea70-9e58-4dee-970c-cb7bb9bff352",
      "m1": "بپیچید هومان جنگی عنان",
      "m2": "سپهبد بدو راست کرده سنان",
      "verse_number": 88
    },
    {
      "id": "9d2f571a-658f-4367-92a5-64bdc1bf3fb1",
      "m1": "بنزدیک پیران شد از رزمگاه",
      "m2": "خروشی برآمد ز توران سپاه",
      "verse_number": 89
    },
    {
      "id": "8a4bd8d5-c814-4dc6-bed4-8ab9ff34d592",
      "m1": "ز تو خشم گردنکشان دور باد",
      "m2": "درین جنگ فرجام ما سور باد",
      "verse_number": 90
    },
    {
      "id": "74adb75b-18b8-4733-87d4-a45b484f4bbe",
      "m1": "که چون بود رزم تو ای نامجوی",
      "m2": "چو با طوس روی اندر آمد بروی",
      "verse_number": 91
    },
    {
      "id": "2cb9f266-cfd1-4e2d-bbec-7150a43fafd8",
      "m1": "همه پاک ما دل پر از خون بدیم",
      "m2": "جز ایزد نداند که ما چون بدیم",
      "verse_number": 92
    },
    {
      "id": "7b311662-23ac-456a-b8ac-bf86cfc47ef9",
      "m1": "بلشکر چنین گفت هومان شیر",
      "m2": "که ای رزم دیده سران دلیر",
      "verse_number": 93
    },
    {
      "id": "8b75a596-11d7-44a7-93bf-f71e45a4a86a",
      "m1": "چو روشن شود تیره شب روز ماست",
      "m2": "که این اختر گیتی افروز ماست",
      "verse_number": 94
    },
    {
      "id": "492700b6-05fe-4b76-8797-3ef87c777473",
      "m1": "شما را همه شادکامی بود",
      "m2": "مرا خوبی و نیکنامی بود",
      "verse_number": 95
    },
    {
      "id": "c72e3d73-09a1-443d-88a0-3951a6cf90ae",
      "m1": "ز لشکر همی برخروشید طوس",
      "m2": "شب تیره تا گاه بانگ خروس",
      "verse_number": 96
    },
    {
      "id": "303b4e0c-7b1e-4b17-8b4f-34ec4bff2584",
      "m1": "همی گفت هومان چه مرد منست",
      "m2": "که پیل ژیان هم نبرد منست",
      "verse_number": 97
    },
    {
      "id": "4b0be0e3-de70-4665-bc6d-18ccd7a9b4be",
      "m1": "چو چرخ بلند از شبه تاج کرد",
      "m2": "شمامه پراگند بر لاژورد",
      "verse_number": 98
    },
    {
      "id": "05f99b58-bbb8-4a37-8a83-ea7246ecc35f",
      "m1": "طلایه ز هر سو برون تاختند",
      "m2": "بهر پردهای پاسبان ساختند",
      "verse_number": 99
    },
    {
      "id": "41ed2b70-2001-4dcd-91da-a93232617e74",
      "m1": "چو برزد سر از برج خرچنگ شید",
      "m2": "جهان گشت چون روی رومی سپید",
      "verse_number": 100
    },
    {
      "id": "fd637745-fdbf-4528-9ec4-070e44e617b2",
      "m1": "تبیره برآمد ز هر دو سرای",
      "m2": "جهان شد پر از نالهٔ کر نای",
      "verse_number": 101
    },
    {
      "id": "0d83d777-2885-459c-be97-07d712dfd42a",
      "m1": "هوا تیره گشت از فروغ درفش",
      "m2": "طبر خون و شبگون و زرد و بنفش",
      "verse_number": 102
    },
    {
      "id": "92987e0d-d113-4145-a01e-e9be2da62c65",
      "m1": "کشیده همه تیغ و گرز و سنان",
      "m2": "همه جنگ را گرد کرده عنان",
      "verse_number": 103
    },
    {
      "id": "f88f96c6-85c5-441c-aa2f-ab73e32aa594",
      "m1": "تو گفتی سپهر و زمان و زمین",
      "m2": "بپوشد همی چادر آهنین",
      "verse_number": 104
    },
    {
      "id": "d06f48cb-f0e2-4364-8580-01930d91ff18",
      "m1": "بپرده درون شد خور تابناک",
      "m2": "ز جوش سواران و از گرد و خاک",
      "verse_number": 105
    },
    {
      "id": "cd85eb4d-46c7-402f-b39f-feae2d1416c9",
      "m1": "ز هرای اسپان و آوای کوس",
      "m2": "همی آسمان بر زمین داد بوس",
      "verse_number": 106
    },
    {
      "id": "289343d6-07c5-43cc-9cd1-3d9fd7b4dd05",
      "m1": "سپهدار هومان دمان پیش صف",
      "m2": "یکی خشت رخشان گرفته بکف",
      "verse_number": 107
    },
    {
      "id": "9a094b98-b579-4dba-8d45-07317a5aad1d",
      "m1": "همی گفت چون من برایم بجوش",
      "m2": "برانگیزم اسپ و برارم خروش",
      "verse_number": 108
    },
    {
      "id": "34df0da5-5e0e-466f-8328-9b2a1719c1cf",
      "m1": "شما یک بیک تیغها برکشید",
      "m2": "سپرهای چینی بسر در کشید",
      "verse_number": 109
    },
    {
      "id": "f19647db-6b1b-4928-a72f-32b0b0e777dd",
      "m1": "مبینید جز یال اسپ و عنان",
      "m2": "نشاید کمان و نباید سنان",
      "verse_number": 110
    },
    {
      "id": "cf7d13df-6fd4-4fd4-a341-c398229d3cc3",
      "m1": "عنان پاک بر یال اسپان نهید",
      "m2": "بدانسان که آید خورید و دهید",
      "verse_number": 111
    },
    {
      "id": "a4a215be-dd66-4249-b50f-37c3fff724fa",
      "m1": "بپیران چنین گفت کای پهلوان",
      "m2": "تو بگشای بند سلیح گوان",
      "verse_number": 112
    },
    {
      "id": "9d69555a-7ada-457c-9322-e1ea17fbfaa8",
      "m1": "ابا گنج دینار جفتی مکن",
      "m2": "ز بهر سلیح ایچ زفتی مکن",
      "verse_number": 113
    },
    {
      "id": "e12fd7b6-1c13-4f5e-ac9c-0081d6f5fee0",
      "m1": "که امروز گردیم پیروزگر",
      "m2": "بیابد دل از اختر نیک بر",
      "verse_number": 114
    },
    {
      "id": "c5b05ade-8b37-4f10-ba0b-ceb3fbb3457c",
      "m1": "وزین روی لشکر سپهدار طوس",
      "m2": "بیاراست برسان چشم خروش",
      "verse_number": 115
    },
    {
      "id": "7ba766f4-aa26-40c4-8e42-80d69ba00deb",
      "m1": "بروبر یلان آفرین خواندند",
      "m2": "ورا پهلوان زمین خواندند",
      "verse_number": 116
    },
    {
      "id": "ca561ab1-e4e6-4bc6-aee9-5620bee5e605",
      "m1": "که پیروزگر بود روز نبرد",
      "m2": "ز هومان ویسه برآورد گرد",
      "verse_number": 117
    },
    {
      "id": "4acb159c-28e3-443e-92b8-3f8ad29187ab",
      "m1": "سپهبد بگودرز کشواد گفت",
      "m2": "که این راز بر کس نباید نهفت",
      "verse_number": 118
    },
    {
      "id": "cbd76701-e8be-4bdd-a7fd-9ad2a4454b4f",
      "m1": "اگر لشکر ما پذیره شوند",
      "m2": "سواران بدخواه چیره شوند",
      "verse_number": 119
    },
    {
      "id": "c496ce4d-12e5-4827-8e8b-8da1e7f0b718",
      "m1": "همه دست یکسر بیزدان زنیم",
      "m2": "منی از تن خویش بفگنیم",
      "verse_number": 120
    },
    {
      "id": "f85c10b9-3ce7-4b0f-8f02-703cd9074711",
      "m1": "مگر دست گیرد جهاندار ما",
      "m2": "وگر نه بد است اختر کار ما",
      "verse_number": 121
    },
    {
      "id": "3173568c-f109-455e-a2b6-fe13d87740f8",
      "m1": "کنون نامداران زرینه کفش",
      "m2": "بباشید با کاویانی درفش",
      "verse_number": 122
    },
    {
      "id": "6f0ab9e6-21cc-49c9-8198-4e79320d9f7a",
      "m1": "ازین کوه پایه مجنبید هیچ",
      "m2": "نه روز نبرد است و گاه بسیچ",
      "verse_number": 123
    },
    {
      "id": "0361a757-49ea-4edc-9aad-d4b4cdedb166",
      "m1": "همانا که از ما بهر یک دویست",
      "m2": "فزونست بدخواه اگر بیش نیست",
      "verse_number": 124
    },
    {
      "id": "6bec8251-14e4-4c3c-82ee-bd52bb5d9603",
      "m1": "بدو گفت گودرز اگر کردگار",
      "m2": "بگرداند از ما بد روزگار",
      "verse_number": 125
    },
    {
      "id": "4af2a7d7-b8ae-4f70-a527-77f78023afae",
      "m1": "به بیشی و کمی نباشد سخن",
      "m2": "دل و مغز ایرانیان بد مکن",
      "verse_number": 126
    },
    {
      "id": "4dccec13-e893-428d-8644-35afe41a0259",
      "m1": "اگر بد بود بخشش آسمان",
      "m2": "بپرهیز و بیشی نگردد زمان",
      "verse_number": 127
    },
    {
      "id": "05910c63-d8aa-4a85-a33a-35b1299c0d31",
      "m1": "تو لشکر بیارای و از بودنی",
      "m2": "روان را مکن هیچ بشخودنی",
      "verse_number": 128
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 128
  }
}