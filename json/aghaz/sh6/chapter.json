{
  "id": "9b18db83-ad6b-460c-83a8-3e37ff10a11f",
  "title": "بخش ۶ - در آفرینش ماه",
  "verses": [
    {
      "id": "e860a0d7-70ff-4b96-b04e-1039e18275d5",
      "m1": "چراغ است مر تیره شب را بسیچ",
      "m2": "به بد تا توانی تو هرگز مپیچ",
      "verse_number": 1
    },
    {
      "id": "b637c1c7-f9f7-4ac9-bb12-1e9c52932ad0",
      "m1": "چو سی روز گردش بپیمایدا",
      "m2": "شود تیره گیتی بدو روشنا",
      "verse_number": 2
    },
    {
      "id": "0837d840-aea3-43b1-8f6c-d2fd2509c301",
      "m1": "پدید آید آنگاه باریک و زرد",
      "m2": "چو پشت کسی کو غم عشق خْوَرد",
      "verse_number": 3
    },
    {
      "id": "e41dd126-0743-4ced-b15e-0ad676d7c16c",
      "m1": "چو بیننده دیدارش از دور دید",
      "m2": "هم اندر زمان او شود ناپدید",
      "verse_number": 4
    },
    {
      "id": "400ebbfe-7d3b-40a0-8270-bf81064d3d39",
      "m1": "دگر شب نمایش کند بیشتر",
      "m2": "تو را روشنایی دهد بیشتر",
      "verse_number": 5
    },
    {
      "id": "c2186457-a6fd-449b-8340-666063439894",
      "m1": "به دو هفته گردد تمام و درست",
      "m2": "بدان باز گردد که بود از نخست",
      "verse_number": 6
    },
    {
      "id": "9d6ece4f-0a2f-4cb2-a9fc-a0a68e07b732",
      "m1": "بود هر شبانگاه باریکتر",
      "m2": "به خورشید تابنده نزدیکتر",
      "verse_number": 7
    },
    {
      "id": "dfbcea62-9211-4954-b04b-af2d4a0359ee",
      "m1": "بدینسان نهادش خداوند داد",
      "m2": "بود تا بود هم بدین یک نهاد",
      "verse_number": 8
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 8
  }
}