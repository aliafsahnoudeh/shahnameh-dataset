{
  "id": "fe47918b-d0b0-49b8-9282-f17985f251b9",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "5b086b54-4111-4cd4-9cbb-aeeb7aa01644",
      "m1": "چو شیروی بنشست برتخت ناز",
      "m2": "به سر برنهاد آن کیی تاج آز",
      "verse_number": 1
    },
    {
      "id": "1f8f03aa-5e61-40fc-81f2-c094205cddd4",
      "m1": "برفتند گوینده ایرانیان",
      "m2": "برو خواندند آفرین کیان",
      "verse_number": 2
    },
    {
      "id": "7cbdf9a1-5b20-43ca-ac3b-d364430f212f",
      "m1": "همیگفت هریک به بانگ بلند",
      "m2": "که ای پر هنر خسرو ارجمند",
      "verse_number": 3
    },
    {
      "id": "8d0a9469-07bf-4d17-a53b-cf9c621361b6",
      "m1": "چنان هم که یزدان تو را داد تاج",
      "m2": "نشستی به آرام بر تخت عاج",
      "verse_number": 4
    },
    {
      "id": "d3464808-774b-44c8-b180-ddd0a07e9fc2",
      "m1": "بماناد گیتی به فرزند تو",
      "m2": "چنین هم به خویشان و پیوند تو",
      "verse_number": 5
    },
    {
      "id": "1a8fb4fe-6b22-48c8-b79d-89eb2d873ad2",
      "m1": "چنین داد پاسخ بدیشان قباد",
      "m2": "که همواره پیروز باشید و شاد",
      "verse_number": 6
    },
    {
      "id": "da3d7d93-b2a2-4114-8a7a-62c265459eed",
      "m1": "نباشیم تا جاودان بدکنش",
      "m2": "چه نیکو بود داد باخوش منش",
      "verse_number": 7
    },
    {
      "id": "9f91b7d7-a085-4700-8f99-7eeede61b4a2",
      "m1": "جهان را بداریم با ایمنی",
      "m2": "ببُریم کردار آهرمنی",
      "verse_number": 8
    },
    {
      "id": "0c55b06c-29f9-4fb0-b5ff-35e31cb9d424",
      "m1": "ز بایستهتر کار پیشی مرا",
      "m2": "که افزون بود فر و خویشی مرا",
      "verse_number": 9
    },
    {
      "id": "01b3b265-00be-48bf-8abe-65db555f8584",
      "m1": "پیامی فرستم به نزد پدر",
      "m2": "بگویم بدو این سخن در به در",
      "verse_number": 10
    },
    {
      "id": "a2363a51-df97-464b-a198-745611522ac6",
      "m1": "ز ناخوب کاری که او راندست",
      "m2": "برین گونه کاری به پیش آمدست",
      "verse_number": 11
    },
    {
      "id": "c3d33934-f328-4b8a-b3b2-17cfbc14a899",
      "m1": "به یزدان کند پوزش او از گناه",
      "m2": "گراینده گردد به آیین و راه",
      "verse_number": 12
    },
    {
      "id": "3ba3038c-9dec-4d7d-997b-67a35f26267e",
      "m1": "بپردازم آن گه به کار جهان",
      "m2": "بکوشم به داد آشکار و نهان",
      "verse_number": 13
    },
    {
      "id": "7a7a13d0-2440-4311-a8a8-810d2e9bcaf9",
      "m1": "به جای نکوکار نیکی کنیم",
      "m2": "دل مرد درویش رانشکنیم",
      "verse_number": 14
    },
    {
      "id": "293dfed7-ede8-4b2e-bb76-3d22a45e1bb7",
      "m1": "دوتن بایدم راد و نیکوسخن",
      "m2": "کجا یاد دارند ز کار کهن",
      "verse_number": 15
    },
    {
      "id": "fb27cdcf-477a-4a2e-998c-5d0b7f013b59",
      "m1": "بدان انجمن گفت کاین کارکیست",
      "m2": "ز ایرانیان پاک و بیدار کیست",
      "verse_number": 16
    },
    {
      "id": "7e3eb957-9631-435b-acf4-bb0275b92e5c",
      "m1": "نمودند گردان سراسر به چشم",
      "m2": "دو استاد را گر نگیرند خشم",
      "verse_number": 17
    },
    {
      "id": "41b13001-0976-4a40-ac57-4020afaee63e",
      "m1": "بدانست شیروی کایرانیان",
      "m2": "که را برگزینند پاک از میان",
      "verse_number": 18
    },
    {
      "id": "9ebacb9f-3db9-4d6b-932d-8a29cc6319e7",
      "m1": "چو اشتاد و خراد برزین پیر",
      "m2": "دو دانا و گوینده و یادگیر",
      "verse_number": 19
    },
    {
      "id": "d8e20867-aea7-4b7e-92fe-b61d3c81eba5",
      "m1": "بدیشان چنین گفت کای بخردان",
      "m2": "جهاندیده و کارکرده ردان",
      "verse_number": 20
    },
    {
      "id": "4bc268a8-877c-400a-a6dc-1b98d8ce998e",
      "m1": "مدارید کار جهان را به رنج",
      "m2": "که از رنج یابد سرافراز گنج",
      "verse_number": 21
    },
    {
      "id": "28f6bc0d-d490-43b4-b7a8-2d40aa0ef856",
      "m1": "دو داننده بیکام برخاستند",
      "m2": "پر از آب مژگان بیاراستند",
      "verse_number": 22
    },
    {
      "id": "4e2562a6-7e9c-4819-a7bb-2f240d2ba5f6",
      "m1": "چو خراد بر زین و اشتاگشسپ",
      "m2": "به فرمان نشستند هر دو بر اسپ",
      "verse_number": 23
    },
    {
      "id": "099f021e-c1ae-4893-a7d5-85009ffa2eef",
      "m1": "بدیشان چنین گفت کز دل کنون",
      "m2": "بباید گرفتن ره طیسفون",
      "verse_number": 24
    },
    {
      "id": "6bbd364d-249b-4f7b-aff6-b536575d50b9",
      "m1": "پیامی رسانید نزد پدر",
      "m2": "سخن یادگیری همه در بدر",
      "verse_number": 25
    },
    {
      "id": "17e16e99-90fa-4ff7-a654-a4b47e3aaf07",
      "m1": "بگویی که ما را نبد این گناه",
      "m2": "نه ایرانیان را بد این دستگاه",
      "verse_number": 26
    },
    {
      "id": "9a6aefad-9303-4cec-8b24-440f0ded30cd",
      "m1": "که باد افرهٔ ایزدی یافتی",
      "m2": "چو از نیکوی روی بر تافتی",
      "verse_number": 27
    },
    {
      "id": "ae3e9216-afeb-4e57-ad1a-81bbdec71f57",
      "m1": "یکی آنک ناباک خون پدر",
      "m2": "نریزد ز تن پاک زاده پسر",
      "verse_number": 28
    },
    {
      "id": "b99b84e1-5b64-497f-91f5-03d5e8b85785",
      "m1": "نباشد همان نیز هم داستان",
      "m2": "که پیشش کسی گوید این داستان",
      "verse_number": 29
    },
    {
      "id": "389f8a90-a789-4ccb-9ec6-e4f93968334b",
      "m1": "دگر آنک گیتی پر از گنج تست",
      "m2": "رسیده به هر کشوری رنج تست",
      "verse_number": 30
    },
    {
      "id": "ac8deedb-fd47-45e6-a6ae-5ee43d2cb7bb",
      "m1": "نبودی بدین نیز هم داستان",
      "m2": "پر از درد کردی دل راستان",
      "verse_number": 31
    },
    {
      "id": "a5cabf9c-8ac8-4187-bbf5-b3fbb9b6ae73",
      "m1": "سدیگر که چندان دلیر و سوار",
      "m2": "که بود اندر ایران همه نامدار",
      "verse_number": 32
    },
    {
      "id": "6f8aeeaf-79f7-4a17-a560-5cb98d0f2faf",
      "m1": "نبودند شادان ز فرزند خویش",
      "m2": "ز بوم و بر و پاک پیوند خویش",
      "verse_number": 33
    },
    {
      "id": "199cd693-fc1c-4f96-86f8-319868234abd",
      "m1": "یکی سوی چین بد یکی سوی روم",
      "m2": "پراگنده گشته به هر مرز و بوم",
      "verse_number": 34
    },
    {
      "id": "0df6b07b-6e01-4b53-9feb-296dec58f4af",
      "m1": "دگر آنک قیصر بجای تو کرد",
      "m2": "ز هر گونه از تو چه تیمار خورد",
      "verse_number": 35
    },
    {
      "id": "697d30e9-4a1b-4a61-91d7-541b27510839",
      "m1": "سپه داد و دختر تو را داد نیز",
      "m2": "همان گنج و با گنج بسیار چیز",
      "verse_number": 36
    },
    {
      "id": "11f2fbce-9131-471d-949b-26042bd32c40",
      "m1": "همیخواست دار مسیحا به روم",
      "m2": "بدان تا شود خرم آباد بوم",
      "verse_number": 37
    },
    {
      "id": "e866c334-c495-4cf3-bb5d-e90b1ffefcf0",
      "m1": "به گنج تو از دار عیسی چه سود",
      "m2": "که قیصر به خوبی همی شاد بود",
      "verse_number": 38
    },
    {
      "id": "f3c8fa46-7bc8-4c92-89d8-ae7e2ec7327b",
      "m1": "ز بیچارگان خواسته بستدی",
      "m2": "ز نفرین به روی تو آمد بدی",
      "verse_number": 39
    },
    {
      "id": "88b57efa-5af4-40f2-81d2-0491d9cc9d6c",
      "m1": "ز یزدان شناس آنچ آمدت پیش",
      "m2": "بر اندیش زان زشت کردار خویش",
      "verse_number": 40
    },
    {
      "id": "551a2981-9fe1-433e-b933-8fb32da5f8fb",
      "m1": "بدان بد که کردی بهانه منم",
      "m2": "سخن را نخست آستانه منم",
      "verse_number": 41
    },
    {
      "id": "8dbb225b-aa10-4275-85bf-d076dad3b594",
      "m1": "به یزدان که از من نبد این گناه",
      "m2": "نجستم که ویران شود گاه شاه",
      "verse_number": 42
    },
    {
      "id": "e2d10306-0f82-46a0-83b0-cc38c5009e33",
      "m1": "کنون پوزش این همه بازجوی",
      "m2": "بدین نامداران ایران بگوی",
      "verse_number": 43
    },
    {
      "id": "4b579cd3-7b89-42eb-bf32-61c5aeead9bc",
      "m1": "ز هر بد که کردی به یزدان گرای",
      "m2": "کجا هست بر نیکوی رهنمای",
      "verse_number": 44
    },
    {
      "id": "adef5606-ba92-472a-be2a-b6ccb9d7e9e6",
      "m1": "مگر مر تو را او بود دستگیر",
      "m2": "بدین رنجهایی که بودت گزیر",
      "verse_number": 45
    },
    {
      "id": "2e371065-c3b1-487e-89db-5427d0e1ae10",
      "m1": "دگر آنک فرزند بودت دو هشت",
      "m2": "شب و روز ایشان به زندان گذشت",
      "verse_number": 46
    },
    {
      "id": "cb478bfb-7891-487d-82fc-e48418135cda",
      "m1": "به در بر کسی ایمن از تو نخفت",
      "m2": "ز بیم تو بگذاشتندی نهفت",
      "verse_number": 47
    },
    {
      "id": "2762f6cc-4da0-4417-8dfe-a1f5ad9ec974",
      "m1": "چو بشنید پیغام او این دو مرد",
      "m2": "برفتند دلها پر از داغ و درد",
      "verse_number": 48
    },
    {
      "id": "6fb6f9c0-049a-498c-9df1-ec48fb05a5c8",
      "m1": "برین گونه تا کشور طیسفون",
      "m2": "همه دیده پرآب و دل پر ز خون",
      "verse_number": 49
    },
    {
      "id": "dd6b8f92-5bad-41d5-8b3c-93c51ce58e7e",
      "m1": "نشسته بدر بر گلینوش بود",
      "m2": "که گفتی زمین زو پر از جوش بود",
      "verse_number": 50
    },
    {
      "id": "42efeee2-24af-4bff-a58d-f24b8216e27b",
      "m1": "همه لشکرش یک سر آراسته",
      "m2": "کشیده همه تیغ و پیراسته",
      "verse_number": 51
    },
    {
      "id": "11a16b48-e294-4d62-8549-565c5e26606b",
      "m1": "ابا جوشن و خود بسته میان",
      "m2": "همان تازی اسپان به برگستوان",
      "verse_number": 52
    },
    {
      "id": "c3d8cf2e-16ca-4e7b-b46d-574be4cd7410",
      "m1": "به جنگ اندرون گرز پولاد داشت",
      "m2": "همه دل پر از آتش و باد داشت",
      "verse_number": 53
    },
    {
      "id": "002e31d2-4de5-40cc-8dba-adf36e2e58cb",
      "m1": "چو خراد برزین و اشتاگشسپ",
      "m2": "فرود آمدند این دو دانا از اسپ",
      "verse_number": 54
    },
    {
      "id": "97be40fb-420b-4fcf-97c1-d1b0a1c15876",
      "m1": "گلینوش بر پای جست آن زمان",
      "m2": "ز دیدار ایشان ببد شادمان",
      "verse_number": 55
    },
    {
      "id": "38eabdbb-36ad-4d3e-8e5b-61310163cdc5",
      "m1": "بجایی که بایست بنشاندشان",
      "m2": "همی مهتر نامور خواندشان",
      "verse_number": 56
    },
    {
      "id": "3cc39ccf-0ca1-461d-8d91-9da0d546b64f",
      "m1": "سخن گوی خراد برزین نخست",
      "m2": "زبان را به آب دلیری بشست",
      "verse_number": 57
    },
    {
      "id": "87a8db86-2df0-4ebb-aff0-ac562543e3f3",
      "m1": "گلینوش را گفت فرخ قباد",
      "m2": "به آرام تاج کیان برنهاد",
      "verse_number": 58
    },
    {
      "id": "ff004376-4a9d-4dd7-9fb4-d673b92d8af5",
      "m1": "به ایران و توران و روم آگهیست",
      "m2": "که شیروی بر تخت شاهنشهیست",
      "verse_number": 59
    },
    {
      "id": "5ced86b9-aba2-40b4-a001-38769f973f1c",
      "m1": "تواین جوشن و خود و گبر و کمان",
      "m2": "چه داری همی کیستت بد گمان",
      "verse_number": 60
    },
    {
      "id": "c67bb5de-52d1-4965-97b1-6b182fa61a01",
      "m1": "گلینوش گفت ای جهاندیده مرد",
      "m2": "به کام تو بادا همه کارکرد",
      "verse_number": 61
    },
    {
      "id": "a5074b12-62ca-4c1e-ad9a-3c6ef29b4b9c",
      "m1": "که تیمار بردی ز نازک تنم",
      "m2": "کجا آهنین بود پیراهنم",
      "verse_number": 62
    },
    {
      "id": "b34fb62b-0fc7-4d1d-95ab-dc2045a35742",
      "m1": "برین مهر بر آفرین خوانمت",
      "m2": "سزایی که گوهر برافشانمت",
      "verse_number": 63
    },
    {
      "id": "52b9b925-f040-46c4-bca7-a41be7ab4d22",
      "m1": "نباشد به جز خوب گفتار تو",
      "m2": "که خورشید بادا نگهدار تو",
      "verse_number": 64
    },
    {
      "id": "381624a9-672f-4089-aa3a-aeb2f74b353c",
      "m1": "به کاری کجا آمدستی بگوی",
      "m2": "پس آنگه سخنهای من بازجوی",
      "verse_number": 65
    },
    {
      "id": "7067d272-183c-4244-b5fc-46d495eb8edb",
      "m1": "چنین داد پاسخ که فرخ قباد",
      "m2": "به خسرو مرا چند پیغام داد",
      "verse_number": 66
    },
    {
      "id": "2a6d5bde-2b6f-487c-b599-37d08302503f",
      "m1": "اگر باز خواهی بگویم همه",
      "m2": "پیام جهاندار شاه رمه",
      "verse_number": 67
    },
    {
      "id": "f2e15f07-3294-44a4-a5f6-4709cf0939fb",
      "m1": "گلینوش گفت این گرانمایه مرد",
      "m2": "که داند سخنها همه یاد کرد",
      "verse_number": 68
    },
    {
      "id": "0bf13351-0fea-401c-933c-0bd757df319a",
      "m1": "ز لیکن مرا شاه ایران قباد",
      "m2": "بسی اندرین پند و اندرز داد",
      "verse_number": 69
    },
    {
      "id": "616f8c38-3b18-4d5f-b08e-90eb041e6517",
      "m1": "که همداستانی مکن روز و شب",
      "m2": "که کس پیش خسرو گشاید دو لب",
      "verse_number": 70
    },
    {
      "id": "0f20a747-1bdb-4de5-b24f-3746ea73bd97",
      "m1": "مگر آنک گفتار او بشنوی",
      "m2": "اگر پارسی گوید ار پهلوی",
      "verse_number": 71
    },
    {
      "id": "51ef2522-4bc1-41b5-a4d0-b0126d3bd8f2",
      "m1": "چنین گفت اشتاد کای شادکام",
      "m2": "من اندر نهانی ندارم پیام",
      "verse_number": 72
    },
    {
      "id": "933bdc0c-268c-4bac-825b-c69aeacd2749",
      "m1": "پیامیست کان تیغ بار آورد",
      "m2": "سر سرکشان در کنار آورد",
      "verse_number": 73
    },
    {
      "id": "ef2bc4db-fa0f-44ca-a44c-e8b70f33cef1",
      "m1": "تو اکنون ز خسرو برین بارخواه",
      "m2": "بدان تا بگویم پیامش ز شاه",
      "verse_number": 74
    },
    {
      "id": "637ff303-88d0-4453-b2f9-013d943a4cff",
      "m1": "گلینوش بشنید و بر پای جست",
      "m2": "همه بندها رابهم برشکست",
      "verse_number": 75
    },
    {
      "id": "b99a3c02-799c-4cbf-ae34-b9e089fd4c5c",
      "m1": "بر شاه شد دست کرده بکش",
      "m2": "چنان چون بباید پرستارفش",
      "verse_number": 76
    },
    {
      "id": "272856be-d9bf-4b64-8ca0-3f6c71088c8e",
      "m1": "بدو گفت شاها انوشه بدی",
      "m2": "مبادا دل تو نژند از بدی",
      "verse_number": 77
    },
    {
      "id": "1ff2f44b-a254-4205-a9f1-93941475950b",
      "m1": "چو اشتاد و خراد برزین به شاه",
      "m2": "پیام آوریدند زان بارگاه",
      "verse_number": 78
    },
    {
      "id": "330b9ed4-adf0-4779-8eb7-6209e7ad78eb",
      "m1": "بخندید خسرو به آواز گفت",
      "m2": "که این رای تو با خرد نیست جفت",
      "verse_number": 79
    },
    {
      "id": "2f561cf1-8fe1-4988-bbcd-00ff32e72c61",
      "m1": "گرو شهریارست پس من کیم",
      "m2": "درین تنگ زندان ز بهر چیم",
      "verse_number": 80
    },
    {
      "id": "59ee4143-d259-48f1-8832-9464579bcda5",
      "m1": "که از من همی بار بایدت خواست",
      "m2": "اگر کژ گویی اگر راه راست",
      "verse_number": 81
    },
    {
      "id": "f9a61288-1fa0-4647-b474-6d6c18769d99",
      "m1": "بیامد گلینوش نزد گوان",
      "m2": "بگفت این سخن گفتن پهلوان",
      "verse_number": 82
    },
    {
      "id": "93ff5b2b-2750-4a28-8f12-cf65f16444da",
      "m1": "کنون دست کرده بکش در شوید",
      "m2": "بگویید و گفتار او بشنوید",
      "verse_number": 83
    },
    {
      "id": "487e1ebd-0104-40df-8a34-c06df200a69e",
      "m1": "دو مرد خردمند و پاکیزهگوی",
      "m2": "به دستار چینی بپوشید روی",
      "verse_number": 84
    },
    {
      "id": "1152e3fc-1161-432b-bf4b-8a919cd77636",
      "m1": "چو دیدند بردند پیشش نماز",
      "m2": "ببودند هر دو زمانی دراز",
      "verse_number": 85
    },
    {
      "id": "5205ddb5-a1d2-42b0-aa13-688d8111a0ac",
      "m1": "جهاندار بر شادورد بزرگ",
      "m2": "نوشته همه پیکرش میش و گرگ",
      "verse_number": 86
    },
    {
      "id": "08b0c6fe-a225-493d-8728-2c19e45d1d68",
      "m1": "همان زر و گوهر برو بافته",
      "m2": "سراسر یک اندر دگر تافته",
      "verse_number": 87
    },
    {
      "id": "ff256cdb-f974-4cb7-8db7-35b78f750caf",
      "m1": "نهالیش در زیر دیبای زرد",
      "m2": "پس پشت او مسند لاژورد",
      "verse_number": 88
    },
    {
      "id": "a42ebd92-7a9d-431b-9253-ee41e2bb2dd3",
      "m1": "بهی تناور گرفته بدست",
      "m2": "دژم خفته بر جایگاه نشست",
      "verse_number": 89
    },
    {
      "id": "6873fd0d-7351-405d-b6c5-8152c87cc9d3",
      "m1": "چودید آن دو مرد گرانمایه را",
      "m2": "به دانایی اندر سرمایه را",
      "verse_number": 90
    },
    {
      "id": "01338acb-5cbb-4781-a8a9-95a0783e5dd2",
      "m1": "از آن خفتگی خویشتن کرد راست",
      "m2": "جهان آفریننده را یار خواست",
      "verse_number": 91
    },
    {
      "id": "16bf7904-cda8-4bcc-b9f0-88f42b769c8c",
      "m1": "به بالین نهاد آن گرامی بهی",
      "m2": "بدان تا بپرسید ز هر دو رهی",
      "verse_number": 92
    },
    {
      "id": "9e5d1570-0ab4-4dc6-901c-e8db29710c2f",
      "m1": "بهی زان دو بالش به نرمی بگشت",
      "m2": "بیآزار گردان ز مرقد گذشت",
      "verse_number": 93
    },
    {
      "id": "bc6739d3-4711-4753-a4eb-a56f5c83d068",
      "m1": "بدین گونه تا شادورد مهین",
      "m2": "همیگشت تاشد به روی زمین",
      "verse_number": 94
    },
    {
      "id": "d23f9950-746e-4aa0-8f27-b79f0ca28c0a",
      "m1": "به پویید اشتاد و آن برگرفت",
      "m2": "به مالیدش از خاک و بر سر گرفت",
      "verse_number": 95
    },
    {
      "id": "0305eae2-0bb6-46ba-9024-59d7a9ea53ac",
      "m1": "جهاندار از اشتاد برگاشت روی",
      "m2": "بدان تا ندید از بهی رنگ و بوی",
      "verse_number": 96
    },
    {
      "id": "2a08f1dc-bbdc-4f40-b535-54f83ab8f925",
      "m1": "بهی رانهادند بر شادورد",
      "m2": "همیبود برپای پیش این دو مرد",
      "verse_number": 97
    },
    {
      "id": "94cbe266-9286-4b0e-8d72-de40c1d2b500",
      "m1": "پر اندیشه شد نامدار از بهی",
      "m2": "ندید اندر و هیچ فال بهی",
      "verse_number": 98
    },
    {
      "id": "99c9ff18-19bb-4f47-9ec2-b9323dbcd552",
      "m1": "همانگه سوی آسمان کرد روی",
      "m2": "چنین گفت کای داور راست گوی",
      "verse_number": 99
    },
    {
      "id": "1f33d948-a7d2-4de8-9d55-2cf953b50017",
      "m1": "که برگیرد آن راکه تو افگنی",
      "m2": "که پیوندد آن را که تو بشکنی",
      "verse_number": 100
    },
    {
      "id": "a6588f0c-2935-456c-8895-3f27e2029430",
      "m1": "چو از دودهام بخت روشن بگشت",
      "m2": "غم آورد چون روشنایی گذشت",
      "verse_number": 101
    },
    {
      "id": "1178a6ba-08fc-4fc7-9aa2-a31e9a8f7c00",
      "m1": "به اشتاد گفت آنچ داری پیام",
      "m2": "ازان بی منش کودک زشت کام",
      "verse_number": 102
    },
    {
      "id": "d793eeaf-f07e-4bfc-8c77-5a67e3a95ffe",
      "m1": "وزان بد سگالان که بیدانشند",
      "m2": "ز بی دانشی ویژه بی رامشاند",
      "verse_number": 103
    },
    {
      "id": "91af6058-8234-4c33-9ccb-caecd0917e18",
      "m1": "همان زان سپاه پراگندگان",
      "m2": "پر اندیشه و تیره دل بندگان",
      "verse_number": 104
    },
    {
      "id": "e1331679-5d09-4f50-a04f-68d990298a14",
      "m1": "بخواهد شدن بخت زین دودمان",
      "m2": "نماند درین تخمه کس شادمان",
      "verse_number": 105
    },
    {
      "id": "5b3b7e3b-779f-4240-9224-5fd5284e49ca",
      "m1": "سوی ناسزایان شود تاج وتخت",
      "m2": "تبه گردد این خسروانی درخت",
      "verse_number": 106
    },
    {
      "id": "813407ef-0f90-4b7d-bdfe-c4321d9892bc",
      "m1": "نماند بزرگی به فرزند من",
      "m2": "نه بر دوده و خویش و پیوند من",
      "verse_number": 107
    },
    {
      "id": "e2b032b7-68a3-420b-9c25-f3df004f99d2",
      "m1": "همه دوستان ویژه دشمن شوند",
      "m2": "بدین دوده بَدگوی و بَدتَن شوند",
      "verse_number": 108
    },
    {
      "id": "9c092e7b-8907-45a5-9ad8-51659f6c46ed",
      "m1": "نهان آشکارا بکرد این بهی",
      "m2": "که بی تو شود تخت شاهی تهی",
      "verse_number": 109
    },
    {
      "id": "c38551f1-8f49-4419-8594-76fb74315aec",
      "m1": "سخن هرچ بشنیدی اکنون بگوی",
      "m2": "پیامش مرا کمتر از آب جوی",
      "verse_number": 110
    },
    {
      "id": "f5a90414-94e1-4c5b-bc69-6a55a46e1ea6",
      "m1": "گشادند گویا زبان این دو مرد",
      "m2": "برآورد پیچان یکی باد سرد",
      "verse_number": 111
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 111
  },
  "order_in_parent": 1
}