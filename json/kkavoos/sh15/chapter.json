{
  "id": "194eb703-fee1-4197-8d31-35db3a638578",
  "title": "بخش ۱۵",
  "verses": [
    {
      "id": "74835546-197d-4231-bfdc-8c8f7a2286f1",
      "m1": "چو رستم ز مازندران گشت باز",
      "m2": "شه اندر زمان رزم را کرد ساز",
      "verse_number": 1
    },
    {
      "id": "24cb53e3-bd71-4b3f-a3c2-22496aa58280",
      "m1": "سراپرده از شهر بیرون کشید",
      "m2": "سپه را همه سوی هامون کشید",
      "verse_number": 2
    },
    {
      "id": "ca73e7af-3dd9-4f9d-842d-21ba624d1339",
      "m1": "سپاهی که خورشید شد ناپدید",
      "m2": "چو گرد سیاه از میان بردمید",
      "verse_number": 3
    },
    {
      "id": "894efadc-3ccc-4510-a39a-421e32a41ec4",
      "m1": "نه دریا پدید و نه هامون و کوه",
      "m2": "زمین آمد از پای اسپان ستوه",
      "verse_number": 4
    },
    {
      "id": "74daa3dc-929b-4afb-9628-5cc28983a423",
      "m1": "همی راند لشکر بران سان دمان",
      "m2": "نجست ایچ هنگام رفتن زمان",
      "verse_number": 5
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 5
  }
}