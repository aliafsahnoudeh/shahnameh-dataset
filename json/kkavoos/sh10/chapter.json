{
  "id": "4295223a-dec9-4918-9a9d-cf406acbb9e9",
  "title": "بخش ۱۰",
  "verses": [
    {
      "id": "ee1020b2-24d9-4013-a273-05183bc0f285",
      "m1": "یکی مغفری خسروی بر سرش",
      "m2": "خوی آلوده ببر بیان در برش",
      "verse_number": 1
    },
    {
      "id": "c5210dd5-33ff-4653-bb6c-6f5ea8597720",
      "m1": "به ارژنگ سالار بنهاد روی",
      "m2": "چو آمد بر لشکر نامجوی",
      "verse_number": 2
    },
    {
      "id": "ba4cdf00-6c1f-48e6-8467-0d059abaee99",
      "m1": "یکی نعره زد در میان گروه",
      "m2": "تو گفتی بدرید دریا و کوه",
      "verse_number": 3
    },
    {
      "id": "22446610-181f-4f02-8359-657b9d499604",
      "m1": "برون آمد از خیمه ارژنگ دیو",
      "m2": "چو آمد به گوش اندرش آن غریو",
      "verse_number": 4
    },
    {
      "id": "5110cded-1792-4c7e-9507-35ddfb673c2e",
      "m1": "چو رستم بدیدش برانگیخت اسپ",
      "m2": "بیامد بر وی چو آذر گشسپ",
      "verse_number": 5
    },
    {
      "id": "4b099082-0c75-4c5b-a6b7-915b7b74c500",
      "m1": "سر و گوش بگرفت و یالش دلیر",
      "m2": "سر از تن بکندش به کردار شیر",
      "verse_number": 6
    },
    {
      "id": "67fad5b9-f147-4237-8d4c-f560e6fa163c",
      "m1": "پر از خون سر دیو کنده ز تن",
      "m2": "بینداخت ز آنسو که بود انجمن",
      "verse_number": 7
    },
    {
      "id": "cfaac54a-4119-4cf9-86b1-06a5a59bc376",
      "m1": "چو دیوان بدیدند گوپال اوی",
      "m2": "بدریدشان دل ز چنگال اوی",
      "verse_number": 8
    },
    {
      "id": "c268a980-1e32-4e3a-b65c-bef024f57bbc",
      "m1": "نکردند یاد بر و بوم و رست",
      "m2": "پدر بر پسر بر همی راه جست",
      "verse_number": 9
    },
    {
      "id": "26909c2c-6524-4e4d-bf34-f9a288b727db",
      "m1": "برآهیخت شمشیر کین پیلتن",
      "m2": "بپردخت یکباره زان انجمن",
      "verse_number": 10
    },
    {
      "id": "0fb35c1f-f804-4101-ba78-e07a65afe40b",
      "m1": "چو برگشت پیروز گیتیفروز",
      "m2": "بیامد دمان تا به کوه اسپروز",
      "verse_number": 11
    },
    {
      "id": "64241685-d1a9-4edc-90a8-c2ce16b75be6",
      "m1": "ز اولاد بگشاد خم کمند",
      "m2": "نشستند زیر درختی بلند",
      "verse_number": 12
    },
    {
      "id": "b944d259-8c97-48c9-a2fd-4a9d94bb76f3",
      "m1": "تهمتن ز اولاد پرسید راه",
      "m2": "به شهری کجا بود کاووس شاه",
      "verse_number": 13
    },
    {
      "id": "ed57c79f-3f6a-48be-bf23-40da2fff2b50",
      "m1": "چو بشنید ازو تیز بنهاد روی",
      "m2": "پیاده دوان پیش او راهجوی",
      "verse_number": 14
    },
    {
      "id": "cf83bf3f-e8f2-4c25-bb75-e6ef45fa8ae4",
      "m1": "چو آمد به شهر اندرون تاجبخش",
      "m2": "خروشی برآورد چون رعد رخش",
      "verse_number": 15
    },
    {
      "id": "e26c6b1b-7af2-41ef-bd05-b3c46ba570d4",
      "m1": "به ایرانیان گفت پس شهریار",
      "m2": "که بر ما سرآمد بد روزگار",
      "verse_number": 16
    },
    {
      "id": "deb79e59-64b0-44bb-949f-368446a0eb78",
      "m1": "خروشیدن رخشم آمد به گوش",
      "m2": "روان و دلم تازه شد زان خروش",
      "verse_number": 17
    },
    {
      "id": "c07d2ecb-f3e0-486b-9e49-66e5e3c1f4e7",
      "m1": "به گاه قباد این خروشش نکرد",
      "m2": "کجا کرد با شاه ترکان نبرد",
      "verse_number": 18
    },
    {
      "id": "ae74c6ed-dec1-42f4-b609-1e99146912fa",
      "m1": "بیامد هم اندر زمان پیش اوی",
      "m2": "یل دانشافروز پرخاشجوی",
      "verse_number": 19
    },
    {
      "id": "25aadd29-5fb8-43a3-9fd0-f4672ffb4cc5",
      "m1": "به نزدیک کاووس شد پیلتن",
      "m2": "همه سرفرازان شدند انجمن",
      "verse_number": 20
    },
    {
      "id": "48443736-86ad-4bcd-8f7a-cdf3dcbc00b2",
      "m1": "غریوید بسیار و بردش نماز",
      "m2": "بپرسیدش از رنجهای دراز",
      "verse_number": 21
    },
    {
      "id": "f5b954f5-b89f-4ffa-a3b9-4642e58fe696",
      "m1": "گرفتش به آغوش کاووس شاه",
      "m2": "ز زالش بپرسید و از رنج راه",
      "verse_number": 22
    },
    {
      "id": "8647a405-73b6-4d95-acb1-764b5d0e931d",
      "m1": "بدو گفت پنهان ازین جادوان",
      "m2": "همی رخش را کرد باید روان",
      "verse_number": 23
    },
    {
      "id": "9f82a1d6-6ccb-40bb-9fd3-7a34e6d12460",
      "m1": "چو آید به دیو سپید آگهی",
      "m2": "کز ارژنگ شد روی گیتی تهی",
      "verse_number": 24
    },
    {
      "id": "88ab7b86-2ce2-4434-9555-5e98dc80aea7",
      "m1": "که نزدیک کاووس شد پیلتن",
      "m2": "همه نره دیوان شوند انجمن",
      "verse_number": 25
    },
    {
      "id": "75ceda56-2672-4f0f-a107-e1b055dc8a93",
      "m1": "همه رنجهای تو بیبر شود",
      "m2": "ز دیوان جهان پر ز لشکر شود",
      "verse_number": 26
    },
    {
      "id": "111a7294-64ee-4d3f-85db-649bcd94e558",
      "m1": "تو اکنون ره خانهٔ دیو گیر",
      "m2": "به رنج اندرآور تن و تیغ و تیر",
      "verse_number": 27
    },
    {
      "id": "097342d5-c19e-4680-bcef-e21c9b09c0a3",
      "m1": "مگر یار باشدت یزدان پاک",
      "m2": "سر جادوان اندر آری به خاک",
      "verse_number": 28
    },
    {
      "id": "2630943d-a169-413f-bf8a-ef3bdfd42a89",
      "m1": "گذر کرد باید بر هفت کوه",
      "m2": "ز دیوان به هر جای کرده گروه",
      "verse_number": 29
    },
    {
      "id": "95742c69-019c-41e1-9689-290d053a43a0",
      "m1": "یکی غار پیش آیدت هولناک",
      "m2": "چنان چون شنیدم پر از بیم و باک",
      "verse_number": 30
    },
    {
      "id": "658a19ba-d820-415c-bc57-137a85f5e322",
      "m1": "گذارت بران نره دیوان جنگ",
      "m2": "همه رزم را ساخته چون پلنگ",
      "verse_number": 31
    },
    {
      "id": "2e305d04-6abd-4dd3-8683-4e4901db9feb",
      "m1": "به غار اندرون گاه دیو سپید",
      "m2": "کزویند لشکر به بیم و امید",
      "verse_number": 32
    },
    {
      "id": "56248add-88e6-443a-beed-b6c606ed40e6",
      "m1": "توانی مگر کردن او را تباه",
      "m2": "که اویست سالار و پشت سپاه",
      "verse_number": 33
    },
    {
      "id": "ed68b126-345f-4a7a-9458-82fbd35690c3",
      "m1": "سپه را ز غم چشمها تیره شد",
      "m2": "مرا چشم در تیرگی خیره شد",
      "verse_number": 34
    },
    {
      "id": "7210b2e4-de4f-48e1-a3ed-8ff796875528",
      "m1": "پزشکان به درمانش کردند امید",
      "m2": "به خون دل و مغز دیو سپید",
      "verse_number": 35
    },
    {
      "id": "b8812c14-f027-49db-af39-321761208832",
      "m1": "چنین گفت فرزانه مردی پزشک",
      "m2": "که چون خون او را بسان سرشک",
      "verse_number": 36
    },
    {
      "id": "0229748b-f3de-41f7-86f7-d54c0d9e9040",
      "m1": "چکانی سه قطره به چشم اندرون",
      "m2": "شود تیرگی پاک با خون برون",
      "verse_number": 37
    },
    {
      "id": "f7d2f469-2a94-448c-9dc8-a328bcb6df1b",
      "m1": "گو پیلتن جنگ را ساز کرد",
      "m2": "ازان جایگه رفتن آغاز کرد",
      "verse_number": 38
    },
    {
      "id": "aa10d074-cbb4-4713-aa54-6b6275f3dffa",
      "m1": "به ایرانیان گفت بیدار بید",
      "m2": "که من کردم آهنگ دیو سپید",
      "verse_number": 39
    },
    {
      "id": "375a36a5-36d7-437f-b15f-252a40abbad0",
      "m1": "یکی پیل جنگی و چارهگرست",
      "m2": "فراوان به گرداندرش لشکرست",
      "verse_number": 40
    },
    {
      "id": "9e67bea1-9814-4432-b14a-5c4c8f6b43e0",
      "m1": "گر ایدونک پشت من آرد به خم",
      "m2": "شما دیر مانید خوار و دژم",
      "verse_number": 41
    },
    {
      "id": "f0ba26be-7812-4dd2-9dc8-6155fe815a96",
      "m1": "وگر یار باشد خداوند هور",
      "m2": "دهد مر مرا اختر نیک زور",
      "verse_number": 42
    },
    {
      "id": "e4a016c1-47f0-4129-b01d-1ae38b8dc02c",
      "m1": "همان بوم و بر باز یابید و تخت",
      "m2": "به بار آید آن خسروانی درخت",
      "verse_number": 43
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 43
  }
}