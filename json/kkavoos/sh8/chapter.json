{
  "id": "15940b69-6df6-434c-a174-e42d3cef4bb9",
  "title": "بخش ۸",
  "verses": [
    {
      "id": "2f3da974-d0f7-4c37-bb59-9f69ca396373",
      "m1": "چو از آفرین گشت پرداخته",
      "m2": "بیاورد گلرنگ را ساخته",
      "verse_number": 1
    },
    {
      "id": "0cb1ac30-0a5d-4dbd-ae7a-fd6eb4aa93c8",
      "m1": "نشست از بر زین و ره برگرفت",
      "m2": "خم منزل جادو اندر گرفت",
      "verse_number": 2
    },
    {
      "id": "db4513a4-8d22-41df-bb39-ae1211aa59c8",
      "m1": "همی رفت پویان به راه دراز",
      "m2": "چو خورشید تابان بگشت از فراز",
      "verse_number": 3
    },
    {
      "id": "607ec848-78eb-4493-afd0-afecf6428061",
      "m1": "درخت و گیا دید و آب روان",
      "m2": "چنان چون بود جای مرد جوان",
      "verse_number": 4
    },
    {
      "id": "43628bca-63a2-4462-96d2-7c676dc51403",
      "m1": "چو چشم تذروان یکی چشمه دید",
      "m2": "یکی جام زرین برو پر نبید",
      "verse_number": 5
    },
    {
      "id": "47309507-5635-4960-8ae9-f383072eb072",
      "m1": "یکی غرم بریان و نان از برش",
      "m2": "نمکدان و ریچال گرد اندرش",
      "verse_number": 6
    },
    {
      "id": "4441f45d-e1e9-4b84-8269-9324979b34e5",
      "m1": "خور جادوان بد چو رستم رسید",
      "m2": "از آواز او دیو شد ناپدید",
      "verse_number": 7
    },
    {
      "id": "62810637-19fa-478d-ae11-1903ad3dc382",
      "m1": "فرود آمد از باره زین برگرفت",
      "m2": "به غرم و بنان اندر آمد شگفت",
      "verse_number": 8
    },
    {
      "id": "eca4d3aa-f333-4da1-8fbd-9d1aeec965cd",
      "m1": "نشست از بر چشمه فرخندهپی",
      "m2": "یکی جام زر دید پر کرده می",
      "verse_number": 9
    },
    {
      "id": "1093addb-c366-475d-9a67-3b8b96e00361",
      "m1": "ابا می یکی نیز تنبور یافت",
      "m2": "بیابان چنان خانهٔ سور یافت",
      "verse_number": 10
    },
    {
      "id": "54e61ea7-1073-4f87-88c6-9b67c1136c5a",
      "m1": "تهمتن مر آن را به بر در گرفت",
      "m2": "بزد رود و گفتارها برگرفت",
      "verse_number": 11
    },
    {
      "id": "281d9e45-b04a-41ad-9c15-62bed7484bd0",
      "m1": "که آواره و بد نشان رستم است",
      "m2": "که از روز شادیش بهره غم است",
      "verse_number": 12
    },
    {
      "id": "2f46098d-4348-461f-b35a-c2502c69c704",
      "m1": "همه جای جنگست میدان اوی",
      "m2": "بیابان و کوهست بستان اوی",
      "verse_number": 13
    },
    {
      "id": "683f3439-38fc-493b-87e7-7e36608722ac",
      "m1": "همه جنگ با شیر و نر اژدهاست",
      "m2": "کجا اژدها از کفش نا رهاست",
      "verse_number": 14
    },
    {
      "id": "3f3165bb-ee0b-4db0-a3d1-31c2bc455b76",
      "m1": "می و جام و بویا گل و میگسار",
      "m2": "نکردست بخشش ورا کردگار",
      "verse_number": 15
    },
    {
      "id": "1e9a1d1f-244e-41db-8dda-011c5451c66a",
      "m1": "همیشه به جنگ نهنگ اندر است",
      "m2": "و گر با پلنگان به جنگ اندر است",
      "verse_number": 16
    },
    {
      "id": "9342c011-698b-4ae2-b297-d0587faa64bf",
      "m1": "به گوش زن جادو آمد سرود",
      "m2": "همان نالهٔ رستم و زخم رود",
      "verse_number": 17
    },
    {
      "id": "862d7195-995d-4e0a-a0a6-bc40baae1382",
      "m1": "بیاراست رخ را بسان بهار",
      "m2": "وگر چند زیبا نبودش نگار",
      "verse_number": 18
    },
    {
      "id": "c06fea90-6ff6-44e6-8caa-80d2329f3f1f",
      "m1": "بر رستم آمد پر از رنگ و بوی",
      "m2": "بپرسید و بنشست نزدیک اوی",
      "verse_number": 19
    },
    {
      "id": "0a2d2add-fe1a-48cd-aaec-c937437d3111",
      "m1": "تهمتن به یزدان نیایش گرفت",
      "m2": "ابر آفرینها فزایش گرفت",
      "verse_number": 20
    },
    {
      "id": "a18d64a4-3c07-4b16-9523-f31f409764b0",
      "m1": "که در دشت مازندران یافت خوان",
      "m2": "می و جام با میگسار جوان",
      "verse_number": 21
    },
    {
      "id": "35199693-87dc-4270-8847-7e8f675144b0",
      "m1": "ندانست کاو جادوی ریمنست",
      "m2": "نهفته به رنگ اندر اهریمنست",
      "verse_number": 22
    },
    {
      "id": "7aa7bf6e-87f9-4a87-a387-60884669d839",
      "m1": "یکی طاس می بر کفش برنهاد",
      "m2": "ز دادار نیکی دهش کرد یاد",
      "verse_number": 23
    },
    {
      "id": "ac4863f2-fe57-4cba-b5bf-7f5f0171d082",
      "m1": "چو آواز داد از خداوند مهر",
      "m2": "دگرگونهتر گشت جادو به چهر",
      "verse_number": 24
    },
    {
      "id": "6152b690-b3e1-4195-b2e8-e487112ccb75",
      "m1": "روانش گمان نیایش نداشت",
      "m2": "زبانش توان ستایش نداشت",
      "verse_number": 25
    },
    {
      "id": "a8866922-6cd1-4cb3-b218-35ed5f131650",
      "m1": "سیه گشت چون نام یزدان شنید",
      "m2": "تهمتن سبک چون درو بنگرید",
      "verse_number": 26
    },
    {
      "id": "aee02f62-cdd5-48ca-9b0d-ccb63927f55d",
      "m1": "بینداخت از باد خم کمند",
      "m2": "سر جادو آورد ناگه ببند",
      "verse_number": 27
    },
    {
      "id": "9f22596a-08ab-4dd2-8519-4585f07548c8",
      "m1": "بپرسید و گفتش چه چیزی بگوی",
      "m2": "بدانگونه کت هست بنمای روی",
      "verse_number": 28
    },
    {
      "id": "98681903-3bab-4333-931c-4af8426a0381",
      "m1": "یکی گنده پیری شد اندر کمند",
      "m2": "پر آژنگ و نیرنگ و بند و گزند",
      "verse_number": 29
    },
    {
      "id": "3a848c36-1c0d-4383-9fe3-2df7a8d2607c",
      "m1": "میانش به خنجر به دو نیم کرد",
      "m2": "دل جادوان زو پر از بیم کرد",
      "verse_number": 30
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 30
  }
}