{
  "id": "8b2de538-9096-4f2a-ba55-cf6aaec69521",
  "title": "بخش ۱۷",
  "verses": [
    {
      "id": "d4b1870c-e65b-463c-b6c7-dbe2b12fbc07",
      "m1": "چو بهرام و خسرو به هامون شدند",
      "m2": "بر شیر با دل پر از خون شدند",
      "verse_number": 1
    },
    {
      "id": "9a89272e-2ee6-4327-bef9-7987ad1140d4",
      "m1": "چو خسرو بدید آن دو شیر ژیان",
      "m2": "نهاده یکی افسر اندر میان",
      "verse_number": 2
    },
    {
      "id": "3f21cdbd-2632-494f-8e59-9e13c3f65255",
      "m1": "بدان موبدان گفت تاج از نخست",
      "m2": "مر آن را سزاتر که شاهی بجست",
      "verse_number": 3
    },
    {
      "id": "14e9aa92-ef57-4791-89a9-97b765c245ed",
      "m1": "و دیگر که من پیرم و او جوان",
      "m2": "به چنگال شیر ژیان ناتوان",
      "verse_number": 4
    },
    {
      "id": "7414b4a8-fc12-4d29-9284-5b57fcebb2d9",
      "m1": "بران بد که او پیشدستی کند",
      "m2": "به برنایی و تندرستی کند",
      "verse_number": 5
    },
    {
      "id": "7c2aff22-ead3-4b13-91ba-2a5467ea6939",
      "m1": "بدو گفت بهرام کری رواست",
      "m2": "نهانی نداریم گفتار راست",
      "verse_number": 6
    },
    {
      "id": "3ce00c5c-8543-43b1-bf1f-666a298f7a18",
      "m1": "یکی گُرزه گاوسر برگرفت",
      "m2": "جهانی بدو مانده اندر شگفت",
      "verse_number": 7
    },
    {
      "id": "6dd73e63-533b-45dd-ace5-19f019cb3554",
      "m1": "بدو گفت موبد که ای پادشا",
      "m2": "خردمند و بادانش و پارسا",
      "verse_number": 8
    },
    {
      "id": "44938f26-1836-4c99-8c73-286b27749c06",
      "m1": "همی جنگ شیران که فرمایدت",
      "m2": "جز از تاج شاهی چه افزایدت",
      "verse_number": 9
    },
    {
      "id": "262acbdf-2a31-4d0c-b105-ed835d21cb5f",
      "m1": "تو جان از پی پادشاهی مده",
      "m2": "خورش بیبهانه به ماهی مده",
      "verse_number": 10
    },
    {
      "id": "4ba832c1-c488-4e6a-a28c-f3b04dc12c78",
      "m1": "همه بیگناهیم و این کار تست",
      "m2": "جهان را همه دل به بازار تست",
      "verse_number": 11
    },
    {
      "id": "12d8c00f-74d7-4067-ad07-82408ac27daf",
      "m1": "بدو گفت بهرام کای دینپژوه",
      "m2": "تو زین بیگناهی و دیگر گروه",
      "verse_number": 12
    },
    {
      "id": "4bc283bc-374c-4329-8926-88aa2da484a4",
      "m1": "همآورد این نره شیران منم",
      "m2": "خریدار جنگ دلیران منم",
      "verse_number": 13
    },
    {
      "id": "42655d20-df96-4d7c-8502-e0cc7e6597c2",
      "m1": "بدو گفت موبد به یزدان پناه",
      "m2": "چو رفتی دلت را بشوی از گناه",
      "verse_number": 14
    },
    {
      "id": "e12622a0-41fa-4a76-ad7a-879de03381dd",
      "m1": "چنان کرد کو گفت بهرامشاه",
      "m2": "دلش پاک شد توبه کرد از گناه",
      "verse_number": 15
    },
    {
      "id": "ad828f5a-ce15-472b-93cd-50f91b8b1487",
      "m1": "همی رفت با گُرزهٔ گاوروی",
      "m2": "چو دیدند شیران پرخاشجوی",
      "verse_number": 16
    },
    {
      "id": "4d5b20ee-8ac2-40ec-b6f0-37feb476fee1",
      "m1": "یکی زود زنجیر بگسست و بند",
      "m2": "بیامد بر شهریار بلند",
      "verse_number": 17
    },
    {
      "id": "2cb058c6-6ced-4f68-90eb-9170933694ae",
      "m1": "بزد بر سرش گُرز بهرام گُرد",
      "m2": "ز چشمش همی روشنایی ببرد",
      "verse_number": 18
    },
    {
      "id": "0576ac5b-28a9-4510-a107-fbd74d2a9b3b",
      "m1": "بر دیگر آمد بزد بر سرش",
      "m2": "فرو ریخت از دیده خون از برش",
      "verse_number": 19
    },
    {
      "id": "22ca88ae-1694-4642-a1b0-f348b9e3a9e8",
      "m1": "جهاندار بنشست بر تخت عاج",
      "m2": "به سر بر نهاد آن دلفروز تاج",
      "verse_number": 20
    },
    {
      "id": "a6f4f5ae-d7c7-477e-8eca-1ba3c376cc09",
      "m1": "به یزدان پناهید کو بد پناه",
      "m2": "نمایندهٔ راه گم کرده راه",
      "verse_number": 21
    },
    {
      "id": "fc3f827b-6030-4c01-b6cb-250fe5d42f13",
      "m1": "بشد خسرو و برد پیشش نماز",
      "m2": "چنین گفت کای شاه گردنفراز",
      "verse_number": 22
    },
    {
      "id": "0f545af5-ba87-453d-bc94-6e6336835253",
      "m1": "نشست تو بر گاه فرخنده باد",
      "m2": "یلان جهان پیش تو بنده باد",
      "verse_number": 23
    },
    {
      "id": "75dec4d5-978c-4499-8248-b1530283c80f",
      "m1": "تو شاهی و ما بندگان توایم",
      "m2": "به خوبی فزایندگان توایم",
      "verse_number": 24
    },
    {
      "id": "04bbb866-9265-4d3a-8913-5aa52bccce93",
      "m1": "بزرگان برو گوهر افشاندند",
      "m2": "بران تاج نو آفرین خواندند",
      "verse_number": 25
    },
    {
      "id": "f78a343e-4e45-44d0-abc7-43d6f60b6e0a",
      "m1": "ز گیتی برآمد سراسر خروش",
      "m2": "در آذر بد این جشن روز سروش",
      "verse_number": 26
    },
    {
      "id": "5317374b-1603-48db-8f84-ea6f28503d6d",
      "m1": "برآمد یکی ابر و شد تیرهماه",
      "m2": "همی تیر بارید ز ابر سیاه",
      "verse_number": 27
    },
    {
      "id": "186cae95-720b-400c-8a79-1c7d91f25c14",
      "m1": "نه دریا پدید و نه دشت و نه راغ",
      "m2": "نبینم همی در هوا پر زاغ",
      "verse_number": 28
    },
    {
      "id": "1cf6a8b3-d87f-4b55-94f9-fea5a36c103b",
      "m1": "حواصل فشاند هوا هر زمان",
      "m2": "چه سازد همی زین بلند آسمان",
      "verse_number": 29
    },
    {
      "id": "3fd3c948-3347-46d2-abab-c74941ebc947",
      "m1": "نماندم نمکسود و هیزم نه جو",
      "m2": "نه چیزی پدیدست تا جودرو",
      "verse_number": 30
    },
    {
      "id": "4aa1db68-207a-4347-ab93-049d88b103a9",
      "m1": "بدین تیرگی روز و بیم خراج",
      "m2": "زمین گشته از برف چون کوه عاج",
      "verse_number": 31
    },
    {
      "id": "5c634850-febd-47bf-ab17-541fb851a98f",
      "m1": "همه کارها را سراندر نشیب",
      "m2": "مگر دست گیرد حسین قتیب",
      "verse_number": 32
    },
    {
      "id": "ca5383c5-bd2a-4c11-9fd0-d9b448ecde00",
      "m1": "کنون داستانی بگویم شگفت",
      "m2": "کزان برتر اندازه نتوان گرفت",
      "verse_number": 33
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 33
  },
