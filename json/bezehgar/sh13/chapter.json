{
  "id": "b51a8956-1970-4e18-a71b-1263dba4490a",
  "title": "بخش ۱۳",
  "verses": [
    {
      "id": "a59655f1-9204-4286-8f38-ac30f171a0b2",
      "m1": "خود و شاه بهرام با رایزن",
      "m2": "نشستند و گفتند بیانجمن",
      "verse_number": 1
    },
    {
      "id": "f6222cf5-41fc-420d-85e0-222ae4fdddba",
      "m1": "سخنشان بران راست شد کز یمن",
      "m2": "به ایران خرامند با انجمن",
      "verse_number": 2
    },
    {
      "id": "c8d6a7a5-e5de-41f9-baa9-f712bf1f0222",
      "m1": "گزین کرد از تازیان سی هزار",
      "m2": "همه نیزهداران خنجرگزار",
      "verse_number": 3
    },
    {
      "id": "e98e63b4-2d2b-4cc5-a48b-b9fd768a060b",
      "m1": "به دینارشان یکسر آباد کرد",
      "m2": "سر نامداران پر از باد کرد",
      "verse_number": 4
    },
    {
      "id": "e525ba64-158f-4d94-9b8c-7b4ba5168fdd",
      "m1": "چو آگاهی این به ایران رسید",
      "m2": "جوانوی نزد دلیران رسید",
      "verse_number": 5
    },
    {
      "id": "381c831e-dcd1-404c-8f7c-713919c29357",
      "m1": "بزرگان ازان کار غمگین شدند",
      "m2": "بر آذر پاک برزین شدند",
      "verse_number": 6
    },
    {
      "id": "6186f0a8-3930-43e5-91f7-176c2b130a00",
      "m1": "ز یزدان همی خواستند آنک رزم",
      "m2": "مگر باز گردد به شادی و بزم",
      "verse_number": 7
    },
    {
      "id": "443d2d57-94c1-4efd-b658-0a879c2759c3",
      "m1": "چو منذر به نزدیک جهرم رسید",
      "m2": "برآن دشت بیآب لشکر کشید",
      "verse_number": 8
    },
    {
      "id": "f826c2ff-7430-44d5-b3ed-8e6a83b50c0f",
      "m1": "سراپرده زد راد بهرامشاه",
      "m2": "به گرد اندر آمد ز هر سو سپاه",
      "verse_number": 9
    },
    {
      "id": "9347c81a-4ad0-4704-a864-0753fe33eae1",
      "m1": "به منذر چنین گفت کای رایزن",
      "m2": "به جهرم رسیدی ز شهر یمن",
      "verse_number": 10
    },
    {
      "id": "ca52d522-0ce5-458a-8789-407d5d5c580c",
      "m1": "کنون جنگ سازیم گر گفتوگوی",
      "m2": "چو لشکر به روی اندر آورد روی",
      "verse_number": 11
    },
    {
      "id": "832ad466-967e-4ee5-b698-eaa53538a9c0",
      "m1": "بدو گفت منذر مهان را بخوان",
      "m2": "چو آیند پیشت بیارای خوان",
      "verse_number": 12
    },
    {
      "id": "724fc655-5b47-492a-887b-65e787c821f5",
      "m1": "سخن گوی و بشنو ازیشان سخن",
      "m2": "کسی تیز گردد تو تیزی مکن",
      "verse_number": 13
    },
    {
      "id": "f108257f-c2a4-422f-b262-41e6c00c4ebb",
      "m1": "بخوانیم تا چیستشان در نهان",
      "m2": "کرا خواند خواهند شاه جهان",
      "verse_number": 14
    },
    {
      "id": "96273560-4fd6-4e18-b9bd-614dc36022e2",
      "m1": "چو دانسته شد چارهٔ آن کنیم",
      "m2": "گر آسان بود کینه پنهان کنیم",
      "verse_number": 15
    },
    {
      "id": "ee3c9423-b8f2-4665-82dc-163280c65475",
      "m1": "ور ایدون کجا کین و جنگ آورند",
      "m2": "بپیچند و خوی پلنگ آورند",
      "verse_number": 16
    },
    {
      "id": "bd3cfcc5-9b9c-4f12-8eda-8449e099f15e",
      "m1": "من این دشت جهرم چو دریا کنم",
      "m2": "ز خورشید تابان ثریا کنم",
      "verse_number": 17
    },
    {
      "id": "16741a2b-255c-442c-9bfd-e3908ec1f2bc",
      "m1": "بر آنم که بینند چهر ترا",
      "m2": "چنین برز و بالا و مهر ترا",
      "verse_number": 18
    },
    {
      "id": "dd95f564-c798-4a4a-afee-9f89e9a07214",
      "m1": "خردمندی و رای و فرهنگ تو",
      "m2": "شکیبایی و دانش و سنگ تو",
      "verse_number": 19
    },
    {
      "id": "f919111a-eb7c-4fd3-bcad-778f34c357e0",
      "m1": "نخواهند جز تو کسی تخت را",
      "m2": "کله را و زیبایی بخت را",
      "verse_number": 20
    },
    {
      "id": "9d9dbd51-b1c3-4bba-b823-98db5f9d061c",
      "m1": "ور ایدونک گم کرده دارند راه",
      "m2": "بخواهند بردن همی از تو گاه",
      "verse_number": 21
    },
    {
      "id": "7bc1fc39-8dbb-4d75-be8b-e8131f9b0cff",
      "m1": "من و این سواران و شمشیر تیز",
      "m2": "برانگیزم اندر جهان رستخیز",
      "verse_number": 22
    },
    {
      "id": "413eab64-ff13-4965-8040-de62bf82cc94",
      "m1": "ببینی بروهای پرچین من",
      "m2": "فدای تو بادا تن و دین من",
      "verse_number": 23
    },
    {
      "id": "09746de4-73f7-439a-9648-07e8feb28175",
      "m1": "چو بینند بیمر سپاه مرا",
      "m2": "همان رسم و آیین و راه مرا",
      "verse_number": 24
    },
    {
      "id": "ae6bfd9a-7c02-4ea5-8e9e-c0ce78046f27",
      "m1": "همین پادشاهی که میراث تست",
      "m2": "پدر بر پدر کرد شاید درست",
      "verse_number": 25
    },
    {
      "id": "18327383-56e4-4860-b2e3-842e0b923d01",
      "m1": "سه دیگر که خون ریختن کار ماست",
      "m2": "همان ایزد دادگر یار ماست",
      "verse_number": 26
    },
    {
      "id": "f1de8a3d-3237-4beb-89e2-ab89a1321ded",
      "m1": "کسی را جز از تو نخواهند شاه",
      "m2": "که زیبای تاجی و زیبای گاه",
      "verse_number": 27
    },
    {
      "id": "d97995cf-47c7-4034-8d93-3a2f8ef883f4",
      "m1": "ز منذر چو شاه این سخنها شنید",
      "m2": "بخندید و شادان دلش بردمید",
      "verse_number": 28
    },
    {
      "id": "8418d9c0-6517-42c2-8a26-bcff386243f7",
      "m1": "چو خورشید برزد سر از تیغ کوه",
      "m2": "ردان و بزرگان ایران گروه",
      "verse_number": 29
    },
    {
      "id": "8957b039-aeb1-439c-915d-3a0c00c89453",
      "m1": "پذیره شدن را بیاراستند",
      "m2": "یکی دانشی انجمن خواستند",
      "verse_number": 30
    },
    {
      "id": "72896501-02c0-4b63-9141-c13490c8685d",
      "m1": "نهادند بهرام را تخت عاج",
      "m2": "به سر بر نهاده بهاگیر تاج",
      "verse_number": 31
    },
    {
      "id": "c4efd854-d7e0-4276-9eaf-9a5e0a1b0fc0",
      "m1": "نشستی به آیین شاهنشهان",
      "m2": "بیاراست کو بود شاه جهان",
      "verse_number": 32
    },
    {
      "id": "71a84365-4d3b-411b-9734-cc779a2cc245",
      "m1": "ز یک دست بهرام منذر نشست",
      "m2": "دگر دست نعمان و تیغی به دست",
      "verse_number": 33
    },
    {
      "id": "c04602a9-cb94-4ebc-bfe2-91e176491191",
      "m1": "همان گرد بر گرد پردهسرای",
      "m2": "ستاده بزرگان تازی به پای",
      "verse_number": 34
    },
    {
      "id": "096a07e6-aa53-4d19-a12e-c10bec513b72",
      "m1": "از ایرانیان آنک بد پاکرای",
      "m2": "بیامد به دهلیز پردهسرای",
      "verse_number": 35
    },
    {
      "id": "8a625824-d1b4-408b-952e-bbf2cfc02f5a",
      "m1": "بفرمود تا پرده برداشتند",
      "m2": "ز درشان به آواز بگذاشتند",
      "verse_number": 36
    },
    {
      "id": "b2cccafc-561c-4918-bdb3-5d6cfdc8d517",
      "m1": "به شاه جهان آفرین خواندند",
      "m2": "به مژگان همی خون برافشاندند",
      "verse_number": 37
    },
    {
      "id": "97e2ea03-d35c-46e1-a6a3-5b3e5c70be78",
      "m1": "رسیدند نزدیک بهرامشاه",
      "m2": "بدیدند زیبا یکی تاج و گاه",
      "verse_number": 38
    },
    {
      "id": "5c61220b-045f-4a12-ae7e-e2384705af17",
      "m1": "به آواز گفتند انوشه بدی",
      "m2": "همیشه ز تو دور دست بدی",
      "verse_number": 39
    },
    {
      "id": "ef43cefe-8cbb-4540-9473-e752acffb609",
      "m1": "شهنشاه پرسید و بنواختشان",
      "m2": "به اندازه بر پایگه ساختشان",
      "verse_number": 40
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 40
  },
  "order_in_parent": 13
}