{
  "id": "f3dac98e-a4c2-4cfb-9bac-ed8303c5933d",
  "title": "بخش ۱۰",
  "verses": [
    {
      "id": "a39a69e2-d05b-4312-b10c-030e03186c9a",
      "m1": "چو در دخمه شد شهریار جهان",
      "m2": "ز ایران برفتند گریان مهان",
      "verse_number": 1
    },
    {
      "id": "6bdb5b0d-a834-4bf6-b47a-77242e79f2ff",
      "m1": "کنارنگ با موبد و پهلوان",
      "m2": "هشیوار دستور روشنروان",
      "verse_number": 2
    },
    {
      "id": "62d2ee4f-207d-4b7b-b678-e654787077f3",
      "m1": "همه پاک در پارس گرد آمدند",
      "m2": "بر دخمه یزدگرد آمدند",
      "verse_number": 3
    },
    {
      "id": "2ebe8ca9-cac0-4f8b-a759-94059bf4aea9",
      "m1": "چو گستهم کو پیل کشتی بر اسپ",
      "m2": "دگر قارن گرد پور گشسپ",
      "verse_number": 4
    },
    {
      "id": "ef4ca077-b645-4462-a61c-a06931f7e430",
      "m1": "چو میلاد و چون پارس مرزبان",
      "m2": "چو پیروز اسپافگن از گرزبان",
      "verse_number": 5
    },
    {
      "id": "1284415b-6caa-4796-b1cf-87d406e0b855",
      "m1": "دگر هرک بودند ز ایران مهان",
      "m2": "بزرگان و کنداوران جهان",
      "verse_number": 6
    },
    {
      "id": "f434a42e-500f-450f-82c5-efd9a3c5946e",
      "m1": "کجا خوارشان داشتی یزدگرد",
      "m2": "همه آمدند اندران شهرگرد",
      "verse_number": 7
    },
    {
      "id": "2830d092-933c-45f5-9fc7-fa8e0822afae",
      "m1": "چنین گفت گویا گشسپ دبیر",
      "m2": "که ای نامداران برنا و پیر",
      "verse_number": 8
    },
    {
      "id": "b9d9e7e4-49ab-48c0-9faf-5fe77c447cdb",
      "m1": "جهاندارمان تا جهان آفرید",
      "m2": "کسی زین نشان شهریاری ندید",
      "verse_number": 9
    },
    {
      "id": "20c5c900-c630-419f-9a2c-dbb4d8a0b237",
      "m1": "که جز کشتن و خواری و درد و رنج",
      "m2": "بیاگندن از چیز درویش گنج",
      "verse_number": 10
    },
    {
      "id": "bb322ea2-a7a4-4bf3-8ff3-a38a32179340",
      "m1": "ازین شاه ناپاکتر کس ندید",
      "m2": "نه از نامداران پیشین شنید",
      "verse_number": 11
    },
    {
      "id": "d1a6f09d-437f-45fd-89c5-0b90975a7fcc",
      "m1": "نخواهیم بر تخت زین تخمهکس",
      "m2": "ز خاکش به یزدان پناهیم و بس",
      "verse_number": 12
    },
    {
      "id": "3891a844-8112-4912-9bb9-d3eaf2e086d0",
      "m1": "سرافراز بهرام فرزند اوست",
      "m2": "ز مغز و دل و رای پیوند اوست",
      "verse_number": 13
    },
    {
      "id": "041ee8d5-2e21-4d35-a163-1a3cbe281a8c",
      "m1": "ز منذر گشاید سخن سربسر",
      "m2": "نخواهیم بر تخت بیدادگر",
      "verse_number": 14
    },
    {
      "id": "a33c4e02-3be1-4eb6-b2c4-514503cbd8cc",
      "m1": "بخوردند سوگندهای گران",
      "m2": "هرانکس که بودند ایرانیان",
      "verse_number": 15
    },
    {
      "id": "619911ad-f879-4ef6-a475-94cdd7f005e2",
      "m1": "کزین تخمه کس را به شاهنشهی",
      "m2": "نخواهیم با تاج و تخت مهی",
      "verse_number": 16
    },
    {
      "id": "7fdad067-960c-4d4b-bd22-60bd0a8e97ed",
      "m1": "برین برنهادند و برخاستند",
      "m2": "همی شهریاری دگر خواستند",
      "verse_number": 17
    },
    {
      "id": "abfee9ac-6c8a-4555-96b8-fc412eb83b7a",
      "m1": "چو آگاهی مرگ شاه جهان",
      "m2": "پراگنده شد در میان مهان",
      "verse_number": 18
    },
    {
      "id": "436a02e6-faf0-4d68-a147-e1710fbb7851",
      "m1": "الان شاه و چون پارس پهلوسپاه",
      "m2": "چو بیورد و شگنان زرین کلاه",
      "verse_number": 19
    },
    {
      "id": "6f0b09c6-b452-458c-8496-f3a66aa12ad6",
      "m1": "همی هریکی گفت شاهی مراست",
      "m2": "هم از خاک تا برج ماهی مراست",
      "verse_number": 20
    },
    {
      "id": "82e659e4-1724-4db2-91cc-91e979442cff",
      "m1": "جهانی پرآشوب شد سر به سر",
      "m2": "چو از تخت گم شد سر تاجور",
      "verse_number": 21
    },
    {
      "id": "8bdf448e-2f21-4bdd-8d5e-94553a90f526",
      "m1": "به ایران رد و موبد و پهلوان",
      "m2": "هرانکس که بودند روشنروان",
      "verse_number": 22
    },
    {
      "id": "86a01efb-ef4d-44da-a4e3-ddfa60aee622",
      "m1": "بدین کار در پارس گرد آمدند",
      "m2": "بسی زین نشان داستانها زدند",
      "verse_number": 23
    },
    {
      "id": "18241b9b-6755-484c-ba9f-994961a880cd",
      "m1": "که این تاج شاهی سزاوار کیست",
      "m2": "ببینید تا از در کار کیست",
      "verse_number": 24
    },
    {
      "id": "2862950d-2d9d-4f49-891b-188f4c7a79d8",
      "m1": "بجویید بخشندهای دادگر",
      "m2": "که بندد برین تخت زرین کمر",
      "verse_number": 25
    },
    {
      "id": "3ad78bbc-005a-4325-8e9c-5b783c311a60",
      "m1": "که آشوب بنشاند از روزگار",
      "m2": "جهان مرغزاریست بیشهریار",
      "verse_number": 26
    },
    {
      "id": "f52a747e-0cc8-4c87-acc7-48c5acb25bf3",
      "m1": "یکی مرد بد پیر خسرو به نام",
      "m2": "جوانمرد و روشندل و شادکام",
      "verse_number": 27
    },
    {
      "id": "d03a796d-ea5d-455f-bfae-59db68d6ebc6",
      "m1": "هم از تخمه سرفرازان بد اوی",
      "m2": "به مرز اندر از بینیازان بد اوی",
      "verse_number": 28
    },
    {
      "id": "9e7a2c81-b181-4024-a8f6-c927d210d0f9",
      "m1": "سپردند گردان بدو تاج و گاه",
      "m2": "برو انجمن شد ز هر سو سپاه",
      "verse_number": 29
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 29
  }
}