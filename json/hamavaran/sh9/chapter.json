{
  "id": "94b919e0-2f0e-4e91-821a-f8f810dcda0c",
  "title": "بخش ۹",
  "verses": [
    {
      "id": "5d7e530c-dcf1-4d65-9230-b59aedb03ed8",
      "m1": "همی کرد پوزش ز بهر گناه",
      "m2": "مر او را همی جست هر سو سپاه",
      "verse_number": 1
    },
    {
      "id": "9cddd669-7638-451f-85c3-86cb5e1bd6e3",
      "m1": "خبر یافت زو رستم و گیو و طوس",
      "m2": "برفتند با لشکری گشن و کوس",
      "verse_number": 2
    },
    {
      "id": "05a1078f-6d28-49cd-a7b5-80b273b075a4",
      "m1": "به رستم چنین گفت گودرز پیر",
      "m2": "که تا کرد مادر مرا سیر شیر",
      "verse_number": 3
    },
    {
      "id": "725da600-53b0-43c9-8997-e2c543457371",
      "m1": "همی بینم اندر جهان تاج و تخت",
      "m2": "کیان و بزرگان بیدار بخت",
      "verse_number": 4
    },
    {
      "id": "9a09fdfe-0e1a-481f-9f58-5103003f1c75",
      "m1": "چو کاووس نشنیدم اندر جهان",
      "m2": "ندیدم کس از کهتران و مهان",
      "verse_number": 5
    },
    {
      "id": "a3d5184b-e701-4246-b0da-d5dab0dc755d",
      "m1": "خرد نیست او را نه دانش نه رای",
      "m2": "نه هوشش بجایست و نه دل بجای",
      "verse_number": 6
    },
    {
      "id": "676ca10d-45a9-4f3c-b9c9-49e40429fad6",
      "m1": "رسیدند پس پهلوانان بدوی",
      "m2": "نکوهش گر و تیز و پرخاشجوی",
      "verse_number": 7
    },
    {
      "id": "80cbc0f3-cf6d-41b1-94e9-ab2b04c6274c",
      "m1": "بدو گفت گودرز بیمارستان",
      "m2": "ترا جای زیباتر از شارستان",
      "verse_number": 8
    },
    {
      "id": "10a942de-d22f-4972-b67a-f686e142ca26",
      "m1": "به دشمن دهی هر زمان جای خویش",
      "m2": "نگویی به کس بیهده رای خویش",
      "verse_number": 9
    },
    {
      "id": "fa958183-b303-4fe4-a37f-12385997e060",
      "m1": "سه بارت چنین رنج و سختی فتاد",
      "m2": "سرت ز آزمایش نگشت اوستاد",
      "verse_number": 10
    },
    {
      "id": "d814ce26-13e3-4913-82e9-0f3e2dc858db",
      "m1": "کشیدی سپه را به مازندران",
      "m2": "نگر تا چه سختی رسید اندران",
      "verse_number": 11
    },
    {
      "id": "e86f11c3-6cac-479d-894d-161be55bf106",
      "m1": "دگرباره مهمان دشمن شدی",
      "m2": "صنم بودی اکنون برهمن شدی",
      "verse_number": 12
    },
    {
      "id": "97cd3aff-8fe7-4b04-9354-327d1ce28bb7",
      "m1": "به گیتی جز از پاک یزدان نماند",
      "m2": "که منشور تیغ ترا برنخواند",
      "verse_number": 13
    },
    {
      "id": "9358fecd-ea90-47da-a5e0-254162d56419",
      "m1": "به جنگ زمین سر به سر تاختی",
      "m2": "کنون باسمان نیز پرداختی",
      "verse_number": 14
    },
    {
      "id": "cf534707-e8f2-4f27-b497-d89d7c9041ac",
      "m1": "پس از تو بدین داستانی کنند",
      "m2": "که شاهی برآمد به چرخ بلند",
      "verse_number": 15
    },
    {
      "id": "879f50b3-f075-42dd-9f95-72765148d79b",
      "m1": "که تا ماه و خورشید را بنگرد",
      "m2": "ستاره یکایک همی بشمرد",
      "verse_number": 16
    },
    {
      "id": "7f6180aa-cf54-49b1-9be2-818885c978c4",
      "m1": "همان کن که بیدار شاهان کنند",
      "m2": "ستاینده و نیکخواهان کنند",
      "verse_number": 17
    },
    {
      "id": "6d6f184d-c449-4a04-95fb-0823822d82e5",
      "m1": "جز از بندگی پیش یزدان مجوی",
      "m2": "مزن دست در نیک و بد جز بدوی",
      "verse_number": 18
    },
    {
      "id": "f12df236-d523-4cfb-b79f-c70844f1cf58",
      "m1": "چنین داد پاسخ که از راستی",
      "m2": "نیاید به کار اندرون کاستی",
      "verse_number": 19
    },
    {
      "id": "04eca5e2-2152-498c-ba76-841161610e5d",
      "m1": "همی داد گفتی و بیداد نیست",
      "m2": "ز نام تو جان من آزاد نیست",
      "verse_number": 20
    },
    {
      "id": "51276c40-1883-49c3-bbb1-a675f675145a",
      "m1": "فروماند کاووس و تشویر خورد",
      "m2": "ازان نامداران روز نبرد",
      "verse_number": 21
    },
    {
      "id": "05ff4f2f-6205-415c-9fad-701328a36d89",
      "m1": "بسیچید و اندر عماری نشست",
      "m2": "پشیمانی و درد بودش بدست",
      "verse_number": 22
    },
    {
      "id": "489816ab-33d7-4a72-bbd1-b20ec07fda0e",
      "m1": "چو آمد بر تخت و گاه بلند",
      "m2": "دلش بود زان کار مانده نژند",
      "verse_number": 23
    },
    {
      "id": "39c6b66e-5f2b-47ad-98c0-d75d7d86b10d",
      "m1": "چهل روز بر پیش یزدان به پای",
      "m2": "بپیمود خاک و بپرداخت جای",
      "verse_number": 24
    },
    {
      "id": "376adf4d-9281-463f-853b-c96b9ad55e9e",
      "m1": "همی ریخت از دیدگان آب زرد",
      "m2": "همی از جهانآفرین یاد کرد",
      "verse_number": 25
    },
    {
      "id": "a2547830-4201-41cc-8ef8-03634237c822",
      "m1": "ز شرم از در کاخ بیرون نرفت",
      "m2": "همی پوست گفتی برو بر به کفت",
      "verse_number": 26
    },
    {
      "id": "8845404c-c4ee-487d-84cc-bb0815d60a1b",
      "m1": "همی ریخت از دیده پالوده خون",
      "m2": "همی خواست آمرزش رهنمون",
      "verse_number": 27
    },
    {
      "id": "3bdb08e9-661b-406d-b300-32e347e67186",
      "m1": "ز شرم دلیران منش کرد پست",
      "m2": "خرام و در بار دادن ببست",
      "verse_number": 28
    },
    {
      "id": "963a1392-bee1-47ba-bf42-0a0494a04e9c",
      "m1": "پشیمان شد و درد بگزید و رنج",
      "m2": "نهاده ببخشید بسیار گنج",
      "verse_number": 29
    },
    {
      "id": "b4347b7d-eb92-4762-8b6d-4f6dab965b58",
      "m1": "همی رخ بمالید بر تیره خاک",
      "m2": "نیایش کنان پیش یزدان پاک",
      "verse_number": 30
    },
    {
      "id": "d1c10c2b-ae2c-4983-bd8f-32c5b9e0d441",
      "m1": "چو بگذشت یک چند گریان چنین",
      "m2": "ببخشود بر وی جهانآفرین",
      "verse_number": 31
    },
    {
      "id": "77092135-cd84-446e-a8f5-17bffc9dc6c3",
      "m1": "یکی داد نو ساخت اندر جهان",
      "m2": "که تابنده شد بر کهان و مهان",
      "verse_number": 32
    },
    {
      "id": "5e735997-956a-4490-85f0-15d4c2253608",
      "m1": "جهان گفتی از داد دیبا شدست",
      "m2": "همان شاه بر گاه زیبا شدست",
      "verse_number": 33
    },
    {
      "id": "53030886-fdfc-4062-bf25-d3807ab69ec3",
      "m1": "ز هر کشوری نامور مهتری",
      "m2": "که بر سر نهادی بلند افسری",
      "verse_number": 34
    },
    {
      "id": "512bcd8d-27f0-4805-83db-87e03b805a0f",
      "m1": "به درگاه کاووس شاه آمدند",
      "m2": "وزان سرکشیدن به راه آمدند",
      "verse_number": 35
    },
    {
      "id": "775fb8b8-3a67-4382-9208-79985e08b894",
      "m1": "زمانه چنان شد که بود از نخست",
      "m2": "به آب وفا روی خسرو بشست",
      "verse_number": 36
    },
    {
      "id": "88790ca5-1aaa-447f-8472-c55891897751",
      "m1": "همه مهتران کهتر او شدند",
      "m2": "پرستنده و چاکر او شدند",
      "verse_number": 37
    },
    {
      "id": "6e43b9dc-ad6b-46a3-9d0a-c375bcaf083f",
      "m1": "کجا پادشا دادگر بود و بس",
      "m2": "نیازش نیاید بفریادرس",
      "verse_number": 38
    },
    {
      "id": "62f18d88-5b98-4ea4-9530-bebd35114c0b",
      "m1": "بدین داستان گفتم آن کم شنود",
      "m2": "کنون رزم رستم بباید سرود",
      "verse_number": 39
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 39
  },
