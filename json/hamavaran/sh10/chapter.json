{
  "id": "2e2c7ef4-8b9a-4958-91e6-d56f3818323e",
  "title": "بخش ۱۰",
  "verses": [
    {
      "id": "166db18a-f7de-4f96-8f76-c748dabf5f46",
      "m1": "چه گفت آن سراینده مرد دلیر",
      "m2": "که ناگه برآویخت با نره شیر",
      "verse_number": 1
    },
    {
      "id": "b060c7a1-e27a-4275-b1b6-69595e960531",
      "m1": "که گر نام مردی بجویی همی",
      "m2": "رخ تیغ هندی بشویی همی",
      "verse_number": 2
    },
    {
      "id": "b208f023-11b3-4dc6-961a-b19462a6ead5",
      "m1": "ز بدها نبایدت پرهیز کرد",
      "m2": "که پیش آیدت روز ننگ و نبرد",
      "verse_number": 3
    },
    {
      "id": "a402eeed-b038-40e8-b021-8748da448bf7",
      "m1": "زمانه چو آمد بتنگی فراز",
      "m2": "هم از تو نگردد به پرهیز باز",
      "verse_number": 4
    },
    {
      "id": "6aab12a1-ef0c-486c-b594-030185111548",
      "m1": "چو همره کنی جنگ را با خرد",
      "m2": "دلیرت ز جنگآوران نشمرد",
      "verse_number": 5
    },
    {
      "id": "f6849e53-877a-41ad-b622-c509c9bb68a9",
      "m1": "خرد را و دین را رهی دیگرست",
      "m2": "سخنهای نیکو به بند اندرست",
      "verse_number": 6
    },
    {
      "id": "455f49d6-fc5f-4954-8d02-30d6abbb5fa3",
      "m1": "کنون از ره رستم جنگجوی",
      "m2": "یکی داستانست با رنگ و بوی",
      "verse_number": 7
    },
    {
      "id": "216c5635-bf61-448f-829f-876a0ffb386e",
      "m1": "شنیدم که روزی گو پیلتن",
      "m2": "یکی سور کرد از در انجمن",
      "verse_number": 8
    },
    {
      "id": "bf1572b3-7629-4418-8c26-1553bceb17f3",
      "m1": "به جایی کجا نام او بد نوند",
      "m2": "بدو اندرون کاخهای بلند",
      "verse_number": 9
    },
    {
      "id": "7f41235c-e7ec-4866-82cc-2ff679ca8c02",
      "m1": "کجا آذر تیز برزین کنون",
      "m2": "بدانجا فروزد همی رهنمون",
      "verse_number": 10
    },
    {
      "id": "ad05671c-47ab-4839-997b-7c9e4a47ec41",
      "m1": "بزرگان ایران بدان بزمگاه",
      "m2": "شدند انجمن نامور یک سپاه",
      "verse_number": 11
    },
    {
      "id": "786ec959-91aa-4cbc-bd8c-c417979fd216",
      "m1": "چو طوس و چو گودرز کشوادگان",
      "m2": "چو بهرام و چون گیو آزادگان",
      "verse_number": 12
    },
    {
      "id": "e1cb33d1-1437-4ec8-b358-754f14a3d3a3",
      "m1": "چو گرگین و چون زنگهٔ شاوران",
      "m2": "چو گستهم و خراد جنگآوران",
      "verse_number": 13
    },
    {
      "id": "40694f46-daa7-423b-b00b-15f045d4ab6a",
      "m1": "چو برزین گردنکش تیغ زن",
      "m2": "گرازه کجا بد سر انجمن",
      "verse_number": 14
    },
    {
      "id": "7a2a5b0e-afa1-44b1-ab66-4a36038e8ea7",
      "m1": "ابا هر یک از مهتران مرد چند",
      "m2": "یکی لشکری نامدار ارجمند",
      "verse_number": 15
    },
    {
      "id": "c3b6fc09-86f6-4997-ac8a-b133bc873870",
      "m1": "نیاسود لشکر زمانی ز کار",
      "m2": "ز چوگان و تیر و نبید و شکار",
      "verse_number": 16
    },
    {
      "id": "808f2ade-a7db-4247-afc4-e3949a50cf9f",
      "m1": "به مستی چنین گفت یک روز گیو",
      "m2": "به رستم که ای نامبردار نیو",
      "verse_number": 17
    },
    {
      "id": "730a588f-05ca-41c6-ace8-9e6e4dfb4420",
      "m1": "گر ایدون که رای شکار آیدت",
      "m2": "چو یوز دونده به کار آیدت",
      "verse_number": 18
    },
    {
      "id": "e2883e03-afd8-4de0-8e54-b27a1b6be07d",
      "m1": "به نخچیرگاه رد افراسیاب",
      "m2": "بپوشیم تابان رخ آفتاب",
      "verse_number": 19
    },
    {
      "id": "263cc6a8-af4c-4013-97db-d3c2cefd59c4",
      "m1": "ز گرد سواران و از یوز و باز",
      "m2": "بگیریم آرام روز دراز",
      "verse_number": 20
    },
    {
      "id": "90dcbae9-6511-4355-acb1-bba9b13e7f19",
      "m1": "به گور تگاور کمند افگنیم",
      "m2": "به شمشیر بر شیر بند افگنیم",
      "verse_number": 21
    },
    {
      "id": "3203efb4-0193-434f-b730-8b22aa2ab871",
      "m1": "بدان دشت توران شکاری کنیم",
      "m2": "که اندر جهان یادگاری کنیم",
      "verse_number": 22
    },
    {
      "id": "c9ad1f4d-f245-4c18-ad00-05db67e434c1",
      "m1": "بدو گفت رستم که بیکام تو",
      "m2": "مبادا گذر تا سرانجام تو",
      "verse_number": 23
    },
    {
      "id": "7e72a006-cead-43bb-b6ca-207b74efac30",
      "m1": "سحرگه بدان دشت توران شویم",
      "m2": "ز نخچیر و از تاختن نغنویم",
      "verse_number": 24
    },
    {
      "id": "c7a7405b-1025-4e26-8fe6-5d97075f93b4",
      "m1": "ببودند یکسر برین هم سخن",
      "m2": "کسی رای دیگر نیفگند بن",
      "verse_number": 25
    },
    {
      "id": "840c226b-8347-4459-8461-cdaebe265244",
      "m1": "سحرگه چو از خواب برخاستند",
      "m2": "بران آرزو رفتن آراستند",
      "verse_number": 26
    },
    {
      "id": "9ccb8181-5a93-4ec1-8d8b-34e13ae2957d",
      "m1": "برفتند با باز و شاهین و مهد",
      "m2": "گرازنده و شاد تا رود شهد",
      "verse_number": 27
    },
    {
      "id": "b2df17ed-3079-4e05-a5dc-e24be3d2c6db",
      "m1": "به نخچیرگاه رد افراسیاب",
      "m2": "ز یک دست ریگ و ز یک دست آب",
      "verse_number": 28
    },
    {
      "id": "806734b8-ea69-4a53-8df5-cd893ff357d0",
      "m1": "دگر سو سرخس و بیابانش پیش",
      "m2": "گله گشته بر دشت آهو و میش",
      "verse_number": 29
    },
    {
      "id": "943bca54-4158-4b2a-b304-5fa01bba23d1",
      "m1": "همه دشت پر خرگه و خیمه گشت",
      "m2": "از انبوه آهو سراسیمه گشت",
      "verse_number": 30
    },
    {
      "id": "95cd8f60-61f8-4eff-931b-59024ebbc7f3",
      "m1": "ز درنده شیران زمین شد تهی",
      "m2": "به پرنده مرغان رسید آگهی",
      "verse_number": 31
    },
    {
      "id": "1027caea-e807-46e0-9583-e1fafb8cdd6a",
      "m1": "تلی هر سویی مرغ و نخجیر بود",
      "m2": "اگر کشته گر خستهٔ تیر بود",
      "verse_number": 32
    },
    {
      "id": "4ed2b0f3-8d30-4938-8df7-c82b9cc80ff9",
      "m1": "ز خنده نیاسود لب یک زمان",
      "m2": "ببودند روشن دل و شادمان",
      "verse_number": 33
    },
    {
      "id": "0faabe93-d9d7-4a04-bad3-07a19b59ea72",
      "m1": "به یک هفته زینگونه با می بدست",
      "m2": "گهی تاختن گه نشاط نشست",
      "verse_number": 34
    },
    {
      "id": "4c8ff482-3486-41b4-a486-96486cf6807e",
      "m1": "بهشتم تهمتن بیامد پگاه",
      "m2": "یکی رای شایسته زد با سپاه",
      "verse_number": 35
    },
    {
      "id": "90ce6815-ad67-406f-8811-e8ab888538dc",
      "m1": "چنین گفت رستم بدان سرکشان",
      "m2": "بدان گرزداران مردمکشان",
      "verse_number": 36
    },
    {
      "id": "9340d723-025f-49df-a421-6ac0e04f05a8",
      "m1": "که از ما به افراسیاب این زمان",
      "m2": "همانا رسید آگهی بیگمان",
      "verse_number": 37
    },
    {
      "id": "d065dd1e-cd20-4ed4-ba24-44462733a7e6",
      "m1": "یکی چاره سازد بیاید بجنگ",
      "m2": "کند دشت نخچیر بر یوز تنگ",
      "verse_number": 38
    },
    {
      "id": "e8e4f062-34a2-406b-9581-4bc3b3c55724",
      "m1": "بباید طلایه به ره بر یکی",
      "m2": "که چون آگهی یابد او اندکی",
      "verse_number": 39
    },
    {
      "id": "3be434ff-737a-47b0-a4f2-077d7dae3d17",
      "m1": "بیاید دهد آگهی از سپاه",
      "m2": "نباید که گیرد بداندیش راه",
      "verse_number": 40
    },
    {
      "id": "a3c3f932-208d-4f47-9d4b-a788b016bd93",
      "m1": "گرازه به زه بر نهاده کمان",
      "m2": "بیامد بران کار بسته میان",
      "verse_number": 41
    },
    {
      "id": "b5b1dbe7-c1d7-46c4-85fb-ea64382c7c42",
      "m1": "سپه را که چون او نگهدار بود",
      "m2": "همه چارهٔ دشمنان خوار بود",
      "verse_number": 42
    },
    {
      "id": "688f952c-8dce-42c3-9f9d-5fd328fbaf1d",
      "m1": "به نخچیر و خوردن نهادند روی",
      "m2": "نکردند کس یاد پرخاشجوی",
      "verse_number": 43
    },
    {
      "id": "1c8f3497-31f4-4713-b1ed-3c92602bc842",
      "m1": "پس آگاهی آمد به افراسیاب",
      "m2": "ازیشان شب تیره هنگام خواب",
      "verse_number": 44
    },
    {
      "id": "6b798a67-0192-49ee-b127-d6730ceec9b8",
      "m1": "ز لشکر جهاندیدگان را بخواند",
      "m2": "ز رستم بسی داستانها براند",
      "verse_number": 45
    },
    {
      "id": "f67f5892-25a5-4c16-8de3-52e706d3bb1d",
      "m1": "وزان هفت گرد سوار دلیر",
      "m2": "که بودند هر یک به کردار شیر",
      "verse_number": 46
    },
    {
      "id": "e9c3260d-2419-4e37-9bb3-8fe280ae316b",
      "m1": "که ما را بباید کنون ساختن",
      "m2": "بناگاه بردن یکی تاختن",
      "verse_number": 47
    },
    {
      "id": "08375328-94dc-4fe3-925f-ab6ba6442a9e",
      "m1": "گراین هفت یل را بچنگ آوریم",
      "m2": "جهان پیش کاووس تنگ آوریم",
      "verse_number": 48
    },
    {
      "id": "881cf90b-c682-42a1-9d82-9b2dccf50539",
      "m1": "بکردار نخچیر باید شدن",
      "m2": "بناگاه لشکر برایشان زدن",
      "verse_number": 49
    },
    {
      "id": "93a25225-b8cf-45c5-82ae-c838fc8bc843",
      "m1": "گزین کرد شمشیر زن سیهزار",
      "m2": "همه رزمجو از در کارزار",
      "verse_number": 50
    },
    {
      "id": "4c1874d4-c991-45c9-8220-6d0a42b2332c",
      "m1": "چنین گفت با نامداران جنگ",
      "m2": "که ما را کنون نیست جای درنگ",
      "verse_number": 51
    },
    {
      "id": "2bb218ee-7828-4b67-8132-c91c59133ab4",
      "m1": "به راه بیابان برون تاختند",
      "m2": "همه جنگ را گردن افراختند",
      "verse_number": 52
    },
    {
      "id": "cfd0e1df-48a1-413a-9113-ea319281f778",
      "m1": "ز هر سو فرستاد بیمر سپاه",
      "m2": "بدان سرکشان تا بگیرند راه",
      "verse_number": 53
    },
    {
      "id": "9cbc9b33-5247-4629-8d3f-a603d96e722b",
      "m1": "گرازه چو گرد سپه را بدید",
      "m2": "بیامد سپه را همه بنگرید",
      "verse_number": 54
    },
    {
      "id": "433eaf32-6963-45a8-9747-25e1680219c2",
      "m1": "بدید آنک شد روی گیتی سیاه",
      "m2": "درفش سپهدار توران سپاه",
      "verse_number": 55
    },
    {
      "id": "99baebfc-546f-4462-ae61-efa2ef1e11e4",
      "m1": "ازانجا چو باد دمان گشت باز",
      "m2": "تو گفتی به زخم اندر آمد گراز",
      "verse_number": 56
    },
    {
      "id": "25765fc1-f59c-4c0b-b17d-4f66ca6a2247",
      "m1": "بیامد دمان تا به نخچیرگاه",
      "m2": "تهمتن همی خورد می با سپاه",
      "verse_number": 57
    },
    {
      "id": "45c7ee5d-209d-4725-9a6a-bb335c7ef1d2",
      "m1": "چنین گفت با رستم شیرمرد",
      "m2": "که برخیز و از خرمی بازگرد",
      "verse_number": 58
    },
    {
      "id": "43380e82-d9c5-49f2-b712-aaf6fd9dd262",
      "m1": "که چندان سپاهست کاندازه نیست",
      "m2": "ز لشکر بلندی و پستی یکیست",
      "verse_number": 59
    },
    {
      "id": "a6ebc394-1769-438d-a587-305133e13f04",
      "m1": "درفش جفاپیشه افراسیاب",
      "m2": "همی تابد از گرد چون آفتاب",
      "verse_number": 60
    },
    {
      "id": "712c777c-38b1-4bb9-9f14-d26c19aded4a",
      "m1": "چو بشنید رستم بخندید سخت",
      "m2": "بدو گفت با ماست پیروز بخت",
      "verse_number": 61
    },
    {
      "id": "80d9534f-3131-46ef-bb0d-a2818924795e",
      "m1": "تو از شاه ترکان چه ترسی چنین",
      "m2": "ز گرد سواران توران زمین",
      "verse_number": 62
    },
    {
      "id": "b3bcf26c-22e1-464f-a0fd-683fa80faa4f",
      "m1": "سپاهش فزون نیست از صدهزار",
      "m2": "عنان پیچ و برگستوانور سوار",
      "verse_number": 63
    },
    {
      "id": "47a06df5-f686-4e3d-b497-a2a0340e4724",
      "m1": "بدین دشت کین بر گر از ما یکیست",
      "m2": "همی جنگ ترکان بچشم اندکیست",
      "verse_number": 64
    },
    {
      "id": "091acd4d-d146-41cd-aea8-33b57a410202",
      "m1": "شده هفت گرد سوار انجمن",
      "m2": "چنین نامبردار و شمشیرزن",
      "verse_number": 65
    },
    {
      "id": "cf6c84e8-eb95-4b73-99bf-f3f6289a7888",
      "m1": "یکی باشد از ما وزیشان هزار",
      "m2": "سپه چند باید ز ترکان شمار",
      "verse_number": 66
    },
    {
      "id": "9fa83512-33fe-4003-a0e1-57da2de8c2fc",
      "m1": "برین دشت اگر ویژه تنها منم",
      "m2": "که بر پشت گلرنگ در جوشنم",
      "verse_number": 67
    },
    {
      "id": "d8a3654f-74e4-48ad-95c9-995d871f569f",
      "m1": "چنو کینه خواهی بیاید مرا",
      "m2": "از ایران سپاهی نباید مرا",
      "verse_number": 68
    },
    {
      "id": "66b1fbc6-7a6d-45f0-be22-a3c8bbe3d3b1",
      "m1": "تو ای میگسار از می بابلی",
      "m2": "بپیمای تا سر یکی بلبلی",
      "verse_number": 69
    },
    {
      "id": "7af2dbd1-0482-41ed-841e-be1bdf7d74bf",
      "m1": "بپیمود می ساقی و داد زود",
      "m2": "تهمتن شد از دادنش شاد زود",
      "verse_number": 70
    },
    {
      "id": "9c1bb4bb-b0b9-4f7f-9129-27769ab9316f",
      "m1": "به کف بر نهاد آن درخشنده جام",
      "m2": "نخستین ز کاووس کی برد نام",
      "verse_number": 71
    },
    {
      "id": "4951c282-acf6-45c1-bba4-9896890bde84",
      "m1": "که شاه زمانه مرا یاد باد",
      "m2": "همیشه بروبومش آباد باد",
      "verse_number": 72
    },
    {
      "id": "a5f8b7b7-96e9-4cd6-911a-b9235774b470",
      "m1": "ازان پس تهمتن زمین داد بوس",
      "m2": "چنین گفت کاین باده بر یاد طوس",
      "verse_number": 73
    },
    {
      "id": "b9d0db91-e2ec-479b-bc6d-082b32c5b92d",
      "m1": "سران جهاندار برخاستند",
      "m2": "ابا پهلوان خواهش آراستند",
      "verse_number": 74
    },
    {
      "id": "29752d40-a2a3-4c5e-9b4c-5f9c6310cf58",
      "m1": "که ما را بدین جام می جای نیست",
      "m2": "به می با تو ابلیس را پای نیست",
      "verse_number": 75
    },
    {
      "id": "18ba3397-bf76-4655-a686-fab25cf4634b",
      "m1": "می و گرز یک زخم و میدان جنگ",
      "m2": "جز از تو کسی را نیامد به چنگ",
      "verse_number": 76
    },
    {
      "id": "09fa7094-1b01-4fc9-ac46-29ed5238efb3",
      "m1": "می بابلی سرخ در جام زرد",
      "m2": "تهمتن بروی زواره بخورد",
