{
  "id": "91525a10-7502-4359-af8a-8a4619a1803d",
  "title": "بخش ۶",
  "verses": [
    {
      "id": "ce1ec435-5f4c-4435-814e-c7a474f415fb",
      "m1": "فرستاده شد نزد قیصر ز شاه",
      "m2": "سواری که اندر نوردید راه",
      "verse_number": 1
    },
    {
      "id": "a0438d43-5e8e-4e2c-8699-8c4ba6e11203",
      "m1": "بفرمود کز نامداران روم",
      "m2": "کسی کاو بنازد بران مرز و بوم",
      "verse_number": 2
    },
    {
      "id": "258f043c-7f1d-4960-8777-e7957caa36b5",
      "m1": "جهان دیده باید عناندار کس",
      "m2": "سنان و سپر بایدش یار بس",
      "verse_number": 3
    },
    {
      "id": "f244a6d3-0319-4f29-824c-d437a2dbaca4",
      "m1": "چنین لشکری باید از مرز روم",
      "m2": "که آیند با من به آباد بوم",
      "verse_number": 4
    },
    {
      "id": "634373e5-d0d1-48c2-b7fc-69a06ff0158a",
      "m1": "پس آگاهی آمد ز هاماوران",
      "m2": "بدشت سواران نیزهوران",
      "verse_number": 5
    },
    {
      "id": "cb014481-e436-4350-a61b-e1032b05504d",
      "m1": "که رستم به مصر و به بربر چه کرد",
      "m2": "بران شهریاران به روز نبرد",
      "verse_number": 6
    },
    {
      "id": "fe732acb-f6cf-433d-8c54-ed14bc7a4de2",
      "m1": "دلیری بجستند گرد و سوار",
      "m2": "عنان پیچ و مردافگن و نیزهدار",
      "verse_number": 7
    },
    {
      "id": "7846de4e-e1de-4201-8e15-feebc8389e0f",
      "m1": "نوشتند نامه یکی مردوار",
      "m2": "سخنهای شایسته و آبدار",
      "verse_number": 8
    },
    {
      "id": "9b7df71b-4305-4d51-9b65-ef99a61e1e6e",
      "m1": "چو از گرگساران بیامد سپاه",
      "m2": "که جویند گاه سرافراز شاه",
      "verse_number": 9
    },
    {
      "id": "5f55e907-82eb-46bd-a391-66fa4e45d2b3",
      "m1": "دل ما شد از کار ایشان بدرد",
      "m2": "که دلشان چنین برتری یاد کرد",
      "verse_number": 10
    },
    {
      "id": "305c92c4-82c8-4324-9e6d-3699899bdcfd",
      "m1": "همی تاج او خواست افراسیاب",
      "m2": "ز راه خرد سرش گشته شتاب",
      "verse_number": 11
    },
    {
      "id": "9dfd8f6c-1982-45ba-9cd3-f4a0f80b7ce0",
      "m1": "برفتیم با نیزههای دراز",
      "m2": "برو تلخ کردیم آرام و ناز",
      "verse_number": 12
    },
    {
      "id": "0ef4ca01-3710-4fc8-9ac9-47f8063960f3",
      "m1": "ازیشان و از ما بسی کشته شد",
      "m2": "زمانه به هر نیک و بد گشته شد",
      "verse_number": 13
    },
    {
      "id": "62454698-a9f1-4663-b9e3-1e8d1c5d9526",
      "m1": "کنون کآمد از کار او آگهی",
      "m2": "که تازه شد آن تخت شاهنشهی",
      "verse_number": 14
    },
    {
      "id": "6fac0cf8-a19f-41c4-a1d4-994a580f080f",
      "m1": "همه نامداران شمشیرزن",
      "m2": "برین کینه گه بر شدند انجمن",
      "verse_number": 15
    },
    {
      "id": "8bbdbb82-f43d-49d1-a754-fe374a917899",
      "m1": "چو شه برگراید ز بربر عنان",
      "m2": "به گردن برآریم یکسر سنان",
      "verse_number": 16
    },
    {
      "id": "fcd6df81-ecf7-40c7-8456-19c5d0a1bf0d",
      "m1": "زمین کوه تا کوه پرخون کنیم",
      "m2": "ز دشمن بیابان چو جیحون کنیم",
      "verse_number": 17
    },
    {
      "id": "4e7ece03-6e6a-47ee-a690-fbd937c02e14",
      "m1": "فرستاده تازی برافگند و رفت",
      "m2": "به بربرستان روی بنهاد و تفت",
      "verse_number": 18
    },
    {
      "id": "ef04a6e9-e210-426f-b663-9e305e9e1fe2",
      "m1": "چو نامه بر شاه ایران رسید",
      "m2": "بران گونه گفتار بایسته دید",
      "verse_number": 19
    },
    {
      "id": "da4e7679-b68d-4b55-beb8-22608757f077",
      "m1": "ازیشان پسند آمدش کارکرد",
      "m2": "به افراسیاب آن زمان نامه کرد",
      "verse_number": 20
    },
    {
      "id": "b0f5a1d7-01d9-4ee3-8748-2c4ecd1c2273",
      "m1": "که ایران بپرداز و بیشی مجوی",
      "m2": "سر ما شد از تو پر از گفتوگوی",
      "verse_number": 21
    },
    {
      "id": "edac9ffa-b27e-4dcf-af92-fd86c80d7b0c",
      "m1": "ترا شهر توران بسندست خود",
      "m2": "به خیره همی دست یازی ببد",
      "verse_number": 22
    },
    {
      "id": "70c68dde-fec4-48d3-b634-ad375fa067cd",
      "m1": "فزونی مجوی ار شدی بینیاز",
      "m2": "که درد آردت پیش رنج دراز",
      "verse_number": 23
    },
    {
      "id": "e7c74b48-2406-458a-9925-4483fd1bd658",
      "m1": "ترا کهتری کار بستن نکوست",
      "m2": "نگه داشتن بر تن خویش پوست",
      "verse_number": 24
    },
    {
      "id": "6a981e1c-6906-49c3-bd6f-f6307f26f98c",
      "m1": "ندانی که ایران نشست منست",
      "m2": "جهان سر به سر زیر دست منست",
      "verse_number": 25
    },
    {
      "id": "275d5c75-7e57-40ca-beb0-95535ed1c457",
      "m1": "پلنگ ژیان گرچه باشد دلیر",
      "m2": "نیارد شدن پیش چنگال شیر",
      "verse_number": 26
    },
    {
      "id": "f47d1b3f-039d-4d57-9127-3e652e41f96a",
      "m1": "چو آگاهی آمد به افراسیاب",
      "m2": "سرش پر ز کین گشت و دل پرشتاب",
      "verse_number": 27
    },
    {
      "id": "3fcea169-4d7d-454a-bf40-beba13a18699",
      "m1": "فرستاد پاسخش کاین گفتوگوی",
      "m2": "نزیبد جز از مردم زشت خوی",
      "verse_number": 28
    },
    {
      "id": "0300ba86-aaef-4df2-88cf-74ae7d4410c9",
      "m1": "ترا گر سزا بودی ایران بدان",
      "m2": "نیازت نبودی به مازندران",
      "verse_number": 29
    },
    {
      "id": "8eeb299d-5ae0-4be3-b4d9-67a3ab17eacc",
      "m1": "چنین گفت کایران دو رویه مراست",
      "m2": "بباید شنیدن سخنهای راست",
      "verse_number": 30
    },
    {
      "id": "12499ec2-482c-4b18-bd4b-ba08b8c31f63",
      "m1": "که پور فریدون نیای منست",
      "m2": "همه شهر ایران سرای منست",
      "verse_number": 31
    },
    {
      "id": "345cc7a1-9310-46c5-8bc3-50cb7a8eecd7",
      "m1": "و دیگر به بازوی شمشیرزن",
      "m2": "تهی کردم از تازیان انجمن",
      "verse_number": 32
    },
    {
      "id": "6e3f1d01-29aa-4331-a957-c85fbcffcb28",
      "m1": "به شمشیر بستانم از کوه تیغ",
      "m2": "عقاب اندر آرم ز تاریک میغ",
      "verse_number": 33
    },
    {
      "id": "63709914-1932-4058-9706-7c44c2748ece",
      "m1": "کنون آمدم جنگ را ساخته",
      "m2": "درفش درفشان برافراخته",
      "verse_number": 34
    },
    {
      "id": "321fdd2f-2ab6-4e1e-8815-eaf99c63b356",
      "m1": "فرستاده برگشت مانند باد",
      "m2": "سخنها به کاووس کی کرد یاد",
      "verse_number": 35
    },
    {
      "id": "1d283e89-0884-47ae-8b84-3cee5f555a2c",
      "m1": "چو بشنید کاووس گفتار اوی",
      "m2": "بیاراست لشکر به پیکار اوی",
      "verse_number": 36
    },
    {
      "id": "d1add0e9-62e0-4f50-8e57-76abb510e4ca",
      "m1": "ز بربر بیامد سوی سوریان",
      "m2": "یکی لشکری بیکران و میان",
      "verse_number": 37
    },
    {
      "id": "f85694f1-1aa4-4f57-b1d1-010c3b7394df",
      "m1": "به جنگش بیاراست افراسیاب",
      "m2": "به گردون همی خاک برزد ز آب",
      "verse_number": 38
    },
    {
      "id": "4e350adc-0ebe-4821-9a5d-ed83f7f2efcc",
      "m1": "جهان کر شد از نالهٔ بوق و کوس",
      "m2": "زمین آهنین شد هوا آبنوس",
      "verse_number": 39
    },
    {
      "id": "0aa0f2af-7ab0-42aa-8dd3-83624fae6f20",
      "m1": "ز زخم تبرزین و از بس ترنگ",
      "m2": "همی موج خون خاست از دشت جنگ",
      "verse_number": 40
    },
    {
      "id": "8e3269f7-b997-4448-a841-4a5b175e1e60",
      "m1": "سر بخت گردان افراسیاب",
      "m2": "بران رزمگاه اندر آمد بخواب",
      "verse_number": 41
    },
    {
      "id": "687627c7-27e3-4501-a10c-59a4b15ba5d0",
      "m1": "دو بهره ز توران سپه کشته شد",
      "m2": "سرسرکشان پاک برگشته شد",
      "verse_number": 42
    },
    {
      "id": "0c68004f-16cb-4e01-84b3-9379d17ae722",
      "m1": "سپهدار چون کار زانگونه دید",
      "m2": "بیآتش بجوشید همچون نبید",
      "verse_number": 43
    },
    {
      "id": "2ed11cef-5f08-462f-83b3-c1c478415d13",
      "m1": "به آواز گفت ای دلیران من",
      "m2": "گزیده یلان نره شیران من",
      "verse_number": 44
    },
    {
      "id": "fbf29565-a867-4f29-8ba3-eb61ee41f47f",
      "m1": "شما را ز بهر چنین روزگار",
      "m2": "همی پرورانیدم اندر کنار",
      "verse_number": 45
    },
    {
      "id": "bc771086-913b-4704-8abb-42c246d9ffae",
      "m1": "بکوشید و هم پشت جنگ آورید",
      "m2": "جهان را به کاووس تنگ آورید",
      "verse_number": 46
    },
    {
      "id": "a54c38ba-1f2e-41ea-8e8c-dcce94621bc7",
      "m1": "یلان را به ژوپین و خنجر زنید",
      "m2": "دلیرانشان سر به سر بفگنید",
      "verse_number": 47
    },
    {
      "id": "a44c1c9a-2121-41ab-8bda-e3d28d25e879",
      "m1": "همان سگزی رستم شیردل",
      "m2": "که از شیر بستد به شمشیر دل",
      "verse_number": 48
    },
    {
      "id": "ec7b76a8-ec41-4da7-8a1c-fd8faffc89b4",
      "m1": "بود کز دلیری ببند آورید",
      "m2": "سرش را به دام گزند آورید",
      "verse_number": 49
    },
    {
      "id": "ac79afdf-5e72-4a7c-b4c5-eb8833c0dc8e",
      "m1": "هرآنکس که او را به روز نبرد",
      "m2": "ز زین پلنگ اندر آرد به گرد",
      "verse_number": 50
    },
    {
      "id": "d4375005-4135-4035-a7d8-b0604f781075",
      "m1": "دهم دختر خویش و شاهی ورا",
      "m2": "برآرم سر از برج ماهی ورا",
      "verse_number": 51
    },
    {
      "id": "03e503f1-7d34-4517-b1fa-092136d91192",
      "m1": "چو ترکان شنیدند گفتار اوی",
      "m2": "سراسر سوی رزم کردند روی",
      "verse_number": 52
    },
    {
      "id": "99d1d162-5ffd-4c8e-b6dd-effdff75b11c",
      "m1": "بشد تیز با لشکر سوریان",
      "m2": "بدان سود جستن سرآمد زیان",
      "verse_number": 53
    },
    {
      "id": "7d49facd-0d45-4078-aa89-1efb37e71568",
      "m1": "چو روشن زمانه بران گونه دید",
      "m2": "ازانجا سوی شهر توران کشید",
      "verse_number": 54
    },
    {
      "id": "d76522d5-808b-4cd1-a77b-57ee68d151c6",
      "m1": "دلش خسته و کشته لشکر دو بهر",
      "m2": "همی نوش جست از جهان یافت زهر",
      "verse_number": 55
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 55
  }
}