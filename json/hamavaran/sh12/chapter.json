{
  "id": "26310145-82c7-4009-aeb0-a1c9b2070d96",
  "title": "بخش ۱۲",
  "verses": [
    {
      "id": "d1fbb88a-1154-4cd4-b75f-94bf337d8f25",
      "m1": "تهمتن برانگیخت رخش از شتاب",
      "m2": "پس پشت جنگ آور افراسیاب",
      "verse_number": 1
    },
    {
      "id": "4c89a91d-617a-48bd-8cad-46b4754ed4f5",
      "m1": "چنین گفت با رخش کای نیک یار",
      "m2": "مکن سستی اندر گه کارزار",
      "verse_number": 2
    },
    {
      "id": "4e4ffe59-6314-4911-93d3-cd9e4029482d",
      "m1": "که من شاه را بر تو بیجان کنم",
      "m2": "به خون سنگ را رنگ مرجان کنم",
      "verse_number": 3
    },
    {
      "id": "cde73360-3222-4090-a0c8-72293f65a9a6",
      "m1": "چنان گرم شد رخش آتش گهر",
      "m2": "که گفتی برآمد ز پهلوش پر",
      "verse_number": 4
    },
    {
      "id": "fb1157dd-ec36-4de2-b754-e85e7b6275d9",
      "m1": "ز فتراک بگشاد رستم کمند",
      "m2": "همی خواست آورد او را ببند",
      "verse_number": 5
    },
    {
      "id": "42a91607-8901-4bf9-aaa9-37a3f1b2e51c",
      "m1": "به ترک اندر افتاد خم دوال",
      "m2": "سپهدار ترکان بدزدید یال",
      "verse_number": 6
    },
    {
      "id": "8a277c97-68b6-4648-9fe9-488cc3dc4ada",
      "m1": "و دیگر که زیر اندرش بادپای",
      "m2": "به کردار آتش برآمد ز جای",
      "verse_number": 7
    },
    {
      "id": "35a32053-43e7-4c9c-9171-43dde2946ec4",
      "m1": "بجست از کمند گو پیلتن",
      "m2": "دهن خشک وز رنج پر آب تن",
      "verse_number": 8
    },
    {
      "id": "627f64cf-04da-4341-9f3c-3b7459062a60",
      "m1": "ز لشکر هرانکس که بد جنگساز",
      "m2": "دو بهره نیامد به خرگاه باز",
      "verse_number": 9
    },
    {
      "id": "f2fa555c-7bc2-4ac7-a7c3-6aebe321a7d0",
      "m1": "اگر کشته بودند اگر خسته تن",
      "m2": "گرفتار در دست آن انجمن",
      "verse_number": 10
    },
    {
      "id": "a8073a7a-4e1a-415b-8f87-19c95607b58a",
      "m1": "ز پرمایه اسپان زرین ستام",
      "m2": "ز ترگ و ز شمشیر زرین نیام",
      "verse_number": 11
    },
    {
      "id": "0056a6be-03e6-4832-bd5b-cd4e71603ecc",
      "m1": "جزین هرچه پرمایهتر بود نیز",
      "m2": "به ایرانیان ماند بسیار چیز",
      "verse_number": 12
    },
    {
      "id": "ebc4543e-bfcf-4bc7-ab46-ca855e25543c",
      "m1": "میان بازنگشاد کس کشته را",
      "m2": "نجستند مردان برگشته را",
      "verse_number": 13
    },
    {
      "id": "b61ebde8-7100-4fb3-b5f5-a5673cd00c59",
      "m1": "بدان دشت نخچیر باز آمدند",
      "m2": "ز هر نیکویی بینیاز آمدند",
      "verse_number": 14
    },
    {
      "id": "bfe08251-66e1-42eb-affe-be9c2593c4d7",
      "m1": "نوشتند نامه به کاووس شاه",
      "m2": "ز ترکان وز دشت نخچیرگاه",
      "verse_number": 15
    },
    {
      "id": "438ecfd8-289b-463e-82a1-97b09c0975af",
      "m1": "وزان کز دلیران نشد کشته کس",
      "m2": "زواره ز اسپ اندر افتاد و بس",
      "verse_number": 16
    },
    {
      "id": "b6ccd6be-a102-41f1-a5de-84c3723fc97c",
      "m1": "بران دشت فرخنده بر پهلوان",
      "m2": "دو هفته همی بود روشنروان",
      "verse_number": 17
    },
    {
      "id": "2d25c7ef-7bce-4ea4-83a5-975c43610a25",
      "m1": "سیم را به درگاه شاه آمدند",
      "m2": "به دیدار فرخ کلاه آمدند",
      "verse_number": 18
    },
    {
      "id": "8a43e3e5-ee53-4716-af92-c1aa846576b5",
      "m1": "چنین است رسم سرای سپنج",
      "m2": "یکی زو تن آسان و دیگر به رنج",
      "verse_number": 19
    },
    {
      "id": "8c20a57b-fd15-45c9-9f5b-b58f503be3bf",
      "m1": "برین و بران روز هم بگذرد",
      "m2": "خردمند مردم چرا غم خورد",
      "verse_number": 20
    },
    {
      "id": "7246db8f-e88f-431f-b72d-d18221c0c0bc",
      "m1": "سخنهای این داستان شد به بن",
      "m2": "ز سهراب و رستم سرایم سخن",
      "verse_number": 21
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 21
  }
}