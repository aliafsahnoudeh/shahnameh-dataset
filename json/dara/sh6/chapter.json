{
  "id": "1111996d-7e04-49ed-a19b-1c4316fcfd25",
  "title": "بخش ۶",
  "verses": [
    {
      "id": "21ae9f4a-ddab-4844-b84e-23e6f06a4317",
      "m1": "سکندر چو از کارش آگاه شد",
      "m2": "که دارا به تخت افسر ماه شد",
      "verse_number": 1
    },
    {
      "id": "c49fa80e-0096-451f-940f-088ce679c033",
      "m1": "سپه برگرفت از عراق و براند",
      "m2": "به رومی همی نام یزدان بخواند",
      "verse_number": 2
    },
    {
      "id": "2a04f069-ec3e-4407-9756-0761863fe81b",
      "m1": "سپه را میان و کرانه نبود",
      "m2": "همان بخت دارا جوانه نبود",
      "verse_number": 3
    },
    {
      "id": "8bbdc7df-74b5-4370-8cc3-3d107aff76da",
      "m1": "پذیره شدن را بیاراست شاه",
      "m2": "بیاورد ز اصطخر چندان سپاه",
      "verse_number": 4
    },
    {
      "id": "4cedb017-a877-4be4-9905-4d8ca8c6f225",
      "m1": "که گفتی ستاره نتابد همی",
      "m2": "فلک راه رفتن نیابد همی",
      "verse_number": 5
    },
    {
      "id": "dda8555f-c775-4a05-a315-9f6e29cf5dd0",
      "m1": "سپاه دو کشور کشیدند صف",
      "m2": "همه نیزه و گرز و خنجر به کف",
      "verse_number": 6
    },
    {
      "id": "c54b1cea-b275-46e7-99b5-3fa94c7b377f",
      "m1": "برآمد چنان از دو لشکر خروش",
      "m2": "که چرخ فلک را بدرید گوش",
      "verse_number": 7
    },
    {
      "id": "41c05091-c1d9-4f40-832a-faa1ab9590c2",
      "m1": "چو دریا شد از خون گردان زمین",
      "m2": "تن بیسران بد همه دشت کین",
      "verse_number": 8
    },
    {
      "id": "702e8f6f-a554-4e97-a624-2ce3220a7ea5",
      "m1": "پدر را نبد بر پسر جای مهر",
      "m2": "بریشان نبخشید گردان سپهر",
      "verse_number": 9
    },
    {
      "id": "7397c9d0-0385-48c4-94c6-1495c1d968c9",
      "m1": "سیم ره به دارا درآمد شکست",
      "m2": "سکندر میان تاختن را ببست",
      "verse_number": 10
    },
    {
      "id": "86099c5d-5b4f-4dff-89ce-2a0de73833e8",
      "m1": "جهاندار لشکر به کرمان کشید",
      "m2": "همی از بد دشمنان جان کشید",
      "verse_number": 11
    },
    {
      "id": "231943d4-0fde-42c2-985e-5eb68561077e",
      "m1": "سکندر بیامد زی اصطخر پارس",
      "m2": "که دیهیم شاهان بد و فخر پارس",
      "verse_number": 12
    },
    {
      "id": "34cf7d5a-b00d-4517-b3ca-9a1939bfe840",
      "m1": "خروشی بلند آمد از بارگاه",
      "m2": "که ای مهتران نماینده راه",
      "verse_number": 13
    },
    {
      "id": "d7536c16-e455-4e62-8dce-927df3414c80",
      "m1": "هرانکس که زنهار خواهد همی",
      "m2": "ز کرده به یزدان پناهد همی",
      "verse_number": 14
    },
    {
      "id": "39110374-eb86-4a67-8831-60f72da3e8a1",
      "m1": "همه یکسره در پناه منید",
      "m2": "بدانید اگر نیکخواه منید",
      "verse_number": 15
    },
    {
      "id": "1d24f913-95bb-40a4-8702-0d01bab6132a",
      "m1": "همه خستگان را ببخشیم چیز",
      "m2": "همان خون دشمن نریزیم نیز",
      "verse_number": 16
    },
    {
      "id": "24df2667-6a75-449e-a054-980297aeb9da",
      "m1": "ز چیز کسان دست کوته کنیم",
      "m2": "خرد را سوی روشنی ره کنیم",
      "verse_number": 17
    },
    {
      "id": "8428f329-0ff1-4ef9-9283-a9e28d565173",
      "m1": "که پیروزگر دادمان فرهی",
      "m2": "بزرگی و دیهیم شاهنشهی",
      "verse_number": 18
    },
    {
      "id": "576a3180-d0c5-42a4-9044-8a55f5dbcf72",
      "m1": "کسی کو ز فرمان ما بگذرد",
      "m2": "همی گردن اژدها بشکرد",
      "verse_number": 19
    },
    {
      "id": "744a6530-12ea-49f6-b31f-a0edc200d4c6",
      "m1": "ز چیزی که دید اندران رزمگاه",
      "m2": "ببخشید یکسر همه بر سپاه",
      "verse_number": 20
    },
    {
      "id": "52d35af9-e36a-48b0-a0ba-ce839fc82d02",
      "m1": "چو دارا ز ایران به کرمان رسید",
      "m2": "دو بهر از بزرگان لشکر ندید",
      "verse_number": 21
    },
    {
      "id": "347f4b4f-e4f5-4688-a763-dd56ff122316",
      "m1": "خروشی بد اندر میان سپاه",
      "m2": "یکی را ندیدند بر سر کلاه",
      "verse_number": 22
    },
    {
      "id": "ee4f8e0b-028a-4298-ba32-6ee01ca0d024",
      "m1": "بزرگان فرزانه را گرد کرد",
      "m2": "کسی را که با او بد اندر نبرد",
      "verse_number": 23
    },
    {
      "id": "87d3d365-7681-4022-80ff-fc890aba3054",
      "m1": "همه مهتران زار و گریان شدند",
      "m2": "ز بخت بد خویش بریان شدند",
      "verse_number": 24
    },
    {
      "id": "ea8390cd-7700-409f-b6a5-52e7e1c8f15e",
      "m1": "چنین گفت دارا که هم بیگمان",
      "m2": "ز ما بود بر ما بد آسمان",
      "verse_number": 25
    },
    {
      "id": "3bdc4f10-5ad9-4539-a8dc-0a9af498e2d9",
      "m1": "شکن زین نشان در جهان کس ندید",
      "m2": "نه از کاردانان پیشین شنید",
      "verse_number": 26
    },
    {
      "id": "71cd3b25-ee8f-4df8-bab8-c55f3b9fb093",
      "m1": "زن و کودک شهریاران اسیر",
      "m2": "وگر کشته خسته به ژوپین و تیر",
      "verse_number": 27
    },
    {
      "id": "918c42d2-a3bc-434c-b9fa-39a188b8e812",
      "m1": "چه بینید و این را چه درمان کنید",
      "m2": "که بدخواه را زین پشیمان کنید",
      "verse_number": 28
    },
    {
      "id": "1e255b19-e929-47a7-8ca7-80aa3256385c",
      "m1": "نه کشور نه لشکر نه تخت و کلاه",
      "m2": "نه شاهی نه فرزند و گنج و سپاه",
      "verse_number": 29
    },
    {
      "id": "7286dab2-47d8-4b2b-9ac1-11a0fccc9c60",
      "m1": "ار ایدونک بخشایش کردگار",
      "m2": "نباشد تبه شد به ما روزگار",
      "verse_number": 30
    },
    {
      "id": "58a5d672-0d11-44e8-bf12-fe24915daa16",
      "m1": "کسی کز گرانمایگان زیستند",
      "m2": "به پیش شهنشاه بگریستند",
      "verse_number": 31
    },
    {
      "id": "5996e539-5a29-4fe5-ad03-37736f14b806",
      "m1": "به آواز گفتند کای شهریار",
      "m2": "همه خستهایم از بد روزگار",
      "verse_number": 32
    },
    {
      "id": "1e708ee4-4e11-4c68-a4a7-81a46b7d75ba",
      "m1": "سپه را ز کوشش سخن درگذشت",
      "m2": "ز تارک دم آب برتر گذشت",
      "verse_number": 33
    },
    {
      "id": "91379f81-5b5b-4452-bef7-22393d518ffb",
      "m1": "پدر بیپسر شد پسر بیپدر",
      "m2": "چنین آمد از چرخ گردان به سر",
      "verse_number": 34
    },
    {
      "id": "768ddf8a-ffef-4de1-819d-760c58d22eaf",
      "m1": "کرا مادر و خواهر و دختر است",
      "m2": "همه پاک بر دست اسکندر است",
      "verse_number": 35
    },
    {
      "id": "83a1e666-3325-4607-bcd6-6423bb009475",
      "m1": "همان پاک پوشیدهرویان تو",
      "m2": "که بودند لرزنده بر جان تو",
      "verse_number": 36
    },
    {
      "id": "06d546c5-5aa4-48d3-b93b-74f05b6ea762",
      "m1": "چو گنج نیاکان برترمنش",
      "m2": "که آمد به دست تو بیسرزنش",
      "verse_number": 37
    },
    {
      "id": "7c645c10-1835-4735-8627-afe31c7756df",
      "m1": "کنون مانده اندر کف رومیان",
      "m2": "نژاد بزرگان و گنج کیان",
      "verse_number": 38
    },
    {
      "id": "ebd49c93-f43c-4101-a7c4-b52943d37999",
      "m1": "ترا چاره با او مداراست بس",
      "m2": "که تاج بزرگی نماند به کس",
      "verse_number": 39
    },
    {
      "id": "94b5d5b0-3b62-4c2e-b46f-72ce6dbcdee7",
      "m1": "کسی گوید آتش زبانش نسوخت",
      "m2": "به چاره بد از تن بباید سپوخت",
      "verse_number": 40
    },
    {
      "id": "1883834a-9e56-4740-a288-d75ae22904f7",
      "m1": "تو او را به تن زیردستی نمای",
      "m2": "یکی در سخن نیز چربی فزای",
      "verse_number": 41
    },
    {
      "id": "24033ad1-209f-442d-aeb6-09baa4ab4d61",
      "m1": "ببینیم فرجام تا چون بود",
      "m2": "که گردش ز اندیشه بیرون بود",
      "verse_number": 42
    },
    {
      "id": "dbffc219-87d8-458c-9d57-362c666b859e",
      "m1": "یکی نامه بنویس نزدیک او",
      "m2": "پراندیشه کن جان تاریک او",
      "verse_number": 43
    },
    {
      "id": "83363e8a-6880-45a1-917a-9728fd905118",
      "m1": "هم این چرخ گردان برو بگذرد",
      "m2": "چنین داند آنکس که دارد خرد",
      "verse_number": 44
    },
    {
      "id": "08181c36-e469-4df4-beb5-3e59c19ae95a",
      "m1": "از ایشان چو بشنید فرمان گزید",
      "m2": "چنان کز دل شهریاران سزید",
      "verse_number": 45
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 45
  },
