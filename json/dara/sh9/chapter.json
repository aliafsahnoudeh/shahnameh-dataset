{
  "id": "65551c69-bd0a-4bbc-aa13-3c1da48ce306",
  "title": "بخش ۹",
  "verses": [
    {
      "id": "e66131c1-5b1d-43c9-82f7-c8a064bcd261",
      "m1": "به نزدیک اسکندر آمد وزیر",
      "m2": "که ای شاه پیروز و دانشپذیر",
      "verse_number": 1
    },
    {
      "id": "f885ead4-ed50-4768-ab3b-0595d160cd02",
      "m1": "بکشتیم دشمنت را ناگهان",
      "m2": "سرآمد برو تاج و تخت مهان",
      "verse_number": 2
    },
    {
      "id": "c10bcfb7-e2e0-416e-9044-f67d673ddc8e",
      "m1": "چو بشنید گفتار جانوشیار",
      "m2": "سکندر چنین گفت با ماهیار",
      "verse_number": 3
    },
    {
      "id": "f285258e-6bd4-475c-af0c-d58d339ff79d",
      "m1": "که دشمن که افگندی اکنون کجاست",
      "m2": "بباید نمودن به من راه راست",
      "verse_number": 4
    },
    {
      "id": "dd24d4aa-42f5-4507-b709-ad03c7d45946",
      "m1": "برفتند هر دو به پیش اندرون",
      "m2": "دل و جان رومی پر از خشم و خون",
      "verse_number": 5
    },
    {
      "id": "9c0ebc6e-bad4-4286-ac9e-8939027f6ac8",
      "m1": "چو نزدیک شد روی دارا بدید",
      "m2": "پر از خون بر و روی چون شنبلید",
      "verse_number": 6
    },
    {
      "id": "f29df8d2-26be-460c-8e84-761cf4ae0fe9",
      "m1": "بفرمود تا راه نگذاشتند",
      "m2": "دو دستور او را نگه داشتند",
      "verse_number": 7
    },
    {
      "id": "16c021f5-571d-48c2-ad53-b4ebf18e9238",
      "m1": "سکندر ز باره درآمد چو باد",
      "m2": "سر مرد خسته به ران بر نهاد",
      "verse_number": 8
    },
    {
      "id": "e481f6ea-51b6-454a-bc91-9c0757308e87",
      "m1": "نگه کرد تا خسته گوینده هست",
      "m2": "بمالید بر چهر او هر دو دست",
      "verse_number": 9
    },
    {
      "id": "7336c63e-83be-4b2b-b75e-de8ae00b29cd",
      "m1": "ز سر برگرفت افسر خسرویش",
      "m2": "گشاد آن بر و جوشن پهلویش",
      "verse_number": 10
    },
    {
      "id": "480a1994-8245-4ed5-9483-6c58fff86a9c",
      "m1": "ز دیده ببارید چندی سرشک",
      "m2": "تن خسته را دور دید از پزشک",
      "verse_number": 11
    },
    {
      "id": "ca8ad8b6-b1f7-472f-a588-70596cf946a6",
      "m1": "بدو گفت کین بر تو آسان شود",
      "m2": "دل بدسگالت هراسان شود",
      "verse_number": 12
    },
    {
      "id": "e2d37c36-6128-4c40-b94b-b819aeee8434",
      "m1": "تو برخیز و بر مهد زرین نشین",
      "m2": "وگر هست نیروت بر زین نشین",
      "verse_number": 13
    },
    {
      "id": "ba107993-7b04-45ac-bf44-727dd1a4066b",
      "m1": "ز هند و ز رومت پزشک آورم",
      "m2": "ز درد تو خونین سرشک آورم",
      "verse_number": 14
    },
    {
      "id": "9cab786b-85a0-442d-a371-e33eaa09c5f8",
      "m1": "سپارم ترا پادشاهی و تخت",
      "m2": "چو بهتر شوی ما ببندیم رخت",
      "verse_number": 15
    },
    {
      "id": "b2adac38-06fe-4915-afcf-9cd79e1263a9",
      "m1": "جفا پیشگان ترا هم کنون",
      "m2": "بیاویزم از دارشان سرنگون",
      "verse_number": 16
    },
    {
      "id": "fe67efea-431a-4c51-96f9-7006a65067fd",
      "m1": "چنانچون ز پیران شنیدیم دوش",
      "m2": "دلم گشت پر خون و جان پر ز جوش",
      "verse_number": 17
    },
    {
      "id": "791ab78d-9056-403c-bd37-2cc391556ca9",
      "m1": "ز یک شاخ و یک بیخ و پیراهنیم",
      "m2": "به بیشی چرا تخمه را برکنیم",
      "verse_number": 18
    },
    {
      "id": "1c8e8e1d-2e75-45c0-a7ea-f6cd6b3a1b52",
      "m1": "چو بشنید دارا به آواز گفت",
      "m2": "که همواره با تو خرد باد جفت",
      "verse_number": 19
    },
    {
      "id": "1854f2c0-fc60-49bf-8a10-13c7f40160f9",
      "m1": "برآنم که از پاک دادار خویش",
      "m2": "بیابی تو پاداش گفتار خویش",
      "verse_number": 20
    },
    {
      "id": "a0303170-07aa-4f83-bc14-7cb9527fe5ea",
      "m1": "یکی آنک گفتی که ایران تراست",
      "m2": "سر تاج و تخت دلیران تراست",
      "verse_number": 21
    },
    {
      "id": "0b913b4d-7299-4991-893a-95091c0f98fa",
      "m1": "به من مرگ نزدیکتر زانک تخت",
      "m2": "بپردخت تخت و نگون گشت بخت",
      "verse_number": 22
    },
    {
      "id": "e0d0d491-2652-411d-8a5f-84e2640d4b5d",
      "m1": "برین است فرجام چرخ بلند",
      "m2": "خرامش سوی رنج و سودش گزند",
      "verse_number": 23
    },
    {
      "id": "0d4e98e0-a930-4c89-9668-1116f17a0da3",
      "m1": "به من در نگر تا نگویی که من",
      "m2": "فزونم ازین نامدار انجمن",
      "verse_number": 24
    },
    {
      "id": "d012af54-928c-4ef8-b747-0b07d763fe4b",
      "m1": "بد و نیک هر دو ز یزدان شناس",
      "m2": "وزو دار تا زنده باشی سپاس",
      "verse_number": 25
    },
    {
      "id": "03d72a47-6ce8-4f1f-9d03-5e35b3cd01e3",
      "m1": "نمودار گفتار من من بسم",
      "m2": "بدین در نکوهیدهٔ هرکسم",
      "verse_number": 26
    },
    {
      "id": "d6333696-b83c-40c6-ba0c-7b3560ef57aa",
      "m1": "که چندان بزرگی و شاهی و گنج",
      "m2": "نبد در زمانه کس از من به رنج",
      "verse_number": 27
    },
    {
      "id": "894acbc4-b1f5-458f-9e5a-92cb0cdfb1a8",
      "m1": "همان نیز چندان سلیح و سپاه",
      "m2": "گرانمایه اسپان و تخت و کلاه",
      "verse_number": 28
    },
    {
      "id": "fa88c221-a201-42a7-a2b9-5d03d7240afb",
      "m1": "همان نیز فرزند و پیوستگان",
      "m2": "چه پیوستگان داغ دل خستگان",
      "verse_number": 29
    },
    {
      "id": "1ce55a4d-ab7a-457d-84c8-323c27202288",
      "m1": "زمان و زمین بنده بد پیش من",
      "m2": "چنین بود تا بخت بد خویش من",
      "verse_number": 30
    },
    {
      "id": "87bea855-3190-4d8f-b69d-0f35c0c85353",
      "m1": "ز نیکی جدا ماندهام زین نشان",
      "m2": "گرفتار در دست مردمکشان",
      "verse_number": 31
    },
    {
      "id": "c05b6893-0803-4c5d-b17c-c6c3e7090803",
      "m1": "ز فرزند و خویشان شده ناامید",
      "m2": "سیه شد جهان و دو دیده سپید",
      "verse_number": 32
    },
    {
      "id": "d5a5e527-64b8-4872-99c8-776167f2a767",
      "m1": "ز خویشان کسی نیست فریادرس",
      "m2": "امیدم به پروردگارست و بس",
      "verse_number": 33
    },
    {
      "id": "10a548d9-8b5f-41f8-9491-6db2892f6931",
      "m1": "برین گونه خسته به خاک اندرم",
      "m2": "ز گیتی به دام هلاک اندرم",
      "verse_number": 34
    },
    {
      "id": "afa2b501-6e46-4dfd-ac8d-8b43ed119277",
      "m1": "چنین است آیین چرخ روان",
      "m2": "اگر شهریارم و گر پهلوان",
      "verse_number": 35
    },
    {
      "id": "046c53bc-6f6e-428b-bcde-4c26b2baf97f",
      "m1": "بزرگی به فرجام هم بگذرد",
      "m2": "شکارست مرگش همی بشکرد",
      "verse_number": 36
    },
    {
      "id": "a48c9321-3afe-49bd-8d46-5a07ab32da7c",
      "m1": "سکندر ز دیده ببارید خون",
      "m2": "بران شاه خسته به خاک اندرون",
      "verse_number": 37
    },
    {
      "id": "81e82352-10ac-4549-b15e-ad0c536bbcfe",
      "m1": "چو دارا بدید آن ز دل درد او",
      "m2": "روان اشک خونین رخ زرد او",
      "verse_number": 38
    },
    {
      "id": "51d43591-c314-4121-90b4-d016b9051e02",
      "m1": "بدو گفت مگری کزین سود نیست",
      "m2": "از آتش مرا بهره جز دود نیست",
      "verse_number": 39
    },
    {
      "id": "1caef019-62e5-484b-8a7c-75d87791c065",
      "m1": "چنین بود بخشش ز بخشندهام",
      "m2": "هم از روزگار درخشندهام",
      "verse_number": 40
    },
    {
      "id": "920571bb-cf01-4508-b573-ef60ec3828dd",
      "m1": "به اندرز من سر به سر گوش دار",
      "m2": "پذیرنده باش و بدل هوش دار",
      "verse_number": 41
    },
    {
      "id": "a348f327-f44b-4138-8009-cbfd1e655d69",
      "m1": "سکندر بدو گفت فرمان تراست",
      "m2": "بگو آنچ خواهی که پیمان تراست",
      "verse_number": 42
    },
    {
      "id": "ec034688-2fc8-46aa-8e8e-53498f09c5c7",
      "m1": "زبان تیز دارا بدو برگشاد",
      "m2": "همی کرد سرتاسر اندرز یاد",
      "verse_number": 43
    },
    {
      "id": "cf4b38c9-1b3f-4a07-906b-8c33c9aff4f1",
      "m1": "نخستین چنین گفت کای نامدار",
      "m2": "بترس از جهان داور کردگار",
      "verse_number": 44
    },
    {
      "id": "a3fb04cc-10b6-41bf-90df-b13c27755512",
      "m1": "که چرخ و زمین و زمان آفرید",
      "m2": "توانایی و ناتوان آفرید",
      "verse_number": 45
    },
    {
      "id": "c3b8ed63-28a1-4c2d-977f-a507edae4fe1",
      "m1": "نگه کن به فرزند و پیوند من",
      "m2": "به پوشیدگان خردمند من",
      "verse_number": 46
    },
    {
      "id": "f12b81c9-1802-4716-a179-26f9956beb9e",
      "m1": "ز من پاکدل دختر من بخواه",
      "m2": "بدارش به آرام بر پیشگاه",
      "verse_number": 47
    },
    {
      "id": "7c5b91dd-ca55-49e4-8630-1594841575e3",
      "m1": "کجا مادرش روشنک نام کرد",
      "m2": "جهان را بدو شاد و پدرام کرد",
      "verse_number": 48
    },
    {
      "id": "832fd510-b278-4484-a749-9fa23349c2bd",
      "m1": "نیاری به فرزند من سرزنش",
      "m2": "نه پیغاره از مردم بدکنش",
      "verse_number": 49
    },
    {
      "id": "7236e304-b69e-4611-8592-a4c71c405d8f",
      "m1": "چو پروردهٔ شهریاران بود",
      "m2": "به بزم افسر نامداران بود",
      "verse_number": 50
    },
    {
      "id": "88bb90d9-098b-40ff-9678-1e3fbe33527f",
      "m1": "مگر زو ببینی یکی نامدار",
      "m2": "کجا نو کند نام اسفندیار",
      "verse_number": 51
    },
    {
      "id": "ba0f0721-ce22-432c-a24c-eb1ee9b88b57",
      "m1": "بیاراید این آتش زردهشت",
      "m2": "بگیرد همان زند و استا بمشت",
      "verse_number": 52
    },
    {
      "id": "ed045504-1eb7-4497-9c86-2bcf15d6e39b",
      "m1": "نگه دارد این فال جشن سده",
      "m2": "همان فر نوروز و آتشکده",
      "verse_number": 53
    },
    {
      "id": "c9123381-54e5-4809-914a-c7a21c6c70cd",
      "m1": "همان اورمزد و مه و روز مهر",
      "m2": "بشوید به آب خرد جان و چهر",
      "verse_number": 54
    },
    {
      "id": "b50719b2-c9f3-44ee-af09-9153e5e3af4a",
      "m1": "کند تازه آیین لهراسپی",
      "m2": "بماند کیی دین گشتاسپی",
      "verse_number": 55
    },
    {
      "id": "b2f3243e-09cb-433e-b3a3-7fa467084bc2",
      "m1": "مهان را به مه دارد و که به که",
      "m2": "بود دین فروزنده و روزبه",
      "verse_number": 56
    },
    {
      "id": "05c70664-34ae-4d46-a9dc-0354c908dde5",
      "m1": "سکندر چنین داد پاسخ بدوی",
      "m2": "که ای نیکدل خسرو راستگوی",
      "verse_number": 57
    },
    {
      "id": "e554297e-d1cc-44d2-b7d7-319067493eb8",
      "m1": "پذیرفتم این پند و اندرز تو",
      "m2": "فزون زین نباشم برین مرز تو",
      "verse_number": 58
    },
    {
      "id": "95451773-017c-4ff0-b7e2-8e85137cffb7",
      "m1": "همه نیکویها به جای آورم",
      "m2": "خرد را بدین رهنمای آورم",
      "verse_number": 59
    },
    {
      "id": "5cbfe2d1-1b0b-4f39-ac37-23bae0adca11",
      "m1": "جهاندار دست سکندر گرفت",
      "m2": "به زاری خروشیدن اندر گرفت",
      "verse_number": 60
    },
    {
      "id": "29a41de6-f05e-46a1-a4fa-4c249a46593b",
      "m1": "کف دست او بر دهان برنهاد",
      "m2": "بدو گفت یزدان پناه تو باد",
      "verse_number": 61
    },
    {
      "id": "a68e9178-f6e3-4363-a326-091cf24e8ecd",
      "m1": "سپردم ترا جای و رفتم به خاک",
      "m2": "سپردم روانرا به یزدان پاک",
      "verse_number": 62
    },
    {
      "id": "99317bb1-bc27-4982-a8a9-e7104ce9308b",
      "m1": "بگفت این و جانش برآمد ز تن",
      "m2": "برو زار بگریستند انجمن",
      "verse_number": 63
    },
    {
      "id": "78e08171-6283-4e44-9ba2-d5cceb1bbff8",
      "m1": "سکندر همه جامهها کرد چاک",
      "m2": "به تاج کیان بر پراگند خاک",
      "verse_number": 64
    },
    {
      "id": "75fbc731-caff-43da-ad6e-923b7d858dfa",
      "m1": "یکی دخمه کردش بر آیین او",
      "m2": "بدان سان که بد فره و دین او",
      "verse_number": 65
    },
    {
      "id": "a58812a1-b50f-445b-a5dd-d1315b1e4a8d",
      "m1": "بشستن ازان خون به روشن گلاب",
      "m2": "چو آمدش هنگام جاوید خواب",
      "verse_number": 66
    },
    {
      "id": "2c55f6fa-51d3-4ca4-83c9-710bc7868707",
      "m1": "بیاراستندش به دیبای روم",
      "m2": "همه پیکرش گوهر و زر بوم",
      "verse_number": 67
    },
    {
      "id": "8724ba2a-3dee-434f-91e0-f0de08317ad9",
      "m1": "تنش زیر کافور شد ناپدید",
      "m2": "ازان پس کسی روی دارا ندید",
      "verse_number": 68
    },
    {
      "id": "21789f5f-b4c2-4dd6-b04c-d94c9dc11d88",
      "m1": "به دخمه درون تخت زرین نهاد",
      "m2": "یکی بر سرش تاج مشکین نهاد",
      "verse_number": 69
    },
    {
      "id": "439785da-61bc-47cf-bd07-b3f9f49dd818",
      "m1": "نهادش به تابوت زر اندرون",
      "m2": "بروبر ز مژگان ببارید خون",
      "verse_number": 70
    },
    {
      "id": "63c96936-f5d6-46bb-b018-9d9f5d34cf16",
      "m1": "چو تابوتش از جای برداشتند",
      "m2": "همه دست بر دست بگذاشتند",
      "verse_number": 71
    },
    {
      "id": "df6ab955-6072-4a48-9b05-ac9c68685069",
      "m1": "سکندر پیاده به پیش اندرون",
      "m2": "بزرگان همه دیدگان پر ز خون",
      "verse_number": 72
    },
    {
      "id": "da39fbc0-4a94-49f0-8788-9ac34c3bca28",
      "m1": "چنین تا ستودان دارا برفت",
      "m2": "همی پوست گفتی بروبر بکفت",
      "verse_number": 73
    },
    {
      "id": "76cae74b-2c9c-48eb-b71b-b8a8fe7f2187",
      "m1": "چو بر تخت بنهاد تابوت شاه",
      "m2": "بر آیین شاهان برآورد راه",
      "verse_number": 74
    },
    {
      "id": "dcaa0241-9396-4c5b-829c-48e532db0f79",
      "m1": "چو پردخت از دخمهٔ ارجمند",
      "m2": "ز بیرون بزد دارهای بلند",
      "verse_number": 75
    },
    {
      "id": "788c1918-0d4e-48ce-9266-68af63fbd982",
      "m1": "یکی دار بر نام جانوشیار",
      "m2": "دگر همچنان از در ماهیار",
      "verse_number": 76
    },
    {
      "id": "12234f38-08a7-4d35-8704-19c05c2c4cd5",
      "m1": "دو بدخواه را زنده بردار کرد",
      "m2": "سر شاهکش مرد بیدار کرد",
      "verse_number": 77
    },
    {
