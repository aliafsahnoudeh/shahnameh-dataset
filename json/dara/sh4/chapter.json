{
  "id": "85c6e77c-8d55-422a-a253-66a9f634b951",
  "title": "بخش ۴",
  "verses": [
    {
      "id": "e68d244f-3054-4c98-9585-0c116c16b335",
      "m1": "چو خورشید برزد سر از کوه و راغ",
      "m2": "زمین شد به کردار زرین چراغ",
      "verse_number": 1
    },
    {
      "id": "5c3fccc0-ea77-4918-9102-dcbb96a846dc",
      "m1": "جهاندار دارا سپه برگرفت",
      "m2": "جهان چادر قیر بر سرگرفت",
      "verse_number": 2
    },
    {
      "id": "1ae4dc17-e3db-44e9-b30e-bd7eb2643944",
      "m1": "بیاورد لشکر ز رود فرات",
      "m2": "به هامون سپه بیش بود از نبات",
      "verse_number": 3
    },
    {
      "id": "f3a60384-2a50-4d71-ba33-5c8ace449928",
      "m1": "سکندر چو بشنید کامد سپاه",
      "m2": "بزد کوس و آورد لشکر به راه",
      "verse_number": 4
    },
    {
      "id": "dada75fe-8055-4778-a349-de5b0b31b285",
      "m1": "دو لشکر که آن را کرانه نبود",
      "m2": "چو اسکندر اندر زمانه نبود",
      "verse_number": 5
    },
    {
      "id": "3bd8ce82-2052-45bd-a04d-2df98d41f358",
      "m1": "ز ساز و ز گردان هر دو گروه",
      "m2": "زمین همچو دریا بد و گرد کوه",
      "verse_number": 6
    },
    {
      "id": "52fddcd0-365a-4ab4-97b2-bc409573fd2a",
      "m1": "ز خفتان وز خنجر هندوان",
      "m2": "ز بالا و اسپ وز برگستوان",
      "verse_number": 7
    },
    {
      "id": "586370e0-e910-450b-beef-318153afb4a4",
      "m1": "دو رویه سپه برکشیدند صف",
      "m2": "ز خنجر همی یافت خورشید تف",
      "verse_number": 8
    },
    {
      "id": "de9f9731-bdab-4b38-a714-b4a64106f2c9",
      "m1": "به پیش سپاه آوریدند پیل",
      "m2": "جهان شد به کردار دریای نیل",
      "verse_number": 9
    },
    {
      "id": "822f8c7a-a453-4da3-84e4-28c37460df40",
      "m1": "سواران جنگ از پس و پیل پیش",
      "m2": "همه برگرفته دل از جان خویش",
      "verse_number": 10
    },
    {
      "id": "06440f3f-582e-4a81-9bd6-57d64a959a86",
      "m1": "تو گفتی هوا خون خروشد همی",
      "m2": "زمین از خروشش بجوشد همی",
      "verse_number": 11
    },
    {
      "id": "139e60a3-0b4c-4e12-a12a-4d218d31bcbb",
      "m1": "ز بس نالهٔ بوق و هندی درای",
      "m2": "همی کوه را دل برآمد ز جای",
      "verse_number": 12
    },
    {
      "id": "38d64ed9-946f-4497-afcd-be11a8cd22b0",
      "m1": "ز آواز اسپان و بانگ سران",
      "m2": "چرنگیدن گرزهای گران",
      "verse_number": 13
    },
    {
      "id": "c6111358-5425-41ce-b39e-f67415721c06",
      "m1": "تو گفتی زمین کوه جنگی شدست",
      "m2": "ز گرد آسمان روی زنگی شدست",
      "verse_number": 14
    },
    {
      "id": "16783f51-7829-45ac-9903-e0fa4dab5254",
      "m1": "به یک هفته گردان پرخاشجوی",
      "m2": "به روی اندر آورده بودند روی",
      "verse_number": 15
    },
    {
      "id": "3e1883cf-41f5-46b6-8a63-c5267ee72a6d",
      "m1": "بهشتم برآمد یکی تیره گرد",
      "m2": "بران سان که خورشید شد لاژورد",
      "verse_number": 16
    },
    {
      "id": "210281f4-6766-41e2-970c-28f68538040f",
      "m1": "بپوشید دیدار ایران سپاه",
      "m2": "گریزان برفتند از آن رزمگاه",
      "verse_number": 17
    },
    {
      "id": "332dee5a-39e3-4ffb-b2cf-0c71067f9bd5",
      "m1": "سپاه سکندر پس اندر دمان",
      "m2": "یکی پرغم و دیگری شادمان",
      "verse_number": 18
    },
    {
      "id": "9e5d38ca-fee3-433e-99ab-8ab2cc2549df",
      "m1": "سکندر بشد تا لب رودبار",
      "m2": "بکشتند ز ایرانیان بیشمار",
      "verse_number": 19
    },
    {
      "id": "737c9e0f-0549-4f61-abb2-e7193298dbeb",
      "m1": "سپاه از لب رود برگاشتند",
      "m2": "بفرمود تا رود بگذاشتند",
      "verse_number": 20
    },
    {
      "id": "0b2e3e23-2711-44a5-95c5-f8bd9fa56346",
      "m1": "به پیروزی آمد بران رزمگاه",
      "m2": "کجا پیش بود آن گزیده سپاه",
      "verse_number": 21
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 21
  },
