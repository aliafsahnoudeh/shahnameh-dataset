{
  "id": "f051a767-6072-4669-a1c0-39ffea0e03ca",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "2c2f9400-be61-4f39-b7ea-574ec8ae83f5",
      "m1": "چو بنشست بر تخت شاه اردشیر",
      "m2": "از ایران برفتند برنا و پیر",
      "verse_number": 1
    },
    {
      "id": "2fb29b16-30b2-439b-a3ca-a59d1a90269d",
      "m1": "بسی نامداران گشته کهن",
      "m2": "بدان تا چگونه سرآید سخن",
      "verse_number": 2
    },
    {
      "id": "801baf67-e6f2-4cc1-bb70-766750682587",
      "m1": "زبان برگشاد اردشیر جوان",
      "m2": "چنین گفت کای کار دیده گوان",
      "verse_number": 3
    },
    {
      "id": "2b0234f9-89c3-46dc-a60e-5a39aeec76cf",
      "m1": "هر آنکس که برگاه شاهی نشست",
      "m2": "گشاده زبان باد و یزدان پرست",
      "verse_number": 4
    },
    {
      "id": "e70a5efc-4be3-4f09-9902-0f98f0843934",
      "m1": "بر آیین شاهان پیشین رویم",
      "m2": "همان از پس فره و دین رویم",
      "verse_number": 5
    },
    {
      "id": "4eacaf0e-c263-4217-9415-a2e4d46277a3",
      "m1": "ز یزدان نیکی دهش یاد باد",
      "m2": "همه کار و کردار ما داد باد",
      "verse_number": 6
    },
    {
      "id": "459fc1a4-aad2-4a66-a4a3-ebda6389e4eb",
      "m1": "پرستندگان راهمه برکشیم",
      "m2": "ستمگارگان را به خون درکشیم",
      "verse_number": 7
    },
    {
      "id": "7597d64d-5d79-4285-b874-9af9614ed38b",
      "m1": "بسی کس به گفتارش آرام یافت",
      "m2": "از آرام او هرکسی کام یافت",
      "verse_number": 8
    },
    {
      "id": "00722743-1af2-4619-9a85-d10d7951067e",
      "m1": "به پیروز خسرو سپردم سپاه",
      "m2": "که از داد شادست و شادان ز شاه",
      "verse_number": 9
    },
    {
      "id": "3113e1a0-b233-4f14-af28-09a80ee968cf",
      "m1": "به ایران چو باشد چنو پهلوان",
      "m2": "بمانید شادان و روشن روان",
      "verse_number": 10
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 10
  }
}