{
  "id": "803cc01e-8d03-4582-8ef6-16764fcbfc11",
  "title": "بخش ۱ - به خواب دیدن فردوسی دقیقی را",
  "verses": [
    {
      "id": "1727cad8-49fa-472c-9e17-d10883cd3611",
      "m1": "چنان دید گوینده یک شب به خواب",
      "m2": "که یک جام می داشتی چون گلاب",
      "verse_number": 1
    },
    {
      "id": "ea77ee43-489d-4502-98b1-bba7bb797a8e",
      "m1": "دقیقی ز جایی پدید آمدی",
      "m2": "بران جام می داستانها زدی",
      "verse_number": 2
    },
    {
      "id": "61b374de-f021-4c3a-868e-b89b01b203cd",
      "m1": "به فردوسی آواز دادی که می",
      "m2": "مخور جز بر آیین کاوس کی",
      "verse_number": 3
    },
    {
      "id": "54202138-5db1-4ca1-a157-5046608e80e4",
      "m1": "که شاهی ز گیتی گزیدی که بخت",
      "m2": "بدو نازد و لشگر و تاج و تخت",
      "verse_number": 4
    },
    {
      "id": "d32b1ea1-844b-49a6-a998-17baa8ea5595",
      "m1": "شهنشاه محمود گیرنده شهر",
      "m2": "ز شادی به هر کس رسانیده بهر",
      "verse_number": 5
    },
    {
      "id": "25cdf567-b5fc-48cb-87d0-bbb901b0ed51",
      "m1": "از امروز تا سال هشتاد و پنج",
      "m2": "بکاهدش رنج و نکاهدش گنج",
      "verse_number": 6
    },
    {
      "id": "0aed9968-9f07-41c3-ab6c-2f4b60c4cacd",
      "m1": "ازین پس به چین اندر آرد سپاه",
      "m2": "همه مهتران برگشایند راه",
      "verse_number": 7
    },
    {
      "id": "60049f86-6d02-442c-a432-15f1303736a9",
      "m1": "نبایدش گفتن کسی را درشت",
      "m2": "همه تاج شاهانش آمد به مشت",
      "verse_number": 8
    },
    {
      "id": "953de147-a050-44b7-9089-77fa19237c79",
      "m1": "بدین نامه گر چند بشتافتی",
      "m2": "کنون هرچ جستی همه یافتی",
      "verse_number": 9
    },
    {
      "id": "3a42a9af-94ea-4c94-94f2-03e1d633cc6f",
      "m1": "ازین باره من پیش گفتم سخن",
      "m2": "سخن را نیامد سراسر به بن",
      "verse_number": 10
    },
    {
      "id": "8f11cb97-8f68-406c-a245-8bb7f157a53b",
      "m1": "ز گشتاسپ و ارجاسپ بیتی هزار",
      "m2": "بگفتم سرآمد مرا روزگار",
      "verse_number": 11
    },
    {
      "id": "d30f4d64-0bf6-4557-aab0-ab9ae425334a",
      "m1": "گر آن مایه نزد شهنشه رسد",
      "m2": "روان من از خاک بر مه رسد",
      "verse_number": 12
    },
    {
      "id": "73efdd77-cd8c-4165-878e-4a10e5542872",
      "m1": "کنون من بگویم سخن کو بگفت",
      "m2": "منم زنده او گشت با خاک جفت",
      "verse_number": 13
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 13
  },
  "order_in_parent": 1
}