{
  "id": "1eb61055-bec7-4e09-b893-13369cc2739d",
  "title": "بخش ۱۱ - سخن دقیقی",
  "verses": [
    {
      "id": "66fd2728-90f2-4bc1-bc94-0481f2eedbc6",
      "m1": "چو جاماسپ گفت این سپیده دمید",
      "m2": "فروغ ستاره بشد ناپدید",
      "verse_number": 1
    },
    {
      "id": "c49e6d8b-6602-4855-867f-dff5fe3a4eec",
      "m1": "سپه را به هامون فرود آورید",
      "m2": "بزد کوس بر پیل و لشکر کشید",
      "verse_number": 2
    },
    {
      "id": "fa1d9543-8d88-4448-bfd9-ec9bddb9fd3e",
      "m1": "وزانجا خرامید تا رزمگاه",
      "m2": "فرود آورید آن گزیده سپاه",
      "verse_number": 3
    },
    {
      "id": "9ac328df-d16b-4dc3-ad56-4b81b84440eb",
      "m1": "به گاهی که باد سپیده دمان",
      "m2": "به کاخ آرد از باغ بوی گلان",
      "verse_number": 4
    },
    {
      "id": "26254830-102c-4e63-b210-cdcf161785be",
      "m1": "فرستاده بد هر سوی دیدهبان",
      "m2": "چنانچون بود رسم آزادگان",
      "verse_number": 5
    },
    {
      "id": "78fae4b0-9124-43a1-8600-6627f232b5a6",
      "m1": "بیامد سواری و گفتا به شاه",
      "m2": "که شاها به نزدیکی آمد سپاه",
      "verse_number": 6
    },
    {
      "id": "c35fd591-9374-43a7-869b-0d601200075f",
      "m1": "سپاهیست ای شهریار زمین",
      "m2": "که هرگز چنان نامد از ترک و چین",
      "verse_number": 7
    },
    {
      "id": "deee8cc7-b940-4f58-957a-82b7c10bcc5b",
      "m1": "به نزدیکی ما فرود آمدند",
      "m2": "به کوه و در و دشت خیمه زدند",
      "verse_number": 8
    },
    {
      "id": "888d1cfc-a9ea-4054-aaec-cbc074d22396",
      "m1": "سپهدارشان دیدهبان برگزید",
      "m2": "فرستاد و دیده به دیده رسید",
      "verse_number": 9
    },
    {
      "id": "329539f6-feb9-4502-a013-8fa0197cd65d",
      "m1": "پس آزاده گشتاسپ شاه دلیر",
      "m2": "سپهبدش را خواند فرخ زریر",
      "verse_number": 10
    },
    {
      "id": "b3b654ec-c4ad-414a-a46e-3ed075aabf0f",
      "m1": "درفشی بدو داد و گفتا بتاز",
      "m2": "بیارای پیلان و لشکر بساز",
      "verse_number": 11
    },
    {
      "id": "f4896942-a6d0-438d-8970-3bd5674e5b11",
      "m1": "سپهبد بشد لشکرش راست کرد",
      "m2": "همی رزم سالار چین خواست کرد",
      "verse_number": 12
    },
    {
      "id": "18eda37d-caaa-47e5-b077-75bf68e9cabd",
      "m1": "بدادش جهاندار پنجه هزار",
      "m2": "سوار گزیده به اسفندیار",
      "verse_number": 13
    },
    {
      "id": "1d8adc59-5353-4418-9b4e-6e65f5c9efc1",
      "m1": "بدو داد یک دست زان لشکرش",
      "m2": "که شیری دلش بود و پیلی برش",
      "verse_number": 14
    },
    {
      "id": "a79b82f6-d67c-4b90-9192-77a13deedbee",
      "m1": "دگر دست لشکرش را همچنان",
      "m2": "برآراست از شیر دل سرکشان",
      "verse_number": 15
    },
    {
      "id": "324cf7f7-baec-4ab5-b2d9-f1ae74e4e840",
      "m1": "به گرد گرامی سپرد آن سپاه",
      "m2": "که شیر جهان بود و همتای شاه",
      "verse_number": 16
    },
    {
      "id": "88727e13-a85e-4079-861f-e6281d832292",
      "m1": "پس پشت لشکر به بستور داد",
      "m2": "چراغ سپهدار خسرو نژاد",
      "verse_number": 17
    },
    {
      "id": "2f02b5d9-b230-4b8d-bbb7-de01e264539b",
      "m1": "چو لشکر بیاراست و بر شد به کوه",
      "m2": "غمی گشته از رنج و گشته ستوه",
      "verse_number": 18
    },
    {
      "id": "7e6535f3-7cb7-4d67-bf41-66570deac705",
      "m1": "نشست از بر خوب تابنده گاه",
      "m2": "همی کرد زانجا به لشکر نگاه",
      "verse_number": 19
    },
    {
      "id": "2786c17d-6d75-4655-a91b-3930fc8606e8",
      "m1": "پس ارجاسپ شاه دلیران چین",
      "m2": "بیاراست لشکرش را همچنین",
      "verse_number": 20
    },
    {
      "id": "a820c42e-b9d2-4afc-bc23-4d11404420ab",
      "m1": "جدا کرد از خلخی سی هزار",
      "m2": "جهان آزموده نبرده سوار",
      "verse_number": 21
    },
    {
      "id": "8525cdc0-10a3-45c2-a241-93d7296ae660",
      "m1": "فرستادشان سوی آن بیدرفش",
      "m2": "که کوس مهین داشت و رنگین درفش",
      "verse_number": 22
    },
    {
      "id": "3ed4d940-5adc-42c4-bec2-0ba1f60ef820",
      "m1": "بدو داد یک دست زان لشکرش",
      "m2": "که شیر ژیان نامدی همبرش",
      "verse_number": 23
    },
    {
      "id": "919e9293-344f-434f-94a7-27101a852544",
      "m1": "دگر دست را داد بر گرگسار",
      "m2": "بدادش سوار گزین صدهزار",
      "verse_number": 24
    },
    {
      "id": "94315dfc-02e3-4785-b44a-5da2b889cfaf",
      "m1": "میانگاه لشکرش را همچنین",
      "m2": "سپاهی بیاراست خوب و گزین",
      "verse_number": 25
    },
    {
      "id": "0893f80b-ceac-4cd5-a7f6-4efb238f07a1",
      "m1": "بدادش بدان جادوی خویش کام",
      "m2": "کجا نام خواست و هزارانش نام",
      "verse_number": 26
    },
    {
      "id": "6250e70a-324d-4d2d-bbed-51be0880048d",
      "m1": "خود و صدهزاران سواران گرد",
      "m2": "نموده همه در جهان دستبرد",
      "verse_number": 27
    },
    {
      "id": "1169d4fc-a99c-45d0-bd1d-81373523b569",
      "m1": "نگاهش همی داشت پشت سپاه",
      "m2": "همی کرد هر سوی لشکر نگاه",
      "verse_number": 28
    },
    {
      "id": "04d5d787-2cbb-4f6c-aab9-8d91b9bf608b",
      "m1": "پسر داشتی یک گرانمایه مرد",
      "m2": "جهاندیده و دیده هر گرم و سرد",
      "verse_number": 29
    },
    {
      "id": "51355a99-31b5-46d0-8bbb-fd6f5ca1587f",
      "m1": "سواری جهاندیده نامش کهرم",
      "m2": "رسیده بسی بر سرش سرد و گرم",
      "verse_number": 30
    },
    {
      "id": "479deec6-44a1-4023-8d03-c71690b7bd96",
      "m1": "مران پور خود را سپهدار کرد",
      "m2": "بران لشکر گشن سالار کرد",
      "verse_number": 31
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 31
  }
}