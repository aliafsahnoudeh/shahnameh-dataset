{
  "id": "2c87b482-3bbb-4da1-bc50-88f8346770e5",
  "title": "بخش ۱۰ - سخن دقیقی",
  "verses": [
    {
      "id": "6702bffe-fe55-4994-adf1-ff689e621445",
      "m1": "چو از بلخ بامی به جیحون رسید",
      "m2": "سپهدار لشکر فرود آورید",
      "verse_number": 1
    },
    {
      "id": "25e02848-5371-47f0-9659-2fedf7084a2a",
      "m1": "بشد شهریار از میان سپاه",
      "m2": "فرود آمد از باره بر شد به گاه",
      "verse_number": 2
    },
    {
      "id": "f58788bb-c0a0-4053-8dbd-f63ac8cb4437",
      "m1": "بخواند او گرانمایه جاماسپ را",
      "m2": "کجا رهنمون بود گشتاسپ را",
      "verse_number": 3
    },
    {
      "id": "1e3f50f6-f7f2-4818-bf49-d1e5becf61a9",
      "m1": "سر موبدان بودو شاه ردان",
      "m2": "چراغ بزرگان و اسپهبدان",
      "verse_number": 4
    },
    {
      "id": "a10cc30e-704e-41e0-8260-514026fd01ec",
      "m1": "چنان پاک تن بود و تابنده جان",
      "m2": "که بودی بر او آشکارا نهان",
      "verse_number": 5
    },
    {
      "id": "78781f2f-e525-4ba7-921c-95bf01d4fd47",
      "m1": "ستارهشناس و گرانمایه بود",
      "m2": "ابا او به دانش کرا پایه بود",
      "verse_number": 6
    },
    {
      "id": "7322a6dd-37e9-4fa1-97bb-b369a3868bb8",
      "m1": "بپرسید ازو شاه و گفتا خدای",
      "m2": "ترا دین به داد و پاکیزه رای",
      "verse_number": 7
    },
    {
      "id": "f08ec3ab-dceb-44eb-a082-a1fa7901bfba",
      "m1": "چو تو نیست اندر جهان هیچ کس",
      "m2": "جهاندار دانش ترا داد و بس",
      "verse_number": 8
    },
    {
      "id": "16726999-9715-4526-b414-bc780581e8a6",
      "m1": "ببایدت کردن ز اختر شمار",
      "m2": "بگویی همی مر مرا روی کار",
      "verse_number": 9
    },
    {
      "id": "180298a3-7a1c-4dee-bbf0-8874a043bad5",
      "m1": "که چون باشد آغاز و فرجام جنگ",
      "m2": "کرا بیشتر باشد اینجا درنگ",
      "verse_number": 10
    },
    {
      "id": "cd077204-d40f-41f8-be16-5d5893fc4d79",
      "m1": "نیامد خوش آن پیر جاماسپ را",
      "m2": "به روی دژم گفت گشتاسپ را",
      "verse_number": 11
    },
    {
      "id": "ab749740-5060-4f31-ba83-a5c609f33d21",
      "m1": "که میخواستم کایزد دادگر",
      "m2": "ندادی مرا این خرد وین هنر",
      "verse_number": 12
    },
    {
      "id": "d396f52e-b34b-4fb4-8729-8b6d46bfe82e",
      "m1": "مرا گر نبودی خرد شهریار",
      "m2": "نکردی زمن بودنی خواستار",
      "verse_number": 13
    },
    {
      "id": "4564d4ca-a23f-41f0-91f3-c8e9eb915078",
      "m1": "مگر با من از داد پیمان کند",
      "m2": "که نه بد کند خود نه فرمان کند",
      "verse_number": 14
    },
    {
      "id": "499dfab5-78b0-4028-a477-2f2ef1ed2af5",
      "m1": "جهانجوی گفتا به نام خدای",
      "m2": "بدین و به دین آور پاک رای",
      "verse_number": 15
    },
    {
      "id": "2c251959-7da5-48ab-b83c-19fc4a5430be",
      "m1": "به جان زریر آن نبرده سوار",
      "m2": "به جان گرانمایه اسفندیار",
      "verse_number": 16
    },
    {
      "id": "6a16cb61-b498-4bcd-aaa9-0eef1fe718e3",
      "m1": "که نه هرگزت روی دشمن کنم",
      "m2": "نفرمایمت بد نه خود من کنم",
      "verse_number": 17
    },
    {
      "id": "86c89f6d-7484-4473-98cf-170f6c74729c",
      "m1": "تو هرچ اندرین کار دانی بگوی",
      "m2": "که تو چارهدانی و من چارهجوی",
      "verse_number": 18
    },
    {
      "id": "6859e762-c541-45b1-914d-17bcbb02191c",
      "m1": "خردمند گفت ای گرانمایه شاه",
      "m2": "همیشه بتو تازه بادا کلاه",
      "verse_number": 19
    },
    {
      "id": "e8511ab3-ac7d-4eb7-aec1-84c16f459bd1",
      "m1": "ز بنده میازار و بنداز خشم",
      "m2": "خنک آنکسی کو نبیند به چشم",
      "verse_number": 20
    },
    {
      "id": "0fcfb37d-8b13-4aba-99d0-1545364f5ea3",
      "m1": "بدان ای نبرده کی نامجوی",
      "m2": "چو در رزم روی اندر آری بروی",
      "verse_number": 21
    },
    {
      "id": "819e4954-a1a9-464b-9a1d-7623d2287750",
      "m1": "بدانگه کجا بانگ و ویله کنند",
      "m2": "تو گویی همی کوه را برکنند",
      "verse_number": 22
    },
    {
      "id": "1593cfa0-1e4a-4a43-8b3e-5c0729902fc6",
      "m1": "به پیش اندر آیند مردان مرد",
      "m2": "هوا تیره گردد ز گرد نبرد",
      "verse_number": 23
    },
    {
      "id": "75f43642-1956-43cb-9daa-b816e6ecadac",
      "m1": "جهان را ببینی بگشته کبود",
      "m2": "زمین پر ز آتش هوا پر زدود",
      "verse_number": 24
    },
    {
      "id": "05251ab6-36e2-4813-ba92-a0acae616b2d",
      "m1": "وزان زخم آن گُرزهای گران",
      "m2": "چنان پتک پولاد آهنگران",
      "verse_number": 25
    },
    {
      "id": "7c7b239c-b267-45d4-9bb8-257615262c95",
      "m1": "به گوش اندر آید ترنگا ترنگ",
      "m2": "هوا پر شده نعرهٔ بور و خنگ",
      "verse_number": 26
    },
    {
      "id": "33e4c15c-4dc1-464b-b86f-b833a9b288e3",
      "m1": "شکسته شود چرخ گردونها",
      "m2": "زمین سرخ گردد از ان خونها",
      "verse_number": 27
    },
    {
      "id": "766020ac-5064-421e-8b47-9700ebf262d5",
      "m1": "تو گویی هوا ابر دارد همی",
      "m2": "وزان ابر الماس بارد همی",
      "verse_number": 28
    },
    {
      "id": "82d7fc5c-b7ca-4fca-bb79-8858c3487774",
      "m1": "بسی بی پدر گشته بینی پسر",
      "m2": "بسی بی پسر گشته بینی پدر",
      "verse_number": 29
    },
    {
      "id": "930e469b-17bb-4b42-b8d6-fd2f368aca81",
      "m1": "نخستین کس نامدار اردشیر",
      "m2": "پس شهریار آن نبرده دلیر",
      "verse_number": 30
    },
    {
      "id": "c720374b-6139-4577-b1f8-b54269934480",
      "m1": "به پیش افگند اسپ تازان خویش",
      "m2": "به خاک افگند هر ک آیدش پیش",
      "verse_number": 31
    },
    {
      "id": "0d3def26-a657-44ae-8436-ee36dd943b95",
      "m1": "پیاده کند ترک چندان سوار",
      "m2": "کز اختر نباشد مر آن را شمار",
      "verse_number": 32
    },
    {
      "id": "c6c238dd-9b83-42b7-b0db-78acd8afcf45",
      "m1": "ولیکن سرانجام کشته شود",
      "m2": "نکونامش اندر نوشته شود",
      "verse_number": 33
    },
    {
      "id": "823a8224-bc47-4e7d-9043-5988ad1050b3",
      "m1": "دریغ آنچنان مرد نام آورا",
      "m2": "ابا رادمردان همه سرورا",
      "verse_number": 34
    },
    {
      "id": "cd949f05-9842-4301-ad03-f4044f97fe85",
      "m1": "پس آزاده شیدسپ فرزند شاه",
      "m2": "چو رستم درآید به روی سپاه",
      "verse_number": 35
    },
    {
      "id": "cac1ed36-d98d-49ea-a4cd-561f21589373",
      "m1": "پس آنگاه مر تیغ را برکشد",
      "m2": "بتازد بسی اسپ و دشمن کشد",
      "verse_number": 36
    },
    {
      "id": "80843c15-2253-413a-86fe-7146129c855b",
      "m1": "بسی نامداران و گردان چین",
      "m2": "که آن شیر مرد افگند بر زمین",
      "verse_number": 37
    },
    {
      "id": "994b93a1-2cb0-4f96-bf5b-534c9c6d6f21",
      "m1": "سرانجام بختش کند خاکسار",
      "m2": "برهنه کند آن سر تاجدار",
      "verse_number": 38
    },
    {
      "id": "dac8cdc9-de9b-4161-96d8-b34c1ec15b33",
      "m1": "بیاید پس آنگاه فرزند من",
      "m2": "ببسته میان را جگر بند من",
      "verse_number": 39
    },
    {
      "id": "c05605fa-7e3b-4d2a-9d90-d0d4c7196794",
      "m1": "ابر کین شیدسپ فرزند شاه",
      "m2": "به میدان کند تیز اسپ سیاه",
      "verse_number": 40
    },
    {
      "id": "ac467822-bb3f-4830-a432-83b92c2baa42",
      "m1": "بسی رنج بیند به رزم اندرون",
      "m2": "شه خسروان را بگویم که چون",
      "verse_number": 41
    },
    {
      "id": "540708c6-a3df-4e95-8603-3f9764e90587",
      "m1": "درفش فروزندهٔ کاویان",
      "m2": "بیفگنده باشند ایرانیان",
      "verse_number": 42
    },
    {
      "id": "a244b5da-112c-42d8-b193-17ab17f1a790",
      "m1": "گرامی بگیرد به دندان درفش",
      "m2": "به دندان بدارد درفش بنفش",
      "verse_number": 43
    },
    {
      "id": "a7d13cf1-b50b-4e9c-af75-b9bf24f11c93",
      "m1": "به یک دست شمشیر و دیگر کلاه",
      "m2": "به دندان درفش فریدون شاه",
      "verse_number": 44
    },
    {
      "id": "7c0e937e-66ce-4b1b-b425-72f701bc9e32",
      "m1": "برین سان همیافگند دشمنان",
      "m2": "همی برکند جان آهرمنان",
      "verse_number": 45
    },
    {
      "id": "e29375bd-93cd-4183-a06e-6527833f42fe",
      "m1": "سرانجام در جنگ کشته شود",
      "m2": "نکو نامش اندر نوشته شود",
      "verse_number": 46
    },
    {
      "id": "fc6e8046-1572-4802-9cf7-2ab422677eb9",
      "m1": "پس ازاده بستور پور زریر",
      "m2": "به پیش افگند اسپ چون نره شیر",
      "verse_number": 47
    },
    {
      "id": "a8146792-bb77-4195-8f02-aa42abf9e077",
      "m1": "بسی دشمنان را کند ناپدید",
      "m2": "شگفتیتر از کار او کس ندید",
      "verse_number": 48
    },
    {
      "id": "7e287dac-6f4e-47e4-abd7-c78fbeae56e4",
      "m1": "چو آید سرانجام پیروز باز",
      "m2": "ابر دشمنان دست کرده دراز",
      "verse_number": 49
    },
    {
      "id": "2f8b0fc9-e2cb-4a36-a5d6-ebe6f2065253",
      "m1": "بیاید پس آن برگزیده سوار",
      "m2": "پس شهریار جهان نامدار",
      "verse_number": 50
    },
    {
      "id": "39174c8c-58b3-4abc-a083-80c7428ec6ea",
      "m1": "ز آهرمنان بفگند شست گرد",
      "m2": "نماید یکی پهلوی دستبرد",
      "verse_number": 51
    },
    {
      "id": "63b61e7e-0f9c-4801-a134-f99e979b0bfc",
      "m1": "سرانجام ترکان به تیرش زنند",
      "m2": "تن پیلوارش به خاک افگنند",
      "verse_number": 52
    },
    {
      "id": "fe12043f-271b-41d0-9a90-8573e02a7aec",
      "m1": "بیاید پس آن نره شیر دلیر",
      "m2": "سوار دلاور که نامش زریر",
      "verse_number": 53
    },
    {
      "id": "9b11c36d-e651-4d04-99a1-340f1f0b91bd",
      "m1": "به پیش اندر آید گرفته کمند",
      "m2": "نشسته بر اسفندیاری سمند",
      "verse_number": 54
    },
    {
      "id": "19368035-7517-4be4-9fcc-6b489fb1776a",
      "m1": "ابا جوشن زر درخشان چو ماه",
      "m2": "بدو اندرون خیره گشته سپاه",
      "verse_number": 55
    },
    {
      "id": "2eba6de1-0ed9-4468-b1e9-fb64631828b8",
      "m1": "بگیرد ز گردان لشکر هزار",
      "m2": "ببندد فرستد بر شهریار",
      "verse_number": 56
    },
    {
      "id": "9bdbdb81-0b4d-4ed9-9e0b-ead309f9eb79",
      "m1": "به هر سو کجا بنهد آن شاه روی",
      "m2": "همی راند از خون بدخواه جوی",
      "verse_number": 57
    },
    {
      "id": "2b06337b-327d-42e5-8fbd-b334323f80b2",
      "m1": "نه استد کس آن پهلوان شاه را",
      "m2": "ستوه آورد شاه خرگاه را",
      "verse_number": 58
    },
    {
      "id": "afec47a5-eded-41ef-9049-c0a8bd101b3a",
      "m1": "پس افگنده بیند بزرگ اردشیر",
      "m2": "سیه گشته رخسار و تن چون زریر",
      "verse_number": 59
    },
    {
      "id": "c1c603ea-e925-4b2f-b57f-5efbe63248ae",
      "m1": "بگرید برو زار و گردد نژند",
      "m2": "برانگیزد اسفندیاری سمند",
      "verse_number": 60
    },
    {
      "id": "c6db4724-0b85-4906-b216-d2f08d97b314",
      "m1": "به خاقان نهد روی پر خشم و تیز",
      "m2": "تو گویی ندیدست هرگز گریز",
      "verse_number": 61
    },
    {
      "id": "75e41c78-4ce0-4d8f-8134-20d5fbfebdc4",
      "m1": "چو اندر میان بیند ارجاسپ را",
      "m2": "ستایش کند شاه گشتاسپ را",
      "verse_number": 62
    },
    {
      "id": "a791f9ab-a19a-4525-81ff-2137b3061b32",
      "m1": "صف دشمنان سر بسر بردرد",
      "m2": "ز گیتی سوی هیچ کس ننگرد",
      "verse_number": 63
    },
    {
      "id": "ea31e7e6-f95c-424c-94e4-51281ee0c837",
      "m1": "همی خواند او زند زردشت را",
      "m2": "به یزدان نهاده کیی پشت را",
      "verse_number": 64
    },
    {
      "id": "1c255b89-a209-4a46-bc43-83d00707b6c6",
      "m1": "سرانجام گردد برو تیرهبخت",
      "m2": "بریده کندش آن نکو تاج و تخت",
      "verse_number": 65
    },
    {
      "id": "200c3b3e-bf23-4670-8753-6a0497a12e51",
      "m1": "بیاید یکی نام او بیدرفش",
      "m2": "به سرنیزه دارد درفش بنفش",
      "verse_number": 66
    },
    {
      "id": "12ef3fdc-3b76-4624-9e67-a77e1463ac4c",
      "m1": "نیارد شدن پیش گرد گزین",
      "m2": "نشیند به راه وی اندر کمین",
      "verse_number": 67
    },
    {
      "id": "718a75fd-539c-4e88-a183-faabbd52def3",
      "m1": "باستد بران راه چون پیل مست",
      "m2": "یکی تیغ زهر آب داده به دست",
      "verse_number": 68
    },
    {
      "id": "36423c3e-f941-409c-8a0e-ca996968989d",
      "m1": "چو شاه جهان بازگردد ز رزم",
      "m2": "گرفته جهان را و کشته گُرَزم",
      "verse_number": 69
    },
    {
      "id": "a2228278-18f4-43f0-9907-985aa0bb0aeb",
      "m1": "بیندازد آن ترک تیری بروی",
      "m2": "نیارد شدن آشکارا بروی",
      "verse_number": 70
    },
    {
      "id": "299ce0e2-8c19-4fa1-aa98-aa8d1c150c2c",
      "m1": "پس از دست آن بیدرفش پلید",
      "m2": "شود شاه آزادگان ناپدید",
      "verse_number": 71
    },
    {
      "id": "0b56baf7-d310-4893-b0f2-f5d24c6437c2",
      "m1": "به ترکان برد باره و زین اوی",
      "m2": "بخواهد پسرت آن زمان کین اوی",
      "verse_number": 72
    },
    {
      "id": "9632592b-a8c1-4030-8d97-7ed8b4c97e57",
      "m1": "پس آن لشکر نامدار بزرگ",
      "m2": "به دشمن درافتد چو شیر سترگ",
      "verse_number": 73
    },
    {
      "id": "498afb73-adec-400b-a123-0a49733ecb06",
      "m1": "همی تازند این بر آن آن برین",
      "m2": "ز خون یلان سرخ گردد زمین",
      "verse_number": 74
    },
    {
      "id": "43e16276-2f22-414b-8f80-379983653d93",
      "m1": "یلان را بباشد همه روی زرد",
      "m2": "چو لرزه برافتد به مردان مرد",
      "verse_number": 75
    },
    {
      "id": "5de3025a-3509-4764-9136-b12a0b281130",
      "m1": "برآید به خورشید گرد سپاه",
      "m2": "نبیند کس از گرد تاریک راه",
      "verse_number": 76
    },
    {
      "id": "4b16d869-a864-46ea-a446-42391d292f82",
      "m1": "فروغ سر نیزه و تیر و تیغ",
      "m2": "بتابد چنان چون ستاره ز میغ",
      "verse_number": 77
    },
    {
      "id": "0661047c-ca11-475f-8a9f-7e3917441039",
      "m1": "وزان زخم مردان کجا میزنند",
      "m2": "و بر یکدگر بر همی افگند",
      "verse_number": 78
    },
    {
      "id": "53670e38-3a99-402d-a83a-d638b671dbeb",
      "m1": "همه خسته و کشته بر یکدگر",
      "m2": "پسر بر پدر بر پدر بر پسر",
      "verse_number": 79
    },
    {
      "id": "c6606379-b377-438e-888a-cb05f08cc0d1",
      "m1": "وزان ناله و زاری خستگان",
      "m2": "به بند اندر آیند نابستگان",
      "verse_number": 80
    },
    {
      "id": "768c4a9c-7a9c-466d-b490-c61163316366",
      "m1": "شود کشته چندان ز هر سو سپاه",
      "m2": "که از خونشان پر شود رزمگاه",
      "verse_number": 81
    },
    {
      "id": "6cf5c1ef-66bd-405a-83e9-3b264ab99b4f",
      "m1": "پس آن بیدرفش پلید و سترگ",
      "m2": "به پیش اندر آید چو ارغنده گرگ",
      "verse_number": 82
    },
    {
      "id": "8c9834a4-57e0-456f-89b1-ee707820e2a9",
      "m1": "همان تیغ زهر آب داده به دست",
      "m2": "همی تازد او باره چون پیل مست",
      "verse_number": 83
    },
    {
      "id": "3dfc033c-5096-476c-b25c-7ea12f1a2c67",
      "m1": "به دست وی اندر فراوان سپاه",
      "m2": "تبه گردد از برگزینان شاه",
      "verse_number": 84
    },
    {
      "id": "ed158022-d6de-4ec2-b049-9b8c74e484e5",
      "m1": "بیاید پس آن فرخ اسفندیار",
      "m2": "سپاه از پس پشت و یزدانش یار",
      "verse_number": 85
    },
    {
      "id": "a785340c-b00d-4dcc-b4bb-5ecc39570b5d",
      "m1": "ابر بیدرفش افگند اسپ تیز",
      "m2": "برو جامه پر خون و دل پر ستیز",
      "verse_number": 86
    },
    {
      "id": "a11b49a3-1dd2-4acf-90fa-b1ba47ec7db5",
      "m1": "مر او را یکی تیغ هندی زند",
      "m2": "ز بر نیمهٔ تنش زیر افگند",
      "verse_number": 87
    },
    {
      "id": "6352f47b-1e2e-48ae-b611-ad89571a5079",
      "m1": "بگیرد پس آن آهنین گُرز را",
      "m2": "بتاباند آن فره و برز را",
      "verse_number": 88
    },
    {
      "id": "03a1d2d1-a62d-45a4-b5cc-482666a26003",
      "m1": "به یک حمله از جایشان بگسلد",
      "m2": "چو بگسستشان بر زمین کی هلد",
      "verse_number": 89
    },
    {
      "id": "eb5bf734-d633-437b-a8a1-27dc67015fe8",
      "m1": "بنوک سر نیزهشان بر چند",
      "m2": "کندشان تبه پاک و بپراگند",
      "verse_number": 90
    },
    {
      "id": "bf353466-356d-4f84-835d-9072be87e417",
      "m1": "گریزد سرانجام سالار چین",
      "m2": "از اسفندیار آن گو بافرین",
      "verse_number": 91
    },
    {
      "id": "a74d4a5d-6d12-4d00-a1b6-23f02e28095d",
      "m1": "به ترکان نهد روی بگریخته",
      "m2": "شکسته سپر نیزها ریخته",
      "verse_number": 92
    },
    {
      "id": "cdc90e33-e9b7-4937-9a7b-b57a3d885ee8",
      "m1": "بیابان گذارد به اندک سپاه",
      "m2": "شود شاه پیروز و دشمن تباه",
      "verse_number": 93
    },
    {
      "id": "560b1baa-beab-4e89-a9d5-79cff1df5119",
      "m1": "بدان ای گزیده شه خسروان",
      "m2": "که من هرچ گفتم نباشد جز آن",
      "verse_number": 94
    },
    {
      "id": "080ed418-670c-49a7-b9fc-7c6ecd352b01",
      "m1": "نباشد ازین یک سخن بیش و کم",
      "m2": "تو زین پس مکن روی بر من دژم",
      "verse_number": 95
    },
    {
      "id": "bb3c0d85-08bb-414e-aa67-c3addf86050e",
      "m1": "که من آنچ گفتم نگفتم مگر",
      "m2": "به فرمانت ای شاه پیروزگر",
      "verse_number": 96
    },
    {
      "id": "702f97b0-cb55-4cd7-b9a8-8a00ab29b0bf",
      "m1": "وزان کم بپرسید فرخنده شاه",
      "m2": "ازین ژرف دریا و تاریک راه",
      "verse_number": 97
    },
    {
      "id": "d8d30c0d-7a6c-49ce-bb86-7b6444ace604",
      "m1": "ندیدم که بر شاه بنهفتمی",
      "m2": "وگرنه من این راز کی گفتمی",
      "verse_number": 98
    },
    {
      "id": "86f7f12f-41bf-40a3-b07d-dfcd99abd8d9",
      "m1": "چو شاه جهاندار بشنید راز",
      "m2": "بران گوشهٔ تخت خسپید باز",
      "verse_number": 99
    },
    {
      "id": "24052713-462e-4633-903d-b8db0b693404",
      "m1": "ز دستش بیفتاد زرینه گُرز",
      "m2": "تو گفتی برفتش همی فر و برز",
      "verse_number": 100
    },
    {
      "id": "31ceadf8-6d8d-4489-adc2-3fb6c3b81911",
      "m1": "به روی اندر افتاد و بیهوش گشت",
      "m2": "نگفتش سخن نیز و خاموش گشت",
      "verse_number": 101
    },
    {
      "id": "ed14c935-6c82-4c9f-85f1-047d989cb693",
      "m1": "چو با هوش آمد جهان شهریار",
      "m2": "فرود آمد از تخت و بگریست زار",
      "verse_number": 102
    },
    {
      "id": "f2595081-6954-478f-a513-e663a539625e",
      "m1": "چه باید مرا گفت شاهی و گاه",
      "m2": "که روزم همی گشت خواهد سیاه",
      "verse_number": 103
    },
    {
      "id": "8f2317cb-c6be-4bcb-93eb-ffc082c305e4",
      "m1": "که آنان که بر من گرامیترند",
      "m2": "گزین سپاهند و نامیترند",
      "verse_number": 104
    },
    {
      "id": "586ca311-42f8-4de7-934a-613066bfe477",
      "m1": "همی رفت و خواهند از پیش من",
      "m2": "ز تن برکنند این دل ریش من",
      "verse_number": 105
    },
    {
      "id": "9e3d253a-b875-48b9-a984-03f92aca1235",
      "m1": "به جاماسپ گفت ار چنینست کار",
      "m2": "به هنگام رفتن سوی کارزار",
      "verse_number": 106
    },
    {
      "id": "f0cc6b01-7f22-42dc-a422-9e493b7596fa",
      "m1": "نخوانم نبرده برادرم را",
      "m2": "نسوزم دل پیر مادرم را",
      "verse_number": 107
    },
    {
      "id": "b940049d-e200-49f3-868e-905f271f584a",
      "m1": "نفرمایمش نیز رفتن به رزم",
      "m2": "سپه را سپارم به فرخ گُرَزم",
      "verse_number": 108
    },
    {
      "id": "9a0d36f0-6035-4788-b3fa-1e10d8562d7b",
      "m1": "کیان زادگان و جوانان من",
      "m2": "که هر یک چنانند چون جان من",
      "verse_number": 109
    },
    {
      "id": "6017d58f-e111-4b94-8355-0a5733579f74",
      "m1": "بخوانم همه سربسر پیش خویش",
      "m2": "زرهشان نپوشم نشانم به پیش",
      "verse_number": 110
    },
    {
      "id": "709a92f8-8c8c-4e26-aefc-52a771a5d8a7",
      "m1": "چگونه رسد نوک تیر خدنگ",
      "m2": "برین آسمان بر شده کوه سنگ",
      "verse_number": 111
    },
    {
      "id": "4cca9e09-7bc0-4d7c-bf21-2a0c12556731",
      "m1": "خردمند گفتا به شاه زمین",
      "m2": "که ای نیکخو مهتر بافرین",
      "verse_number": 112
    },
    {
      "id": "6d506295-24f0-46cc-bcc6-2b99daf16849",
      "m1": "گر ایشان نباشند پیش سپاه",
      "m2": "نهاده بسر بر کیانی کلاه",
      "verse_number": 113
    },
    {
      "id": "bbc16712-adc8-4278-ae37-b0758fb76640",
      "m1": "که یارد شدن پیش ترکان چین",
      "m2": "که بازآورد فره پاک دین",
      "verse_number": 114
    },
    {
      "id": "9f3e1682-eccc-402e-93f3-91c16dd7e84e",
      "m1": "تو زین خاک برخیز و برشو به گاه",
      "m2": "مکن فره پادشاهی تباه",
      "verse_number": 115
    },
    {
      "id": "c0f74c9b-dcae-44a3-8ef7-999e387fe154",
      "m1": "که داد خدایست وزین چاره نیست",
      "m2": "خداوند گیتی ستمگاره نیست",
      "verse_number": 116
    },
    {
      "id": "ffc90a3c-c1cf-4e0e-87ba-026cdf958d1e",
      "m1": "ز اندوه خوردن نباشدت سود",
      "m2": "کجا بودنی بود و شد کار بود",
      "verse_number": 117
    },
    {
      "id": "671d41f7-a345-4b28-8a2c-6063f2941d3b",
      "m1": "مکن دلت را بیشتر زین نژند",
      "m2": "بداد خدای جهان کن بسند",
      "verse_number": 118
    },
    {
      "id": "585f28f8-84b5-4fcc-9572-30f2d1a03a7b",
      "m1": "بدادش بسی پند و بشنید شاه",
      "m2": "چو خورشید گون گشت بر شد به گاه",
      "verse_number": 119
    },
    {
      "id": "2c692d7f-ba05-4b57-8c8d-f37573396cc9",
      "m1": "نشست از برگاه و بنهاد دل",
      "m2": "به رزم جهانجوی شاه چگل",
      "verse_number": 120
    },
    {
      "id": "cb893613-b7a7-4251-b0fc-6d18bb140a61",
      "m1": "از اندیشهٔ دل نیامدش خواب",
      "m2": "به رزم و به بزمش گرفته شتاب",
      "verse_number": 121
    }
  ],
