{
  "id": "23ad9b75-f589-4a6f-8e3c-f3076ed5b74d",
  "title": "بخش ۳۳",
  "verses": [
    {
      "id": "1f39022e-fb82-40ae-b97d-be727730f634",
      "m1": "ازان پس بیامد به پردهسرای",
      "m2": "ز هرگونه انداخت با شاه رای",
      "verse_number": 1
    },
    {
      "id": "b8dfc18d-11b5-4496-8fd4-4ac2df450c81",
      "m1": "ز لهراسپ وز کین فرشیدورد",
      "m2": "ازان نامداران روز نبرد",
      "verse_number": 2
    },
    {
      "id": "510bceb7-e9d1-47b2-8685-d5a047c9da2c",
      "m1": "بدو گفت گشتاسپ کای زورمند",
      "m2": "تو شادانی و خواهرانت به بند",
      "verse_number": 3
    },
    {
      "id": "1b07811c-d6bb-4184-8f6c-cc9f6e6d117d",
      "m1": "خنک آنک بر کینه گه کشته شد",
      "m2": "نه در چنگ ترکان سرگشته شد",
      "verse_number": 4
    },
    {
      "id": "2a85d1f1-f237-4bda-905a-9f7a74c98ae0",
      "m1": "چو بر تخت بینند ما را نشست",
      "m2": "چه گوید کسی کو بود زیر دست",
      "verse_number": 5
    },
    {
      "id": "f67dab75-3aaf-4ec2-8b29-aad21f323d43",
      "m1": "بگریم برین ننگ تا زندهام",
      "m2": "به مغز اندرون آتش افگندهام",
      "verse_number": 6
    },
    {
      "id": "d14cb61f-6fd5-4787-9945-0dae27e447e9",
      "m1": "پذیرفتم از کردگار بلند",
      "m2": "که گر تو به توران شوی بیگزند",
      "verse_number": 7
    },
    {
      "id": "818f060c-c5fc-4ac5-978a-534475c906f4",
      "m1": "به مردی شوی در دم اژدها",
      "m2": "کنی خواهران را ز ترکان رها",
      "verse_number": 8
    },
    {
      "id": "6142ae36-a538-4cb7-9c20-9dccb30bbba2",
      "m1": "سپارم ترا تاج شاهنشهی",
      "m2": "همان گنج بیرنج و تخت مهی",
      "verse_number": 9
    },
    {
      "id": "e4f6cf2a-719c-4082-a1a4-97a0ad2b928c",
      "m1": "مرا جایگاه پرستش بس است",
      "m2": "نه فرزند من نزد دیگر کس است",
      "verse_number": 10
    },
    {
      "id": "75a20f5e-0a3e-4248-81e1-81f81d7b992a",
      "m1": "چنین پاسخ آورد اسفندیار",
      "m2": "که بیتو مبیناد کس روزگار",
      "verse_number": 11
    },
    {
      "id": "d2351d10-a30c-4f6e-bf65-d9f21d6fffbb",
      "m1": "به پیش پدر من یکی بندهام",
      "m2": "روان را به فرمانش آگندهام",
      "verse_number": 12
    },
    {
      "id": "98f87820-ba5b-47b0-8139-d50e3273e674",
      "m1": "فدای تو دارم تن و جان خویش",
      "m2": "نخواهم سر و تخت و فرمان خویش",
      "verse_number": 13
    },
    {
      "id": "5182cad7-92b5-43c1-9a81-43ed5ff8ac4a",
      "m1": "شوم باز خواهم ز ارجاسپ کین",
      "m2": "نمانم بر و بوم توران زمین",
      "verse_number": 14
    },
    {
      "id": "a5e0785f-2d79-4b25-9e2c-e3ca7eea323b",
      "m1": "به تخت آورم خواهران را ز بند",
      "m2": "به بخت جهاندار شاه بلند",
      "verse_number": 15
    },
    {
      "id": "1f78daf1-dda0-4f94-87a6-7e1743a71ac4",
      "m1": "برو آفرین کرد گشتاسپ و گفت",
      "m2": "که با تو روان و خرد باد جفت",
      "verse_number": 16
    },
    {
      "id": "462bf81d-5835-497c-8f4f-19fe5f9d4941",
      "m1": "برفتنت یزدان پناه تو باد",
      "m2": "به باز آمدن تخت گاه تو باد",
      "verse_number": 17
    },
    {
      "id": "ad97562c-c24e-4516-ba12-97daa9fc3170",
      "m1": "بخواند آن زمان لشگر از هر سوی",
      "m2": "به جایی که بد موبدی گر گوی",
      "verse_number": 18
    },
    {
      "id": "74c9792c-ae83-4fc0-bc75-b7be807e94a3",
      "m1": "ازیشان گزیده ده و دو هزار",
      "m2": "سواران مرد افگن و کینهدار",
      "verse_number": 19
    },
    {
      "id": "3f3a8327-65ce-4fe4-99d6-d833268feca4",
      "m1": "بر ایشان ببخشید گنج درم",
      "m2": "نکرد ایچ کس را به بخشش دژم",
      "verse_number": 20
    },
    {
      "id": "142940bc-0af5-46b5-b4cb-4cd06961a10a",
      "m1": "ببخشید گنجی بر اسفندیار",
      "m2": "یکی تاج پر گوهر شاهوار",
      "verse_number": 21
    },
    {
      "id": "4f134088-b013-4575-9a70-879dc8f7b496",
      "m1": "خروشی برآمد ز درگاه شاه",
      "m2": "شد از گرد خورشید تابان سیاه",
      "verse_number": 22
    },
    {
      "id": "3166b680-813c-4df7-ac02-045d91977422",
      "m1": "ز ایوان به دشت آمد اسفندیار",
      "m2": "سپاهی گزید از در کارزار",
      "verse_number": 23
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 23
  }
}