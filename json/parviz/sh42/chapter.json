{
  "id": "8f2d93ab-85c9-488f-9fad-d43ae958030a",
  "title": "بخش ۴۲",
  "verses": [
    {
      "id": "dd37a181-b006-407f-964e-37f22cb2d874",
      "m1": "چو پیدا شد ازآسمان گرد ماه",
      "m2": "شب تیره بفشاند گرد سیاه",
      "verse_number": 1
    },
    {
      "id": "e1db7ff7-166d-461f-bd95-6c4e28bba0b5",
      "m1": "پراکنده گشتند و مستان شدند",
      "m2": "وز آنجای هرکس به ایوان شدند",
      "verse_number": 2
    },
    {
      "id": "4810ae4d-bc66-4b21-8533-ccda91358279",
      "m1": "چو پیداشد آن فرخورشید زرد",
      "m2": "به پیچید زلف شب لاژورد",
      "verse_number": 3
    },
    {
      "id": "2f8f0262-ada9-422b-bd6f-715a5b1f7145",
      "m1": "قژ آگند پوشید بهرام گرد",
      "m2": "گرامی تنش را به یزدان سپرد",
      "verse_number": 4
    },
    {
      "id": "cd0b6054-89c0-40f4-a095-3b6505ea5de5",
      "m1": "کمند و کمان برد و شش چوبه تیر",
      "m2": "یکی نیزه دو شاخ نخچیرگیر",
      "verse_number": 5
    },
    {
      "id": "612ebd9b-fbea-47be-b847-d5f89c52ce3d",
      "m1": "چوآمد به نزدیک آن برزکوه",
      "m2": "بفرمود تا بازگردد گروه",
      "verse_number": 6
    },
    {
      "id": "e931b62f-5260-485c-92ad-5e15d223e60d",
      "m1": "بران شیر کپی چو نزدیک شد",
      "m2": "تو گفتی برو کوه تاریک شد",
      "verse_number": 7
    },
    {
      "id": "8d7e39da-5178-4a0c-8c5c-edc982db4afa",
      "m1": "میان اندارن کوه خارا ببست",
      "m2": "بخم کمند از بر زین نشست",
      "verse_number": 8
    },
    {
      "id": "980b2d62-4f3d-402f-bdb1-c185535bf15d",
      "m1": "کمان را بمالید وبر زه نهاد",
      "m2": "ز یزدان نیکی دهش کرد باد",
      "verse_number": 9
    },
    {
      "id": "1432a1f4-c9c3-4062-b337-e9fa677569d7",
      "m1": "چو بر اژدها برشدی مویتر",
      "m2": "نبودی برو تیر کس کارگر",
      "verse_number": 10
    },
    {
      "id": "9a2aa95e-0d48-42b1-a315-a5873535c8ed",
      "m1": "شد آن شیر کپی به چشمه درون",
      "m2": "به غلتید و برخاست و آمد برون",
      "verse_number": 11
    },
    {
      "id": "483cff67-f567-42d4-bf67-2c0ae6f4c836",
      "m1": "بغرید و بر زد بران سنگ دست",
      "m2": "همی آتش از کوه خارا بجست",
      "verse_number": 12
    },
    {
      "id": "e88effc6-7ea1-4805-827d-e93653fa0cae",
      "m1": "کمان را بمالید بهرام گرد",
      "m2": "به تیر از هوا روشنایی ببرد",
      "verse_number": 13
    },
    {
      "id": "7f7b180c-591b-4ab3-9cf3-43cc10939160",
      "m1": "خدنگی بینداخت شیر دلیر",
      "m2": "برشیر کپی شد از جنگ سیر",
      "verse_number": 14
    },
    {
      "id": "83d2aa85-e3bb-4634-845b-3bc7eaa26fe5",
      "m1": "دگر تیر بهرام زد بر سرش",
      "m2": "فرو ریخت چون آب خون ازبرش",
      "verse_number": 15
    },
    {
      "id": "9f5f7ed5-0dc6-4d0a-8a41-904196e17bc9",
      "m1": "سیوم تیر و چارم بزد بر دهانش",
      "m2": "که بردوخت برهم دهان و زبانش",
      "verse_number": 16
    },
    {
      "id": "3e0b8842-223e-4ee9-8d96-ea9e75e05a3c",
      "m1": "به پنجم بزد تیر بر چنگ اوی",
      "m2": "همیدید نیروی و آهنگ اوی",
      "verse_number": 17
    },
    {
      "id": "cc227aea-4243-4f96-a126-1058694600a7",
      "m1": "بهشتم میانش گشاد از کمند",
      "m2": "بجست از بر کوهسار بلند",
      "verse_number": 18
    },
    {
      "id": "00bf76b6-c7e7-4c1e-bfd9-32aadc9ce442",
      "m1": "بزد نیزهای بر میان دده",
      "m2": "که شد سنگ خارا به خون آژده",
      "verse_number": 19
    },
    {
      "id": "27d995c7-d5c2-4637-97b7-701f3909a7dc",
      "m1": "وزان پس بشمشیر یازید مرد",
      "m2": "تن اژدها را به دونیم کرد",
      "verse_number": 20
    },
    {
      "id": "6babae1d-c636-4d2f-adbb-28347fb3c8dd",
      "m1": "سر از تن جدا کند و بفگند خوار",
      "m2": "ازان پس فرود آمد از کوهسار",
      "verse_number": 21
    },
    {
      "id": "0dd8c846-1d58-4fcc-8211-9d53f6f02d47",
      "m1": "ازان بیشه خاقان و خاتون برفت",
      "m2": "دمان و دنان تا برکوه تفت",
      "verse_number": 22
    },
    {
      "id": "d9e96a16-abaf-46a4-b2b0-277457d4205e",
      "m1": "خروشی برآمد ز گردان چین",
      "m2": "کز آواز گفتی بلرزد زمین",
      "verse_number": 23
    },
    {
      "id": "18921975-bc97-42dc-afb1-c4e6805dee43",
      "m1": "به بهرام برآفرین خواندند",
      "m2": "بسی گوهر و زر برافشاندند",
      "verse_number": 24
    },
    {
      "id": "6d98b612-2a79-407e-89b4-e8d73bdd9631",
      "m1": "چو خاتون بشد دست او بوس داد",
      "m2": "برفتند گردان فرخ نژاد",
      "verse_number": 25
    },
    {
      "id": "f7fb9ef8-7fb0-4c34-8389-07a369dd1064",
      "m1": "همه هم زبان آفرین خواندند",
      "m2": "ورا شاه ایران زمین خواندند",
      "verse_number": 26
    },
    {
      "id": "6b72dcf0-b806-4222-8b1a-e70feed0a299",
      "m1": "گرفتش سپهدار چین در کنار",
      "m2": "وزان پس ورا خواندی شهریار",
      "verse_number": 27
    },
    {
      "id": "923ea1ad-2c1f-4d1a-9719-b88bc51927da",
      "m1": "چو خاقان چینی به ایوان رسید",
      "m2": "فرستادهای مهربان برگزید",
      "verse_number": 28
    },
    {
      "id": "9cd47089-372a-4767-bdfa-3a86c1f0faea",
      "m1": "فرستاد ده بدره گنجی درم",
      "m2": "همان بدره و برده از بیش و کم",
      "verse_number": 29
    },
    {
      "id": "b3243e93-dea4-4de7-a2ca-10f0dc680998",
      "m1": "که رو پیش بهرام جنگی بگوی",
      "m2": "که نزدیک ما یافتی آب روی",
      "verse_number": 30
    },
    {
      "id": "08d05b6a-1625-43f3-a17d-9972e703fcd0",
      "m1": "پس پردهٔ ما یکی دخترست",
      "m2": "که بر تارک اختران افسرست",
      "verse_number": 31
    },
    {
      "id": "70441c6f-6e5f-48ef-b541-ba8d84ac4775",
      "m1": "کنون گر بخواهی ز من دخترم",
      "m2": "سپارم بتو لشکر و کشورم",
      "verse_number": 32
    },
    {
      "id": "54cee860-02b1-4a30-830d-6cf372876d09",
      "m1": "بدو گفت بهرام کاری رواست",
      "m2": "جهاندار بر بندگان پادشاست",
      "verse_number": 33
    },
    {
      "id": "979072b4-48b7-4abd-a01d-e77b57eff893",
      "m1": "به بهرام داد آن زمان دخترش",
      "m2": "به فرمان او شد همه کشورش",
      "verse_number": 34
    },
    {
      "id": "dc5aaab0-6fa5-47f9-a9e3-9e5ec186eec0",
      "m1": "بفرمود تا پیش او شد دبیر",
      "m2": "نوشتند منشور نو بر حریر",
      "verse_number": 35
    },
    {
      "id": "74f81830-e052-4683-9b8e-e775d1b17901",
      "m1": "بدو گفت هرکس کز ایران سرست",
      "m2": "ببخشش نگر تا کرا در خورست",
      "verse_number": 36
    },
    {
      "id": "44568ce7-f159-4122-b86a-37012c7bcb4a",
      "m1": "بر آیین چین خلعت آراستند",
      "m2": "فراوان کلاه و کمر خواستند",
      "verse_number": 37
    },
    {
      "id": "731e14d6-2106-47d4-906d-2323233a01c4",
      "m1": "جزاز داد و خورد شکارش نبود",
      "m2": "غم گردش روزگارش نبود",
      "verse_number": 38
    },
    {
      "id": "265fbc83-b65a-4cb7-8a99-8fffccb85b4f",
      "m1": "بزرگان چینی و گردنکشان",
      "m2": "ز بهرام یل داشتندی نشان",
      "verse_number": 39
    },
    {
      "id": "8658deda-c595-4b6e-a35b-f6d32528cc18",
      "m1": "همه چین همیگفت ما بندهایم",
      "m2": "ز بهر تو اندر جهان زندهایم",
      "verse_number": 40
    },
    {
      "id": "f0c4c0d6-9096-47db-8c39-74d29629e6ce",
      "m1": "همیخورد بهرام و بخشید چیز",
      "m2": "برو بر بسی آفرین بود نیز",
      "verse_number": 41
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 41
  },
  "order_in_parent": 42
}