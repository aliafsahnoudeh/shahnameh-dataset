{
  "id": "b6846473-622a-4ad6-87c9-b1d936459cbb",
  "title": "بخش ۱۴",
  "verses": [
    {
      "id": "1425ac00-4e9d-43b1-928f-826418ff9c0f",
      "m1": "چوپیدا شد آن چادر قیرگون",
      "m2": "درفشان شد اختر بچرخ اندرون",
      "verse_number": 1
    },
    {
      "id": "dcec3369-73d1-43e5-bd03-33bafd968f28",
      "m1": "چو آواز دارندهٔ پاس خاست",
      "m2": "قلم خواست بهرام و قرطاس خواست",
      "verse_number": 2
    },
    {
      "id": "1cf917a2-fb00-4c9a-89fe-a5d484877256",
      "m1": "بیامد دبیر خردمند و راد",
      "m2": "دوات و قلم پیش دانا نهاد",
      "verse_number": 3
    },
    {
      "id": "ebae4704-9422-499b-a17e-bda23c06265e",
      "m1": "بدو گفت عهدی ز ایرانیان",
      "m2": "بباید نوشتن برین پرنیان",
      "verse_number": 4
    },
    {
      "id": "bd765b27-d23e-4703-a0a1-e0ca4cd95fb5",
      "m1": "که بهرام شاهست و پیروزبخت",
      "m2": "سزاوار تاج است و زیبای تخت",
      "verse_number": 5
    },
    {
      "id": "2c64dd3f-d665-4d93-9718-636030066028",
      "m1": "نجوید جز از راستی درجهان",
      "m2": "چه در آشکار و چه اندر نهان",
      "verse_number": 6
    },
    {
      "id": "60146fd1-e9a5-43a0-ad2f-6efd9304d39c",
      "m1": "نوشته شد آن شمع برداشتند",
      "m2": "شب تیره باندیشه بگذاشتند",
      "verse_number": 7
    },
    {
      "id": "89094c9d-313b-4a1a-8062-b453ec97c960",
      "m1": "چو پنهان شد آن چادر لاژورد",
      "m2": "جهان شد ز دیدار خورشید زرد",
      "verse_number": 8
    },
    {
      "id": "5b77d7cb-cb11-4ab7-bd2a-2dd7623985ff",
      "m1": "بیامد یکی مرد پیروزبخت",
      "m2": "نهاد اندر ایوان بهرام تخت",
      "verse_number": 9
    },
    {
      "id": "495de03c-950e-47b7-bcb1-99d5bc726515",
      "m1": "برفتند ایوان شاهی چو عاج",
      "m2": "بیاویختند از برگاه تاج",
      "verse_number": 10
    },
    {
      "id": "c54a9a43-b8f2-4d48-94d2-0c3f553f9975",
      "m1": "برتخت زرین یکی زیرگاه",
      "m2": "نهادند و پس برگشادند راه",
      "verse_number": 11
    },
    {
      "id": "1cddb8a2-d9c6-47de-9c60-fc785655a12f",
      "m1": "نشست از بر تخت بهرامشاه",
      "m2": "به سر برنهاد آن کیانی کلاه",
      "verse_number": 12
    },
    {
      "id": "eb2c1051-dd62-4f2c-8c16-57c9170097ee",
      "m1": "دبیرش بیاورد عهد کیان",
      "m2": "نوشته بران پربها پرنیان",
      "verse_number": 13
    },
    {
      "id": "7f913c6e-91c8-464e-b32d-e576af336765",
      "m1": "گوایی نوشتند یکسر مهان",
      "m2": "که بهرام شد شهریار جهان",
      "verse_number": 14
    },
    {
      "id": "19ccae90-d1a8-48e4-9ce4-fdceeab49070",
      "m1": "بران نامه چون نام کردند یاد",
      "m2": "بروبر یکی مهر زرین نهاد",
      "verse_number": 15
    },
    {
      "id": "eb4beab2-15a8-40f7-a063-3a95af877a06",
      "m1": "چنین گفت کاین پادشاهی مراست",
      "m2": "بدین بر شما پاک یزدان گواست",
      "verse_number": 16
    },
    {
      "id": "ce208759-079c-4014-91bd-1d12519f0847",
      "m1": "چنین هم بماناد سالی هزار",
      "m2": "که از تخمهٔ من بود شهریار",
      "verse_number": 17
    },
    {
      "id": "35b7711b-3d23-4643-8605-e11641b45f46",
      "m1": "پسر بر پسر هم چنین ارجمند",
      "m2": "بماناد با تاج و تخت بلند",
      "verse_number": 18
    },
    {
      "id": "23a49dca-002e-4fe4-b46e-8e8cee7ddb49",
      "m1": "بآذر مه اندر بد و روز هور",
      "m2": "که از شیر پردخته شد پشت گور",
      "verse_number": 19
    },
    {
      "id": "e5ca5520-626d-40f0-8e61-868e6c03d3e9",
      "m1": "چنین گفت زان پس بایرانیان",
      "m2": "که برخاست پرخاش و کین از میان",
      "verse_number": 20
    },
    {
      "id": "13a3a34b-d3bf-4157-94ff-1388f140612d",
      "m1": "کسی کوبرین نیست همداستان",
      "m2": "اگر کژ باشید اگر راستان",
      "verse_number": 21
    },
    {
      "id": "c4012268-f8c2-4228-9ebd-b71be0d3a02a",
      "m1": "به ایران مباشید بیش از سه روز",
      "m2": "چهارم چو از چرخ گیتی فروز",
      "verse_number": 22
    },
    {
      "id": "96ad7a3b-cad3-48ff-8efb-6d9b685f84ce",
      "m1": "بر آید همه نزد خسرو شوید",
      "m2": "برین بوم و بر بیش ازین مغنوید",
      "verse_number": 23
    },
    {
      "id": "9dfdf661-d582-4613-8305-c9bf22ee5796",
      "m1": "نه از دل برو خواندند آفرین",
      "m2": "که پردخته از تو مبادا زمین",
      "verse_number": 24
    },
    {
      "id": "8e2bb87c-ed2a-4771-bd81-bb7f481ccea2",
      "m1": "هرآنکس که با شاه پیوسته بود",
      "m2": "بران پادشاهی دلش خسته بود",
      "verse_number": 25
    },
    {
      "id": "886d02b5-e239-48db-8d39-5325a4b37df3",
      "m1": "برفتند زان بوم تا مرز روم",
      "m2": "پراگنده گشتند ز آباد بوم",
      "verse_number": 26
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 26
  },
  "order_in_parent": 14
}