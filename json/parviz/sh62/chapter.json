{
  "id": "5923e839-abe6-4fdb-aa87-f471600e4e8a",
  "title": "بخش ۶۲",
  "verses": [
    {
      "id": "4fa54d18-1464-4232-b69a-2f01ee55c5bd",
      "m1": "چو بر پادشاهیش شد پنجسال",
      "m2": "به گیتی نبودش سراسر همال",
      "verse_number": 1
    },
    {
      "id": "ba11b053-3fd6-4c6a-aa4a-a50cf3a648c8",
      "m1": "ششم سال زان دخت قیصر چو ماه",
      "m2": "یکی پورش آمد همانند شاه",
      "verse_number": 2
    },
    {
      "id": "c20a1c31-ddc6-4bf0-af63-208e4f3fadd7",
      "m1": "نبود آن زمان رسم بانگ نماز",
      "m2": "به گوش چنان پروریده بناز",
      "verse_number": 3
    },
    {
      "id": "5b00f418-7e91-4cc1-a6ae-6fda51bd49b1",
      "m1": "یکی نام گفتی مر او را پدر",
      "m2": "نهانی دگر آشکارا دگر",
      "verse_number": 4
    },
    {
      "id": "69cf3994-ad4f-46bb-a932-50174486d9c8",
      "m1": "نهانی به گفتی بگوش اندرون",
      "m2": "همیخواندی آشکارا برون",
      "verse_number": 5
    },
    {
      "id": "59ed4289-e85e-46d8-bb99-4ea29ac3a12f",
      "m1": "بگوش اندرون خواند خسرو قباد",
      "m2": "همیگفت شیر وی فرخ نژاد",
      "verse_number": 6
    },
    {
      "id": "d43ba456-1918-44e3-ab74-36c2a1dba991",
      "m1": "چو شب کودک آمد گذشته سه پاس",
      "m2": "بیامد بر خسرو اخترشناس",
      "verse_number": 7
    },
    {
      "id": "1387c9af-93fe-4a11-a234-37024d40f466",
      "m1": "از اخترشناسان بپرسید شاه",
      "m2": "که هرکس که دارند اختر نگاه",
      "verse_number": 8
    },
    {
      "id": "5e9c174c-7fc0-4a2a-b43b-77554f73dd77",
      "m1": "بدیدی که فرجام این کار چیست",
      "m2": "ز زیچ اختر این جهاندار چیست",
      "verse_number": 9
    },
    {
      "id": "e02c6047-d672-4d65-b5b6-78fc73f46e28",
      "m1": "چنین داد پاسخ ستاره شمر",
      "m2": "که بر چرخ گردان نیابی گذر",
      "verse_number": 10
    },
    {
      "id": "23039fe9-fd75-4001-a4ff-02bfa775f053",
      "m1": "ازین کودک آشوب گیرد زمین",
      "m2": "نخواند سپاهت برو آفرین",
      "verse_number": 11
    },
    {
      "id": "a3bcc96e-59cd-43dc-803d-98d046902189",
      "m1": "هم از راه یزدان بگردد به نیز",
      "m2": "ازین بیشتر چون سراییم چیز",
      "verse_number": 12
    },
    {
      "id": "4d07e280-30c7-4cea-8db4-43fd407d3a41",
      "m1": "دل شاه غمگین شد از کارشان",
      "m2": "وزان ناسزاوار گفتارشان",
      "verse_number": 13
    },
    {
      "id": "971eb5b1-17f7-46e9-92b6-8c02194be4e1",
      "m1": "چنین گفت با مرد داننده شاه",
      "m2": "که نیکو کنید اندر اختر نگاه",
      "verse_number": 14
    },
    {
      "id": "90c1dacd-ac71-4f6e-8016-ca4ab75beff9",
      "m1": "نگر تا نگردد زبانتان برین",
      "m2": "به پیش بزرگان ایران زمین",
      "verse_number": 15
    },
    {
      "id": "4cd24f7c-8e45-4107-a343-a2ebb5a79dc6",
      "m1": "همیداشت آن اختران را نگاه",
      "m2": "نهاده بران بسته بر مهر شاه",
      "verse_number": 16
    },
    {
      "id": "0404d7fe-5243-4fa0-82e8-9c34105a7766",
      "m1": "پر اندیشه بد زان سخن شهریار",
      "m2": "بران هفته کس را ندادند بار",
      "verse_number": 17
    },
    {
      "id": "5e98685c-3bd2-480b-bbd5-b8d6d10edb99",
      "m1": "ز نخچیر و از می به یکسو کشید",
      "m2": "بدان چندگه روی کس را ندید",
      "verse_number": 18
    },
    {
      "id": "0307c660-9c27-490c-8f4b-665d4a0173f5",
      "m1": "همه مهتران سوی موبد شدند",
      "m2": "ز هر گونهای داستانها زدند",
      "verse_number": 19
    },
    {
      "id": "46a21fe9-cabe-4de3-a911-f81a12575bb5",
      "m1": "بدان تا چه بد نامور شاه را",
      "m2": "که بربست بر کهتران راه را",
      "verse_number": 20
    },
    {
      "id": "52eab748-0658-4fad-a034-e614138fd6a9",
      "m1": "چو بشنید موبد بشد نزد شاه",
      "m2": "بدو داد یکسر پیام سپاه",
      "verse_number": 21
    },
    {
      "id": "c6b92881-cc67-471f-8c19-941f18bbb72f",
      "m1": "چنین داد پاسخ ورا شهریار",
      "m2": "که من تنگ دل گشتم از روزگار",
      "verse_number": 22
    },
    {
      "id": "668090dd-46a5-4f70-863b-cb110c1d1a05",
      "m1": "ز گفتار این مرد اخترشناس",
      "m2": "ز گردون گردان شدم ناسپاس",
      "verse_number": 23
    },
    {
      "id": "4d053c1e-6df5-453c-acbd-77ed1b7855a9",
      "m1": "به گنجور گفت آن یکی پرنیان",
      "m2": "بیاور یکی رقعه اندر میان",
      "verse_number": 24
    },
    {
      "id": "1488e5fb-6738-4301-bd57-9c4607c40401",
      "m1": "بیاورد گنجور و موبد بدید",
      "m2": "دلش تنگ شد خامشی برگزید",
      "verse_number": 25
    },
    {
      "id": "d98a033a-e5f9-4936-be24-5dfa03516c76",
      "m1": "ازان پس بدو گفت یزدان بس است",
      "m2": "کجا برتر از دانش هر کس است",
      "verse_number": 26
    },
    {
      "id": "c6f6fc9c-6d6e-4863-86d1-bee3ea999c81",
      "m1": "گر ایدون که ناچار گردان سپهر",
      "m2": "دگرگون نماید به جوینده چهر",
      "verse_number": 27
    },
    {
      "id": "d848caac-6cca-4997-9fc9-9c3a545a0d36",
      "m1": "به تیمار کی باز گردد ز بد",
      "m2": "چنین گفته از دانشی کی سزد",
      "verse_number": 28
    },
    {
      "id": "1f82be06-c749-41dd-ab2e-a6635c1bb1d4",
      "m1": "جز از شادمانیت هرگز مباد",
      "m2": "ز گفتار ایشان مکن هیچ یاد",
      "verse_number": 29
    },
    {
      "id": "bbc04fcc-e4e7-406b-903a-62815d774cb2",
      "m1": "ز موبد چو بشنید خسرو سخن",
      "m2": "بخندید و کاری نو افگند بن",
      "verse_number": 30
    },
    {
      "id": "257d5ddf-bf4b-4194-bf69-693eaee6b785",
      "m1": "دبیر پسندیده را خواند پیش",
      "m2": "سخن گفت با او ز اندازه بیش",
      "verse_number": 31
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 31
  },
