{
  "id": "90b705d2-7bad-4402-a707-e4aa810eeaf7",
  "title": "بخش ۳",
  "verses": [
    {
      "id": "a2f9bba0-da29-4634-b97a-4ce0f92753c4",
      "m1": "چو پنهان شد آن چادر آبنوس",
      "m2": "بگوش آمد از دوربانگ خروش",
      "verse_number": 1
    },
    {
      "id": "8dd3981f-811e-4991-bd03-f09e5d0be1db",
      "m1": "جهانگیر شد تابنزد پدر",
      "m2": "نهانش پر ازدرد وخسته جگر",
      "verse_number": 2
    },
    {
      "id": "af286260-7ce5-4d9f-84b6-5257b8e0f35e",
      "m1": "چو دیدش بنالید و بردش نماز",
      "m2": "همیبود پیشش زمانی دراز",
      "verse_number": 3
    },
    {
      "id": "b959094c-3225-4f5d-87a7-aaaa36621f97",
      "m1": "بدو گفت کای شاه نابختیار",
      "m2": "ز نوشین روان در جهان یادگار",
      "verse_number": 4
    },
    {
      "id": "6ba909d3-24c0-4a03-883d-b92724ed0cc7",
      "m1": "تو دانی که گر بودمی پشت تو",
      "m2": "بسوزن نخستی سر انگشت تو",
      "verse_number": 5
    },
    {
      "id": "151ac255-29e4-4e40-9db5-8bd99662bf40",
      "m1": "نگر تا چه فرمایی اکنون مرا",
      "m2": "غم آمد تو را دل پر از خون مرا",
      "verse_number": 6
    },
    {
      "id": "96652423-3a5f-4874-b0fa-0b6aebd11661",
      "m1": "گر ای دون که فرمان دهی بر درت",
      "m2": "یکی بندهام پاسبان سرت",
      "verse_number": 7
    },
    {
      "id": "201ec83e-2995-4c48-86e1-329854145089",
      "m1": "نجویم کلاه و نخواهم سپاه",
      "m2": "ببرم سرخویش در پیش شاه",
      "verse_number": 8
    },
    {
      "id": "e5daab29-5d8a-45a8-80d7-2b99f267c712",
      "m1": "بدو گفت هرمزد ای پرخرد",
      "m2": "همین روز سختی ز من بگذرد",
      "verse_number": 9
    },
    {
      "id": "4ae3be29-049d-488d-9e89-009d4bc3e782",
      "m1": "مرا نزد تو آرزو بد سه چیز",
      "m2": "برین بر فزونی نخواهیم نیز",
      "verse_number": 10
    },
    {
      "id": "7653e0c5-0ea2-4029-be16-dc7e01c73a96",
      "m1": "یکی آنک شبگیر هر بامداد",
      "m2": "کنی گوش ما را به آواز شاد",
      "verse_number": 11
    },
    {
      "id": "b53672b8-b545-4a90-8678-7dd1480933f4",
      "m1": "و دیگر سواری ز گردنکشان",
      "m2": "که از رزم دیرینه دارد نشان",
      "verse_number": 12
    },
    {
      "id": "f2e42346-958b-4fc4-8426-403423e15cba",
      "m1": "بر من فرستی که از کارزار",
      "m2": "سخن گوید و کرده باشد شکار",
      "verse_number": 13
    },
    {
      "id": "6343a26b-5eba-4234-8715-0e108231ead7",
      "m1": "دگر آنک داننده مرد کهن",
      "m2": "که از شهریاران گزارد سخن",
      "verse_number": 14
    },
    {
      "id": "01e28fa7-b622-4152-a90d-ec48953e0013",
      "m1": "نوشته یکی دفتر آرد مرا",
      "m2": "بدان درد و سختی سرآرد مرا",
      "verse_number": 15
    },
    {
      "id": "c0d3490f-192d-42e3-8d9b-69875b4dd3bc",
      "m1": "سیم آرزوی آنک خال تواند",
      "m2": "پرستنده و ناهمال تواند",
      "verse_number": 16
    },
    {
      "id": "d24c06d0-cf5e-46b2-b4cf-b52f83f9d597",
      "m1": "نبینند زین پس جهان را بچشم",
      "m2": "بریشان برانی برین سوک خشم",
      "verse_number": 17
    },
    {
      "id": "bcd60c74-47ca-4c2f-9df3-6be9ee5a6dbf",
      "m1": "بدو گفت خسرو که ای شهریار",
      "m2": "مباد آنک برچشم تو سوکوار",
      "verse_number": 18
    },
    {
      "id": "692d2bf2-4724-4d91-93d5-79d6e6916f95",
      "m1": "نباشد و گرچه بود درنهان",
      "m2": "که بدخواه تو دور بادازجهان",
      "verse_number": 19
    },
    {
      "id": "91be9e4f-2226-49cb-a08d-65849bd0f4c0",
      "m1": "ولیکن نگه کن بروشن روان",
      "m2": "که بهرام چوبینه شد پهلوان",
      "verse_number": 20
    },
    {
      "id": "89a47e97-cbff-4ec1-b963-93e0e3270edc",
      "m1": "سپاهست با او فزون از شمار",
      "m2": "سواران و گردان خنجرگزار",
      "verse_number": 21
    },
    {
      "id": "7a57154a-6eb0-41fc-8c3a-689f2adb43ba",
      "m1": "اگر ما بگستهم یازیم دست",
      "m2": "بگیتی نیابیم جای نشست",
      "verse_number": 22
    },
    {
      "id": "3d0d549e-5f8c-45ff-b995-d35a0a8c3ac8",
      "m1": "دگر آنک باشد دبیر کهن",
      "m2": "که برشاه خواند گذشته سخن",
      "verse_number": 23
    },
    {
      "id": "02f9e883-671a-4aee-8882-93d52b22811d",
      "m1": "سواری که پرورده باشد برزم",
      "m2": "بداند همان نیز آیین بزم",
      "verse_number": 24
    },
    {
      "id": "386a1bab-db78-4346-98b0-d955d94b9006",
      "m1": "ازین هر زمان نو فرستم یکی",
      "m2": "تو با درد پژمان مباش اندکی",
      "verse_number": 25
    },
    {
      "id": "27050545-e519-467c-84e9-0cabfdd1a62c",
      "m1": "مدان این زگستهم کاین ایزدیست",
      "m2": "ز گفتار و کردار نابخردیست",
      "verse_number": 26
    },
    {
      "id": "8687b7b5-1eb3-49b2-87ef-ea2c0f91db8d",
      "m1": "دل تو بدین درد خرسند باد",
      "m2": "همان با خرد نیز پیوند باد",
      "verse_number": 27
    },
    {
      "id": "b97803b8-f063-4ea3-b3fb-436eee1d9bef",
      "m1": "بگفت این و گریان بیامد زپیش",
      "m2": "نکرد آشکارا بکس راز خویش",
      "verse_number": 28
    },
    {
      "id": "2293dc5b-3716-4d36-ad63-5dda73f42e98",
      "m1": "پسر مهربانتر بد از شهریار",
      "m2": "بدین داستان زد یکی هوشیار",
      "verse_number": 29
    },
    {
      "id": "a1c7446e-1533-4ed1-a7e4-4ac530445f19",
      "m1": "که یار زبان چرب و شیرین سخن",
      "m2": "که از پیر نستوه گشته کهن",
      "verse_number": 30
    },
    {
      "id": "a36a1fc5-34d5-4f15-aed8-8b63969f1b37",
      "m1": "هنرمند گر مردم بیهنر",
      "m2": "بفرجام هم خاک دارد ببر",
      "verse_number": 31
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 31
  },
  "order_in_parent": 3
}