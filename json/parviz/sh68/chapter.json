{
  "id": "fdcb02b8-c35f-4161-99fc-d09cea1158cf",
  "title": "بخش ۶۸",
  "verses": [
    {
      "id": "a185e2fa-5a12-46f6-b012-352c4c736d8c",
      "m1": "ازان پس فزون شد بزرگی شاه",
      "m2": "که خورشید شد آن کجا بود ماه",
      "verse_number": 1
    },
    {
      "id": "8797874f-2cd3-4b85-9953-e410ca77e88b",
      "m1": "همه روز با دخت قیصر بدی",
      "m2": "همو بر شبستانش مهتر بدی",
      "verse_number": 2
    },
    {
      "id": "de51257d-42de-4f7e-8ed1-d6114cde24f6",
      "m1": "ز مریم همیبود شیرین بدرد",
      "m2": "همیشه ز رشکش دو رخساره زرد",
      "verse_number": 3
    },
    {
      "id": "6916be1a-6305-497a-9cac-1638282f6f8d",
      "m1": "به فرجام شیرین ورا زهر داد",
      "m2": "شد آن نامور دخت قیصرنژاد",
      "verse_number": 4
    },
    {
      "id": "07ecc5f0-e9db-46ab-90c3-8da1fff74e8f",
      "m1": "ازان چاره آگه نبد هیچکس",
      "m2": "که او داشت آن راز تنها و بس",
      "verse_number": 5
    },
    {
      "id": "2ec54760-d6e3-4ae2-ae3b-62bd8576907e",
      "m1": "چو سالی برآمد که مریم بمرد",
      "m2": "شبستان زرین به شیرین سپرد",
      "verse_number": 6
    },
    {
      "id": "7621461e-a42a-4e43-b826-e3236957ed5b",
      "m1": "چو شیرویه را سال شد بر دو هشت",
      "m2": "به بالا زسی سالگان برگذشت",
      "verse_number": 7
    },
    {
      "id": "642aac9d-920e-41b1-b514-846a7c810119",
      "m1": "بیاورد فرزانگان را پدر",
      "m2": "بدان تا شود نامور پر هنر",
      "verse_number": 8
    },
    {
      "id": "1b4df7ea-075c-4cb9-9589-0332e4d1677e",
      "m1": "همیداشت موبد مر او را نگاه",
      "m2": "شب و روز شادان به فرمان شاه",
      "verse_number": 9
    },
    {
      "id": "5db40257-c766-4ed6-b3c9-4bc899c93ac8",
      "m1": "چنان بد که یک روز موبد ز تخت",
      "m2": "بیامد به نزدیک آن نیک بخت",
      "verse_number": 10
    },
    {
      "id": "1f484806-e520-4f0a-be7f-59615a96f7c4",
      "m1": "چو آمد به نزدیک شیرویه باز",
      "m2": "همیشه به بازیش بودی نیاز",
      "verse_number": 11
    },
    {
      "id": "0c7e6da4-96e2-4901-9707-afde5206e083",
      "m1": "یکی دفتری دید پیش اندرش",
      "m2": "نوشته کلیله بران دفترش",
      "verse_number": 12
    },
    {
      "id": "90f5f8a3-8f44-43e6-8ba1-c4f67e52a840",
      "m1": "بدست چپ آن جوان سترگ",
      "m2": "بریده یکی خشک چنگال گرگ",
      "verse_number": 13
    },
    {
      "id": "e64e9e00-10d2-4d6f-b1c7-050f856511e4",
      "m1": "سروی سر گاومیشی براست",
      "m2": "همی این بران بر زدی چونک خواست",
      "verse_number": 14
    },
    {
      "id": "f7cc39fe-6689-478c-b5ac-75e16dee8e2c",
      "m1": "غمی شد دل موبد از کاراوی",
      "m2": "ز بازی و بیهوده کردار اوی",
      "verse_number": 15
    },
    {
      "id": "2d107dfb-4cd3-404c-86a2-080d398055a7",
      "m1": "به فالش بد آمد هم آن چنگ گرگ",
      "m2": "شخ گاو و رای جوان سترگ",
      "verse_number": 16
    },
    {
      "id": "eec7c9b5-0bf4-4617-aa55-f8fbddf70753",
      "m1": "ز کار زمانه غمی گشت سخت",
      "m2": "ازان برمنش کودک شور بخت",
      "verse_number": 17
    },
    {
      "id": "50898970-d167-4b8c-b8f3-c1f7d65ec407",
      "m1": "کجا طالع زادنش دیده بود",
      "m2": "ز دستور وگنجور بشنیده بود",
      "verse_number": 18
    },
    {
      "id": "b307ea9e-f291-4960-ac55-8b8243fb31bf",
      "m1": "سوی موبد موبد آمد بگفت",
      "m2": "که بازیست باآن گرانمایه جفت",
      "verse_number": 19
    },
    {
      "id": "bd36b73b-6fad-467c-8018-c819b66a265e",
      "m1": "بشد زود موبد بگفت آن به شاه",
      "m2": "همیداشت خسرو مر او را نگاه",
      "verse_number": 20
    },
    {
      "id": "e483c7c6-e70b-428a-a719-86edbb291589",
      "m1": "ز فرزند رنگ رخش زرد شد",
      "m2": "ز کار زمانه پراز درد شد",
      "verse_number": 21
    },
    {
      "id": "63e813b7-c613-445c-a507-69d1ac7aee7c",
      "m1": "ز گفتار مرد ستاره شمر",
      "m2": "دلش بود پر درد و پیچان جگر",
      "verse_number": 22
    },
    {
      "id": "4ed79f43-23a7-4636-98b3-82e7a4b1f924",
      "m1": "همیگفت تا کردگار سپهر",
      "m2": "چگونه نماید بدین کرده چهر",
      "verse_number": 23
    },
    {
      "id": "14eb0292-1e4a-4cf2-a146-eb30d257ab62",
      "m1": "چو بر پادشاهیش بیست وسه سال",
      "m2": "گذر کرد شیرویه بفراخت یال",
      "verse_number": 24
    },
    {
      "id": "f5807d9e-cbdc-4552-b63c-6adc14cb96a3",
      "m1": "بیازرد زو شهریار بزرگ",
      "m2": "که کودک جوان بود و گشته سترگ",
      "verse_number": 25
    },
    {
      "id": "365fb66f-4f7e-4baa-b7c2-f445696f5f50",
      "m1": "پر از درد شد جان خندان اوی",
      "m2": "وز ایوان او کرد زندان اوی",
      "verse_number": 26
    },
    {
      "id": "24ff53af-16fe-4e54-993b-53608273a70f",
      "m1": "هم آن را که پیوستهٔ اوبدند",
      "m2": "گه رای جستن براو شدند",
      "verse_number": 27
    },
    {
      "id": "60b43fdd-e44e-448e-bfc4-033081b64aa4",
      "m1": "بسی دیگر از مهتر و کهتران",
      "m2": "که بودند با او ببندگران",
      "verse_number": 28
    },
    {
      "id": "6214bd91-3f3f-476a-8b66-a8c12e357e5b",
      "m1": "همیبرگرفتند زیشان شمار",
      "m2": "که پرسه فزون آمد از سه هزار",
      "verse_number": 29
    },
    {
      "id": "8e7ffd3d-2b49-4177-9c09-7bea03258334",
      "m1": "همه کاخها رایک اندر دگر",
      "m2": "برید آنک بد شاه را کارگر",
      "verse_number": 30
    },
    {
      "id": "5b0801ed-589c-4414-a7cf-8450641fdc5f",
      "m1": "ز پوشیدنیها و از خوردنی",
      "m2": "ز بخشیدنی هم ز گستردنی",
      "verse_number": 31
    },
    {
      "id": "be34b1dc-a399-4560-9702-508714869e82",
      "m1": "به ایوانهاشان بیاراستند",
      "m2": "پرستنده و بندگان خواستند",
      "verse_number": 32
    },
    {
      "id": "61240e20-dc00-4cf0-ab47-225a2acd3a16",
      "m1": "همان میفرستاد و رامشگران",
      "m2": "همه کاخ دینار بد بیکران",
      "verse_number": 33
    },
    {
      "id": "57ad3d29-7ef1-4cb3-801c-5b35eb127a7a",
      "m1": "به هنگامشان رامش و خورد بود",
      "m2": "نگهبان ایشان چهل مرد بود",
      "verse_number": 34
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 34
  }
}