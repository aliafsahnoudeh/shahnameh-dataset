{
  "id": "6d303120-cffb-4717-b6d5-3ec680e8ab7f",
  "title": "بخش ۲۹",
  "verses": [
    {
      "id": "74cd121a-bd89-42d2-b63c-e008e14e836f",
      "m1": "چوآمد به بهرام زین آگهی",
      "m2": "که تازه شد آن فر شاهنشهی",
      "verse_number": 1
    },
    {
      "id": "422d3ade-3519-4d6c-ae0b-d1c7ae834e28",
      "m1": "همانگه ز لشکر یکی نامجوی",
      "m2": "نگه کرد با دانش و آب روی",
      "verse_number": 2
    },
    {
      "id": "18d4c12f-7c22-4ef6-8f15-43cbd46f21e2",
      "m1": "کجا نام او بود داناپناه",
      "m2": "که بهرام را او بدی نیک خواه",
      "verse_number": 3
    },
    {
      "id": "63c3f9bb-1d89-49f9-a516-44aee9012bf0",
      "m1": "دبیر سرافراز را پیش خواند",
      "m2": "سخنهای بایسته چندی براند",
      "verse_number": 4
    },
    {
      "id": "7a55cb42-2008-4afb-a597-01a235f88fd0",
      "m1": "بفرمود تا نامههای بزرگ",
      "m2": "نویسد بران مهتران سترگ",
      "verse_number": 5
    },
    {
      "id": "cc5abcf0-fd64-44f1-8444-3603924f307a",
      "m1": "بگستهم و گردوی و بندوی گرد",
      "m2": "که از مهتران نام گردی ببرد",
      "verse_number": 6
    },
    {
      "id": "df4e7107-6476-4e7e-9134-4fbeb02e298a",
      "m1": "چو شاپور و چون اندیان سوار",
      "m2": "هرآنکس که بود از یلان نامدار",
      "verse_number": 7
    },
    {
      "id": "e7fc6dad-564e-4c3b-9c68-d5eabd905628",
      "m1": "سرنامه گفت از جهان آفرین",
      "m2": "همیخواهم اندر نهان آفرین",
      "verse_number": 8
    },
    {
      "id": "86c34aab-fc76-4d06-97ef-4c08ae6c3daa",
      "m1": "چوبیدار گردید یکسر ز خواب",
      "m2": "نگیرید بر بد ازین سان شتاب",
      "verse_number": 9
    },
    {
      "id": "2506bf34-5287-443e-90b3-627745b73577",
      "m1": "که تا درجهان تخم ساسانیان",
      "m2": "پدید آمد اندر کنار و میان",
      "verse_number": 10
    },
    {
      "id": "1420f5f9-26cf-4fd4-88c5-544e399e6a6a",
      "m1": "ازیشان نرفتست جزبرتری",
      "m2": "بگرد جهان گشتن و داوری",
      "verse_number": 11
    },
    {
      "id": "c7e19dfc-0586-4ca2-a5b0-39e9798cd349",
      "m1": "نخست از سر بابکان اردشیر",
      "m2": "که اندر جهان تازه شد داروگیر",
      "verse_number": 12
    },
    {
      "id": "d9568df7-4fc8-4946-b9f6-be98a4d03f4e",
      "m1": "زمانه ز شمشیر او تیره گشت",
      "m2": "سر نامداران همه خیره گشت",
      "verse_number": 13
    },
    {
      "id": "7e444a58-b779-4f04-b3a7-891df6336af8",
      "m1": "نخستین سخن گویم از اردوان",
      "m2": "ازان نامداران روشن روان",
      "verse_number": 14
    },
    {
      "id": "80044494-76d5-454c-b0ed-84cdff85b852",
      "m1": "شنیدی که بر نامور سوفزای",
      "m2": "چه آمد ز پیروز ناپاک رای",
      "verse_number": 15
    },
    {
      "id": "b737b22f-e8de-44c1-88bf-e9eb310c348a",
      "m1": "رها کردن ازبند پای قباد",
      "m2": "وزان مهتران دادن او را بباد",
      "verse_number": 16
    },
    {
      "id": "2d7999cb-3c42-48aa-a910-0bfca0251f70",
      "m1": "قباد بد اندیش نیرو گرفت",
      "m2": "هنرها بشست از دل آهو گرفت",
      "verse_number": 17
    },
    {
      "id": "46f6b920-c1ce-4d97-bdbb-96e5d696825c",
      "m1": "چنان نامور نیک دل را بکشت",
      "m2": "برو شد دل نامداران درشت",
      "verse_number": 18
    },
    {
      "id": "d32bbe19-f920-4083-83f8-bb3ef39a5153",
      "m1": "کسی کو نشاید به پیوند خویش",
      "m2": "هوا بر گزیند ز فرزند خویش",
      "verse_number": 19
    },
    {
      "id": "ed151c00-2679-4022-ba5e-bdb2f6637996",
      "m1": "به بیگانگان هم نشاید بنیز",
      "m2": "نجوید کسی عاج از چوب شیز",
      "verse_number": 20
    },
    {
      "id": "204fd31b-425b-4bcc-b773-991c677f35cc",
      "m1": "بساسانیان تا ندارید امید",
      "m2": "مجویید یاقوت از سرخ بید",
      "verse_number": 21
    },
    {
      "id": "6f9d86af-d97d-4ca1-99ad-27349a40a0c8",
      "m1": "چواین نامه آرند نزد شما",
      "m2": "که فرخنده باد اورمزد شما",
      "verse_number": 22
    },
    {
      "id": "6737677f-60a9-4db5-890c-6f1c0b56c902",
      "m1": "به نزدیک من جایتان روشنست",
      "m2": "برو آستی هم ز پیراهنست",
      "verse_number": 23
    },
    {
      "id": "193cc28e-4a49-4619-b113-157ba06c92c6",
      "m1": "بیک جای مان بود آرام و خواب",
      "m2": "اگر تیره بد گر بلند آفتاب",
      "verse_number": 24
    },
    {
      "id": "774ba8fb-8c89-432a-85c6-f933cd55511e",
      "m1": "چو آیید یکسر به نزدیک من",
      "m2": "شود روشن این جان تاریک من",
      "verse_number": 25
    },
    {
      "id": "1e72368f-ffc3-4177-9af8-afbf763a4703",
      "m1": "نیندیشم از روم وز شاهشان",
      "m2": "بپای اندر آرم سر و گاهشان",
      "verse_number": 26
    },
    {
      "id": "ad140af5-d43d-4097-8488-a29ccb47f999",
      "m1": "نهادند برنامهها مهر اوی",
      "m2": "بیامد فرستاده راه جوی",
      "verse_number": 27
    },
    {
      "id": "a1c82af5-9344-4e63-8d30-7af2cffca1d5",
      "m1": "بکردار بازارگانان برفت",
      "m2": "بدرگاه خسرو خرامید تفت",
      "verse_number": 28
    },
    {
      "id": "2296b62d-8e25-4628-ae40-1f25c83bedb3",
      "m1": "یکی کاروانی ز هرگونه چیز",
      "m2": "ابا نامهها هدیهها داشت نیز",
      "verse_number": 29
    },
    {
      "id": "cd6c42d0-5c9d-4a4a-a5ef-52ba1f8c053d",
      "m1": "بدید آن بزرگی و چندان سپاه",
      "m2": "که گفتی مگر بر زمین نیست راه",
      "verse_number": 30
    },
    {
      "id": "06fe0db8-5f5d-4a0c-aad6-d1af4f51587b",
      "m1": "به دل گفت با این چنین شهریار",
      "m2": "نخواهد ز بهرام یل زینهار",
      "verse_number": 31
    },
    {
      "id": "d03bb687-e73e-4628-ae6e-316d7d23e714",
      "m1": "یکی مرد بیدشمنم پارسی",
      "m2": "همان بار دارم شتروار سی",
      "verse_number": 32
    },
    {
      "id": "323c0bb7-bbbb-4b69-a31e-346c41fa7b33",
      "m1": "چراخویشتن کرد باید هلاک",
      "m2": "بلندی پدیدار گشت ازمغاک",
      "verse_number": 33
    },
    {
      "id": "8a0d377a-2cb7-4397-9a64-38a148cf9aaa",
      "m1": "شوم نامه نزدیک خسروبرم",
      "m2": "به نزدیک او هدیهٔ نوبرم",
      "verse_number": 34
    },
    {
      "id": "a436790d-6a3a-4bf2-ab5b-bf5222cc9f99",
      "m1": "باندیشه آمد به نزدیک شاه",
      "m2": "ابا هدیه و نامه ونیک خواه",
      "verse_number": 35
    },
    {
      "id": "66d7c413-41a6-4234-a727-7441165a4e42",
      "m1": "درم برد و با نامهها هدیه برد",
      "m2": "سخنهاش برشاه گیتی شمرد",
      "verse_number": 36
    },
    {
      "id": "c2708d7f-3ca1-403d-bdb2-c03840382a0f",
      "m1": "جهاندار چون نامهها را بخواند",
      "m2": "مر او را بکرسی زرین نشاند",
      "verse_number": 37
    },
    {
      "id": "9090125c-09c6-4be4-94f2-4f61b1a865e9",
      "m1": "بدو گفت کای مرد بسیاردان",
      "m2": "تو بهرام را نزد من خوار دان",
      "verse_number": 38
    },
    {
      "id": "a9a8fdce-af70-45ef-a3aa-f4dfcb988f58",
      "m1": "کنون ز آنچ کردی رسیدی بکام",
      "m2": "فزونتر مجو اندرین کار نام",
      "verse_number": 39
    },
    {
      "id": "9f22c94d-6d38-41d7-9014-28751b02c689",
      "m1": "بفرمود تا نزد او شد دبیر",
      "m2": "مران پاسخ نامه را ناگزیر",
      "verse_number": 40
    },
    {
      "id": "65d4bdef-c128-4b7b-9ca5-c032e89e525c",
      "m1": "نوشت اندران نامههای دراز",
      "m2": "که این مهتر گرد گردن فراز",
      "verse_number": 41
    },
    {
      "id": "2d0fa45a-8f39-4248-a9d9-6488afee3bbe",
      "m1": "همه نامههای تو برخواندیم",
      "m2": "فرستاده را پیش بنشاندیم",
      "verse_number": 42
    },
    {
      "id": "fcc57a85-722c-40fd-b6ae-0a4eb3769f5d",
      "m1": "به گفتار بیکار با خسرویم",
      "m2": "به دل با تو همچون بهار نویم",
      "verse_number": 43
    },
    {
      "id": "dce8fd68-54ba-4461-944f-d33946d65282",
      "m1": "چولشکر بیاری بدین مرز وبوم",
      "m2": "که اندیشد از گرز مردان روم",
      "verse_number": 44
    },
    {
      "id": "8dd6ee14-0094-4184-b6d2-27dfca622672",
      "m1": "همه پاک شمشیرها برکشیم",
      "m2": "به جنگ اندورن رومیان را کشیم",
      "verse_number": 45
    },
    {
      "id": "9a79a081-69e3-4bab-92ca-d2337fd27176",
      "m1": "چو خسرو ببیند سپاه تو را",
      "m2": "همان مردی و پایگاه تو را",
      "verse_number": 46
    },
    {
      "id": "f548276c-499b-4e04-8a6f-d602acb669e6",
      "m1": "دلش زود بیکار ولرزان شود",
      "m2": "زپیشت چو روبه گریزان شود",
      "verse_number": 47
    },
    {
      "id": "0db533e0-8d05-447c-b0d9-1b29e157a013",
      "m1": "بدان نامهها مهر بنهاد شاه",
      "m2": "ببرد ان پسندیدهٔ نیک خواه",
      "verse_number": 48
    },
    {
      "id": "0e97789d-423a-4e38-baa9-ff72bcafbaed",
      "m1": "بدو گفت شاه ای خردمند مرد",
      "m2": "برش گنج یابی ازین کارکرد",
      "verse_number": 49
    },
    {
      "id": "25310671-1152-4167-ab9b-a802de454dcf",
      "m1": "مرو را گهر داد و دینار داد",
      "m2": "گرانمایه یاقوت بسیار داد",
      "verse_number": 50
    },
    {
      "id": "048298b9-82a7-4e6a-a506-e887a1882917",
      "m1": "بدو گفت کاین نزد چوبینه بر",
      "m2": "شنیده سخنها برو بر شمر",
      "verse_number": 51
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 51
  }
}