{
  "id": "e3492467-7316-423a-9c63-6550f84d67fa",
  "title": "بخش ۳۰",
  "verses": [
    {
      "id": "444a396c-05fe-4a96-9514-a5b910f962da",
      "m1": "بیامد به نزدیک چوبینه مرد",
      "m2": "شنیده سخنها همه یادکرد",
      "verse_number": 1
    },
    {
      "id": "88e97c0d-32bf-48e6-a5b8-3cd683744a11",
      "m1": "چو مرد جهانجوی نامه بخواند",
      "m2": "هوارا بخواند وخرد را براند",
      "verse_number": 2
    },
    {
      "id": "7258e388-2c16-48b4-b5b1-6fc27adc8bac",
      "m1": "ازان نامهها ساز رفتن گرفت",
      "m2": "بماندند ایرانیان درشگفت",
      "verse_number": 3
    },
    {
      "id": "75dfc120-1463-4b63-888a-ce3618deb37f",
      "m1": "برفتند پیران به نزدیک اوی",
      "m2": "چودیدند کردار تاریک اوی",
      "verse_number": 4
    },
    {
      "id": "0b030ad1-24ae-4ac5-a20b-c1668383e1d9",
      "m1": "همیگفت هرکس کز ایدر مرو",
      "m2": "زرفتن کهن گردد این روز نو",
      "verse_number": 5
    },
    {
      "id": "d3aae53a-8506-49fc-9f2b-30cd056276cc",
      "m1": "اگر خسرو آید به ایران زمین",
      "m2": "نبینی مگر گرز و شمشیر کین",
      "verse_number": 6
    },
    {
      "id": "438a3520-ba3d-46f0-b690-299e82a43c67",
      "m1": "برین تخت شاهی مخور زینهار",
      "m2": "همیخیره بفریبدت روزگار",
      "verse_number": 7
    },
    {
      "id": "a39ed7cf-c1f5-41d5-a578-c65f902501a2",
      "m1": "نیامد سخنها برو کارگر",
      "m2": "بفرمود تا رفت لشکر بدر",
      "verse_number": 8
    },
    {
      "id": "6412cbcd-ae65-4671-ab15-37d7d6f5af51",
      "m1": "همیتاخت تا آذر آبادگان",
      "m2": "سپاهی دلاور ز آزادگان",
      "verse_number": 9
    },
    {
      "id": "a12a8057-cad7-444e-b015-c14968a77b58",
      "m1": "سپاه اندر آمد بتنگ سپاه",
      "m2": "ببستند بر مور و بر پشه راه",
      "verse_number": 10
    },
    {
      "id": "95ec924b-094a-4144-8a9b-0db244025e1d",
      "m1": "چنین گفت پس مهتر کینه خواه",
      "m2": "که من کرد خواهم به لشکر نگاه",
      "verse_number": 11
    },
    {
      "id": "0bfdce28-033e-485d-a0ee-1d545881fe91",
      "m1": "ببینم که رومی سواران کیند",
      "m2": "سپاهی کدامند و گردان کیند",
      "verse_number": 12
    },
    {
      "id": "afc346aa-b985-4e96-8a00-8be2da4a3338",
      "m1": "همه برنشستند گردان براسپ",
      "m2": "یلان سینه و مهتر ایزد گشسپ",
      "verse_number": 13
    },
    {
      "id": "e90e5a69-2055-428b-86d8-6d4de8fcb18a",
      "m1": "بدیدار آن لشکر کینه خواه",
      "m2": "گرانمایگان برگرفتند راه",
      "verse_number": 14
    },
    {
      "id": "01e22fcc-c657-4e8d-90d8-8f303ff55511",
      "m1": "چولشکر بدیدند باز آمدند",
      "m2": "به نزدیک مهتر فراز آمدند",
      "verse_number": 15
    },
    {
      "id": "38ea4b0d-4e68-4b8f-ad75-e5cb68bb96bf",
      "m1": "که این بی کرانه یکی لشکرند",
      "m2": "ز اندیشه ما همیبگذرند",
      "verse_number": 16
    },
    {
      "id": "f7997156-60a7-40cf-bd99-322493632281",
      "m1": "وزان روی رومی سواران شاه",
      "m2": "برفتند پویان بدان بارگاه",
      "verse_number": 17
    },
    {
      "id": "cf1b49f0-34b9-48ae-801c-435f0e41238d",
      "m1": "ببستند بر پیش خسرو میان",
      "m2": "که ما جنگ جوییم زایرانیان",
      "verse_number": 18
    },
    {
      "id": "13540892-2d25-4cc6-8037-4f4490b2c83a",
      "m1": "بدان کار همداستان گشت شاه",
      "m2": "کزو آرزو خواست رومی سپاه",
      "verse_number": 19
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 19
  }
}