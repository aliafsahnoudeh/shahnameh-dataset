{
  "id": "abf3d081-c0cb-4f04-9e9d-86b325d7e215",
  "title": "بخش ۵۳",
  "verses": [
    {
      "id": "f2f48d46-1ec0-4e6a-acf1-8576a76ed852",
      "m1": "چو پیروز شد سوی ایران کشید",
      "m2": "بر شهریار دلیران کشید",
      "verse_number": 1
    },
    {
      "id": "ce0eccbe-447c-483f-90e5-0b3a36dc4066",
      "m1": "به روز چهارم به آموی شد",
      "m2": "ندیدی زنی کو جهانجوی شد",
      "verse_number": 2
    },
    {
      "id": "468f2b18-6cb2-4618-84f3-1c8b737c7554",
      "m1": "به آموی یک چند بنشست و بود",
      "m2": "به دلش اندرون داوریها فزود",
      "verse_number": 3
    },
    {
      "id": "f8557b8f-8d0a-401f-af2f-75fab4ec9148",
      "m1": "یکی نامه سوی برادر بدرد",
      "m2": "نوشت و ز هر کارش آگاه کرد",
      "verse_number": 4
    },
    {
      "id": "b5e45321-a191-444f-8eb3-4223e4f35e96",
      "m1": "نخستین سخن گفت بهرام گرد",
      "m2": "به تیمار و درد برادر بمرد",
      "verse_number": 5
    },
    {
      "id": "708224ec-b1da-4fb2-8138-058bea3d91e4",
      "m1": "تو را و مرا مزد بسیار باد",
      "m2": "روان وی از ما بیآزار باد",
      "verse_number": 6
    },
    {
      "id": "50098f1c-be09-450d-81bf-707925579427",
      "m1": "دگر گفت با شهریار بلند",
      "m2": "بگوی آنچ از من شنیدی ز پند",
      "verse_number": 7
    },
    {
      "id": "35ae6139-433b-48f3-9b8c-f2f23990100d",
      "m1": "پس ما بیامد سپاهی گران",
      "m2": "همه نامداران جنگاوران",
      "verse_number": 8
    },
    {
      "id": "e27e3343-e57d-4392-92f7-0ce31af69324",
      "m1": "برآن گونه برگاشتمشان ز رزم",
      "m2": "که نه رزم بینند زان پس نه بزم",
      "verse_number": 9
    },
    {
      "id": "1264ff5d-3dfa-4cba-a760-da45edc903e2",
      "m1": "بسی نامور مهتران با منند",
      "m2": "نبادی که آید بریشان گزند",
      "verse_number": 10
    },
    {
      "id": "c838c3a5-e1bf-4475-afc4-c11b753702b2",
      "m1": "نشستم به آموی تا پاسخم",
      "m2": "بیارد مگر اختر فرخم",
      "verse_number": 11
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 11
  },
  "order_in_parent": 53
}