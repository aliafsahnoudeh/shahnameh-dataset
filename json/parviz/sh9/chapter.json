{
  "id": "dd57ab90-86c2-4303-ba3c-78782a9a2079",
  "title": "بخش ۹",
  "verses": [
    {
      "id": "ac41529a-7157-493d-b1d5-fbd60ddf2e64",
      "m1": "ز لشکر گزین کرد بهرام شیر",
      "m2": "سپاهی جهانگیر وگرد دلیر",
      "verse_number": 1
    },
    {
      "id": "704b1de0-7c6f-4aea-809f-7ef5597ed59c",
      "m1": "چوکردند و با او دبیران شمار",
      "m2": "سپه بود شمشیر زن صد هزار",
      "verse_number": 2
    },
    {
      "id": "0a10da7e-9c65-44f0-9dab-16613102d06f",
      "m1": "ز خاقانیان آن سه ترک سترگ",
      "m2": "که بودند غرنده برسان گرگ",
      "verse_number": 3
    },
    {
      "id": "51af7fd5-3be2-4545-9bdc-504bf4d9772b",
      "m1": "به جنگآوران گفت چون زخم کوس",
      "m2": "برآید بهنگام بانگ خروس",
      "verse_number": 4
    },
    {
      "id": "72e05af3-100b-4cea-a1b0-2c76eb4fd5c6",
      "m1": "شما بر خروشید و اندر دهید",
      "m2": "سران را ز خون بر سرافسر نهید",
      "verse_number": 5
    },
    {
      "id": "2e22f3da-d652-4b43-9b10-2f396bbf0a72",
      "m1": "بشد تیز لشکر بفرمان گو",
      "m2": "سه ترک سر افرازشان پیش رو",
      "verse_number": 6
    },
    {
      "id": "4fff66ef-f9a2-4f37-85ce-e40ca2934877",
      "m1": "برلشکر شهریار آمدند",
      "m2": "جفاپیشه و کینه دار آمدند",
      "verse_number": 7
    },
    {
      "id": "84134544-c145-4b38-968a-58dcb382c8c3",
      "m1": "خروش آمد از گرز و گوپال و تیغ",
      "m2": "از آهن زمین بود وز گرد میغ",
      "verse_number": 8
    },
    {
      "id": "670b3b7c-3331-4ddd-9057-393e96d73e37",
      "m1": "همیگفت هرکس که خسرو کجاست",
      "m2": "که امروز پیروزی روز ماست",
      "verse_number": 9
    },
    {
      "id": "12e210a3-1a42-4657-b9c7-8eade562dd40",
      "m1": "ببالا همیبود خسرو بدرد",
      "m2": "دودیده پر از خون و رخ لاژورد",
      "verse_number": 10
    },
    {
      "id": "6bbd7e26-7bf4-41ad-b41a-011d5743a313",
      "m1": "چنین تا سپیده برآمد ز کوه",
      "m2": "شد از زخم شمشیر و کشته ستوه",
      "verse_number": 11
    },
    {
      "id": "21f284e4-c993-426f-b9cd-9589c0d4c715",
      "m1": "چوشد دامن تیره شب تا پدید",
      "m2": "همه رزمگه کشته و خسته دید",
      "verse_number": 12
    },
    {
      "id": "06402bc0-f178-4bf0-bb89-d14f59efed7c",
      "m1": "بگردنکشان گفت یاری کنید",
      "m2": "برین دشمنان کامگاری کنید",
      "verse_number": 13
    },
    {
      "id": "8fed411e-127d-4a49-985b-8cb6ce9c5ea0",
      "m1": "که پیروزگر پشت و یارمنست",
      "m2": "همان زخم شمشیر کارمنست",
      "verse_number": 14
    },
    {
      "id": "5de116e0-62d2-4675-9a52-f4292ad7f1d9",
      "m1": "بیامد دمان تا بر آن سه ترک",
      "m2": "نه ترک دلاور سه پیل سترگ",
      "verse_number": 15
    },
    {
      "id": "ed7762e2-c8e6-4a21-96b0-2774ec127f2c",
      "m1": "یکی تاخت تا نزد خسرو رسید",
      "m2": "پرنداوری از میان برکشید",
      "verse_number": 16
    },
    {
      "id": "468ec7a9-dcc0-48e9-8f74-73302de48835",
      "m1": "همیخواست زد بر سر شهریار",
      "m2": "سپر بر سرآورد شاه سوار",
      "verse_number": 17
    },
    {
      "id": "004a3d8c-1808-406d-8701-574348adb36f",
      "m1": "بزیر سپر تیغ زهر آبگون",
      "m2": "بزد تیغ و انداختش سرنگون",
      "verse_number": 18
    },
    {
      "id": "33800338-2f93-485b-b31a-16ea45e514de",
      "m1": "خروشید کای نامداران جنگ",
      "m2": "زمانی دگر کرد باید درنگ",
      "verse_number": 19
    },
    {
      "id": "02aa3518-355a-4439-b9f2-b9aabbfb6a34",
      "m1": "سپاهش همه پشت برگاشتند",
      "m2": "جهانجوی را خوار بگذاشتند",
      "verse_number": 20
    },
    {
      "id": "74277adb-d0a6-418f-acde-3f62d334c055",
      "m1": "به بندوی و گستهم گفت آن زمان",
      "m2": "که اکنون شدم زین سخن بدگمان",
      "verse_number": 21
    },
    {
      "id": "27e9f87c-4d42-4bfc-abbb-09561866fd01",
      "m1": "رسیده مرا هیچ فرزند نیست",
      "m2": "همان از در تاج پیوند نیست",
      "verse_number": 22
    },
    {
      "id": "65d5ea0e-c06b-4fa2-9402-f125ed06ce0d",
      "m1": "اگر من شوم کشته در کارزار",
      "m2": "جهان را نماند یکی شهریار",
      "verse_number": 23
    },
    {
      "id": "c21282ee-2e96-4e6f-87ac-a17698780cf9",
      "m1": "بدو گفت بندوی کای سرفراز",
      "m2": "بدین روز هرگز مبادت نیاز",
      "verse_number": 24
    },
    {
      "id": "424c522c-f945-4f94-bc59-fea2a73c11f5",
      "m1": "سپه رفت اکنون تو ایدر مه ایست",
      "m2": "که کس در زمانه تو را یار نیست",
      "verse_number": 25
    },
    {
      "id": "74d23b9d-1879-4a5f-9781-585f1eef989d",
      "m1": "بزنگوی گفت آن زمان شهریار",
      "m2": "کز ایدر برو تازیان تاتخوار",
      "verse_number": 26
    },
    {
      "id": "015fe782-b40b-4129-88cb-6e082349a24e",
      "m1": "ازین ماندگان بر سواری هزار",
      "m2": "بران رزمگاه آنچ یابی بیار",
      "verse_number": 27
    },
    {
      "id": "c620754a-dae8-4863-8edf-46b43e601dc9",
      "m1": "سراپرده دیبه وگنج وتاج",
      "m2": "همان بدره وبرده وتخت عاج",
      "verse_number": 28
    },
    {
      "id": "1901537a-a942-4334-bf3f-12965fc38afe",
      "m1": "بزرگان بنه برنهادند وگنج",
      "m2": "فراوان ببردن کشیدند رنج",
      "verse_number": 29
    },
    {
      "id": "11140793-9879-45ab-888d-f9da75495b82",
      "m1": "هم آنگه یکی اژدهافش درفش",
      "m2": "پدید آمد و گشت گیتی بنفش",
      "verse_number": 30
    },
    {
      "id": "8f11f004-6cea-4dad-a9f7-181d428d202b",
      "m1": "پس اندر همیراند بهرام گرد",
      "m2": "به جنگ از جهان روشنایی ببرد",
      "verse_number": 31
    },
    {
      "id": "15d4e0b8-5d47-4458-b1cb-01b5af6cb109",
      "m1": "رسیدند بهرام و خسرو بهم",
      "m2": "دلاور دو جنگی دو شیر دژم",
      "verse_number": 32
    },
    {
      "id": "f4a01680-6f0a-43c5-83ee-b41749bfe277",
      "m1": "چوپیلان جنگی بر آشوفتند",
      "m2": "همی برسریکدگر کوفتند",
      "verse_number": 33
    },
    {
      "id": "cc21525b-16c7-4ae5-ae2d-3ea37a9c3222",
      "m1": "همیگشت بهرام چون شیر نر",
      "m2": "سلیحش نیامد برو کارگر",
      "verse_number": 34
    },
    {
      "id": "78e3bc6e-44e8-4dcf-94a0-7d499d147bac",
      "m1": "برین گونه تا خور ز گنبد بگشت",
      "m2": "از اندازه آویزش اندر گذشت",
      "verse_number": 35
    },
    {
      "id": "9b9ee055-7bff-4ba3-93ca-bdedb64886f4",
      "m1": "تخوار آن زمان پیش خسرو رسید",
      "m2": "که گنج وبنه زان سوی پل کشید",
      "verse_number": 36
    },
    {
      "id": "d4e6f345-cfd9-41b4-8671-0e29961dfe1f",
      "m1": "چوبشنید خسرو بگستهم گفت",
      "m2": "که با ما کسی نیست در جنگ جفت",
      "verse_number": 37
    },
    {
      "id": "6a520434-caae-49e3-91c5-597c6852b110",
      "m1": "که ما ده تنیم این سپاهی بزرگ",
      "m2": "به پیش اندرون پهلوانی سترگ",
      "verse_number": 38
    },
    {
      "id": "ae4d19cb-2b04-4a14-bac8-f6d2310c199d",
      "m1": "هزیمت بهنگام بهتر زجنگ",
      "m2": "چو تنها شدی نیست جای درنگ",
      "verse_number": 39
    },
    {
      "id": "15c0d5f0-ce53-4f6f-8559-f931014aaf56",
      "m1": "همیراند ناکار دیده جوان",
      "m2": "برین گونه بر تا پل نهروان",
      "verse_number": 40
    },
    {
      "id": "e6caf6c5-38d0-4e38-b589-6d76556ca4f7",
      "m1": "پس اندر همیتاخت بهرام تیز",
      "m2": "سری پر ز کینه دلی پر ستیز",
      "verse_number": 41
    },
    {
      "id": "e2dca425-95b8-48f0-ab6f-411a0678d298",
      "m1": "چو خسرو چنان دید بر پل بماند",
      "m2": "جهاندیده گستهم را پیش خواند",
      "verse_number": 42
    },
    {
      "id": "d32c1378-44be-459e-983e-cad9067bbaa1",
      "m1": "بیارید گفتا کمان مرا",
      "m2": "به جنگ اندرون ترجمان مرا",
      "verse_number": 43
    },
    {
      "id": "fbec5b54-5307-48d0-9226-ca16134c070b",
      "m1": "کمانش ببرد آنک گنجور بود",
      "m2": "بران کار گستهم دستور بود",
      "verse_number": 44
    },
    {
      "id": "786b0e38-4c06-4a8a-9de6-5534c4e16129",
      "m1": "کمان بر گرفت آن سپهدار گرد",
      "m2": "بتیر از هوا روشنایی ببرد",
      "verse_number": 45
    },
    {
      "id": "d3df944d-7353-486d-867c-20ae067aefe0",
      "m1": "همی تیر بارید همچون تگرگ",
      "m2": "بیک چوبه با سر همیدوخت ترگ",
      "verse_number": 46
    },
    {
      "id": "491582a1-9db5-440d-ae84-925be559754d",
      "m1": "پس اندر همیتاخت بهرام شیر",
      "m2": "کمندی بدست اژدهایی بزیر",
      "verse_number": 47
    },
    {
      "id": "a5983778-bc58-4f0d-8e96-42a2bffba355",
      "m1": "چوخسرو و را دید برگشت شاد",
      "m2": "دو زاغ کمان را بزه برنهاد",
      "verse_number": 48
    },
    {
      "id": "758815fc-d677-44f7-beb1-9e15b16f5bf5",
      "m1": "یکی تیر زد بر بربارگی",
      "m2": "بشد کار آن باره یکبارگی",
      "verse_number": 49
    },
    {
      "id": "3235b86a-53fa-40ef-8232-deb731947404",
      "m1": "پیاده سپهبد سپر برگرفت",
      "m2": "ز بیچارگی دست بر سرگرفت",
      "verse_number": 50
    },
    {
      "id": "940ee3c6-02dd-44fb-984a-f398cac86405",
      "m1": "یلان سینه پیش اندر آمد چوگرد",
      "m2": "جهانجوی کی داشت او را بمرد",
      "verse_number": 51
    },
    {
      "id": "c94da621-bcd1-4fa1-9a5f-6d9a7d885ac8",
      "m1": "هم اندر زمان اسپ او رابخست",
      "m2": "پیاده یلان سینه را پل بجست",
      "verse_number": 52
    },
    {
      "id": "59064cee-f369-4093-9b86-751427226661",
      "m1": "سپه بازگشت از پل نهروان",
      "m2": "هرآنکس که بودند پیر و جوان",
      "verse_number": 53
    },
    {
      "id": "3d02899b-2cf5-4dae-a9a9-3de622c78c6c",
      "m1": "چو بهرام برگشت خسرو چوگرد",
      "m2": "پل نهروان سر به سر باز کرد",
      "verse_number": 54
    },
    {
      "id": "bc5e3b74-97a3-4d8d-a8ed-f2f980a7a000",
      "m1": "همیراند غمگین سوی طیسفون",
      "m2": "دلی پر زغم دیدگان پر زخون",
      "verse_number": 55
    },
    {
      "id": "2c64a047-05b6-405d-8210-f60a0d4411d5",
      "m1": "در شارستانها به آهن ببست",
      "m2": "بانبوه اندیشگان درنشست",
      "verse_number": 56
    },
    {
      "id": "671f8dcb-7e82-44e8-8d5d-7c8682b219d9",
      "m1": "زهر برزنی مهتران را بخواند",
      "m2": "به دروازه بر پاسبانان نشاند",
      "verse_number": 57
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 57
  }
}