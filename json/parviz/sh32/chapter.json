{
  "id": "6cc1a5e7-8e65-4185-af39-387c681fdd71",
  "title": "بخش ۳۲",
  "verses": [
    {
      "id": "2c7de6c8-4a71-43eb-a51a-a1a9d7cd3747",
      "m1": "چو بر زد ز دریا درفش سپید",
      "m2": "ستاره شد از تیرگی ناامید",
      "verse_number": 1
    },
    {
      "id": "da2bafb9-847b-4724-8584-32374f9003be",
      "m1": "تبیره زنان از دو پرده سرای",
      "m2": "برفتند با پیل و باکرنای",
      "verse_number": 2
    },
    {
      "id": "938c0e50-ac36-47e0-b174-9df9e108577e",
      "m1": "خروش آمد و نالهٔ گاودم",
      "m2": "هم از کوههٔ پیل رویینه خم",
      "verse_number": 3
    },
    {
      "id": "f91a4d77-364a-438c-9803-8d221030fc41",
      "m1": "تو گفتی بجنبد همی دشت وراغ",
      "m2": "شده روی خورشید چون پر زاغ",
      "verse_number": 4
    },
    {
      "id": "f414ff95-7daa-455c-b878-fc77a8fc1c35",
      "m1": "چو ایرانیان برکشیدند صف",
      "m2": "همه نیزه و تیغ هندی بکف",
      "verse_number": 5
    },
    {
      "id": "a4c32907-2a49-4e3b-af3a-3149ff29533f",
      "m1": "زمین سر به سر گفتی ازجوشنست",
      "m2": "ستاره ز نوک سنان روشنست",
      "verse_number": 6
    },
    {
      "id": "8bd97dce-cade-476f-95cc-a9cae46bbac8",
      "m1": "چو خسرو بیاراست بر قلبگاه",
      "m2": "همه دل گرفتند یکسر سپاه",
      "verse_number": 7
    },
    {
      "id": "8985e95d-ee83-4d79-afff-195ce82ec0b6",
      "m1": "ورامیمنه دار گردوی بود",
      "m2": "که گرد ودلیر وجهانجوی بود",
      "verse_number": 8
    },
    {
      "id": "e5bcfd31-7807-41cb-818a-2e8f1b1542f9",
      "m1": "بدست چپش نامدار ارمنی",
      "m2": "ابا جوشن وتیغ آهرمنی",
      "verse_number": 9
    },
    {
      "id": "05daacbd-3cba-4596-a22f-95581ba7f279",
      "m1": "مبارز چوشاپور وچون اندیان",
      "m2": "بران جنگ بر تنگ بسته میان",
      "verse_number": 10
    },
    {
      "id": "2dd597ee-11ac-48aa-9e40-8839ee97435c",
      "m1": "همیبود گستهم بردست شاه",
      "m2": "که دارد مر او را ز دشمن نگاه",
      "verse_number": 11
    },
    {
      "id": "0e788175-7cfd-4353-954d-8171b7d8bc71",
      "m1": "چوبهرام یل رومیان راندید",
      "m2": "درنگی شد وخامشی برگزید",
      "verse_number": 12
    },
    {
      "id": "62f34076-d6d5-453a-ba1b-065db7bd3a9d",
      "m1": "بفرمود تاکوس برپشت پیل",
      "m2": "ببستند وشد گرد لشکر چونیل",
      "verse_number": 13
    },
    {
      "id": "868ed6ab-9b1c-4d34-8c11-8e50c7aab2f0",
      "m1": "نشست ازبرپشت پیل سپید",
      "m2": "هم آوردش ازبخت شد ناامید",
      "verse_number": 14
    },
    {
      "id": "ca56e0bc-899d-49b3-9813-ceb1a3697185",
      "m1": "همیراند آن پیل تامیمنه",
      "m2": "بشاپور گفت ای بد بدتنه",
      "verse_number": 15
    },
    {
      "id": "701ec5f5-95bc-4b11-a135-91044f5e861d",
      "m1": "نه پیمانت این بد به نامه درون",
      "m2": "که پیش من آیی بدین دشت خون",
      "verse_number": 16
    },
    {
      "id": "35140b61-3789-4225-9309-050e182220c0",
      "m1": "نه این باشد آیین پرمایگان",
      "m2": "همی تن بکشتن دهی رایگان",
      "verse_number": 17
    },
    {
      "id": "a0d78b9b-5d8b-48da-b94b-31c9aba18236",
      "m1": "بدو گفت شاپور کای دیوفش",
      "m2": "سرخویش دربندگی کرده کش",
      "verse_number": 18
    },
    {
      "id": "828faa24-869f-4873-9001-1ef0646511b2",
      "m1": "ازین نامه کی بود نام ونشان",
      "m2": "که گویی کنون پیش گردنکشان",
      "verse_number": 19
    },
    {
      "id": "8139c36b-5713-4a3e-8e4f-b9faf31a44f6",
      "m1": "گرانمایه خسرو بشاپور گفت",
      "m2": "که آن نامه با رای او بود جفت",
      "verse_number": 20
    },
    {
      "id": "8dad3add-fba5-4388-aaf6-847f4852110c",
      "m1": "به نامه توپاداش یابی زمن",
      "m2": "هم ازنامداران این انجمن",
      "verse_number": 21
    },
    {
      "id": "1f7d30ea-9dbc-42c3-bad2-a5a51a6267f9",
      "m1": "چوهنگام باشد بگویم تو را",
      "m2": "زاندیشه بد بشویم تو را",
      "verse_number": 22
    },
    {
      "id": "379c78c3-507e-4fc7-85a7-2a76f9465c20",
      "m1": "چوبهرام آواز خسرو شنید",
      "m2": "باندیشه آن جادوی را بدید",
      "verse_number": 23
    },
    {
      "id": "57941a32-7384-438c-a5bc-465754699743",
      "m1": "برآشفت وزان کار تنگ آمدش",
      "m2": "چوارغنده شد رای جنگ آمدش",
      "verse_number": 24
    },
    {
      "id": "27fe5d80-e0fd-4466-adde-1efa7687a4c1",
      "m1": "جفا پیشه برپیل تنها برفت",
      "m2": "سوی قلب خسرو خرامید تفت",
      "verse_number": 25
    },
    {
      "id": "86d12c7e-a3a6-4b0b-9d61-74ceec4424fc",
      "m1": "چوخسرو چنان دید با اندیان",
      "m2": "چین گفت کای نره شیر ژیان",
      "verse_number": 26
    },
    {
      "id": "a7263809-bffc-468f-a185-57ebe3b853ee",
      "m1": "برین پیل برتیرباران کنید",
      "m2": "کمان را چوابر بهاران کنید",
      "verse_number": 27
    },
    {
      "id": "6d160db2-c80f-4bde-84d9-3296ed64419d",
      "m1": "از ایرانیان آنک بد روزبه",
      "m2": "کمان برنهادند یکسر بزه",
      "verse_number": 28
    },
    {
      "id": "257be25a-8003-4f2d-9cc6-8669f7d07659",
      "m1": "زپیکان چنان گشت خرطوم پیل",
      "m2": "توگفتی شد از خستگی پیل نیل",
      "verse_number": 29
    },
    {
      "id": "3786dc54-a8c4-4019-ba9a-0e6b010c85c0",
      "m1": "هم آنگاه بهرام بالای خواست",
      "m2": "یکی مغفر خسرو آرای خواست",
      "verse_number": 30
    },
    {
      "id": "58db0bc2-9f44-451a-a609-341b3186e22b",
      "m1": "همان تیرباران گرفتند باز",
      "m2": "برآشفت بهرام گردن فراز",
      "verse_number": 31
    },
    {
      "id": "91269043-9a90-4517-b107-a18a5d4c15c9",
      "m1": "پیاده شد آن مرد پرخاشخر",
      "m2": "زره دامنش رابزد برکمر",
      "verse_number": 32
    },
    {
      "id": "634bae6d-42a8-4c59-a96e-8a9349f98f05",
      "m1": "سپر برسرآورد وشمشیر تیز",
      "m2": "برآورد زان جنگیان رستخیز",
      "verse_number": 33
    },
    {
      "id": "31c2b098-c80a-4b06-9b4a-1f926d86d13d",
      "m1": "پیاده زبهرام بگریختند",
      "m2": "کمانهای چاچی فروریختند",
      "verse_number": 34
    },
    {
      "id": "8c32f3c9-52a6-4bdb-909b-361f0ae06322",
      "m1": "یکی باره بردند هم درزمان",
      "m2": "سپهبد نشست از بر اودمان",
      "verse_number": 35
    },
    {
      "id": "df8f9d7c-64f6-44c9-b2c5-54814798d57c",
      "m1": "خروشان همیتاخت تا قلبگاه",
      "m2": "بجایی کجا شاه بد بیسپاه",
      "verse_number": 36
    },
    {
      "id": "63455237-caeb-45d1-a599-734f3c3bf5b9",
      "m1": "همه قلبگه پاک برهم درید",
      "m2": "درفش جهاندار شد ناپدید",
      "verse_number": 37
    },
    {
      "id": "13009900-53b3-4743-9c95-189cb3cba33e",
      "m1": "وزان جایگه شد سوی میسره",
      "m2": "پس پشتش آزادگان یکسره",
      "verse_number": 38
    },
    {
      "id": "12afae94-df41-4595-bcc2-6dcf79be252a",
      "m1": "نگهبان آن دست گردوی بود",
      "m2": "که مردی دلیر وجهانجوی بود",
      "verse_number": 39
    },
    {
      "id": "1bbe616f-9976-4ae6-bb1c-7377e256c8e6",
      "m1": "برادر چوروی برادر بدید",
      "m2": "کمان را بزه کرد واندرکشید",
      "verse_number": 40
    },
    {
      "id": "5c479079-91ae-4501-8f42-6a4630ff5ed7",
      "m1": "دوخونی بران سان برآویختند",
      "m2": "که گفتی بهمشان برآمیختند",
      "verse_number": 41
    },
    {
      "id": "35479a1a-bf18-44d4-b859-7a3208ec8196",
      "m1": "بدین سان زمانی برآمد دراز",
      "m2": "همی یک زدیگر نگشتند باز",
      "verse_number": 42
    },
    {
      "id": "2c969f6c-631a-4f3b-9505-0af7ef349247",
      "m1": "بدو گفت بهرام کای بیپدر",
      "m2": "به خون برادر چه بندی کمر",
      "verse_number": 43
    },
    {
      "id": "94461098-bee4-4b37-a166-826a83de622b",
      "m1": "بدو گفت گردوی کای پیسه گرگ",
      "m2": "تونشنیدی آن داستان بزرگ",
      "verse_number": 44
    },
    {
      "id": "8d4ae1e1-181a-432f-a72c-ce41a1a86fd4",
      "m1": "که هرکو برادر بود دوست به",
      "m2": "چو دشمن بود بی پی و پوست به",
      "verse_number": 45
    },
    {
      "id": "155eb718-d3b7-4b2f-8562-f3b0a85e071d",
      "m1": "تو هم دشمن و بد تن و ریمنی",
      "m2": "جهان آفرین را به دل دشمنی",
      "verse_number": 46
    },
    {
      "id": "70443758-335c-474e-9b21-8c3461aac4cb",
      "m1": "به پیش برادر برادر به جنگ",
      "m2": "نیاید اگر باشدش نام و ننگ",
      "verse_number": 47
    },
    {
      "id": "00e99e69-c982-46bf-88c5-5500e2d981bc",
      "m1": "چوبشنید بهرام زو بازگشت",
      "m2": "برآشفت و با او دژم ساز گشت",
      "verse_number": 48
    },
    {
      "id": "fcb71f67-88b7-4481-95c2-3063f1be5e25",
      "m1": "همیراند گردوی تا نزد شاه",
      "m2": "ز آهن شده روی جنگی سیاه",
      "verse_number": 49
    },
    {
      "id": "136f671c-7da3-42ea-8e5a-089f8674a746",
      "m1": "برو آفرین کرد خسرو به مهر",
      "m2": "که پاداش بادت ز گردان سپهر",
      "verse_number": 50
    },
    {
      "id": "0a20b77d-5eb5-4860-9892-8b833b4e4c7f",
      "m1": "فرستاده خسرو به شاپور کس",
      "m2": "که موسیل راباش فریادرس",
      "verse_number": 51
    },
    {
      "id": "7fd70afa-3998-4f49-b184-380a1abdaa5e",
      "m1": "بکوشید تا پشت پشت آورید",
      "m2": "مگر بخت روشن به مشت آورید",
      "verse_number": 52
    },
    {
      "id": "eb3cf526-b546-4672-bbaf-b850eec80218",
      "m1": "به گستهم گفت آن زمان شهریار",
      "m2": "که گر هیچ رومی کند کارزار",
      "verse_number": 53
    },
    {
      "id": "677e35ca-55ec-415a-872d-a2524800cfab",
      "m1": "چو بهرام جنگی شکسته شود",
      "m2": "وگر نیز در جنگ خسته شود",
      "verse_number": 54
    },
    {
      "id": "1322b3d9-d92c-4859-b225-b1c916caa951",
      "m1": "همه رومیان سر به گردون برند",
      "m2": "سخنها ز اندازه بیرون برند",
      "verse_number": 55
    },
    {
      "id": "402d90fc-2fa0-4923-9a06-527afb9080ca",
      "m1": "نخواهم که رومی بود سرفراز",
      "m2": "به ما برکنند اندرین جنگ ناز",
      "verse_number": 56
    },
    {
      "id": "6b9d3417-276e-4b8f-8d6d-1a12f299d2dd",
      "m1": "بدیدم هنرهای رومی همه",
      "m2": "بسان رمه روزگار دمه",
      "verse_number": 57
    },
    {
      "id": "6aa6adec-fe10-41ff-b9e1-f3016a5783dd",
      "m1": "هم آن به که من با سپاه اندکی",
      "m2": "ز چوبینه آورد خواهم یکی",
      "verse_number": 58
    },
    {
      "id": "a8890da8-0bb8-4ec2-8ca3-3dc70ac351cb",
      "m1": "نخواهم درین کار یاری ز کس",
      "m2": "امیدم به یزدان فریادرس",
      "verse_number": 59
    },
    {
      "id": "ebb4d970-ca00-43f2-ba31-53fcb4986cb5",
      "m1": "بدو گفت گستهم کای شهریار",
      "m2": "به شیرین روانت مخور زینهار",
      "verse_number": 60
    },
    {
      "id": "72de6ccb-037d-4878-95a7-7db1a8294fb7",
      "m1": "چو رایت چنین است مردان کین",
      "m2": "بخواه و مکن تیره روی زمین",
      "verse_number": 61
    },
    {
      "id": "5ab7cc29-9e5f-489a-ac7f-f75cee86ac06",
      "m1": "بدو گفت خسرو که اینست روی",
      "m2": "که گفتی ز لشکر کنون یار جوی",
      "verse_number": 62
    },
    {
      "id": "57659dd2-38f8-4127-b4b3-fb68132ed590",
      "m1": "گزین کرد گستهم ز ایران سوار",
      "m2": "ده و چار گردنکش نامدار",
      "verse_number": 63
    },
    {
      "id": "9c3754ee-f158-4f60-b338-6bb13e19189c",
      "m1": "نخستین ازین جنگیان نام خویش",
      "m2": "نوشت و بیاورد و بنهاد پیش",
      "verse_number": 64
    },
    {
      "id": "38649bed-5eda-4b04-b884-0850ae329130",
      "m1": "دگر گرد شاپور با اندیان",
      "m2": "چو بند وی و گردوی پشت کیان",
      "verse_number": 65
    },
    {
      "id": "4a8173f2-4420-46d3-93d3-2f904f614ff9",
      "m1": "چو آذرگشسپ و دگر شیر ذیل",
      "m2": "چو زنگوی گستاخ با شیر و پیل",
      "verse_number": 66
    },
    {
      "id": "5f67460c-dd96-4350-b3ae-4485c4e848bf",
      "m1": "تخواره که در جنگ غمخواره بود",
      "m2": "یلان سینه را زشت پتیاره بود",
      "verse_number": 67
    },
    {
      "id": "44553476-dbcb-4d47-8f16-7f1c39175600",
      "m1": "فرخ زاد و چون خسرو سرفراز",
      "m2": "چو اشتاد پیروز دشمن گداز",
      "verse_number": 68
    },
    {
      "id": "4a03c75e-5d45-40a8-a4c3-9fae08b787a8",
      "m1": "چو فرخنده خورشید با اورمزد",
      "m2": "که دشمن بدی پیش ایشان فرزد",
      "verse_number": 69
    },
    {
      "id": "c25ad1ad-82c0-45d5-89c4-185cc16bde30",
      "m1": "چومردان گزین کرد ز ایران دو هفت",
      "m2": "ز لشکر بیک سو خرامید تفت",
      "verse_number": 70
    },
    {
      "id": "a347df62-1d83-484d-ba5e-749fee723539",
      "m1": "چنین گفت خسرو بدین مهتران",
      "m2": "که ای سرفرازن و فرمانبران",
      "verse_number": 71
    },
    {
      "id": "1c271526-c460-4e30-9381-ef585b4444ae",
      "m1": "همه پشت را سوی یزدان کنید",
      "m2": "دل خویش را شاد و خندان کنید",
      "verse_number": 72
    },
    {
      "id": "59f9dd75-83aa-4df3-8ed4-c994097b34a0",
      "m1": "جز از خواست یزدان نباشد سخن",
      "m2": "چنین بود تا بود چرخ کهن",
      "verse_number": 73
    },
    {
      "id": "fa12089f-8d01-4948-898b-cbf918f8653b",
      "m1": "برزم اندرون کشته بهتر بود",
      "m2": "که در خانهات بنده مهتر بود",
      "verse_number": 74
    },
    {
      "id": "2030ec4c-af81-4b0d-9c51-5d16f28de680",
      "m1": "نگهدار من بود باید به جنگ",
      "m2": "بهنگام جنبش نسازم درنگ",
      "verse_number": 75
    },
    {
      "id": "4fc852cd-440d-4ce0-9cc1-8079943b4f03",
      "m1": "همه هم زبان آفرین خواندند",
      "m2": "ورا شهریار زمین خواندند",
      "verse_number": 76
    },
    {
      "id": "1f09a005-3d77-4cf7-bc12-cba040fee4c8",
      "m1": "بکردند پیمان که از شهریار",
