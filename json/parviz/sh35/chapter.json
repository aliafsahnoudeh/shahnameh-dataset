{
  "id": "6781c92f-fc9c-4d7a-a001-38fc6b53f741",
  "title": "بخش ۳۵",
  "verses": [
    {
      "id": "8682f95e-d65b-425f-b5e7-e650d34c6f8b",
      "m1": "ازین سوی خسرو بران رزمگاه",
      "m2": "بیامد که بهرام بد با سپاه",
      "verse_number": 1
    },
    {
      "id": "d9d1b4eb-ff96-4abc-bd06-e497d576cbfa",
      "m1": "همه رزمگاهش به تاراج داد",
      "m2": "سپه را همه بدره و تاج داد",
      "verse_number": 2
    },
    {
      "id": "44ed9902-04e4-465f-8d69-c2c9716dd5c1",
      "m1": "یکی بارهٔ تیز رو برنشست",
      "m2": "میان را ز بهر پرستش ببست",
      "verse_number": 3
    },
    {
      "id": "7562030c-c09e-471d-a7bc-2f587f44abc6",
      "m1": "به پیش اندر آمد یکی خارستان",
      "m2": "پیاده ببود اندران کارستان",
      "verse_number": 4
    },
    {
      "id": "29a1ac1b-de09-4fa0-84f6-56ecff2f842e",
      "m1": "به غلتید در پیش یزدان به خاک",
      "m2": "همیگفت کای داور داد و پاک",
      "verse_number": 5
    },
    {
      "id": "62c49ca1-fdd6-46d1-9058-87ac46f2b16a",
      "m1": "پی دشمن از بوم برداشتی",
      "m2": "همه کار ز اندیشه بگذاشتی",
      "verse_number": 6
    },
    {
      "id": "4ed93e16-153b-455d-8433-e2f900065b50",
      "m1": "پرستنده و ناسزا بندهام",
      "m2": "به فرمان و رایت سرافگندهام",
      "verse_number": 7
    },
    {
      "id": "697ec606-5812-4c0c-97f0-37129c327975",
      "m1": "وزان جایگه شد به پرده سرای",
      "m2": "بیامد به نزدیک او رهنمای",
      "verse_number": 8
    },
    {
      "id": "057d5c7c-d193-4b6f-8664-e92013af1070",
      "m1": "بفرمود تا پیش او شد دبیر",
      "m2": "نوشتند زو نامهای برحریر",
      "verse_number": 9
    },
    {
      "id": "1a7dfc99-738e-4fba-890e-93f53d916f60",
      "m1": "ز چیزی که رفت اندران رزمگاه",
      "m2": "به قیصر نوشت اندران نامه شاه",
      "verse_number": 10
    },
    {
      "id": "bbf17548-bfef-4f7a-898f-703ed5b0b100",
      "m1": "نخست آفرین کرد بر دادگر",
      "m2": "کزو دید مردی و بخت و هنر",
      "verse_number": 11
    },
    {
      "id": "9c9cf116-ac48-4357-911c-964c10818e75",
      "m1": "دگر گفت کز کردگار جهان",
      "m2": "همه نیکوی دیدم اندر نهان",
      "verse_number": 12
    },
    {
      "id": "2f65484d-ec7d-4cbb-98e3-d758923b873b",
      "m1": "به آذرگشسپ آمدم با سپاه",
      "m2": "دوان پیش بازآمدم کینه خواه",
      "verse_number": 13
    },
    {
      "id": "6548f3cf-20ce-4411-b5dd-a4a0ac0f5c71",
      "m1": "بدان گونه تنگ اندر آمد به جنگ",
      "m2": "که بر من ببد کار پیکار تنگ",
      "verse_number": 14
    },
    {
      "id": "d8294cfb-c2d6-4d6f-816d-05e4a4939394",
      "m1": "چو یزدان پاکش نبد دستگیر",
      "m2": "بمرد آن دم آتش و دار و گیر",
      "verse_number": 15
    },
    {
      "id": "3be0cc1d-8e46-4c81-95f7-67644a337ce4",
      "m1": "چوبیچارهتر گشت و لشکر نماند",
      "m2": "گریزان به شبگیر ز آنجا براند",
      "verse_number": 16
    },
    {
      "id": "c7795caf-1db6-4197-b5b4-fd31e3812052",
      "m1": "همه لشکرش را بهم بر زدیم",
      "m2": "به لشکر گهش آتش اندرزدیم",
      "verse_number": 17
    },
    {
      "id": "386bf97c-eed4-4963-81d3-4a2a951f65cc",
      "m1": "به فرمان یزدان پیروزگر",
      "m2": "ببندم برو نیز راه گذر",
      "verse_number": 18
    },
    {
      "id": "9df4cf73-d2eb-4e3b-9cdf-31297a9b4556",
      "m1": "نهادند برنامه بر مهرشاه",
      "m2": "فرستادگان بر گرفتند راه",
      "verse_number": 19
    },
    {
      "id": "18589310-1a4b-4080-9a3b-4d4ab74ce91e",
      "m1": "فرستاده با نامه شهریار",
      "m2": "بشد تا بر قیصر نامدار",
      "verse_number": 20
    },
    {
      "id": "727c3423-79a7-4f78-9b8e-aca4a5688d3d",
      "m1": "چو آن نامه برخواند قیصر ز تخت",
      "m2": "فرود آمد آن مرد بیداربخت",
      "verse_number": 21
    },
    {
      "id": "6782ef5c-aee8-45be-a33f-5028d2b02064",
      "m1": "به یزدان چنین گفت کای رهنمای",
      "m2": "همیشه توی جاودانه بجای",
      "verse_number": 22
    },
    {
      "id": "0495e018-d04f-4cea-bad2-ad8a66ae3952",
      "m1": "تو پیروز کردی مر آن بنده را",
      "m2": "کشنده توی مرد افگنده را",
      "verse_number": 23
    },
    {
      "id": "b8dd53a1-b7a1-469f-9d17-5db492dbdc11",
      "m1": "فراوان به درویش دینار داد",
      "m2": "همان خوردنیهای بسیار داد",
      "verse_number": 24
    },
    {
      "id": "dd4ec9cb-26c5-47d4-a14e-ed2fd8d0ca1e",
      "m1": "مر آن نامه را نیز پاسخ نوشت",
      "m2": "بسان درختی به باغ بهشت",
      "verse_number": 25
    },
    {
      "id": "f8d96e80-45d2-4ae4-bd2c-d45efe953c21",
      "m1": "سرنامه کرد از جهاندار یاد",
      "m2": "خداوند پیروزی و فرو داد",
      "verse_number": 26
    },
    {
      "id": "42ab8e4c-d385-4d1d-9787-b0bd0d6bd9fc",
      "m1": "خداوند ماه و خداوند هور",
      "m2": "خداونت پیل و خداوند مور",
      "verse_number": 27
    },
    {
      "id": "6e9910ec-7856-4708-855f-8a5b519653fb",
      "m1": "بزرگی و نیک اختری زو شناس",
      "m2": "وزو دار تا زنده باشی سپاس",
      "verse_number": 28
    },
    {
      "id": "128ac48e-8fc4-440c-80b1-cf2f84b955b9",
      "m1": "جز از داد و خوبی مکن در جهان",
      "m2": "چه در آشکار و چه اندر نهان",
      "verse_number": 29
    },
    {
      "id": "75fc7b2a-cd4b-4412-9976-e5dae051d6c9",
      "m1": "یکی تاج کز قیصران یادگار",
      "m2": "همیداشتی تا کی آید به کار",
      "verse_number": 30
    },
    {
      "id": "bb37189d-f87f-4eff-8d27-52c846b789c6",
      "m1": "همان خسروی طوق با گوشوار",
      "m2": "صدوشست تا جامهٔ زرنگار",
      "verse_number": 31
    },
    {
      "id": "d3c778f5-e0be-4ee0-bac3-242b0270bd2e",
      "m1": "دگر سی شتر بار دینار بود",
      "m2": "همان در و یاقوت بسیار بود",
      "verse_number": 32
    },
    {
      "id": "55b8cab8-d7d6-4929-a763-d3033114a35e",
      "m1": "صلیبی فرستاد گوهر نگار",
      "m2": "یکی تخت پرگوهر شاهوار",
      "verse_number": 33
    },
    {
      "id": "9e164583-cc34-4e68-acb0-9b8d67b2efc6",
      "m1": "یکی سبز خفتان به زر بافته",
      "m2": "بسی شوشه زر برو تافته",
      "verse_number": 34
    },
    {
      "id": "c10dc0e8-b324-4c77-aa59-d65fa5b03151",
      "m1": "ازان فیلسوفان رومی چهار",
      "m2": "برفتند با هدیه وبا نثار",
      "verse_number": 35
    },
    {
      "id": "eada24f2-e99a-4b38-a8f4-c919cd69e939",
      "m1": "چو زان کارها شد به شاه آگهی",
      "m2": "ز قیصر شدش کاربا فرهی",
      "verse_number": 36
    },
    {
      "id": "19b7bd06-f28b-4c2a-b3a3-a3bb8bb87f74",
      "m1": "پذیره فرستاد خسرو سوار",
      "m2": "گرانمایگان گرامی هزار",
      "verse_number": 37
    },
    {
      "id": "67caffcc-d3cd-4485-b6d2-6e89a3381cfa",
      "m1": "بزرگان به نزدیک خسرو شدند",
      "m2": "همه پاک با هدیه نو شدند",
      "verse_number": 38
    },
    {
      "id": "d6b26eea-ffe8-4a39-b8a0-d4c5b12ec32e",
      "m1": "چو خسرو نگه کرد و نامه بخواند",
      "m2": "ازان خواسته در شگفتی بماند",
      "verse_number": 39
    },
    {
      "id": "7d8bf512-00b7-4a66-84bd-5a39490b58db",
      "m1": "به دستور فرمود پس شهریار",
      "m2": "که آن جامهٔ روم گوهر نگار",
      "verse_number": 40
    },
    {
      "id": "a868bc1f-2f2a-4e41-a402-37d8ab668151",
      "m1": "نه آیین پرمایه دهقان بود",
      "m2": "کجا جامهٔ جاثلیقان بود",
      "verse_number": 41
    },
    {
      "id": "01961a0a-6be0-4e83-800a-44577e22e882",
      "m1": "چو بر جامهٔ ما چلیپا بود",
      "m2": "نشست اندر آیین ترسا بود",
      "verse_number": 42
    },
    {
      "id": "66976028-42a4-4917-bea1-c15437e9bf45",
      "m1": "وگر خود نپوشم بیازارد اوی",
      "m2": "همانا دگرگونه پندارد اوی",
      "verse_number": 43
    },
    {
      "id": "1ff6602d-3173-4c46-aaf4-e669f2b21785",
      "m1": "وگر پوشم این نامداران همه",
      "m2": "بگویند کاین شهریار رمه",
      "verse_number": 44
    },
    {
      "id": "e907ac0f-57ec-4fb6-9187-d1b46fab6f85",
      "m1": "مگر کز پی چیز ترسا شدست",
      "m2": "که اندر میان چلیپا شدست",
      "verse_number": 45
    },
    {
      "id": "f15c14c6-a142-4ab7-b2db-569ad03c385e",
      "m1": "به خسرو چنین گفت پس رهنمای",
      "m2": "که دین نیست شاها به پوشش بپای",
      "verse_number": 46
    },
    {
      "id": "ba1decc5-28b4-4e44-bc3f-a2818b8230dc",
      "m1": "تو بردین زر دشت پیغمبری",
      "m2": "اگر چند پیوسته قیصری",
      "verse_number": 47
    },
    {
      "id": "23eedba3-a286-4542-a0d6-0d58e4f8479f",
      "m1": "بپوشید پس جامهٔ شهریار",
      "m2": "بیاویخت آن تاج گوهرنگار",
      "verse_number": 48
    },
    {
      "id": "bea13671-c279-4950-878a-fe126970a332",
      "m1": "برفتند رومی و ایرانیان",
      "m2": "ز هر گونه مردم اندر میان",
      "verse_number": 49
    },
    {
      "id": "e93c1f8e-4f33-49bb-be72-ffe0986bd99a",
      "m1": "کسی کش خرد بود چون جامه دید",
      "m2": "بدانست کو رای قیصر گزید",
      "verse_number": 50
    },
    {
      "id": "3b0da9ff-994a-4c1f-a2c0-a470cc420795",
      "m1": "دگر گفت کاین شهریار جهان",
      "m2": "همانا که ترسا شد اندر نهان",
      "verse_number": 51
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 51
  },
  "order_in_parent": 35
}