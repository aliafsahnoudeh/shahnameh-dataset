{
  "id": "635aeb8c-2095-4556-8388-a19fa3113db5",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "85519222-9848-41d0-9f48-175accd3c363",
      "m1": "چوگستهم وبندوی به آذرگشسپ",
      "m2": "فگندند مردی سبک بر دو اسپ",
      "verse_number": 1
    },
    {
      "id": "3e78c158-d5c4-406e-8b0f-a1b8430cddaf",
      "m1": "که در شب به نزدیک خسرو شود",
      "m2": "از ایران به آگاهی نو شود",
      "verse_number": 2
    },
    {
      "id": "ebb233d6-f80d-4a43-8e33-ffd726165314",
      "m1": "فرستاده آمد بر شاه نو",
      "m2": "گذشته شبی تیره از ماه نو",
      "verse_number": 3
    },
    {
      "id": "b8d7f548-05e7-40b3-bc8b-280ce0dca071",
      "m1": "ز آشوب بغداد گفت آنچ دید",
      "m2": "جوان شد چو برگ گل شنبلید",
      "verse_number": 4
    },
    {
      "id": "01784955-7d3a-4df4-b2f2-919526e0de04",
      "m1": "چنین گفت هرکو زراه خرد",
      "m2": "بتیزی ز بیدانشی بگذرد",
      "verse_number": 5
    },
    {
      "id": "719f9b99-caf0-48cb-82bc-236dafcacd40",
      "m1": "نترسد ز کردار چرخ بلند",
      "m2": "شود زندگانیش ناسودمند",
      "verse_number": 6
    },
    {
      "id": "c27d2b99-d21d-479c-a354-0824785e2fd2",
      "m1": "گراین بد که گفتی خوش آمد مرا",
      "m2": "خور و خواب در آتش آمد مرا",
      "verse_number": 7
    },
    {
      "id": "d89b493e-a166-4c86-9571-a6019520632e",
      "m1": "ولیکن پدر چون به خون آخت دست",
      "m2": "از ایران نکردم سران نشست",
      "verse_number": 8
    },
    {
      "id": "a503616f-0b24-41b7-89e0-406ff2b79492",
      "m1": "هم او را کنون چون یکی بندهام",
      "m2": "سخن هرچ گوید نیوشندهام",
      "verse_number": 9
    },
    {
      "id": "8a8c5d67-094f-4eb7-9f28-6c47d3486f03",
      "m1": "هم اندر زمان داغ دل با سپاه",
      "m2": "بکردار آتش بیامد ز راه",
      "verse_number": 10
    },
    {
      "id": "6a417956-5481-4c9a-93c8-2f0566b2a1e6",
      "m1": "سپاهی بد از بردع و اردبیل",
      "m2": "همیرفت با نامور خیل خیل",
      "verse_number": 11
    },
    {
      "id": "60a1a5c5-bf48-4a5a-a793-e96aff69ff44",
      "m1": "از ارمینیه نیز چندی سپاه",
      "m2": "همیتاخت چون باد با پور شاه",
      "verse_number": 12
    },
    {
      "id": "93d5efbd-88ab-4bca-9194-7e5b9026d4dd",
      "m1": "چوآمد ببغداد زو آگهی",
      "m2": "که آمد خریدار تخت مهی",
      "verse_number": 13
    },
    {
      "id": "29e3ac72-a78e-46fd-9671-494d1c8fa636",
      "m1": "همه شهر ز آگاهی آرام یافت",
      "m2": "جهانجوی از آرامشان کام یافت",
      "verse_number": 14
    },
    {
      "id": "de188b22-703e-4ee8-99f6-167da5e95703",
      "m1": "پذیره شدندش بزرگان شهر",
      "m2": "کسی را که از مهتری بود بهر",
      "verse_number": 15
    },
    {
      "id": "c11ff650-dd80-4acf-a9b0-ca43cc8e1801",
      "m1": "نهادند بر پیشگه تخت عاج",
      "m2": "همان طوق زرین وپرمایه تاج",
      "verse_number": 16
    },
    {
      "id": "7a6b1e00-ee99-418e-ba15-a02d62a8a6dc",
      "m1": "بشهر اندرون رفت خسرو بدرد",
      "m2": "بنزد پدر رفت با بادسرد",
      "verse_number": 17
    },
    {
      "id": "c185f804-336e-4882-8a78-fe488c152ab6",
      "m1": "چه جوییم زین گنبد تیزگرد",
      "m2": "که هرگز نیاساید از کارکرد",
      "verse_number": 18
    },
    {
      "id": "28224f02-962e-4249-bb76-335d17351994",
      "m1": "یکی راهمی تاج شاهی دهد",
      "m2": "یکی را بدریا بماهی دهد",
      "verse_number": 19
    },
    {
      "id": "2b99e579-fc2e-4ba8-acd6-2aa750736daa",
      "m1": "یکی را برهنه سروپای و سفت",
      "m2": "نه آرام و خواب و نه جای نهفت",
      "verse_number": 20
    },
    {
      "id": "a95e60eb-1b8f-403a-b601-e3f9f52648ce",
      "m1": "یکی را دهد توشهٔ شهد و شیر",
      "m2": "بپوشد بدیبا و خز و حریر",
      "verse_number": 21
    },
    {
      "id": "e722d0dd-b5a9-42ca-8fbc-bf74806b8e4b",
      "m1": "سرانجام هردو بخاک اندرند",
      "m2": "بتارک بدام هلاک اندرند",
      "verse_number": 22
    },
    {
      "id": "202ecbb3-f615-4f0a-8561-872b2726aceb",
      "m1": "اگر خود نزادی خردمند مرد",
      "m2": "ندیدی ز گیتی چنین گرم و سرد",
      "verse_number": 23
    },
    {
      "id": "28d7510f-0c38-4313-b0fc-3e03d903bb61",
      "m1": "ندیدی جهان ازبنه به بدی",
      "m2": "اگر که بدی مرد اگر مه بدی",
      "verse_number": 24
    },
    {
      "id": "514bcba8-2c9a-45c7-a1e0-142ea3f5f7e5",
      "m1": "کنون رنج در کارخسرو بریم",
      "m2": "بخواننده آگاهی نو بریم",
      "verse_number": 25
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 25
  },
