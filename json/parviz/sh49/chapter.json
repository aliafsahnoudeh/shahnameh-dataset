{
  "id": "bda06b65-e198-4034-9dc5-231d41c7ba7c",
  "title": "بخش ۴۹",
  "verses": [
    {
      "id": "3336f3da-f41f-41ec-bb66-64a15e80b952",
      "m1": "چوخراد برزین به خسرو رسید",
      "m2": "بگفت آن کجا کرد و دید و شنید",
      "verse_number": 1
    },
    {
      "id": "89bafe5d-5626-4b3d-bf7d-679dff2a1518",
      "m1": "دل شاه پرویز ازان شاد شد",
      "m2": "کزان بد گهر دشمن آزاد شد",
      "verse_number": 2
    },
    {
      "id": "681be5fd-9705-4ca1-8fad-3e0669695977",
      "m1": "به درویش بخشید چندی درم",
      "m2": "ز پوشیدنیها و از بیش وکم",
      "verse_number": 3
    },
    {
      "id": "e5da520f-fc15-4965-b3ad-fa57005e9f43",
      "m1": "بهر پادشاهی و خودکامهای",
      "m2": "نوشتند بر پهلوی نامهای",
      "verse_number": 4
    },
    {
      "id": "60e91bf1-bd2c-4f24-ba55-155d42f5aeb4",
      "m1": "که دارای دارنده یزدان چه کرد",
      "m2": "ز دشمن چگونه برآورد گرد",
      "verse_number": 5
    },
    {
      "id": "57212de1-b086-4d6a-bb60-005cdc99d7e9",
      "m1": "به قیصر یکی نامه بنوشت شاه",
      "m2": "چناچون بود درخور پیشگاه",
      "verse_number": 6
    },
    {
      "id": "2829e82b-69c5-4700-a05a-ba71041a855e",
      "m1": "به یک هفته مجلس بیاراستند",
      "m2": "بهر برزنی رود و میخواستند",
      "verse_number": 7
    },
    {
      "id": "625c88e6-47e4-42b7-a59b-3b8cf006ee8b",
      "m1": "به آتشکده هم فرستاد چیز",
      "m2": "بران موبدان خلعت افگند نیز",
      "verse_number": 8
    },
    {
      "id": "4c573cc6-e97b-4dd7-8495-46ffe2ea55be",
      "m1": "بخراد برزین چنین گفت شاه",
      "m2": "که زیبد تو راگر دهم تاج و گاه",
      "verse_number": 9
    },
    {
      "id": "3d11f3dc-2bde-4e58-a51f-9009b28c4f20",
      "m1": "دهانش پر از گوهر شاهوار",
      "m2": "بیاگند و دینار چون صد هزار",
      "verse_number": 10
    },
    {
      "id": "349db906-e256-4f49-9ea8-04afdff0ceb3",
      "m1": "همیریخت گنجور در پای اوی",
      "m2": "برین گونه تا تنگ شد جای اوی",
      "verse_number": 11
    },
    {
      "id": "d5c848f5-9467-4dd8-9881-1f40a9e6ad07",
      "m1": "بدو گفت هرکس که پیچد ز راه",
      "m2": "شود روز روشن برو بر سیاه",
      "verse_number": 12
    },
    {
      "id": "fcf6e7e6-a37e-4dfd-951c-2447e0283d54",
      "m1": "چو بهرام باشد به دشت نبرد",
      "m2": "کزو ترک پیرش برآورد گرد",
      "verse_number": 13
    },
    {
      "id": "accc85e0-b365-4bbb-bc9c-a451dade518d",
      "m1": "همه موبدان خواندند آفرین",
      "m2": "که بی تو مبیناد کهتر زمین",
      "verse_number": 14
    },
    {
      "id": "9f88a55a-f131-44f4-9614-83f1b28b85c6",
      "m1": "چو بهرام باد آنک با مهر تو",
      "m2": "نخواهد که رخشان بود چهر تو",
      "verse_number": 15
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 15
  }
}