{
  "id": "c002468b-b030-40c7-885b-6eaa0080c21a",
  "title": "بخش ۷۴",
  "verses": [
    {
      "id": "36867774-dc1c-48fb-b08d-610777af99a6",
      "m1": "بدانست هم زادفرخ که شاه",
      "m2": "ز لشکر همه زو شناسد گناه",
      "verse_number": 1
    },
    {
      "id": "01624392-cbf0-4db2-a62f-d7c5948a6199",
      "m1": "چو آمد برون آن بد اندیش شاه",
      "m2": "نیارست شد نیز در پیشگاه",
      "verse_number": 2
    },
    {
      "id": "773e6c03-069e-4ecd-a1ee-c6e3e6799daa",
      "m1": "بدر بر همیبود تا هرکسی",
      "m2": "همیکرد زان آزمایش بسی",
      "verse_number": 3
    },
    {
      "id": "2b50cc1b-cc32-4e52-89b4-6b6e9b49f47e",
      "m1": "همیساخت همواره تا آن سپاه",
      "m2": "بپیچید یکسر ز فرمان شاه",
      "verse_number": 4
    },
    {
      "id": "91916ae1-b3ec-4b22-9cd1-f39344527f49",
      "m1": "همیراند با هر کسی داستان",
      "m2": "شدند اندر آن کار همداستان",
      "verse_number": 5
    },
    {
      "id": "43bd47c8-a606-4208-a3fb-a52f78f23906",
      "m1": "که شاهی دگر برنشیند به تخت",
      "m2": "کزین دور شد فرو آیین و بخت",
      "verse_number": 6
    },
    {
      "id": "eaae3d3e-7bb5-42eb-82a0-8ff36ba69b59",
      "m1": "بر زادفرخ یکی پیر بود",
      "m2": "که برکارها کردن آژیر بود",
      "verse_number": 7
    },
    {
      "id": "9829256b-9e04-434b-8240-e6cc0e003edf",
      "m1": "چنین گفت با زادفرخ که شاه",
      "m2": "همی از تو بیند گناه سپاه",
      "verse_number": 8
    },
    {
      "id": "4b1aa4b8-5203-4901-8407-3f1120f88751",
      "m1": "کنون تا یکی شهریاری پدید",
      "m2": "نیاری فزون زین نباید چخید",
      "verse_number": 9
    },
    {
      "id": "beccbecc-d24a-4ea0-92fd-d3b40ad15aa6",
      "m1": "که این بوم آباد ویران شود",
      "m2": "از اندوه ایران چو نیران شود",
      "verse_number": 10
    },
    {
      "id": "3b6ea584-efab-4111-b6df-1073a7af1b80",
      "m1": "نگه کرد باید به فرزند اوی",
      "m2": "کدامست با شرم و بیگفت و گوی",
      "verse_number": 11
    },
    {
      "id": "9650f561-cb9f-49da-95df-0db3516b7142",
      "m1": "ورا شاد بر تخت باید نشاند",
      "m2": "بران تاج دینار باید فشاند",
      "verse_number": 12
    },
    {
      "id": "7a24b07d-1403-48d0-a669-ca6694ecfc2f",
      "m1": "چو شیروی بیدار مهتر پسر",
      "m2": "به زندان بود کس نباید دگر",
      "verse_number": 13
    },
    {
      "id": "ce67a147-e620-4453-aa40-587790ba4676",
      "m1": "همی رای زد زین نشان هرکسی",
      "m2": "برین روز و شب برنیامد بسی",
      "verse_number": 14
    },
    {
      "id": "83ee36ad-12ae-4016-9b5b-c1a3e7f4be6d",
      "m1": "که برخاست گرد سپاه تخوار",
      "m2": "همه کارها زو گرفتند خوار",
      "verse_number": 15
    },
    {
      "id": "61b63c38-d612-480a-a71c-59b7bf43cae5",
      "m1": "پذیره شدش زادفرخ به راه",
      "m2": "فراوان برفتند با او سپاه",
      "verse_number": 16
    },
    {
      "id": "0018c81c-28eb-46a3-aede-6a523d0a312a",
      "m1": "رسیدند پس یک بدیگر فراز",
      "m2": "سخن رفت چند آشکارا و راز",
      "verse_number": 17
    },
    {
      "id": "b9bb6143-ed92-4a5a-91d8-9d9b6641be8a",
      "m1": "همان زاد فرخ زبان برگشاد",
      "m2": "بدی های خسرو همه کرد یاد",
      "verse_number": 18
    },
    {
      "id": "8675e31d-b22b-42bb-850e-4e463766d9f2",
      "m1": "همیگفت لشکر به مردی و رای",
      "m2": "همیکرد خواهند شاهی بپای",
      "verse_number": 19
    },
    {
      "id": "bc7541f3-9b73-4ec4-bd9b-2f4b94c426ea",
      "m1": "سپهبد چنین داد پاسخ بدوی",
      "m2": "که من نیستم چامهٔ گفت وگوی",
      "verse_number": 20
    },
    {
      "id": "1d7f16b3-0668-4f3e-a068-27c90167ad1a",
      "m1": "اگر با سپاه اندر آیم به جنگ",
      "m2": "کنم بر بدان جهان جای تنگ",
      "verse_number": 21
    },
    {
      "id": "8de22f7a-32e5-4b8e-8234-b0fbcb7944bd",
      "m1": "گرامی بد این شهریار جوان",
      "m2": "به نزد کنارنگ و هم پهلوان",
      "verse_number": 22
    },
    {
      "id": "84311f81-c944-4ebc-a2f4-200311c07a26",
      "m1": "چو روز چنان مرد کرد او سیاه",
      "m2": "مبادا که بیند کسی تاج و گاه",
      "verse_number": 23
    },
    {
      "id": "dbacb1dd-de06-4f8b-b245-3bb1c6c541eb",
      "m1": "نژند آن زمان شد که بیداد شد",
      "m2": "به بیدادگر بندگان شاد شد",
      "verse_number": 24
    },
    {
      "id": "a2588f96-5576-491b-bbb8-e539be09cd85",
      "m1": "سخنهاش چون زاد فرخ شنید",
      "m2": "مر او را ز ایرانیان برگزید",
      "verse_number": 25
    },
    {
      "id": "1e4a540f-d95f-4c4e-a828-3c5c1347d881",
      "m1": "بدو گفت کاکنون به زندان شویم",
      "m2": "به نزدیک آن مستمندان شویم",
      "verse_number": 26
    },
    {
      "id": "1ab97949-91af-4cac-8905-e24f9969f373",
      "m1": "بیاریم بیباک شیروی را",
      "m2": "جوان و دلیر جهانجوی را",
      "verse_number": 27
    },
    {
      "id": "fa6d9ec3-ae64-47ab-bad0-a99a570ea108",
      "m1": "سپهبد نگهبان زندان اوست",
      "m2": "کزو داشتی بیشتر مغز و پوست",
      "verse_number": 28
    },
    {
      "id": "f711ce90-e6ae-4407-8b83-785237a50f10",
      "m1": "ابا شش هزار آزموده سوار",
      "m2": "همیدارد آن بستگان را به زار",
      "verse_number": 29
    },
    {
      "id": "822f4b32-b2dc-4d05-b6a2-5a58c4d2e0ba",
      "m1": "چنین گفت با زاد فرخ تخوار",
      "m2": "که کار سپهبد گرفتیم خوار",
      "verse_number": 30
    },
    {
      "id": "a38108cb-353f-4426-a666-4273a840096e",
      "m1": "گرین بخت پرویز گردد جوان",
      "m2": "نماند به ایران یکی پهلوان",
      "verse_number": 31
    },
    {
      "id": "4929382e-5f22-4b0c-873d-9ae5d54c5901",
      "m1": "مگر دار دارند گر چاه وبند",
      "m2": "نماند به ایران کسی بیگزند",
      "verse_number": 32
    },
    {
      "id": "786ffc42-dd8d-48ef-b29f-46b40c03bd97",
      "m1": "بگفت این و از جای برکند اسپ",
      "m2": "همیتاخت برسان آذر گشسپ",
      "verse_number": 33
    },
    {
      "id": "81802635-9811-4a08-b54a-d718a059a810",
      "m1": "سپاه اندر آورد یکسر به جنگ",
      "m2": "سپهبد پذیره شدش بی درنگ",
      "verse_number": 34
    },
    {
      "id": "8a6e9a18-a06c-4bd5-8424-693268b9d6eb",
      "m1": "سر لشکر نامور گشته شد",
      "m2": "سپهبد به جنگ اندرون کشته شد",
      "verse_number": 35
    },
    {
      "id": "2a046600-332c-41ba-8f3e-15050971b099",
      "m1": "پراگنده شد لشکر شهریار",
      "m2": "سیه گشت روز و تبه گشت کار",
      "verse_number": 36
    },
    {
      "id": "b749c8d2-04a2-4bdb-a661-48ef45397b71",
      "m1": "به زندان تنگ اندر آمد تخوار",
      "m2": "بدان چاره با جامهٔ کارزار",
      "verse_number": 37
    },
    {
      "id": "498a2d34-aab4-4bcb-a6af-68364020b38e",
      "m1": "به شیروی گردنکش آواز داد",
      "m2": "سبک پاسخش نامور باز داد",
      "verse_number": 38
    },
    {
      "id": "3f518328-0f5c-4e50-9b31-b7dc7d7011b4",
      "m1": "بدانست شیروی کان سرفراز",
      "m2": "بدانگه به زندان چرا شد فراز",
      "verse_number": 39
    },
    {
      "id": "a4c68f16-5a87-41be-8800-1349d6bf1f46",
      "m1": "چو روی تخوار او فروزان بدید",
      "m2": "از اندوه چندان دلش بردمید",
      "verse_number": 40
    },
    {
      "id": "dcb94359-86c1-450f-a81f-ad0bc48b65cd",
      "m1": "بدو گفت گریان که خسرو کجاست",
      "m2": "رها کردن مانه کار شماست",
      "verse_number": 41
    },
    {
      "id": "b8a3a7fb-fdbe-466b-baa3-b1b54010646a",
      "m1": "چنین گفت با شاهزاده تخوار",
      "m2": "که گر مردمی کام شیران مخوار",
      "verse_number": 42
    },
    {
      "id": "09a6e7d5-5bca-4d77-8ed6-f3fb28291d98",
      "m1": "اگر تو بدین کار همداستان",
      "m2": "نباشی تو کم گیر زین راستان",
      "verse_number": 43
    },
    {
      "id": "906a3c99-eae9-4eca-a7cc-32a90c5f7083",
      "m1": "یکی کم بود شاید از شانزده",
      "m2": "برادر بماند تو را پانزده",
      "verse_number": 44
    },
    {
      "id": "7fa69085-3cb4-4f17-b731-cada53df5ea2",
      "m1": "بشایند هرکس به شاهنشهی",
      "m2": "بدیشان بود شاد تخت مهی",
      "verse_number": 45
    },
    {
      "id": "7229a1f3-f90c-4ad8-ac38-be9321c3db78",
      "m1": "فروماند شیروی گریان بجای",
      "m2": "ازان خانهٔ تنگ بگذارد پای",
      "verse_number": 46
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 46
  },
