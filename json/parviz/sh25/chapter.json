{
  "id": "2916e60d-034c-4dee-9b9d-bdd9848f0f6f",
  "title": "بخش ۲۵",
  "verses": [
    {
      "id": "f34a456c-3c2b-4819-9250-fd988e68707c",
      "m1": "چو خورشید گردنده بیرنگ شد",
      "m2": "ستاره به برج شباهنگ شد",
      "verse_number": 1
    },
    {
      "id": "7c5ba545-e416-4c04-9437-e57723b18c99",
      "m1": "بفرمود قیصر به نیرنگ ساز",
      "m2": "که پیش آرد اندیشههای دراز",
      "verse_number": 2
    },
    {
      "id": "c124bebf-0368-4683-aee0-5b0fdcf16cf0",
      "m1": "بسازید جای شگفتی طلسم",
      "m2": "که کس بازنشناسد او را به جسم",
      "verse_number": 3
    },
    {
      "id": "71be4e7a-0dcb-43c7-936e-ccbf48d599b0",
      "m1": "نشسته زنی خوب برتخت ناز",
      "m2": "پراز شرم با جامههای طراز",
      "verse_number": 4
    },
    {
      "id": "2002b8f2-112a-42bd-8a40-3082f7f9a79c",
      "m1": "ازین روی و زان رو پرستندگان",
      "m2": "پس پشت و پیش اندرش بندگان",
      "verse_number": 5
    },
    {
      "id": "713803a0-cd74-4990-883b-7b2698395757",
      "m1": "نشسته بران تخت بی گفت وگوی",
      "m2": "بگریان زنی ماند آن خوب روی",
      "verse_number": 6
    },
    {
      "id": "075059c5-981b-404a-bfe2-ed3a678a93f7",
      "m1": "زمان تا زمان دست برآختی",
      "m2": "سرشکی ز مژگان بینداختی",
      "verse_number": 7
    },
    {
      "id": "bca66174-4508-44c0-8d6b-d1bf408c50b8",
      "m1": "هرآنکس که دیدی مر او را ز دور",
      "m2": "زنی یافتی شیفته پر ز نور",
      "verse_number": 8
    },
    {
      "id": "7d4bb28b-0742-4cd4-805e-d0da4b02dbbf",
      "m1": "که بگریستی بر مسیحا بزار",
      "m2": "دو رخ زرد و مژگان چو ابر بهار",
      "verse_number": 9
    },
    {
      "id": "b8d14904-bdb1-4f51-8012-23b645d03691",
      "m1": "طلسم بزرگان چو آمد بجای",
      "m2": "بر قیصر آمد یکی رهنمای",
      "verse_number": 10
    },
    {
      "id": "53d3cae8-8905-455e-8eec-282a0123c91a",
      "m1": "ز دانا چو بشنید قیصر برفت",
      "m2": "به پیش طلسم آمد آنگاه تفت",
      "verse_number": 11
    },
    {
      "id": "e154b1b8-7f37-4fc1-a991-31c1d3c4ac07",
      "m1": "ازان جادویی در شگفتی بماند",
      "m2": "فرستاد و گستهم را پیش خواند",
      "verse_number": 12
    },
    {
      "id": "e7649b9c-af2d-49e4-837f-d5644de204de",
      "m1": "بگستهم گفت ای گو نامدار",
      "m2": "یکی دختری داشتم چون نگار",
      "verse_number": 13
    },
    {
      "id": "0e24ae52-9d16-4d5a-95c7-e8675cec73ab",
      "m1": "ببالید و آمدش هنگام شوی",
      "m2": "یکی خویش بد مرو را نامجوی",
      "verse_number": 14
    },
    {
      "id": "5f599113-370f-49ae-8f78-b03666d621fc",
      "m1": "به راه مسیحا بدو دادمش",
      "m2": "ز بیدانشی روی بگشادمش",
      "verse_number": 15
    },
    {
      "id": "c80f2972-ebf2-4b70-a38e-0f3b32e3303b",
      "m1": "فرستادم او رابخان جوان",
      "m2": "سوی آسمان شد روان جوان",
      "verse_number": 16
    },
    {
      "id": "316d3acf-4e1b-428e-8e12-7cb6a1ab65c1",
      "m1": "کنون او نشستست با سوک و درد",
      "m2": "شده روز روشن برو لاژورد",
      "verse_number": 17
    },
    {
      "id": "7240a451-1e12-42ab-961e-4af12e78f15c",
      "m1": "نه پندم پذیرد نه گوید سخن",
      "m2": "جهان نو از رنج او شد کهن",
      "verse_number": 18
    },
    {
      "id": "13b44b5a-a925-4f6b-8313-224aeb62902c",
      "m1": "یکی رنج بردار و او راببین",
      "m2": "سخنهای دانندگان برگزین",
      "verse_number": 19
    },
    {
      "id": "12c5e81e-cbbe-4357-b0bf-5103f91f2b81",
      "m1": "جوانی و از گوهر پهلوان",
      "m2": "مگر با تو او برگشاید زبان",
      "verse_number": 20
    },
    {
      "id": "a7046038-6b72-4e08-95d7-1e0dd6dfff2a",
      "m1": "بدو گفت گستهم کایدون کنم",
      "m2": "مگر از دلش رنج بیرون کنم",
      "verse_number": 21
    },
    {
      "id": "4aae3696-b39e-4eb5-95e2-3dde51e6d5eb",
      "m1": "بنزد طلسم آمد آن نامدار",
      "m2": "گشاده دل و بر سخن کامگار",
      "verse_number": 22
    },
    {
      "id": "cd8d980b-3fca-43e1-8739-baef991e952a",
      "m1": "چوآمد به نزدیک تختش فراز",
      "m2": "طلسم از بر تخت بردش نماز",
      "verse_number": 23
    },
    {
      "id": "308fda08-2cd9-4d4d-b753-52c1b41b9713",
      "m1": "گرانمایه گستهم بنشست خوار",
      "m2": "سخن گفت با دختر سوکوار",
      "verse_number": 24
    },
    {
      "id": "b60734bf-afcd-4245-adcc-12fe53f8da76",
      "m1": "دلاور نخست اندر آمد بپند",
      "m2": "سخنها که او را بدی سودمند",
      "verse_number": 25
    },
    {
      "id": "0b0887df-c225-42c5-8964-51b3f35edcf1",
      "m1": "بدو گفت کای دخت قیصر نژاد",
      "m2": "خردمند نخروشد از کار داد",
      "verse_number": 26
    },
    {
      "id": "bcc0e12d-6d47-4ae3-a62e-157437ca78eb",
      "m1": "رهانیست از مرگ پران عقاب",
      "m2": "چه در بیشه شیر و چه ماهی در آب",
      "verse_number": 27
    },
    {
      "id": "192717dc-0fe5-4ee3-a075-6985f7e57121",
      "m1": "همه باد بد گفتن پهلوان",
      "m2": "که زن بیزبان بود و تن بیروان",
      "verse_number": 28
    },
    {
      "id": "9dd9437d-7db5-4380-a830-7ec1b2f8b152",
      "m1": "به انگشت خود هر زمانی سرشک",
      "m2": "بینداختی پیش گویا پزشک",
      "verse_number": 29
    },
    {
      "id": "0c20aded-1b8d-404a-9fb6-a7656a731e06",
      "m1": "چوگستهم ازو در شگفتی بماند",
      "m2": "فرستاد قیصر کس او را بخواند",
      "verse_number": 30
    },
    {
      "id": "51f4daf6-4bf2-4f90-bd0b-d561bd55574d",
      "m1": "چه دیدی بدوگفت از دخترم",
      "m2": "کزو تیره گردد همی افسرم",
      "verse_number": 31
    },
    {
      "id": "6f05739b-0b21-4d65-8e35-abe3a5c4cff6",
      "m1": "بدو گفت بسیار دادمش پند",
      "m2": "نبد پند من پیش او کاربند",
      "verse_number": 32
    },
    {
      "id": "d779ad1f-5f3d-4de3-86b5-270b7e77feb7",
      "m1": "دگر روز قیصر به بالوی گفت",
      "m2": "که امروز با اندیان باش جفت",
      "verse_number": 33
    },
    {
      "id": "98638b66-aa15-4c03-8e3f-391793f767a8",
      "m1": "همان نیز شاپور مهتر نژاد",
      "m2": "کند جان ما رابدین دخت شاد",
      "verse_number": 34
    },
    {
      "id": "683a8f27-476b-416b-a2eb-0450e1cc3811",
      "m1": "شوی پیش این دختر سوکوار",
      "m2": "سخن گویی ازنامور شهریار",
      "verse_number": 35
    },
    {
      "id": "7ca4b326-5641-4400-8368-f06b3ec9b867",
      "m1": "مگر پاسخی یابی از دخترم",
      "m2": "کزو آتش آید همی برسرم",
      "verse_number": 36
    },
    {
      "id": "3cb1c3a4-aaba-47e8-b91b-4c771cf13514",
      "m1": "مگر بشنود پند و اندرزتان",
      "m2": "بداند سرماهی وارزتان",
      "verse_number": 37
    },
    {
      "id": "60a9983d-4704-4394-a6de-19d9d7f9061c",
      "m1": "برآنم که امروز پاسخ دهد",
      "m2": "چوپاسخ به آواز فرخ دهد",
      "verse_number": 38
    },
    {
      "id": "02a5f933-ffc2-4d1e-9270-1dac9bce66b5",
      "m1": "شود رسته زین انده سوکوار",
      "m2": "که خوناب بارد همی برکنار",
      "verse_number": 39
    },
    {
      "id": "e58a2e01-8ff4-4458-b5d9-cf76a76d574f",
      "m1": "برفت آن گرامی سه آزادمرد",
      "m2": "سخن گوی وهریک بننگ نبرد",
      "verse_number": 40
    },
    {
      "id": "d6f7baed-c2f8-46a4-94f8-958e35aad218",
      "m1": "ازیشان کسی روی پاسخ ندید",
      "m2": "زن بیزبان خامشی برگزید",
      "verse_number": 41
    },
    {
      "id": "98632b82-daa7-4cef-b716-59e39e8a8867",
      "m1": "ازان چاره نزدیک قیصر شدند",
      "m2": "ببیچارگی نزد داور شدند",
      "verse_number": 42
    },
    {
      "id": "5d142ad0-82c4-4655-b820-d34546970880",
      "m1": "که هرچند گفتیم ودادیم پند",
      "m2": "نبد پند ما مر ورا سودمند",
      "verse_number": 43
    },
    {
      "id": "72d28a5f-2cfe-4583-832f-ca66d8fbc3ad",
      "m1": "چنین گفت قیصر که بد روزگار",
      "m2": "که ما سوکواریم زین سوکوار",
      "verse_number": 44
    },
    {
      "id": "2733ed92-2f70-48ec-852b-d17e2a2e591f",
      "m1": "ازان نامداران چو چاره نیافت",
      "m2": "سوی رای خراد بر زین شتافت",
      "verse_number": 45
    },
    {
      "id": "4c94c05a-f40f-4b31-a648-1ba3916dd08d",
      "m1": "بدو گفت کای نامدار دبیر",
      "m2": "گزین سر تخمهٔ اردشیر",
      "verse_number": 46
    },
    {
      "id": "d8690f9d-5a60-4084-b9f7-5500cffa605a",
      "m1": "یکی سوی این دختر اندر شوی",
      "m2": "مگر یک ره آواز او بشنوی",
      "verse_number": 47
    },
    {
      "id": "715ef719-cd8c-4d43-bc7b-a86f10b360c1",
      "m1": "فرستاد با او یکی استوار",
      "m2": "ز ایوان به نزدیک آن سوکوار",
      "verse_number": 48
    },
    {
      "id": "ad128b36-e262-4e05-8431-feced9387df9",
      "m1": "چوخراد بر زین بیامد برش",
      "m2": "نگه کرد روی و سر و افسرش",
      "verse_number": 49
    },
    {
      "id": "11a68837-b97c-4a05-9ebb-a24cd253746a",
      "m1": "همیبود پیشش زمانی دراز",
      "m2": "طلسم فریبنده بردش نماز",
      "verse_number": 50
    },
    {
      "id": "56fbd395-842e-4ec4-959c-a05895e197fb",
      "m1": "بسی گفت و زن هیچ پاسخ نداد",
      "m2": "پراندیشه شد مرد مهتر نژاد",
      "verse_number": 51
    },
    {
      "id": "e78eac49-1168-4573-b084-ebcc865bd3ee",
      "m1": "سراپای زن راهمیبنگرید",
      "m2": "پرستندگان را بر او بدید",
      "verse_number": 52
    },
    {
      "id": "3f3f6ef7-2f22-470c-a80b-c2415e5bfd37",
      "m1": "همیگفت گر زن زغم بیهش است",
      "m2": "پرستنده باری چرا خامش است",
      "verse_number": 53
    },
    {
      "id": "9ae72a31-ff98-45dc-938c-115f1796a5c9",
      "m1": "اگر خود سرشکست در چشم اوی",
      "m2": "سزیدی اگر کم شدی خشم اوی",
      "verse_number": 54
    },
    {
      "id": "9b139f57-02c4-404c-8b28-e6b70f9a46f9",
      "m1": "به پیش برش بر چکاند همی",
      "m2": "چپ وراست جنبش نداند همی",
      "verse_number": 55
    },
    {
      "id": "353242c8-6b64-4676-aa2c-7a35ed1958a9",
      "m1": "سرشکش که انداخت یک جای رفت",
      "m2": "نه جنبان شدش دست ونه پای رفت",
      "verse_number": 56
    },
    {
      "id": "c86340ca-960c-49d0-8ade-c77779a2c3ef",
      "m1": "اگرخود درین کالبد جان بدی",
      "m2": "جز از دست جاییش جنبان بدی",
      "verse_number": 57
    },
    {
      "id": "68d35f99-1e61-4a94-98d9-b369138c02ae",
      "m1": "سرشکش سوی دیگر انداختی",
      "m2": "وگر دست جای دگر آختی",
      "verse_number": 58
    },
    {
      "id": "d4c6e754-bdde-4bde-aad7-13cd062a012b",
      "m1": "نبینم همی جنبش جان و جسم",
      "m2": "نباشد جز از فیلسوفی طلسم",
      "verse_number": 59
    },
    {
      "id": "b3dcdb19-fdcc-4165-a1b9-c5be40e4affd",
      "m1": "بر قیصر آمد بخندید وگفت",
      "m2": "که این ماه رخ را خرد نیست جفت",
      "verse_number": 60
    },
    {
      "id": "915cfcb8-43cb-4a09-ab2d-a637c9a0e0f7",
      "m1": "طلسمست کاین رومیان ساختند",
      "m2": "که بالوی و گستهم نشناختند",
      "verse_number": 61
    },
    {
      "id": "8d702f77-2593-4748-898a-aeb1a4c71704",
      "m1": "بایرانیان بربخندی همی",
      "m2": "وگر چشم ما را ببندی همی",
      "verse_number": 62
    },
    {
      "id": "a3212c8b-6248-43e0-b8fb-f31453eb1420",
      "m1": "چواین بشنود شاه خندان شود",
      "m2": "گشاده رخ و سیم دندان شود",
      "verse_number": 63
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 63
  },
  "order_in_parent": 25
}