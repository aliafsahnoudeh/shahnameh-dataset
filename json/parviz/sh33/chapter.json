{
  "id": "a2ea3ef0-7b8e-42d9-9621-1e7190bd14b9",
  "title": "بخش ۳۳",
  "verses": [
    {
      "id": "03f2ed07-4ee6-4bc1-8d2e-69bd6fee80aa",
      "m1": "هم آنگه ز کوه اندر آمد سپاه",
      "m2": "جهان شد ز گرد سواران سیاه",
      "verse_number": 1
    },
    {
      "id": "20c18941-198d-4778-91a8-3f84eb8f03ae",
      "m1": "وزان روی بهرام لشکر براند",
      "m2": "به روز اندرون روشنایی نماند",
      "verse_number": 2
    },
    {
      "id": "2c2cf123-2d9b-45f0-9e89-f79bba088514",
      "m1": "همیگفت هرکس که راند سپاه",
      "m2": "خرد باید و مردی و دستگاه",
      "verse_number": 3
    },
    {
      "id": "3345fa61-d297-4abd-b41f-9b59ceb3e5a1",
      "m1": "دلیران که دیدند خشت مرا",
      "m2": "همان پهلوانی سرشت مرا",
      "verse_number": 4
    },
    {
      "id": "d11003f1-243c-4f4a-bc2b-64fa2b2b51c6",
      "m1": "مرا برگزیدند بر خسروان",
      "m2": "به خاک افگنم نام نوشین روان",
      "verse_number": 5
    },
    {
      "id": "ad68985b-22af-4fff-904a-44d0df2babd2",
      "m1": "ز لشکر بر شاه شد خیره خیر",
      "m2": "کمان را بزه کرد و یک چوبه تیر",
      "verse_number": 6
    },
    {
      "id": "8ad06100-eca0-4999-9070-58313dce89b2",
      "m1": "بزد ناگهان بر کمرگاه شاه",
      "m2": "بکژ اندر آویخت پیکان به راه",
      "verse_number": 7
    },
    {
      "id": "64a87069-b663-48dc-9130-db91f0f1614e",
      "m1": "یکی بنده چون زخم پیکان بدید",
      "m2": "بیامد ز دیباش بیرون کشید",
      "verse_number": 8
    },
    {
      "id": "f2244461-d5a0-4b53-84b2-36f365e95761",
      "m1": "سبک شهریار اندر آمد دمان",
      "m2": "به بهرام چوبینهٔ بد نشان",
      "verse_number": 9
    },
    {
      "id": "2aa86ab4-d36a-4aa4-8586-bce15a30b772",
      "m1": "بزد نیزهای بر کمربند اوی",
      "m2": "زره بود نگسست پیوند اوی",
      "verse_number": 10
    },
    {
      "id": "39cf8375-e261-4d76-8fc3-bcdc41b6442b",
      "m1": "سنان سر نیزه شد به دونیم",
      "m2": "دل مرد بیراه شد پر ز بیم",
      "verse_number": 11
    },
    {
      "id": "d239e4ee-220d-47dc-bb18-ca8681e836ee",
      "m1": "چو بشکست نیزه بر آشفت شاه",
      "m2": "بزد تیغ بر مغفر کینه خواه",
      "verse_number": 12
    },
    {
      "id": "0f86885d-1e5f-4b38-b513-e8c113c661fb",
      "m1": "سراسر همه تیغ برهم شکست",
      "m2": "بدان پیکر مغفر اندر نشست",
      "verse_number": 13
    },
    {
      "id": "460d4b0b-f006-4fbc-9f8e-a3e329a5253f",
      "m1": "همی آفرین کرد هرکس که دید",
      "m2": "هم آنکس که آواز آهن شنید",
      "verse_number": 14
    },
    {
      "id": "7a4d5cae-1a97-4537-b27d-a1e0ee4fc93d",
      "m1": "گرانمایگان از پس اندر شدند",
      "m2": "چنان لشکری را بهم بر زدند",
      "verse_number": 15
    },
    {
      "id": "79217087-3a64-4a66-af58-1a7a250164df",
      "m1": "خرامید بندوی نزدیک شاه",
      "m2": "کهای تاج تو برتر از چرخ ماه",
      "verse_number": 16
    },
    {
      "id": "ac95f1a3-97d3-4344-8ba3-596441b7a16d",
      "m1": "یکی لشکرست این چومور وملخ",
      "m2": "گرفته بیابان همه ریگ و شخ",
      "verse_number": 17
    },
    {
      "id": "3d8b29f7-ac5c-4170-a774-b4fe96a264a0",
      "m1": "نه والا بود خیره خون ریختن",
      "m2": "نه این شاه با بنده آویختن",
      "verse_number": 18
    },
    {
      "id": "8c35ccdd-dc00-4a4a-86bb-ed68b3528fc9",
      "m1": "هر آنکس که خواهد ز ما زینهار",
      "m2": "به از کشته یا خسته در کارزار",
      "verse_number": 19
    },
    {
      "id": "8800985b-1d7c-444c-8658-f928ec716aad",
      "m1": "بدو گفت خسرو که هر کز گناه",
      "m2": "بپیچید برو من نیم کینه خواه",
      "verse_number": 20
    },
    {
      "id": "f60d12bb-97fa-4987-91e5-25956004d716",
      "m1": "همه پاک در زینهار منند",
      "m2": "به تاج اندرون گوشوار منند",
      "verse_number": 21
    },
    {
      "id": "3c26682c-ece2-4522-95cf-ed8adae85d53",
      "m1": "برآمد هم آنگه شب از تیره کوه",
      "m2": "سپه بازگشتند هر دو گروه",
      "verse_number": 22
    },
    {
      "id": "a59f23fc-f928-4dea-8488-5b818e70db38",
      "m1": "چوآمد غوپاسبان و جرس",
      "m2": "ز لشکر نبد خفته بسیار کس",
      "verse_number": 23
    },
    {
      "id": "9d9ffbe4-ef20-47fc-ab0a-b00e92ae5d52",
      "m1": "جهان جوی بندوی ز آنجا برفت",
      "m2": "میان دو لشکر خرامید تفت",
      "verse_number": 24
    },
    {
      "id": "f7ff7c14-7bdf-4fc2-882e-16b55968c876",
      "m1": "ز لشکر نگه کرد کنداوری",
      "m2": "خوش آواز و گویا منادیگری",
      "verse_number": 25
    },
    {
      "id": "2d75bd38-591d-45ea-849a-cb09c63925c8",
      "m1": "بفرمود تا بارگی برنشست",
      "m2": "به بیدار کردن میان را ببست",
      "verse_number": 26
    },
    {
      "id": "54fb9f6c-e01a-4f80-ba0c-ee8727963ecf",
      "m1": "چنین تا میان دولشکر براند",
      "m2": "کزو تا بدشمن فراوان نماند",
      "verse_number": 27
    },
    {
      "id": "edb36452-f754-488d-a1a6-6ef045df3cf9",
      "m1": "خروشی برآورد کای بندگان",
      "m2": "گنه کرده و بخت جویندگان",
      "verse_number": 28
    },
    {
      "id": "0cec1995-a2a0-4e5b-bf6b-70ef6c68d5c8",
      "m1": "هران کز شما او گنهکارتر",
      "m2": "به جنگ اندرون نامبردارتر",
      "verse_number": 29
    },
    {
      "id": "8eb6bcca-6a26-4c9b-9c31-d9f730457a5c",
      "m1": "به یزدانش بخشید شاه جهان",
      "m2": "گناهیکه کرد آشکار و نهان",
      "verse_number": 30
    },
    {
      "id": "a28d3673-4ace-48c4-9ac6-00e86d92e7fa",
      "m1": "به تیره شبان چون برآمد خروش",
      "m2": "نهادند هرکس به آواز گوش",
      "verse_number": 31
    },
    {
      "id": "9ff73070-7710-4bf1-beff-5a452a8b5c72",
      "m1": "همه نامداران بهرامیان",
      "m2": "برفتن ببستند یک سر میان",
      "verse_number": 32
    },
    {
      "id": "4bdaf003-adde-41ac-a965-926a098504a9",
      "m1": "چو برزد سر از کوه گیتی فروز",
      "m2": "زمین را به ملحم بیاراست روز",
      "verse_number": 33
    },
    {
      "id": "26bd958e-3069-4079-8b3b-ecae9bf8d1c2",
      "m1": "همه دشت بیمرد و خرگاه بود",
      "m2": "که بهرام زان شب نه آگاه بود",
      "verse_number": 34
    },
    {
      "id": "f4f133e1-3c06-4ba1-b445-0cad156e2fa5",
      "m1": "بدان خیمهها در ندیدند کس",
      "m2": "جز از ویژه یاران بهرام و بس",
      "verse_number": 35
    },
    {
      "id": "00e85b46-8964-42f0-8678-90b95f5eca3c",
      "m1": "چو بهرام زان لشکر آگاه گشت",
      "m2": "بیامد بران خیمهها برگذشت",
      "verse_number": 36
    },
    {
      "id": "a49407cb-42cc-4467-8c60-a16b1f86c07a",
      "m1": "به یاران چنین گفت کاکنون گریز",
      "m2": "به آید ز آرام با رستخیز",
      "verse_number": 37
    },
    {
      "id": "347f4a19-aeef-4826-9a15-909053140b4c",
      "m1": "شتر خواست از ساروان سه هزار",
      "m2": "هیونان کفک افگن و نامدار",
      "verse_number": 38
    },
    {
      "id": "e8263271-6908-44cf-a2e7-2490311fc248",
      "m1": "ز چیزی که در گنج بد بردنی",
      "m2": "ز گستردنیها و از خوردنی",
      "verse_number": 39
    },
    {
      "id": "e09e3c44-4731-467b-8c0a-2eccea985862",
      "m1": "ز زرین و سیمین وز تخت عاج",
      "m2": "همان یاره و طوق زرین وتاج",
      "verse_number": 40
    },
    {
      "id": "0305638f-0c31-42cb-947e-d87643c216d1",
      "m1": "همه بار کردند و خود برنشست",
      "m2": "میان از پی بازگشتن ببست",
      "verse_number": 41
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 41
  }
}