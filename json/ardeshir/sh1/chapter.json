{
  "id": "542c67ce-e0eb-4ad7-b73d-ed82939dc9a7",
  "title": "بخش ۱",
  "verses": [
    {
      "id": "c5910e6d-57e4-4f9e-a1c0-6a9a4b125a23",
      "m1": "به بغداد بنشست بر تخت عاج",
      "m2": "به سر برنهاد آن دلفروز تاج",
      "verse_number": 1
    },
    {
      "id": "f34de14e-6bee-48d1-8462-28ef6502731c",
      "m1": "کمر بسته و گرز شاهان به دست",
      "m2": "بیاراسته جایگاه نشست",
      "verse_number": 2
    },
    {
      "id": "28e836e3-0f35-4624-8531-9d5ff590c91c",
      "m1": "شهنشاه خواندند زان پس ورا",
      "m2": "ز گشتاسپ نشناختی کس ورا",
      "verse_number": 3
    },
    {
      "id": "34fe7a9d-030c-4871-92f8-9a3e247c7b12",
      "m1": "چو تاج بزرگی به سر برنهاد",
      "m2": "چنین کرد بر تخت پیروزه یاد",
      "verse_number": 4
    },
    {
      "id": "82387aec-05fe-4cef-b463-15a225e5414b",
      "m1": "که اندر جهان داد گنج منست",
      "m2": "جهان زنده از بخت و رنج منست",
      "verse_number": 5
    },
    {
      "id": "82ded9b5-6e67-453c-96b2-fd85938161c7",
      "m1": "کس این گنج نتواند از من ستد",
      "m2": "بد آید به مردم ز کردار بد",
      "verse_number": 6
    },
    {
      "id": "adbe9f3b-c47c-4a47-a8e7-9af2506d5e8d",
      "m1": "چو خشنود باشد جهاندار پاک",
      "m2": "ندارد دریغ از من این تیره خاک",
      "verse_number": 7
    },
    {
      "id": "1296be3e-1088-4595-8ab8-e8e3521d9747",
      "m1": "جهان سر به سر در پناه منست",
      "m2": "پسندیدن داد راه منست",
      "verse_number": 8
    },
    {
      "id": "a9b1b552-a949-4ba9-a8d7-bb53c4fe7b2d",
      "m1": "نباید که از کارداران من",
      "m2": "ز سرهنگ و جنگی سواران من",
      "verse_number": 9
    },
    {
      "id": "e00bf63a-244a-4f98-b7a4-0405a23a9a84",
      "m1": "بخسپد کسی دل پر از آرزوی",
      "m2": "گر از بنده گر مردم نیکخوی",
      "verse_number": 10
    },
    {
      "id": "31a82ac4-2cc0-44fb-89c3-57730ae61f53",
      "m1": "گشادست بر هرکس این بارگاه",
      "m2": "ز بدخواه وز مردم نیکخواه",
      "verse_number": 11
    },
    {
      "id": "2a901726-6312-4b37-b57e-a519c54b756d",
      "m1": "همه انجمن خواندند آفرین",
      "m2": "که آباد بادا به دادت زمین",
      "verse_number": 12
    },
    {
      "id": "cf77c789-cfad-4a0b-a6fd-6d3e8c3a437d",
      "m1": "فرستاد بر هر سوی لشکری",
      "m2": "که هرجا که باشد ز دشمن سری",
      "verse_number": 13
    },
    {
      "id": "2ce9289e-378b-4f53-90d3-293c648082e6",
      "m1": "سر کینهورشان به راه آورید",
      "m2": "گر آیین شمشیر و گاه آورید",
      "verse_number": 14
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 14
  },
  "order_in_parent": 1
}