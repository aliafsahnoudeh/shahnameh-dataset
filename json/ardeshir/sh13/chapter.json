{
  "id": "9398e31f-72f0-46ad-8e92-d4f2f6991a8c",
  "title": "بخش ۱۳",
  "verses": [
    {
      "id": "04df4aea-c2bc-49d2-a22c-927bbac892e7",
      "m1": "الا ای خریدار مغز سخن",
      "m2": "دلت برگسل زین سرای کهن",
      "verse_number": 1
    },
    {
      "id": "6f76b08d-7279-4c58-bf60-2e83ccb1a2a1",
      "m1": "کجا چون من و چون تو بسیار دید",
      "m2": "نخواهد همی با کسی آرمید",
      "verse_number": 2
    },
    {
      "id": "a50535a3-9711-4c38-9109-86ca4d4e6df6",
      "m1": "اگر شهریاری و گر پیشکار",
      "m2": "تو ناپایداری و او پایدار",
      "verse_number": 3
    },
    {
      "id": "e2865219-0d86-40fe-ae70-72a5420cdbb1",
      "m1": "چه با رنج باشی چه با تاج و تخت",
      "m2": "ببایدت بستن به فرجام رخت",
      "verse_number": 4
    },
    {
      "id": "bb50f9b1-1448-46a1-bcf5-171b5b51bebb",
      "m1": "اگر ز آهنی چرخ بگدازدت",
      "m2": "چو گشتی کهن نیز ننوازدت",
      "verse_number": 5
    },
    {
      "id": "fecb65da-3c1f-4ddf-af7b-e94a277b5f54",
      "m1": "چو سرو دلارای گردد به خم",
      "m2": "خروشان شود نرگسان دژم",
      "verse_number": 6
    },
    {
      "id": "c21bb75d-5a56-4990-aace-d1cf68f95ffc",
      "m1": "همان چهرهٔ ارغوان زعفران",
      "m2": "سبک مردم شاد گردد گران",
      "verse_number": 7
    },
    {
      "id": "db5b149e-4ccc-43d2-812c-d5a70ffc4737",
      "m1": "اگر شهریاری و گر زیردست",
      "m2": "بجز خاک تیره نیابی نشست",
      "verse_number": 8
    },
    {
      "id": "c586b22a-603a-4297-a6ea-9b3852ff56b0",
      "m1": "کجا آن بزرگان با تاج و تخت",
      "m2": "کجا آن سواران پیروزبخت",
      "verse_number": 9
    },
    {
      "id": "f2966c6a-97ef-411c-aa7d-4a1404fa77fe",
      "m1": "کجا آن خردمند کندآوران",
      "m2": "کجا آن سرافراز و جنگی سران",
      "verse_number": 10
    },
    {
      "id": "be7f8566-d55d-45e3-bd07-cb9012664d60",
      "m1": "کجا آن گزیده نیاکان ما",
      "m2": "کجا آن دلیران و پاکان ما",
      "verse_number": 11
    },
    {
      "id": "84fea44e-b007-434e-b3c2-cbb84c9373c7",
      "m1": "همه خاک دارند بالین و خشت",
      "m2": "خنک آنک جز تخم نیکی نکشت",
      "verse_number": 12
    },
    {
      "id": "4287d869-27b1-4595-a3e9-37f303e6295d",
      "m1": "نشان بس بود شهریار اردشیر",
      "m2": "چو از من سخن بشنوی یادگیر",
      "verse_number": 13
    }
  ],
  "poem_info": {
    "meter": "فعولن فعولن فعولن فعل (متقارب مثمن محذوف یا وزن شاهنامه)",
    "poeticForm": "مثنوی",
    "verseCount": 13
  }
}