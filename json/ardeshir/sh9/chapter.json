{
  "id": "fbf0ec95-fa4e-43d5-bc4c-e2a50dfe6b00",
  "title": "بخش ۹",
  "verses": [
    {
      "id": "c8d758e1-8afe-4614-9593-42b75332b0e4",
      "m1": "کنون از خردمندی اردشیر",
      "m2": "سخن بشنو و یک به یک یادگیر",
      "verse_number": 1
    },
    {
      "id": "bf3bd5a2-3925-43c4-8b31-819c6b6754bc",
      "m1": "بکوشید و آیین نیکو نهاد",
      "m2": "بگسترد بر هر سوی مهر و داد",
      "verse_number": 2
    },
    {
      "id": "b0e57288-30e0-4876-81d4-8616da6e7f64",
      "m1": "به درگاه چون خواست لشکر فزون",
      "m2": "فرستاد بر هر سوی رهنمون",
      "verse_number": 3
    },
    {
      "id": "d5266b99-51c1-4572-9ee3-f8eca19113e1",
      "m1": "که تا هرکسی را که دارد پسر",
      "m2": "نماند که بالا کند بیهنر",
      "verse_number": 4
    },
    {
      "id": "4519ac53-1bba-4d5b-b3e7-4b5f8a6a0123",
      "m1": "سواری بیاموزد و رسم جنگ",
      "m2": "به گرز و کمان و به تیر خدنگ",
      "verse_number": 5
    },
    {
      "id": "9225957a-1485-495a-9b79-b75cf0c65cfe",
      "m1": "چو کودک ز کوشش به مردی شدی",
      "m2": "بهر بخششی در بی آهو بدی",
      "verse_number": 6
    },
    {
      "id": "21c90d7b-c8e8-4d28-901c-9a21b2f011f0",
      "m1": "ز کشور به درگاه شاه آمدند",
      "m2": "بدان نامور بارگاه آمدند",
      "verse_number": 7
    },
    {
      "id": "ad486eec-0e66-4520-8234-107b6d79dbd7",
      "m1": "نوشتی عرض نام دیوان اوی",
      "m2": "بیاراستی کاخ و ایوان اوی",
      "verse_number": 8
    },
    {
      "id": "8430d7ba-c276-42bd-9c58-8a67c94a3920",
      "m1": "چو جنگ آمدی نورسیده جوان",
      "m2": "برفتی ز درگاه با پهلوان",
      "verse_number": 9
    },
    {
      "id": "ad596acc-bb83-4c99-baf6-87e01d6db05c",
      "m1": "یکی موبدان را ز کارآگهان",
      "m2": "که بودی خریدار کار جهان",
      "verse_number": 10
    },
    {
      "id": "1a710cda-cde3-4a4f-8b94-bdad0a83b127",
      "m1": "ابر هر هزاری یکی کارجوی",
      "m2": "برفتی نگه داشتی کار اوی",
      "verse_number": 11
    },
    {
      "id": "3a6745b3-468e-4a0f-8ed8-818d53ce3e00",
      "m1": "هرانکس که در جنگ سست آمدی",
      "m2": "به آورد ناتندرست آمدی",
      "verse_number": 12
    },
    {
      "id": "d247631c-5aa7-44c5-922e-b9f730d3af52",
      "m1": "شهنشاه را نامه کردی بران",
      "m2": "هم از بیهنر هم ز جنگآوران",
      "verse_number": 13
    },
    {
      "id": "b42fa318-b2fa-438a-88c1-c8c01eb3f7d3",
      "m1": "جهاندار چون نامه برخواندی",
      "m2": "فرستاده را پیش بنشاندی",
      "verse_number": 14
    },
    {
      "id": "a8150116-2441-4a41-96c4-8cf70b5e6d54",
      "m1": "هنرمند را خلعت آراستی",
      "m2": "ز گنج آنچ پرمایهتر خواستی",
      "verse_number": 15
    },
    {
      "id": "365e37f9-bebe-40b6-af28-f8612158987f",
      "m1": "چو کردی نگاه اندران بیهنر",
      "m2": "نبستی میان جنگ را بیشتر",
      "verse_number": 16
    },
    {
      "id": "dd8ad466-4669-4c3d-bfaa-731d318df203",
      "m1": "چنین تا سپاهش بدانجا رسید",
      "m2": "که پهنای ایشان ستاره ندید",
      "verse_number": 17
    },
    {
      "id": "e189b079-4e51-4a5b-949d-baeb21d091ce",
      "m1": "ازیشان کسی را که بد رایزن",
      "m2": "برافراختندی سرش ز انجمن",
      "verse_number": 18
    },
    {
      "id": "548d7fc8-f8a4-4942-84cd-eb5f407b0eb4",
      "m1": "که هرکس که خشنودی شاه جست",
      "m2": "زمین را به خوان دلیران بشست",
      "verse_number": 19
    },
    {
      "id": "a9bd93c7-77ad-4967-b04c-ccb380a638ea",
      "m1": "بیابد ز من خلعت شهریار",
      "m2": "بود در جهان نام او یادگار",
      "verse_number": 20
    },
    {
      "id": "af6b2d11-6306-4785-9723-0a327553f730",
      "m1": "به لشکر بیاراست گیتی همه",
      "m2": "شبان گشت و پرخاشجویان رمه",
      "verse_number": 21
    },
    {
      "id": "8cb7fdda-ae0e-49db-a168-a29c49f5c6df",
      "m1": "به دیوانش کارآگهان داشتی",
      "m2": "به بیدانشی کار نگذاشتی",
      "verse_number": 22
    },
    {
      "id": "96293cb7-596d-4545-970e-473e40c2eb08",
      "m1": "بلاغت نگه داشتندی و خط",
      "m2": "کسی کو بدی چیره بر یک نقط",
      "verse_number": 23
    },
    {
      "id": "adbf6366-d46f-48bd-9fb8-98fc370b2f03",
      "m1": "چو برداشتی آن سخن رهنمون",
      "m2": "شهنشاه کردیش روزی فزون",
      "verse_number": 24
    },
    {
      "id": "9fbc17f6-801e-4139-803b-9f1de1b5352f",
      "m1": "کسی را که کمتر بدی خط و ویر",
      "m2": "نرفتی به دیوان شاه اردشیر",
      "verse_number": 25
    },
    {
      "id": "21cc8106-96f4-477f-8d36-c70be8709ff6",
      "m1": "سوی کارداران شدندی به کار",
      "m2": "قلمزن بماندی بر شهریار",
      "verse_number": 26
    },
    {
      "id": "7450d851-40f2-4115-a5f5-f3be7e0b05f5",
      "m1": "شناسنده بد شهریار اردشیر",
      "m2": "چو دیدی به درگاه مرد دبیر",
      "verse_number": 27
    },
    {
      "id": "16c856f3-7376-4087-8616-0b15b1475774",
      "m1": "نویسنده گفتی که گنج آگنید",
      "m2": "هم از رای او رنج بپراگنید",
      "verse_number": 28
    },
    {
      "id": "3c4ed3a2-2d95-49ea-af17-50ccb5c9e087",
      "m1": "بدو باشد آباد شهر و سپاه",
      "m2": "همان زیردستان فریادخواه",
      "verse_number": 29
    },
    {
      "id": "165d5adf-34f3-4ae4-a841-4acec99a58eb",
      "m1": "دبیران چو پیوند جان منند",
      "m2": "همه پادشا بر نهان منند",
      "verse_number": 30
    },
    {
      "id": "f8c9de6a-ebe7-4893-9368-3517d5e64d9b",
      "m1": "چو رفتی سوی کشور کاردار",
      "m2": "بدو شاه گفتی درم خوار دار",
      "verse_number": 31
    },
    {
      "id": "bea109b5-49ce-40c1-8058-1078c40c3ac2",
      "m1": "نباید که مردم فروشی به گنج",
      "m2": "که برکس نماند سرای سپنج",
      "verse_number": 32
    },
    {
      "id": "ba5c3b25-58df-4b0d-a49b-25b5217a89e5",
      "m1": "همه راستی جوی و فرزانگی",
      "m2": "ز تو دور باد آز و دیوانگی",
      "verse_number": 33
    },
    {
      "id": "f4ddb51c-596b-483e-ab96-bf16b0aff502",
      "m1": "ز پیوند و خویشان مبر هیچکس",
      "m2": "سپاه آنچ من یار دادمت بس",
      "verse_number": 34
    },
    {
      "id": "77968fc1-0816-4e46-94be-e14fd98bda7a",
      "m1": "درم بخش هر ماه درویش را",
      "m2": "مده چیز مرد بداندیش را",
      "verse_number": 35
    },
    {
      "id": "d2c0b003-ba74-4f4c-b830-330bf7994977",
      "m1": "اگر کشور آباد داری به داد",
      "m2": "بمانی تو آباد وز داد شاد",
      "verse_number": 36
    },
    {
      "id": "3eaa35f5-47c6-4944-a485-a962d378c13e",
      "m1": "و گر هیچ درویش خسپد به بیم",
      "m2": "همی جان فروشی به زر و به سیم",
      "verse_number": 37
    },
    {
      "id": "86b38bdd-2d28-46be-b876-69971c46460d",
      "m1": "هرانکس که رفتی به درگاه شاه",
      "m2": "به شایسته کاری و گر دادخواه",
      "verse_number": 38
    },
    {
      "id": "813fc574-a159-4518-9960-cad42bb7c382",
      "m1": "بدندی به سر استواران اوی",
      "m2": "بپرسیدن از کارداران اوی",
      "verse_number": 39
    },
    {
      "id": "26e902c6-3cf5-44b7-9983-992978cbb077",
      "m1": "که دادست ازیشان و بگرفت چیز",
      "m2": "وزیشان که خسپد به تیمار نیز",
      "verse_number": 40
    },
    {
      "id": "500d9403-7678-4a47-96f7-25faf89254fe",
      "m1": "دگر آنک در شهر دانا کهاند",
      "m2": "گر از نیستی ناتوانا کهاند",
      "verse_number": 41
    },
    {
      "id": "4b60ec14-eb2f-49f7-871b-82d0270fc909",
      "m1": "دگر کیست آنک از در پادشاست",
      "m2": "جهاندیده پیرست و گر پارساست",
      "verse_number": 42
    },
    {
      "id": "c36b99c3-bdc6-456e-8e96-e0723a65769a",
      "m1": "شهنشاه گوید که از رنج من",
      "m2": "مبادا کسی شاد بیگنج من",
      "verse_number": 43
    },
    {
      "id": "2e8999e3-1814-4862-b991-83f5bc2cb5d5",
      "m1": "مگر مرد با دانش و یادگیر",
      "m2": "چه نیکوتر از مرد دانا و پیر",
      "verse_number": 44
    },
    {
      "id": "3f65341f-7cc3-44aa-bd64-4c76e6395b5e",
      "m1": "جهاندیدگان را همه خواستار",
      "m2": "جوان و پسندیده و بردبار",
      "verse_number": 45
    },
    {
      "id": "a5808d8a-e40a-4618-9536-1fa1e277fabf",
      "m1": "جوانان دانا و دانشپذیر",
      "m2": "سزد گر نشینند بر جای پیر",
      "verse_number": 46
    },
    {
      "id": "2c80b0d4-283a-4709-a314-6d8f38b72134",
      "m1": "چو لشکرش رفتی به جایی به جنگ",
      "m2": "خرد یار کردی و رای و درنگ",
      "verse_number": 47
    },
    {
      "id": "45f52c8d-6cf7-4bcb-a78c-cd34ee5a47dd",
      "m1": "فرستادهای برگزیدی دبیر",
      "m2": "خردمند و با دانش و یادگیر",
      "verse_number": 48
    },
    {
      "id": "17bc5e36-b12e-4605-8675-82cb09a303a4",
      "m1": "پیامی به دادی به آیین و چرب",
      "m2": "بدان تا نباشد به بیداد حرب",
      "verse_number": 49
    },
    {
      "id": "cdc4d833-34c5-4033-bf58-19ac3320e7c1",
      "m1": "فرستاده رفتی بر دشمنش",
      "m2": "که بشناختی راز پیراهنش",
      "verse_number": 50
    },
    {
      "id": "fbd085dc-e169-49c9-bb29-72150427d426",
      "m1": "شنیدی سخن گر خرد داشتی",
      "m2": "غم و رنج بد را به بد داشتی",
      "verse_number": 51
    },
    {
      "id": "45868a76-b08b-4203-a70e-88b9768e5da8",
      "m1": "بدان یافت او خلعت شهریار",
      "m2": "همان عهد و منشور با گوشوار",
      "verse_number": 52
    },
    {
      "id": "eaff167d-ae60-481e-b81d-309aec40de36",
      "m1": "وگر تاب بودی به سرش اندرون",
      "m2": "به دل کین و اندر جگر جوش خون",
      "verse_number": 53
    },
    {
      "id": "22ebdb8b-f3bf-4aee-915b-7ca7a1f4175f",
      "m1": "سپه را بدادی سراسر درم",
      "m2": "بدان تا نباشند یک تن دژم",
      "verse_number": 54
    },
    {
      "id": "02f24480-71d0-4b97-b8a7-373c66959b9d",
      "m1": "یکی پهلوان خواستی نامجوی",
      "m2": "خردمند و بیدار و آرامجوی",
      "verse_number": 55
    },
    {
      "id": "7f6875fe-112d-45c9-b767-3c6c5cf39b33",
      "m1": "دبیری به آیین و با دستگاه",
      "m2": "که دارد ز بیداد لشکر نگاه",
      "verse_number": 56
    },
    {
      "id": "9b54bb3c-3648-44b0-be82-7c167862b83c",
      "m1": "وزان پس یکی مرد بر پشت پیل",
      "m2": "نشستی که رفتی خروشش دو میل",
      "verse_number": 57
    },
    {
      "id": "cd5fb06e-92e3-41d5-8f1c-cd0a1c5f5efe",
      "m1": "زدی بانگ کای نامداران جنگ",
      "m2": "هرانکس که دارد دل و نام و ننگ",
      "verse_number": 58
    },
    {
      "id": "96fe4ae3-e3e4-4728-ad0f-427170fb616e",
      "m1": "نباید که بر هیچ درویش رنج",
      "m2": "رسد گر بر آنکس بود نام و گنج",
      "verse_number": 59
    },
    {
      "id": "c7f2aab4-a227-41ee-b85b-61b230f72813",
      "m1": "به هر منزلی در خورید و دهید",
      "m2": "بران زیردستان سپاسی نهید",
      "verse_number": 60
    },
    {
      "id": "4c605735-4a54-4b5b-97e3-652c6a6a8d1d",
      "m1": "به چیز کسان کس میازید دست",
      "m2": "هرانکس که او هست یزدانپرست",
      "verse_number": 61
    },
    {
      "id": "93bdfb13-c68e-4e84-bdc4-3bb768e9fdf5",
      "m1": "به دشمن هرانکس که بنمود پشت",
      "m2": "شود زان سپس روزگارش درشت",
      "verse_number": 62
    },
    {
      "id": "3ff2bb26-5486-40b7-89aa-4094550b000c",
      "m1": "اگر دخمه باشد به چنگال اوی",
      "m2": "وگر بند ساید بر و یال اوی",
      "verse_number": 63
    },
    {
      "id": "abf50971-d47b-40f3-91df-56f2150f443d",
      "m1": "ز دیوان دگر نام او کرده پاک",
      "m2": "خورش خاک و رفتنش بر تیره خاک",
      "verse_number": 64
    },
    {
      "id": "33b15ba5-4c11-44f7-a8ad-925783a683b5",
      "m1": "به سالار گفتی که سستی مکن",
      "m2": "همان تیز و پیش دستی مکن",
      "verse_number": 65
    },
    {
      "id": "54284c93-0a39-470d-809d-b1b7742482b6",
      "m1": "همیشه به پیش سپه دار پیل",
      "m2": "طلایه پراگنده بر چار میل",
      "verse_number": 66
    },
    {
      "id": "c1b74da3-c79b-48e2-a9ab-71c7f00ca37a",
      "m1": "نخستین یکی گرد لشکر به گرد",
      "m2": "چو پیش آیدت روز ننگ و نبرد",
      "verse_number": 67
    },
    {
      "id": "3deeb62a-fe17-4b41-a49d-0b5b5d27a665",
      "m1": "به لشکر چنین گوی کاین خود کیند",
      "m2": "بدین رزمگاه اندرون برچیند",
      "verse_number": 68
    },
    {
      "id": "d164b79d-af54-4857-b0e6-f8a7da70048a",
      "m1": "از ایشان صد اسپ افگن از ما یکی",
      "m2": "همان صد به پیش یکی اندکی",
      "verse_number": 69
    },
    {
      "id": "a32ba7bb-edca-44f9-9591-f40115951e0e",
      "m1": "شما را همه پاک برنا و پیر",
      "m2": "ستانم همه خلعت از اردشیر",
      "verse_number": 70
    },
    {
      "id": "d1fa4797-e550-43e6-a366-d3c67b69bf93",
      "m1": "چو اسپ افگند لشکر از هر دو روی",
      "m2": "نباید که گردان پرخاشجوی",
      "verse_number": 71
    },
    {
      "id": "16b1271c-e034-4d7e-9699-954eb33edf10",
      "m1": "بیاید که ماند تهی قلب گاه",
      "m2": "وگر چند بسیار باشد سپاه",
      "verse_number": 72
    },
    {
      "id": "52506ab7-3453-4d86-a2d2-3ae43a3c9e68",
      "m1": "چنان کن که با میمنه میسره",
      "m2": "بکوشند جنگآوران یکسره",
      "verse_number": 73
    },
    {
      "id": "4db15eed-0b8e-426d-b475-a8f5b028dd34",
      "m1": "همان نیز با میسره میمنه",
      "m2": "بکوشند و دلها همه بر بنه",
      "verse_number": 74
    },
    {
      "id": "25b04e0d-cdc5-4ec8-8d6e-ed249d4ad21c",
      "m1": "بود لشکر قلب بر جای خویش",
      "m2": "کس از قلبگه نگسلد پای خویش",
      "verse_number": 75
    },
    {
      "id": "3862e1c5-7eae-4f49-b227-30c73fad86bd",
      "m1": "وگر قلب ایشان بجنبد ز جای",
      "m2": "تو با لشکر از قلبگاه اندر آی",
      "verse_number": 76
    },
    {
      "id": "1c6f573a-fe52-41fd-837d-750920316855",
      "m1": "چو پیروز گردی ز کس خون مریز",
      "m2": "که شد دشمن بدکنش در گریز",
      "verse_number": 77
    },
    {
      "id": "0beff4b2-bdc7-4798-9c2d-fabfaf23848a",
      "m1": "چو خواهد ز دشمن کسی زینهار",
      "m2": "تو زنهارده باش و کینه مدار",
      "verse_number": 78
    },
    {
      "id": "09235c32-7ea6-4610-b7c4-e29345003566",
      "m1": "چو تو پشت دشمن ببینی به چیز",
      "m2": "مپرداز و مگذر هم از جای نیز",
      "verse_number": 79
    },
    {
      "id": "8b672ba0-13d5-4494-8a45-f2be0509fb83",
      "m1": "نباید که ایمن شوید از کمین",
      "m2": "سپه باشد اندر در و دشت کین",
      "verse_number": 80
    },
    {
      "id": "6464ceef-eb82-42e8-b8a7-e90b285ddf46",
      "m1": "هرآنگه که از دشمن ایمن شوی",
      "m2": "سخن گفتن کس همی نشنوی",
      "verse_number": 81
    },
    {
      "id": "fea90a9b-1b1c-48bf-85fd-f9168eac156e",
      "m1": "غنیمت بدان بخش کو جنگ جست",
      "m2": "به مردی دل از جان شیرین بشست",
      "verse_number": 82
    },
    {
      "id": "4fdaaab2-8874-4617-948d-086c30a4db9b",
      "m1": "هرانکس که گردد به دستت اسیر",
      "m2": "بدین بارگاه آورش ناگزیر",
      "verse_number": 83
    },
    {
      "id": "ab86f900-85ac-405c-a2a5-b6dfc256741b",
      "m1": "من از بهر ایشان یکی شارستان",
      "m2": "برآرم به بومی که بد خارستان",
      "verse_number": 84
    },
    {
      "id": "31d94b42-b029-4c93-a216-a5f0722dada7",
      "m1": "ازین پندها هیچ گونه مگرد",
      "m2": "چو خواهی که مانی تو بیرنج و درد",
      "verse_number": 85
    },
    {
      "id": "3a5fb856-3168-40cd-b06d-9bd475636583",
      "m1": "به پیروزی اندر به یزدان گرای",
      "m2": "که او باشدت بیگمان رهنمای",
      "verse_number": 86
    },
    {
      "id": "aefd6399-2d72-453f-b221-9786ce34791e",
      "m1": "ز جایی که آمد فرستادهای",
      "m2": "ز ترکی و رومی و آزادهای",
      "verse_number": 87
    },
    {
      "id": "d0255f5f-dd79-4655-83e6-d93c8bba8b6e",
      "m1": "ازو مرزبان آگهی داشتی",
      "m2": "چنین کارها خوار نگذاشتی",
      "verse_number": 88
    },
    {
      "id": "d2e23853-b7f4-450a-8afb-f3891365c1d4",
      "m1": "بره بر بدی خان او ساخته",
      "m2": "کنارنگ زان کار پرداخته",
      "verse_number": 89
    },
    {
      "id": "0615a964-92d9-4d8c-974b-a2ca9bf19443",
      "m1": "ز پوشیدنیها و از خوردنی",
      "m2": "نیازش نبودی به گستردنی",
      "verse_number": 90
    },
    {
      "id": "72b846bc-ca26-4ba9-96a1-345c18ffd8ab",
      "m1": "چو آگه شدی زان سخن کاردار",
      "m2": "که او بر چه آمد بر شهریار",
      "verse_number": 91
    },
    {
      "id": "5052a419-1bbd-4ffa-8863-7e0ed4add957",
      "m1": "هیونی سرافراز و مردی دبیر",
      "m2": "برفتی به نزدیک شاه اردشیر",
      "verse_number": 92
    },
    {
      "id": "4753d958-da9a-426b-be27-d0a51d06d461",
      "m1": "بدان تا پذیره شدندی سپاه",
      "m2": "بیاراستی تخت پیروز شاه",
      "verse_number": 93
    },
    {
      "id": "1a7b8755-c6c9-4e13-a7a8-c134043962ee",
      "m1": "کشیدی پرستنده هر سو رده",
      "m2": "همه جامههاشان به زر آژده",
      "verse_number": 94
    },
    {
      "id": "58efbaad-083a-4830-8ac7-23959d0a8497",
      "m1": "فرستاده را پیش خود خواندی",
      "m2": "به نزدیکی تخت بنشاندی",
      "verse_number": 95
    },
    {
      "id": "7bdf153a-b337-4d06-86ec-1dd5bb6087ee",
      "m1": "به پرسش گرفتی همه راز اوی",
      "m2": "ز نیک و بد و نام و آواز اوی",
      "verse_number": 96
    },
    {
      "id": "7af1e64d-3c99-4713-8521-decbc1ff930b",
      "m1": "ز داد و ز بیداد وز کشورش",
      "m2": "ز آیین وز شاه وز لشکرش",
      "verse_number": 97
    },
    {
      "id": "8da62c4c-c0b4-4f84-9924-910270d712eb",
      "m1": "به ایوانش بردی فرستادهوار",
      "m2": "بیاراستی هرچ بودی به کار",
      "verse_number": 98
    },
    {
      "id": "7a52b7e6-1e6b-40de-b301-b8c72eef53ad",
      "m1": "وزان پس به خوان و میش خواندی",
      "m2": "بر تخت زرینش بنشاندی",
      "verse_number": 99
    },
    {
      "id": "d7abdfdc-302e-450e-ae53-3bae152da360",
      "m1": "به نخچیر بردیش با خویشتن",
      "m2": "شدی لشکر بیشمار انجمن",
      "verse_number": 100
    },
    {
      "id": "bc661c33-787d-4fd3-bea4-5107dd51d629",
      "m1": "کسی کردنش را فرستادهوار",
      "m2": "بیاراستی خلعت شهریار",
      "verse_number": 101
    },
    {
      "id": "45d33380-1d25-49ee-a10e-2bac638e18b8",
      "m1": "به هر سو فرستاد پس موبدان",
      "m2": "بیآزار و بیداردل بخردان",
      "verse_number": 102
    },
    {
      "id": "66faeb89-4429-49ca-aa47-3340b4917564",
      "m1": "که تا هر سوی شهرها ساختند",
      "m2": "بدین نیز گنجی بپرداختند",
      "verse_number": 103
    },
    {
      "id": "58896c88-4dad-4b57-a60b-f00afaf0854d",
      "m1": "بدان تا کسی را که بیخانه بود",
      "m2": "نبودش نوا بخت بیگانه بود",
      "verse_number": 104
    },
    {
      "id": "70489991-69ac-4b63-abe0-35f5b144dfc5",
      "m1": "همان تا فراوان شود زیردست",
      "m2": "خورش ساخت با جایگاه نشست",
      "verse_number": 105
    },
    {
      "id": "03f8de2f-a6dd-42a5-8fbc-99fce24fe7e5",
      "m1": "ازو نام نیکی بود در جهان",
      "m2": "چه بر آشکار و چه اندر نهان",
      "verse_number": 106
    },
    {
      "id": "75a816cc-cbda-4af8-bac5-d32595717e53",
      "m1": "چو او در جهان شهریاری نبود",
      "m2": "پس از مرگ او یادگاری نبود",
      "verse_number": 107
    },
    {
      "id": "2cdf8974-4675-4a23-b45b-268bcbd26ea2",
      "m1": "منم ویژه زنده کن نام اوی",
      "m2": "مبادا جز از نیکی انجام اوی",
      "verse_number": 108
    },
    {
      "id": "ef0f0d1d-2e4b-4d05-bb79-c61112495d2e",
      "m1": "فراوان سخن در نهان داشتی",
      "m2": "به هر جای کارآگهان داشتی",
      "verse_number": 109
    },
    {
      "id": "49541ad5-7338-47b8-83d0-e131f04efe28",
      "m1": "چو بیمایه گشتی یکی مایهدار",
      "m2": "ازان آگهی یافتی شهریار",
      "verse_number": 110
    },
    {
      "id": "c970ef6f-d63b-43e1-bd49-a0e462148d65",
      "m1": "چو بایست برساختی کار اوی",
      "m2": "نماندی چنان تیره بازار اوی",
      "verse_number": 111
    },
    {
      "id": "9ca92548-5785-4576-af13-2879819f1967",
      "m1": "زمین برومند و جای نشست",
      "m2": "پرستیدن مردم زیردست",
      "verse_number": 112
    },
    {
      "id": "9bf42984-33a0-44a1-9694-a26df6351847",
      "m1": "بیاراستی چون ببایست کار",
      "m2": "نگشتی نهانش به کس آشکار",
      "verse_number": 113
    },
    {
      "id": "026b75c4-ca95-46d7-a54e-100efd74640c",
      "m1": "تهیدست را مایه دادی بسی",
      "m2": "بدو شاد کردی دل هرکسی",
      "verse_number": 114
    },
    {
      "id": "1704961a-06ad-4ca0-a792-8aab9155ed1e",
      "m1": "همان کودکان را به فرهنگیان",
      "m2": "سپردی چو بودی ورا هنگ آن",
      "verse_number": 115
    },
    {
      "id": "02ce93af-a1b7-4d43-8ba2-0f22adde577e",
      "m1": "به هر برزنی در دبستان بدی",
      "m2": "همان جای آتشپرستان بدی",
      "verse_number": 116
    },
    {
      "id": "7a93fe3b-ed03-4bcd-86d2-e5c2b45ce7bc",
      "m1": "نماندی که بودی کسی را نیاز",
      "m2": "نگه داشتی سختی خویش راز",
      "verse_number": 117
    },
    {
      "id": "b3b3b711-3ae4-45b6-8fb9-4d985bbce6cd",
      "m1": "به میدان شدی بامداد پگاه",
      "m2": "برفتی کسی کو بدی دادخواه",
      "verse_number": 118
    },
    {
      "id": "ab4df19c-467b-4c1f-b580-abe465721ea2",
      "m1": "نچستی بداد اندر آزرم کس",
      "m2": "چه کهتر چه فرزند فریادرس",
      "verse_number": 119
    },
    {
      "id": "6ba7bf9d-9a43-452f-8959-083ab7202357",
      "m1": "چه کهتر چه مهتر به نزدیک اوی",
      "m2": "نجستی همی رای تاریک اوی",
      "verse_number": 120
    },
    {
      "id": "55f7cb7d-a039-4e71-b102-689159db114a",
      "m1": "ز دادش جهان یکسر آباد کرد",
      "m2": "دل زیردستان به خود شاد کرد",
      "verse_number": 121
    },
    {
      "id": "4ae01d69-8d14-445a-82b9-0f4063f14bb5",
      "m1": "جهاندار چون گشت با داد جفت",
      "m2": "زمانه پی او نیارد نهفت",
      "verse_number": 122
    },
    {
      "id": "c3a35768-1a8d-499a-b13a-43202a87ef14",
      "m1": "فرستاده بودی به گرد جهان",
      "m2": "خردمند و بیدار کارآگهان",
      "verse_number": 123
    },
    {
      "id": "868d5ea6-3c6c-461b-9345-be5df52e72da",
      "m1": "به جایی که بودی زمینی خراب",
      "m2": "وگر تنگ بودی به رود اندر آب",
      "verse_number": 124
    },
    {
      "id": "caecfb9a-4ea4-4c33-83a1-c0790078ff4b",
      "m1": "خراج اندر آن بوم برداشتی",
      "m2": "زمین کسان خوار نگذاشتی",
      "verse_number": 125
    },
    {
      "id": "6b801161-5dc4-4865-9bde-5580e5c30136",
      "m1": "گر ایدونک دهقان بدی تنگ دست",
      "m2": "سوی نیستی گشته کارش ز هست",
      "verse_number": 126
    },
    {
      "id": "f509f206-de01-4226-ab67-da61f77ca1c1",
      "m1": "بدادی ز گنج آلت و چارپای",
      "m2": "نماندی که پایش برفتی ز جای",
      "verse_number": 127
    },
    {
      "id": "ea0b1ddd-7de7-4fd9-be50-953876b889ac",
      "m1": "ز دانا سخن بشنو ای شهریار",
      "m2": "جهان را برین گونه آباد دار",
      "verse_number": 128
    },
    {
      "id": "fd8a5ce4-f652-4448-960b-28b3a8775b9c",
      "m1": "چو خواهی که آزاد باشی ز رنج",
      "m2": "بیآزار و بیرنج آگنده گنج",
      "verse_number": 129
    },
    {
      "id": "65e5fd0f-1713-4f67-89c2-27ff57899545",
      "m1": "بیآزاری زیردستان گزین",
      "m2": "بیابی ز هرکس به داد آفرین",
      "verse_number": 130
    }
  ],
